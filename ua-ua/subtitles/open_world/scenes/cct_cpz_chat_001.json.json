        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чому він раптом згадав Моріса?",
              "maleVariant": "",
              "stringId": "1963272610757255168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Моріс?.. Не пригадую такого.",
              "maleVariant": "",
              "stringId": "1965118425461932032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А-а-а, точно! Ти ж із нами лише місяць. До того часу, Моріса вже забрали в дурку.",
              "maleVariant": "",
              "stringId": "1965118740336721920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Моріс був… Іще тим щуром. Постійно стежив за нами, стукав керівництву…",
              "maleVariant": "",
              "stringId": "1965119318395699200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щоб не засинати, він ковтав таблетки, як дурний… І врешті виявилося, що в нього смертельне безсоння.",
              "maleVariant": "",
              "stringId": "1965119740443344896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він і не підозрював, що таблетки лише прискорили хворобу. Коротше, упав у глибокий сон на минулому тижні.",
              "maleVariant": "",
              "stringId": "1965120148398129152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нарешті — козел їбаний… Полотном дорога.",
              "maleVariant": "",
              "stringId": "1965120546907340800"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}