        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«Електронного вбивцю» можна описати лиш одним словом — шедевр. Я до останньої хвилини не могла нічого допетрати…",
              "maleVariant": "",
              "stringId": "1831685761031331840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А потім — БАМ! І все стало на свої місця!",
              "maleVariant": "",
              "stringId": "1831687367449763840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Героїню зламали через її застарілу модель оптики!",
              "maleVariant": "",
              "stringId": "1831687527269523456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Трошки жахів розбавити з дрібкою наукової фантастики — виходить шикарний кінококтейль.",
              "maleVariant": "",
              "stringId": "1831687823622266880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І жодного трупа там не було! Бо той мережник насилав на неї галюцинації.",
              "maleVariant": "",
              "stringId": "1831687973291810816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зрештою вона збожеволіла та повісилася.",
              "maleVariant": "",
              "stringId": "1965082878785863680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То ж мораль проста: варто стежити за оновленнями хрому. А ти, до речі, коли востаннє оновлювалася, га? Ха-ха.",
              "maleVariant": "",
              "stringId": "1965083077059002368"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}