  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, рада тебе бачити.",
              "maleVariant": "",
              "stringId": "1697943261782384640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Не відмовлюся.",
              "maleVariant": "",
              "stringId": "1697944056284225540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Одне можна.",
              "maleVariant": "",
              "stringId": "1697945210154033152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та ні, дякую.",
              "maleVariant": "",
              "stringId": "1697945291003437056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Ні, дякую.",
              "maleVariant": "",
              "stringId": "1697945770798260224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я хочу взяти участь у церемонії.",
              "maleVariant": "",
              "stringId": "1697948702516092932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я б хотіла взяти участь у церемонії, але… щоб усе було як годиться, згідно з вашими традиціями.",
              "maleVariant": "Я б хотів узяти участь у церемонії, але… щоб усе було як годиться, згідно з вашими традиціями.",
              "stringId": "1697949664689430528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я можу вам допомогти.",
              "maleVariant": "",
              "stringId": "1697949734381985792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Oye\" b=\"\" a=\". Сідай, сідай. Хочеш пива?\"/>",
              "maleVariant": "",
              "stringId": "1702514181381464064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Випити]",
              "maleVariant": "",
              "stringId": "1702514922464649216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я хочу допомогти.",
              "maleVariant": "",
              "stringId": "1722525784814297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Salud.\" t=\"Будьмо.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1825605937628237824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"No hay bronca.\" t=\"Як скажеш.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1825607631439187968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бувало й ліпше.",
              "maleVariant": "",
              "stringId": "1864865408376164356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тримаюся.",
              "maleVariant": "",
              "stringId": "1864865530866618368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чесно кажучи, бувало й ліпше.",
              "maleVariant": "",
              "stringId": "1864866024754302976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Наче якось тримаюся.",
              "maleVariant": "",
              "stringId": "1864866333522186240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що не так?",
              "maleVariant": "",
              "stringId": "1864871197220327424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тієї ночі, коли… коли не стало Джекі… Скажімо так, відтоді все стало набагато важче.",
              "maleVariant": "",
              "stringId": "1864871267181318144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Yo sé cómo te sientes. Todos lo sabemos.\" t=\"Я тебе розумію. Усі ми розуміємо.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864871351604269056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якось.",
              "maleVariant": "",
              "stringId": "1864871887401439232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Nuestra casa es tu casa.\" b=\"Ти б хоч раз заїхала на вечерю. \" a=\"\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"Nuestra casa es tu casa.\" b=\"Ти б хоч раз заїхав на вечерю. \" a=\"\"/>",
              "stringId": "1864871950198558720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм, ну добре, проїхали.",
              "maleVariant": "",
              "stringId": "1864872077336301568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зате в тебе чисте серце, дитя моє.",
              "maleVariant": "",
              "stringId": "1864881922038878208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ох, не турбуйся, треба просто щось символічне. Пошукай щось у його гаражі: там усе його минуле.",
              "maleVariant": "",
              "stringId": "1864881973729480704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У його гаражі?",
              "maleVariant": "",
              "stringId": "1864883745504153604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У Джекі був гараж?",
              "maleVariant": "",
              "stringId": "1864885510282727424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ох, нема за що, Ві. Тобі дякую за твою люб’язність.",
              "maleVariant": "",
              "stringId": "1864886540772888576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Раніше він тримав там свій мотоцикл, але останнім часом повністю туди переїхав. Там ти точно що-небудь знайдеш.",
              "maleVariant": "",
              "stringId": "1864886962216554496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви не проти, що я чіпатиму його речі?",
              "maleVariant": "",
              "stringId": "1864887180974673920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"No, mija.\" b=\"\" a=\". Він помер. А похорони — то для живих, а не для мертвих. <Rich color=\\\"MainColors.ActiveBlue\\\">Toma</>, ось ключ.\"/>",
              "maleVariant": "",
              "stringId": "1864887304723419136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ahí te veo.\" t=\"До зустрічі.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864889318509432832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давно ми з тобою не бачилися. Як у тебе справи?",
              "maleVariant": "",
              "stringId": "1896490727361007616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тут якраз готую офренду. Ще стільки всього треба зробити…",
              "maleVariant": "",
              "stringId": "1924407058265731072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Gracias, mi cielo\" b=\"\" a=\", але я сама впораюся.\"/>",
              "maleVariant": "",
              "stringId": "1926981639971205120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Буде достатньо, якщо ти просто прийдеш попрощатися.",
              "maleVariant": "",
              "stringId": "1926985406405865472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але в мене нема нічого, що можна було б покласти до його домовини для вшанування пам’яті…",
              "maleVariant": "",
              "stringId": "1927026960986206208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але?..",
              "maleVariant": "",
              "stringId": "1928580049748815872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую, мамо Веллз.",
              "maleVariant": "",
              "stringId": "1928581776711544832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що таке офренда?",
              "maleVariant": "",
              "stringId": "1934506005374103552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Офренда? А що це таке?",
              "maleVariant": "",
              "stringId": "1934506268591845376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це коли друзі та рідні збираються разом, щоби вшанувати пам’ять за покійним, згадати все хороше… І принести те, що він любив та що може йому знадобитися надалі.",
              "maleVariant": "",
              "stringId": "1934507718629847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Думаю, Джекі точно в першу чергу попросив би пива.",
              "maleVariant": "",
              "stringId": "1934508314640445440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хе-хе-хе, ну, пиво точно буде.",
              "maleVariant": "",
              "stringId": "1934526562161324032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гараж на Республіканському бульварі.",
              "maleVariant": "",
              "stringId": "1934545599184433152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Узяти ключ і підвестися] Дякую.",
              "maleVariant": "",
              "stringId": "1943264135333453824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Треба прибрати звідси більярдний стіл.",
              "maleVariant": "",
              "stringId": "1976395134477742080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Серйозно?.. Він доволі важкий.",
              "maleVariant": "",
              "stringId": "1976445675203407872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То й що?",
              "maleVariant": "",
              "stringId": "1976446243716149248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, просто посунути його до стінки кудись?",
              "maleVariant": "",
              "stringId": "1976446577716965376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так, це я.",
              "maleVariant": "",
              "stringId": "1983628376209567744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Sí\" b=\"Це Монтсеррат Веллз. \" a=\", з «Ель Койоте».\"/>",
              "maleVariant": "",
              "stringId": "1983631029392060420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Vamos, responde, carajo.\" t=\"Ну, відповідай уже…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1983631138360078340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це я. Ми готуємо офренду по Джекі.",
              "maleVariant": "",
              "stringId": "1983631263484555268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тобі дзвонила, а ти й не відповідаєш, і не передзвонюєш.",
              "maleVariant": "Я тобі дзвонила, а ти й трубку не береш, і не передзвонив ані разу.",
              "stringId": "1988173848277590016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я на офренді. Мені треба вшанувати свого сина.",
              "maleVariant": "",
              "stringId": "1998391940310036480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так, вважай, що тебе запрошено.",
              "maleVariant": "",
              "stringId": "1998392346553544704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу. Побачимося.",
              "maleVariant": "",
              "stringId": "1998392598832541696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У мене особливе замовлення.",
              "maleVariant": "",
              "stringId": "1998395163884318720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Рівно дванадцять ящиків пива. Так, десять звичайного та два імбирного.",
              "maleVariant": "",
              "stringId": "1998395575093882880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Sí\" b=\"\" a=\", лимони та лайми теж.\"/>",
              "maleVariant": "",
              "stringId": "1998396004741607424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Текіли… ох, похрін, теж дванадцять ящиків…",
              "maleVariant": "",
              "stringId": "1998396724970713088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм…",
              "maleVariant": "",
              "stringId": "1998405380252561408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага… Не можеш.",
              "maleVariant": "",
              "stringId": "1998408648806559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розумію.",
              "maleVariant": "",
              "stringId": "1998408909239283712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Pfff. Bai.\" t=\"Пф-ф. Бувай.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1998409164420739072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ух-х… Яка важка… паскуда…",
              "maleVariant": "",
              "stringId": "2238924446696128512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ох-х, уф… Давай уже, блядото…",
              "maleVariant": "",
              "stringId": "2238983336569782272"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}