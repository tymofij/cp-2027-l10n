    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розкажи мені про Ренді. Який він?",
              "maleVariant": "",
              "stringId": "1338559622542442496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дрібний злодюжка, наркоман, тікає з дому. Отакий.",
              "maleVariant": "",
              "stringId": "1396774671519178752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гаразд, питай…",
              "maleVariant": "",
              "stringId": "1401040175171756032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розкажеш більше про Ренді?",
              "maleVariant": "",
              "stringId": "1401042845500563456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Батька Ренді ти не згадував.",
              "maleVariant": "",
              "stringId": "1401045522640920576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А де батько Ренді?",
              "maleVariant": "",
              "stringId": "1401046087731109888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дядько поліціянт бачить у тобі тільки найліпше.",
              "maleVariant": "",
              "stringId": "1401053490375680000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Є люди, які завжди знайдуть проблеми на дупу. Ренді якраз такий.",
              "maleVariant": "",
              "stringId": "1401053747838836736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Уже кілька років як його завалили. Війна банд.",
              "maleVariant": "",
              "stringId": "1401057441661706240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мені просто… Просто важко говорити про деякі речі.",
              "maleVariant": "",
              "stringId": "1537312397961904128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не боїшся, що Явен доповість про тебе своєму керівництву?",
              "maleVariant": "",
              "stringId": "1780929152491163648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вважаєш, Явен нам, зрештою, допоможе?",
              "maleVariant": "",
              "stringId": "1780930867542069248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Домовились. Знаходимо Ренді, і ти ведеш нас кудись на кілька кухлів.",
              "maleVariant": "",
              "stringId": "1780931966516826112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зосередьмося на пошуку Ренді.",
              "maleVariant": "",
              "stringId": "1780932008661192704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зосередьмося на пошуку Ренді.",
              "maleVariant": "",
              "stringId": "1780932115615944708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тоді з тебе пиво.",
              "maleVariant": "",
              "stringId": "1780932497180168192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, варто пошукати помешкання Гарріса?",
              "maleVariant": "",
              "stringId": "1782223670917337088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ха, зразкова рольова модель.",
              "maleVariant": "",
              "stringId": "1782262268496580608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чому б нам не спробувати знайти квартиру Гарріса?",
              "maleVariant": "",
              "stringId": "1782383506447536132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І взагалі ми намагаємося. Я хочу знайти якийсь зв’язок Гарріса та Ренді.",
              "maleVariant": "",
              "stringId": "1782383506447536144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сумнівний шлях.",
              "maleVariant": "",
              "stringId": "1782383506447536156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Найліпший із наявних.",
              "maleVariant": "",
              "stringId": "1782383506464313344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тебе турнули з поліції…",
              "maleVariant": "",
              "stringId": "1782394969077714944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти не казав, що тебе звільнили.",
              "maleVariant": "",
              "stringId": "1782400463632732160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Утратив значок і навіть мені не сказав. Чому?",
              "maleVariant": "",
              "stringId": "1782487452558168064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То тебе турнули з поліції…",
              "maleVariant": "",
              "stringId": "1782487534045106180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ем, відсторонили. Надто голосно протестував проти замовчування справи смерті мера.",
              "maleVariant": "",
              "stringId": "1782489968704360448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіть не питатиму про твоє до того ставлення.",
              "maleVariant": "",
              "stringId": "1782490296598269952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую.",
              "maleVariant": "",
              "stringId": "1782491664813789184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я… Не знаю.",
              "maleVariant": "",
              "stringId": "1782493206992261120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти іноді як йолоп поводишся.",
              "maleVariant": "",
              "stringId": "1782493312353177600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми були добрими друзями. Навряд вона це зробить.",
              "maleVariant": "",
              "stringId": "1783402570485731328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так. Усі наші суперечки завершуються однаково.",
              "maleVariant": "",
              "stringId": "1783446037299597312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Колись розповім тобі за кухлем пива.",
              "maleVariant": "",
              "stringId": "1783447658247098368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звичайно.",
              "maleVariant": "",
              "stringId": "1783452580413837312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Із задоволенням.",
              "maleVariant": "",
              "stringId": "1783458130853761024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джосс залишилася з трьома дітьми. Але вона сильна жінка.",
              "maleVariant": "",
              "stringId": "1815737714720186368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гадаєш, Явен нам допоможе?",
              "maleVariant": "",
              "stringId": "1841609618746380288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Явен тебе не видасть?",
              "maleVariant": "",
              "stringId": "1841609835138912256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він у будинку живе. І я маю сумніви, що тримає своїх жертв у підвалі.",
              "maleVariant": "",
              "stringId": "1908178454259982336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знаю.",
              "maleVariant": "",
              "stringId": "1908216662825918464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До того ж я теж знаю про скелети в її шафі. Звісно, не збираюся ці знання використовувати.",
              "maleVariant": "",
              "stringId": "1908263265267900416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гаразд, ми на місці.",
              "maleVariant": "",
              "stringId": "1912776659653758976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розумію. Скажи хоч, куди ми їдемо.",
              "maleVariant": "",
              "stringId": "1925708937564393472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До Джосс.",
              "maleVariant": "",
              "stringId": "1925710024610549760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Матері Ренді.",
              "maleVariant": "",
              "stringId": "1925710242194264064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона вважає, що він просто втік із дому. Не кажи їй поки про викрадення.",
              "maleVariant": "",
              "stringId": "1925710454023393280"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}