    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, що думаєш?",
              "maleVariant": "",
              "stringId": "1698275538001055744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хто той коп, що врятував Райна?",
              "maleVariant": "",
              "stringId": "1928826001470218244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що за «Двір Багряної Королеви»?",
              "maleVariant": "",
              "stringId": "1928826839810924544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«Двір Багряної Королеви» — що це таке?",
              "maleVariant": "",
              "stringId": "1928830979001769988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мені шкода, але ніколи не чув про таке.",
              "maleVariant": "",
              "stringId": "1928830979068878848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чи має хтось доступ до термінала, окрім охорони Райна?",
              "maleVariant": "",
              "stringId": "1928830979974848512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм-м… поліція. Це все.",
              "maleVariant": "",
              "stringId": "1928830980041957376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щось відомо про копа, який урятував Райна?",
              "maleVariant": "",
              "stringId": "1928830980847263748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це детектив Рівер Ворд. Хороший хлопець. Працювали разом над кількома справами.",
              "maleVariant": "",
              "stringId": "1928830980914372608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хто такий Голт?",
              "maleVariant": "",
              "stringId": "1928830981048590340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Заступник Райна. Ну і його пес на ланцюгу. Зараз балотується замість Райна.",
              "maleVariant": "",
              "stringId": "1928830981098921984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Охоронці мера теж із поліції Найт-Сіті?",
              "maleVariant": "",
              "stringId": "1928830981266694144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні. Усі підібрані Голтом і сидять на зарплатах у корпарів.",
              "maleVariant": "",
              "stringId": "1928830981317025792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хоч серед них є і колишні копи.",
              "maleVariant": "",
              "stringId": "1928830981367357440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На яку тему була конференція?",
              "maleVariant": "",
              "stringId": "1928830981501575172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та ні про що. Райну подобалося дивитися та слухати себе по телебаченню… Хе-хе-хе… Так, якось так.",
              "maleVariant": "",
              "stringId": "1928830981551906816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо правильно пам’ятаю, виправданням саме цієї конференції була тема про зменшення рівня злочинності в Найт-Сіті. Райн хотів цим попіаритися.",
              "maleVariant": "",
              "stringId": "1928830981619015680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Перепрошую, «зменшення»?",
              "maleVariant": "",
              "stringId": "1928830981669347328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Райн оголосив Пацифіку «незалежним районом». Через це її статистика не зараховується до міста.",
              "maleVariant": "",
              "stringId": "1928830981736456192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм. Кмітливо.",
              "maleVariant": "",
              "stringId": "1928830981770010624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я зателефоную Ворду.",
              "maleVariant": "",
              "stringId": "1928845965333848068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Особиста охорона мера теж із поліції Найт-Сіті?",
              "maleVariant": "",
              "stringId": "1928847199566520320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хто такий Голт?",
              "maleVariant": "",
              "stringId": "1928851463865065472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хто має доступ до термінала охорони?",
              "maleVariant": "",
              "stringId": "1928855228236128256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Про що була конференція?",
              "maleVariant": "",
              "stringId": "1928860810435321856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо хочеш поговорити з ним, то можу влаштувати.",
              "maleVariant": "",
              "stringId": "1929998123244675072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Боже. Ворд учасно з’явився.",
              "maleVariant": "",
              "stringId": "1930017986277236736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Або неймовірно пощастило, або був добре проінформований. Мені знадобиться і те, і те.",
              "maleVariant": "",
              "stringId": "1930019268828291072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добре сказано.",
              "maleVariant": "",
              "stringId": "1930020090106568704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо знадобиться ще щось, то дай нам знати. Ми допоможемо.",
              "maleVariant": "",
              "stringId": "1930020546883051520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Надсилаю контакти.",
              "maleVariant": "",
              "stringId": "1930026374012821504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Повернути скіпку] Поки зарано щось казати.",
              "maleVariant": "",
              "stringId": "1930027661748035584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Повернути скіпку] Гадаю, щось таки є.",
              "maleVariant": "",
              "stringId": "1930028216771895296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я б не хотіла робити завчасних висновків. Та й інформації замало, щоби щось сказати.",
              "maleVariant": "Я б не хотів робити завчасних висновків. Та й інформації замало, щоби щось сказати.",
              "stringId": "1930029164164505600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Від цієї справи тхне гірше, ніж від пляжної сральні.",
              "maleVariant": "",
              "stringId": "1930029211140710400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Маєш ще якісь запитання щодо справи?",
              "maleVariant": "",
              "stringId": "1930029290849263616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Повернути скіпку] Схоже, просто збіг обставин.",
              "maleVariant": "",
              "stringId": "1930050031296667648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як казав мій чумба: «Бачив і дивніші збіги».",
              "maleVariant": "",
              "stringId": "1930057031455825920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти побачила хоч щось підозріле? Чи в нас параноя?",
              "maleVariant": "Ти побачив хоч щось підозріле? Чи в нас параноя?",
              "stringId": "1930057708500377600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно-звісно.",
              "maleVariant": "",
              "stringId": "1930063033068642304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм… отже, підозри були не даремні.",
              "maleVariant": "",
              "stringId": "1930063841780785152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм-м… цікаво. Нам здавалося інакше, ми були в цьому впевнені.",
              "maleVariant": "",
              "stringId": "1930068211406692352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж, перші враження, часом, бувають оманливими.",
              "maleVariant": "",
              "stringId": "1930070246197780480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Вийти]",
              "maleVariant": "",
              "stringId": "2065249417394741252"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}