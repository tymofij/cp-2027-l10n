  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«Вихор» охороняє порт. Прикрий мене.",
              "maleVariant": "",
              "stringId": "1750509373849559048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мерщій, ходімо.",
              "maleVariant": "",
              "stringId": "1750509373849559056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вейленд бачив Ґрейсона десь у порті. З ним був ще хтось.",
              "maleVariant": "",
              "stringId": "1751957451383201792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Смешер.",
              "maleVariant": "",
              "stringId": "1751958386327117824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я сподіваюся. Спочатку нам потрібно дістатися до термінала «Вихору».",
              "maleVariant": "",
              "stringId": "1751973563986567168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То який у нас план?",
              "maleVariant": "",
              "stringId": "1752060664715370496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Готова?",
              "maleVariant": "Готовий?",
              "stringId": "1752952913720467456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А ось і «Ебуніке».",
              "maleVariant": "",
              "stringId": "1753741636431208448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Має фіговий вигляд.",
              "maleVariant": "",
              "stringId": "1753742316982198272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіть не віриться, що він тут ховається.",
              "maleVariant": "",
              "stringId": "1753745583606816768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А ти що думала, що він повісить табличку на двері?",
              "maleVariant": "Ти що думав, що він повісить табличку на двері?",
              "stringId": "1753745583606816776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Треба обшукати палубу.",
              "maleVariant": "",
              "stringId": "1753761192021286912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гаразд.",
              "maleVariant": "",
              "stringId": "1753761565549223936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він твій.",
              "maleVariant": "",
              "stringId": "1758078778762551296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Обережно.",
              "maleVariant": "",
              "stringId": "1758079373548412928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Агов, бадьоріше.",
              "maleVariant": "",
              "stringId": "1758079394788368384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Покінчімо з цим.",
              "maleVariant": "",
              "stringId": "1758079415910883328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Цей уїбан повинен бути десь тут.",
              "maleVariant": "",
              "stringId": "1779365959497900032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми йдемо до головного складу «Вихору».",
              "maleVariant": "",
              "stringId": "1779371727420874752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Там є термінал. Треба перевірити, чи є в них якась інфа про Смешера.",
              "maleVariant": "",
              "stringId": "1779372922478747648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона і з тобою так грубо поводилася?",
              "maleVariant": "",
              "stringId": "1779388340320821252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Он головний склад. Там є термінал.",
              "maleVariant": "",
              "stringId": "1790608732934856704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не знала, що ти працювала на «Вихор».",
              "maleVariant": "Не знав, що ти працювала на «Вихор».",
              "stringId": "1790619154236792832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звідки ти знаєш, де розташований термінал?",
              "maleVariant": "",
              "stringId": "1790619330145902596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж. Можемо починати.",
              "maleVariant": "",
              "stringId": "1790619351721402372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я на всіх працювала. Але на власних умовах.",
              "maleVariant": "",
              "stringId": "1790621711050956800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже, я багато чого пропустив за ці п’ятдесят років.",
              "maleVariant": "",
              "stringId": "1790621919373647872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бо я сама організувала їм цю закупівлю.",
              "maleVariant": "",
              "stringId": "1790622720385048576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "За мною. Я знаю це місце.",
              "maleVariant": "",
              "stringId": "1790628138519846912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона напружена.",
              "maleVariant": "",
              "stringId": "1790629297389264896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ходімо.",
              "maleVariant": "",
              "stringId": "1790692867535757312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бачиш їх?",
              "maleVariant": "",
              "stringId": "1790730040242823168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже, вони ховаються на кораблі.",
              "maleVariant": "",
              "stringId": "1790787977690701828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То ходімо їх шукати.",
              "maleVariant": "",
              "stringId": "1790789883163009024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чудово. Ми дійшли.",
              "maleVariant": "",
              "stringId": "1791157074513321984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зіскануй його. Можливо, там є щось, що нам би допомогло.",
              "maleVariant": "",
              "stringId": "1791157840460341248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ай, вона й без того знає, де його знайти.",
              "maleVariant": "",
              "stringId": "1791176008641765376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зможеш відчинити?",
              "maleVariant": "",
              "stringId": "1791421008340279296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це дуже схоже на страту.",
              "maleVariant": "",
              "stringId": "1792091878675144704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Застрелили впритул. Експансивною кулею.",
              "maleVariant": "",
              "stringId": "1792092740956938240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Смешера можна впізнати за його стилем.",
              "maleVariant": "",
              "stringId": "1792099513382166528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Здається, це якісь записи.",
              "maleVariant": "",
              "stringId": "1792141059758448644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можеш їх відтворити?",
              "maleVariant": "",
              "stringId": "1792141059775225860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можу спробувати.",
              "maleVariant": "",
              "stringId": "1792141059775225868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нахріна ми це робимо?",
              "maleVariant": "",
              "stringId": "1792141060026884100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не знаю, хто там тебе криє, але якщо хоч щось станеться з Меллорі…",
              "maleVariant": "",
              "stringId": "1792266351822987264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо не відмовишся від угоди з корпорацією «Найт», то вона помре.",
              "maleVariant": "",
              "stringId": "1792270501449261056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти що, не розумієш? Ти не можеш шантажувати цілий «Мілітех» викраденням однієї…",
              "maleVariant": "",
              "stringId": "1792270850650234880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Двадцять чотири години.",
              "maleVariant": "",
              "stringId": "1792271201864474624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ось яке на вигляд типове життя корпаря.",
              "maleVariant": "",
              "stringId": "1792272104310923264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скільки ще?",
              "maleVariant": "",
              "stringId": "1792275223900020740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Година. Як я й казав, нічого не вийде.",
              "maleVariant": "",
              "stringId": "1792275223900020748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тоді навіщо це все? Ця дівчина…",
              "maleVariant": "",
              "stringId": "1792275223900020756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "За годину вона помре. Це наказ.",
              "maleVariant": "",
              "stringId": "1792275223900020764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нащо взагалі було це записувати?",
              "maleVariant": "",
              "stringId": "1792275223950352388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Те тіло, яке ми знайшли. Це може бути Меллорі.",
              "maleVariant": "",
              "stringId": "1792279675616845824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми не знаємо, коли це сталося.",
              "maleVariant": "",
              "stringId": "1792280478624735232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тепер я знаю, хто пограбував «Кан-Тао» минулого місяця.",
              "maleVariant": "",
              "stringId": "1792501006958551044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Професійна робота.",
              "maleVariant": "",
              "stringId": "1792501006975328260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти впевнена, що це той вантаж?",
              "maleVariant": "",
              "stringId": "1792501006975328268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, просто чудово.",
              "maleVariant": "",
              "stringId": "1792501007143100420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це ти ще середини не бачив.",
              "maleVariant": "",
              "stringId": "1796915671132868608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пішли.",
              "maleVariant": "",
              "stringId": "1796915842361135104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Іди біля мене.",
              "maleVariant": "",
              "stringId": "1796916600724852736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Прикрий її. Про всяк випадок.",
              "maleVariant": "",
              "stringId": "1796922193829236736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Треба знайти термінал. Тихенько.",
              "maleVariant": "",
              "stringId": "1796924355405746176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, нам сьогодні пощастить, і ми вб’ємо Ґрейсона та Смешера одним махом.",
              "maleVariant": "",
              "stringId": "1796925320699645952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«Атена Д-III». Зовнішня база даних.",
              "maleVariant": "",
              "stringId": "1796941254759878656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А я постою на шухері.",
              "maleVariant": "",
              "stringId": "1812872038536515584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона зволікає. Я ж казав, що вона досі боїться того гівнюка.",
              "maleVariant": "",
              "stringId": "1814213857690062848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто знайдіть цього їблана і все.",
              "maleVariant": "",
              "stringId": "1814318375987646464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У тебе двадцять чотири години.",
              "maleVariant": "",
              "stringId": "1816931139113521152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знаю. Тому шантажую тебе.",
              "maleVariant": "",
              "stringId": "1816945094452121600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нагорі всі хуями махають, ходять на зустрічі, про щось домовляються.",
              "maleVariant": "",
              "stringId": "1817010448704540672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А внизу Смешер приставив пістолет до чиєїсь голови.",
              "maleVariant": "",
              "stringId": "1817010751751393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це міг записати Ґрейсон. Щоби мати компромат на Смешера.",
              "maleVariant": "",
              "stringId": "1817012601808556032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мені здається, ти переоцінюєш Ґрейсона.",
              "maleVariant": "",
              "stringId": "1817013630990094336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Еге. Для Смешера це ж звичайний вівторок.",
              "maleVariant": "",
              "stringId": "1817014774541914112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "679275-PN1H. Я надсилала три групи, щоби знайти його для китайців",
              "maleVariant": "",
              "stringId": "1817037794107002880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Усе місто його шукало. Мушу визнати, він профі.",
              "maleVariant": "",
              "stringId": "1817038599581143040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто знайдімо й убиймо його, гаразд?",
              "maleVariant": "",
              "stringId": "1817042427185532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так. Якийсь час.",
              "maleVariant": "",
              "stringId": "1817046671401750528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Три-фет.",
              "maleVariant": "",
              "stringId": "1869862073709510656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Глянь, тут навіть є етикетка! «Добавки».",
              "maleVariant": "",
              "stringId": "1869862334108680196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Лежить собі й чекає, поки його поділять, перепакують та розкинуть по вулицях.",
              "maleVariant": "",
              "stringId": "1894108944321499136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нащо ти на це погодилася?",
              "maleVariant": "",
              "stringId": "1897947614879174656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я зводжу рахунки.",
              "maleVariant": "",
              "stringId": "1897953148457996288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тобто мстишся за Джонні?",
              "maleVariant": "",
              "stringId": "1897954773683687424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І це теж.",
              "maleVariant": "",
              "stringId": "1897959263115694080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Роуґ боїться його після інциденту у вежі.",
              "maleVariant": "",
              "stringId": "1897959622701764608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Настав час їй зіткнутися зі своїми страхами.",
              "maleVariant": "",
              "stringId": "1897970598608130048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти це бачила?",
              "maleVariant": "Ти це бачив?",
              "stringId": "1898025977564123136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Маю ідею.",
              "maleVariant": "",
              "stringId": "1898026287539965952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Он там, праворуч.",
              "maleVariant": "",
              "stringId": "1898035855301038080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щось горить.",
              "maleVariant": "",
              "stringId": "1898042369843679232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Прибери його, тільки тихо.",
              "maleVariant": "",
              "stringId": "1898042622592438272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти що, хочеш їх розбудити?",
              "maleVariant": "",
              "stringId": "1898042885474635776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я теж так можу.",
              "maleVariant": "",
              "stringId": "1898044829769420800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Завжди любив спостерігати за нею в дії.",
              "maleVariant": "",
              "stringId": "1898046673736454144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зачекай трошки, а тоді ходи за мною.",
              "maleVariant": "",
              "stringId": "1898050120464486400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тепер обережно.",
              "maleVariant": "",
              "stringId": "1898055222264877056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж, цього ми точно не чіпаємо.",
              "maleVariant": "",
              "stringId": "1908683275202482176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже, вони винесли звідси багато чого.",
              "maleVariant": "",
              "stringId": "1908691953418784768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже, після мого останнього візиту вони посилили охорону.",
              "maleVariant": "",
              "stringId": "1911495907236265984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нічого, дамо собі раду.",
              "maleVariant": "",
              "stringId": "1911497156987863040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Який у нас план?",
              "maleVariant": "",
              "stringId": "1916908206649241600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не знала, що ти працюєш із «Вихором».",
              "maleVariant": "Не знав, що ти працюєш із «Вихором».",
              "stringId": "1916908206649241616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звідки ти знаєш, де термінал?",
              "maleVariant": "",
              "stringId": "1916908206649241636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можемо починати.",
              "maleVariant": "",
              "stringId": "1916908206649241668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гараздоньки, тоді премо в лобову.",
              "maleVariant": "",
              "stringId": "1960994083227713536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже на страту.",
              "maleVariant": "",
              "stringId": "1961039143893876740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Цікаво…",
              "maleVariant": "",
              "stringId": "1961045412214095872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так, так, так…",
              "maleVariant": "",
              "stringId": "1961046526925881344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Там має бути хоч якась інформація про Смешера.",
              "maleVariant": "",
              "stringId": "1979442188788715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Вийти]",
              "maleVariant": "",
              "stringId": "2121470272725577732"
            }
          ]
        }
      }
