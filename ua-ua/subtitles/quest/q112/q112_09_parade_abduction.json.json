  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"危険だと警告されたの。オダとも連絡が取れないし、何か変だわ\" t=\"Мене попередили про небезпеку. Ода не відповідає. Тут щось не так.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669447913484337152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"私が参加したのは、あくまでもお父様を追悼するため\" t=\"Я на це погодилася лише через пам’ять про батька.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669449579881316352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"操り人形になるためじゃないわ\" t=\"Проте зараз я почуваюся, ніби лялька у твоїх руках.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669450106887225344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"パレードを中止すれば、弱さを露呈したと思われる\" t=\"Якщо ми скасуємо урочистості, це буде сприйнято як ознака слабкості.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714211719007453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"弱さ？ 誰の？ あなたの？\" t=\"Слабкості? Чиєї? Твоєї?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212267806965760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"だが、お前が言うなら中止してもいい。全部やめだ。お前の安全が第一だからな\" t=\"Але я розумію. Скажи лише слово, і ми припинимо, закінчимо все. Твоя безпека для мене важливіша.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212716832374784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お父様…\" t=\"За батька.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714220354475409408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お許しを。どうか、話だけでもお聞きください\" t=\"Прошу пробачити мені, Ханако-сама. Прошу лише вислухати мене.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714224100794187776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様は…\" t=\"Ваш батько…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225132743966720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"父のことを口にしないで！\" t=\"Як ти смієш навіть мовити про нього?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225355629281280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、無礼を働きたくありません。しかし…\" t=\"Ханако-сама, прошу. Не змушуйте мене це робити.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225488303505408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, біжи!",
              "maleVariant": "",
              "stringId": "1714227320375492608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бляха! Бляха! Бля!",
              "maleVariant": "",
              "stringId": "1714228292917149696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"兄さん…\" t=\"Йорінобу?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724068500475875328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"やり終えましょう\" t=\"Ми маємо завершити почате.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724113942068727808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありがとう、ハナコ。せっかくの式典だ――やり終えなければ、悔いが残る\" t=\"Дякую тобі, Ханако. Було б дуже шкода не завершити таку душевну церемонію.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724116336999534592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тс-сп…",
              "maleVariant": "",
              "stringId": "1724118030575603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様\" t=\"Ханако-сама.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724119896722116608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"あなたは…\" t=\"Це ти…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724121657927143424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"これが、俺にできる最後のご奉仕なのです\" t=\"Це останнє, що я можу для нього зробити.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724149966509576192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様の死について真実を明かすことが\" t=\"Відкрити вам правду про його смерть.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724150642950148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"この見せ物では、みんなに役割がある。ミチコは文句を言ってないぞ\" t=\"У цій виставі в кожного своя роль. У Мічіко з цим проблем немає.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724223690596290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"なぜ止まったんだ？\" t=\"Чому ти зупинилась?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724231251970101248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"俺たちだ\" t=\"Нашої.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724233151520051200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що б він не зробив, тепер нам гайки!",
              "maleVariant": "",
              "stringId": "1771027611004047360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ханако всередині.",
              "maleVariant": "",
              "stringId": "1773724698127908864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Намагається комусь подзвонити.",
              "maleVariant": "",
              "stringId": "1773725998731247616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Спробуй підслухати.",
              "maleVariant": "",
              "stringId": "1773726175479218176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не забудь вимкнути сигналізацію. Мені потрібен шанс увійти.",
              "maleVariant": "",
              "stringId": "1773726519227596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ґоро? Усі системи заглушені. Зараз чи ніколи.",
              "maleVariant": "",
              "stringId": "1773729105720655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що вона робить?",
              "maleVariant": "",
              "stringId": "1780695188894519296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Швидше, Ві!",
              "maleVariant": "",
              "stringId": "1790741196538200068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, незабаром уже буде пізно!",
              "maleVariant": "",
              "stringId": "1790741196554977284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви не маєте права тримати нас тут!",
              "maleVariant": "",
              "stringId": "1795474982084558848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Усі на землю! Швидко!",
              "maleVariant": "",
              "stringId": "1795475678641012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Іди нахуй! Це порушення наших громадянських прав!",
              "maleVariant": "",
              "stringId": "1795476053259468800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Стояти!",
              "maleVariant": "",
              "stringId": "1795479668128759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хтось іще хоче розповісти про права?! Доєднуйтеся!",
              "maleVariant": "",
              "stringId": "1795483864295030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ледь не затоптали… Схоже, ребра зламані…",
              "maleVariant": "",
              "stringId": "1801295829407907840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Забираймося звідси!",
              "maleVariant": "",
              "stringId": "1801296487729086464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Який у мене вигляд? Нам уже час в етер.",
              "maleVariant": "",
              "stringId": "1801297323049893888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Три, дві, одна…",
              "maleVariant": "",
              "stringId": "1801298631119093760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це Ґіліан Джордан із каналу «Новини Н54» наживо з Японського кварталу, де парад на честь Сабуро Арасаки завершився масовими безладами.",
              "maleVariant": "",
              "stringId": "1801299000486281216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Очевидці повідомляють про стрілянину на одній зі святкових платформ. Ідеться про платформу, яка, як кажуть, перевозила Ханако Арасаку.",
              "maleVariant": "",
              "stringId": "1801299245702070272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На жаль, це все, що відомо про подію на цей час.",
              "maleVariant": "",
              "stringId": "1801300495923429376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми все ще очікуємо офіційної заяви від корпорації «Арасака» щодо цих подій.",
              "maleVariant": "",
              "stringId": "1801300622893400064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Агов! Не лізь у кадр!",
              "maleVariant": "",
              "stringId": "1801303594255794176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дідько! Давай іще раз!",
              "maleVariant": "",
              "stringId": "1801304953780715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую! Узагалі-то тут люди працюють!",
              "maleVariant": "",
              "stringId": "1801305575091355648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поки ситуація не буде взята під контроль, радимо уникати Кандзакі-стріт.",
              "maleVariant": "",
              "stringId": "1863315822830493696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знав, що не можна йому довіряти! Треба було тримати його на ціпку.",
              "maleVariant": "",
              "stringId": "1906936140640829440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нам ліпше звалити звідси.",
              "maleVariant": "",
              "stringId": "1906937283303456768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він застрелив її?!",
              "maleVariant": "",
              "stringId": "2007084788928552960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Вимкнути охоронні системи]",
              "maleVariant": "",
              "stringId": "2034753975066525696"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}