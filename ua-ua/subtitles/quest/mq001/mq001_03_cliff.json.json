  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "От трясця, знову заклинило… Ходи, допоможеш.",
              "maleVariant": "",
              "stringId": "1811292423955697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Відчинити]",
              "maleVariant": "",
              "stringId": "1811296530565189636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Натисни там, трохи лівіше… Ага, ось так.",
              "maleVariant": "",
              "stringId": "1811305132646486016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Срака мотика, Мітче… Якого хріна?!",
              "maleVariant": "",
              "stringId": "1811373862323769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розслабся, Ві. Скорпіон так хотів.",
              "maleVariant": "",
              "stringId": "1811385601476022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він хотів, щоби ти возив його в багажнику як запаску?",
              "maleVariant": "",
              "stringId": "1811385744166244352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо він так хотів…",
              "maleVariant": "",
              "stringId": "1811386006964555780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А Сол не лютуватиме через це?",
              "maleVariant": "",
              "stringId": "1811386633811677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Солу це не сподобається. Знищити хорошу тачку… це ж марнотратство.",
              "maleVariant": "",
              "stringId": "1811386964826148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скажу, що потрапив у аварію.",
              "maleVariant": "",
              "stringId": "1811387447355658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, якщо Скорпіон так хотів…",
              "maleVariant": "",
              "stringId": "1811387679434887168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хотів. Він це мільйон разів казав.",
              "maleVariant": "",
              "stringId": "1811388212967133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Узяти каністру]",
              "maleVariant": "",
              "stringId": "1811389513553694724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тримай.",
              "maleVariant": "",
              "stringId": "1811398952448970752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чудово. Тепер поклади її на пасажирське місце.",
              "maleVariant": "",
              "stringId": "1811399237124771840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Покласти каністру]",
              "maleVariant": "",
              "stringId": "1811399600636710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бачу, водій уже за кермом…",
              "maleVariant": "",
              "stringId": "1811414052681568256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну то не марнуватимемо час. Ще одна-дві годинки, і його роздує як кульку.",
              "maleVariant": "",
              "stringId": "1811416248164831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До справи.",
              "maleVariant": "",
              "stringId": "1811418292787367936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти певен, що це добра ідея?",
              "maleVariant": "",
              "stringId": "1811419470111723520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти точно певен, що це добра ідея?",
              "maleVariant": "",
              "stringId": "1811421128254312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, Ві. Я не певен.",
              "maleVariant": "",
              "stringId": "1811422682579488768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До справи.",
              "maleVariant": "",
              "stringId": "1811422853220552704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "О, так. Я вже запхав ганчірку в каністру. Ти підпалиш, а я заведу машину.",
              "maleVariant": "",
              "stringId": "1811423155025891328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Підпалити]",
              "maleVariant": "",
              "stringId": "1811423554004865028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Горить. Швидше, поки не їбануло!",
              "maleVariant": "",
              "stringId": "1811423894330691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пішов гучно. Як і хотів.",
              "maleVariant": "",
              "stringId": "1811424662542635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сажа й попіл. Це все, що лишається.",
              "maleVariant": "",
              "stringId": "1811425287259049984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно. Я вип’ю на таких проводах.",
              "maleVariant": "",
              "stringId": "1811425434059689984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно.",
              "maleVariant": "",
              "stringId": "1811425723818987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, що… може, пивка?",
              "maleVariant": "",
              "stringId": "1811722638246825984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Маю йти.",
              "maleVariant": "",
              "stringId": "1811735446527168512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вибач… Проводи — то трохи не моє.",
              "maleVariant": "",
              "stringId": "1811735716288024576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, тоді ходімо.",
              "maleVariant": "",
              "stringId": "1811741647050657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ах… немає нічого ліпшого за холодне пиво.",
              "maleVariant": "",
              "stringId": "1811744593180119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Саме так.",
              "maleVariant": "Згоден.",
              "stringId": "1811747013125754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Узяти]",
              "maleVariant": "",
              "stringId": "1845779842239057924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що це?",
              "maleVariant": "",
              "stringId": "1845780327872352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гадки не маю… Скорпіон завжди тримав це при собі.",
              "maleVariant": "",
              "stringId": "1845792706219757568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Слухай… я поки тут побуду трохи, гаразд? Посиджу, подумаю в тиші.",
              "maleVariant": "",
              "stringId": "1845792948096880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні. Хотів, щоби я привіз його сюди… і провів із вибухом.",
              "maleVariant": "",
              "stringId": "1863237586613751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Посадимо його за кермо, поллємо машину… а тоді підпалимо й пустимо з обриву.",
              "maleVariant": "",
              "stringId": "1863238860105109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти дзьобнувся?!",
              "maleVariant": "",
              "stringId": "1863240077191467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мітче. Ти що, їбанувся?",
              "maleVariant": "",
              "stringId": "1863241269229441024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та-та, годі нити.",
              "maleVariant": "",
              "stringId": "1863243586733395968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дай каністру, будь ласка.",
              "maleVariant": "",
              "stringId": "1863245705880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добре. То зробімо ці яскраві проводи?",
              "maleVariant": "",
              "stringId": "1863249863308005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але це була остання воля мого найліпшого друга. Тому я зроблю все, щоби здійснити її. З тобою чи без.",
              "maleVariant": "",
              "stringId": "1863252538233769984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж, як хочеш.",
              "maleVariant": "",
              "stringId": "1863258104175841280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Слухай, Ві! Дякую. Я ціную це.",
              "maleVariant": "",
              "stringId": "1863258316642504704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мітче, а чому я? Чому попросив допомогти?",
              "maleVariant": "",
              "stringId": "1863262761832992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чому ти попросив мене допомогти?",
              "maleVariant": "",
              "stringId": "1863262994465869828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звідки пішло прізвисько «Скорпіон»?",
              "maleVariant": "",
              "stringId": "1863263606398046208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До речі… А звідки пішло його прізвисько? «Скорпіон»?",
              "maleVariant": "",
              "stringId": "1863264231651332096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Кумедно… Знаєш, я навіть гадки не маю. Ніколи не питав його.",
              "maleVariant": "",
              "stringId": "1863264970436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, це його знак зодіаку?",
              "maleVariant": "",
              "stringId": "1863265301551173632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хе-хе-хе. Або в нього велике жало.",
              "maleVariant": "",
              "stringId": "1863266236545421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ех, Скорпіоне, старий ти пройдисвіт… я сумуватиму за тобою, чуваче.",
              "maleVariant": "",
              "stringId": "1863266849886883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, ти наче непогана людина.",
              "maleVariant": "",
              "stringId": "1863267756393418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я серйозно.",
              "maleVariant": "",
              "stringId": "1863268095561617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не знаю, е-ем… мабуть, через те, що я зараз тут стою та проводжу його завдяки тобі.",
              "maleVariant": "",
              "stringId": "1863268252848017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ваші шляхи перетнулися. Лише на коротку мить… але важливу.",
              "maleVariant": "",
              "stringId": "1863269066895314944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Слухай, тут цей… можу дати деякі його речі. Ось його щасливий амулет. Можеш узяти, він твій.",
              "maleVariant": "",
              "stringId": "1863270157296914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розумію. Я тоді піду.",
              "maleVariant": "",
              "stringId": "1863272334694338560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "О, і… дякую, Ві. За сьогодні… і за те, що витягнула мене з тої сраки теж.",
              "maleVariant": "О, і… дякую, Ві. За сьогодні… і за те, що витягнув мене з тої сраки теж.",
              "stringId": "1863272509596815360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти своя людина. Справді.",
              "maleVariant": "",
              "stringId": "1863273061936320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую, я не буду.",
              "maleVariant": "",
              "stringId": "1873522370078363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, дякую. Потрібна ясна голова.",
              "maleVariant": "",
              "stringId": "1873523224223207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добре, моя міська чумба… кожному болить по-своєму.",
              "maleVariant": "Добре, мій міський чумбо… кожному болить по-своєму.",
              "stringId": "1873524512897945600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як скажеш.",
              "maleVariant": "",
              "stringId": "1873528054668906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно. Слухай, якщо хочеш, то можеш повертатися на моїй машині в табір. Як тобі буде зручніше.",
              "maleVariant": "",
              "stringId": "1908015766788427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Широкої дороги тобі, друже.",
              "maleVariant": "",
              "stringId": "1934467678261235712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти збожеволів?!",
              "maleVariant": "",
              "stringId": "2121969965643616260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві… каністра. Чого чекаєш?",
              "maleVariant": "",
              "stringId": "2238669820146728960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зробімо це, Ві. Заради Скорпіона.",
              "maleVariant": "",
              "stringId": "2238714786642378752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нумо, підпалюй!",
              "maleVariant": "",
              "stringId": "2238716481912946688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну ж бо. Він цього хотів.",
              "maleVariant": "",
              "stringId": "2238716544173195264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щось скажеш?",
              "maleVariant": "",
              "stringId": "2238721843760185344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Агов, зараз не час на хвилину мовчання.",
              "maleVariant": "",
              "stringId": "2238722204570992644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, ти ще тут?",
              "maleVariant": "",
              "stringId": "2238722248493744132"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}