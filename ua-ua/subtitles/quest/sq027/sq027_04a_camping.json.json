    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Тепер тільки чекати.",
              "maleVariant": "",
              "stringId": "1757958994757701636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж, тепер тільки чекаймо.",
              "maleVariant": "",
              "stringId": "1757961219349434368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти така перелякана, наче до тебе ще ніхто не виступав проти Сола.",
              "maleVariant": "",
              "stringId": "1785579829227966468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ой, от тільки не треба мене оце втішати.",
              "maleVariant": "",
              "stringId": "1785579829227966476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А-а… я знаю цю людину?",
              "maleVariant": "",
              "stringId": "1785579829227966484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Знала. Це Скорпіон.",
              "maleVariant": "",
              "stringId": "1785579829227966492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він повернувся додому з війни, голова кишить ідеями, ще й купа нових контактів…",
              "maleVariant": "",
              "stringId": "1785579829227966500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він намагався переконати Сола об’єднатися зі Зміїним Народом. Мовляв, станемо сильнішими.",
              "maleVariant": "",
              "stringId": "1785579829244743680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Переживаєш через Сола?",
              "maleVariant": "",
              "stringId": "1805482396761788416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти через Сола переживаєш?",
              "maleVariant": "",
              "stringId": "1805483051593306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не пам’ятаю, щоби він мені про це розповідав. А Сол що сказав?",
              "maleVariant": "",
              "stringId": "1805488371883380736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вилив на нього помиї за втручання в такі справи. А потім ще й замовлень йому не давав цілий рік.",
              "maleVariant": "",
              "stringId": "1805488943650902016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Оце ти втішив, оце так дякую.",
              "maleVariant": "",
              "stringId": "1805489713255354368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти ж сама сказала, щоби не втішав.",
              "maleVariant": "",
              "stringId": "1805490081951453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Випити] Випиймо. За Скорпіона.",
              "maleVariant": "",
              "stringId": "1805498382646841344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сол мав рацію щодо Зміїного Народу.",
              "maleVariant": "",
              "stringId": "1805498795500572672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви майже не говорите про війну.",
              "maleVariant": "",
              "stringId": "1805499234258325504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Піду-но я трохи подрімаю.",
              "maleVariant": "",
              "stringId": "1805501050257739780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Випиймо за Скорпіона.",
              "maleVariant": "",
              "stringId": "1805503685874487296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сол усе ж мав рацію щодо Зміїного Народу.",
              "maleVariant": "",
              "stringId": "1805504575603806208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви майже не говорите про війну.",
              "maleVariant": "",
              "stringId": "1805505087845765120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я піду, спробую трохи поспати.",
              "maleVariant": "",
              "stringId": "1805508909091590148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу. Конвой ще точно не скоро приїде.",
              "maleVariant": "",
              "stringId": "1805514716877230080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "За Скорпіона.",
              "maleVariant": "",
              "stringId": "1806703285541081088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А про що тут розмовляти? Запхнули тебе в панцер десь посеред Мексики — і повний уперед, бійцю.",
              "maleVariant": "",
              "stringId": "1806706739147771904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Потім декілька тижнів ти розмовляєш або із самим собою, або з бортовим ШІ, і лигаєш йодид як воду, бо лічильник Гейгера ніколи не затикається.",
              "maleVariant": "",
              "stringId": "1806707030534459392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага-а. У мене всю сраку висипає від спогадів про сидіння в цих розпечених добіла домовинах.",
              "maleVariant": "",
              "stringId": "1806719414619672576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага. І все одно вам нестерпно кортить покататися на «Василіску», правильно?",
              "maleVariant": "",
              "stringId": "1806720900024045568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, це інше.",
              "maleVariant": "",
              "stringId": "1806721809030385664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Абсолютно інше. Ти й сама зрозумієш, коли сядеш у нього.",
              "maleVariant": "Абсолютно інше. Ти й сам зрозумієш, коли сядеш у нього.",
              "stringId": "1806721954287521792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Варто залишатися незалежними, інакше можна закінчити, як…",
              "maleVariant": "",
              "stringId": "1806736160344838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як Беккери?",
              "maleVariant": "",
              "stringId": "1806737201320759296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Еге.",
              "maleVariant": "",
              "stringId": "1806744496205271040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми й самі чудово впораємося. Нам треба просто декілька хороших замовлень.",
              "maleVariant": "",
              "stringId": "1806744783162773504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Саме для цього ми й тут.",
              "maleVariant": "",
              "stringId": "1806747641899716608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скорпіон би нами пишався.",
              "maleVariant": "",
              "stringId": "1806748277437435904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щоби люди знову загуділи про Альдекальдо.",
              "maleVariant": "",
              "stringId": "1806749266856972288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І за Джекі.",
              "maleVariant": "",
              "stringId": "1806982561679085568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Посунутися ближче] Якось холодно вже.",
              "maleVariant": "",
              "stringId": "1873467562821177348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ух, похолодало тут якось.",
              "maleVariant": "",
              "stringId": "1873468065030361088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти, схоже, навряд чи зможеш поспати.",
              "maleVariant": "",
              "stringId": "1873468440554786816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти злишся на мене?",
              "maleVariant": "",
              "stringId": "1873468870303174656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Розбудиш, якщо щось станеться.",
              "maleVariant": "",
              "stringId": "1873583158342733824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Від нервів навіть заснути не можеш?",
              "maleVariant": "",
              "stringId": "1873585510340644864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти на мене злишся?",
              "maleVariant": "",
              "stringId": "1873585955205304320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я ліпше просто поряд із тобою полежу, на зорі подивлюся.",
              "maleVariant": "",
              "stringId": "1873586272076582912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо щось трапиться, розбудиш мене. Або теж лягай спати.",
              "maleVariant": "",
              "stringId": "1873597920682201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та не зовсім. Але це мило.",
              "maleVariant": "",
              "stringId": "1873602703480606720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, теж трошки поспиш?",
              "maleVariant": "",
              "stringId": "1873609883172753408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти й на цьому знаєшся? Скажи ще, що в тебе небесні мапи в бібліотеці лежать.",
              "maleVariant": "",
              "stringId": "1873652948373622784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Авжеж. Ось це — Великий Віз. А ото, справа, маленька зірочка майорить, бачиш?",
              "maleVariant": "",
              "stringId": "1873657120682692608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добраніч, Ві.",
              "maleVariant": "",
              "stringId": "1873657765196222464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я? Та ні, ти чого.",
              "maleVariant": "",
              "stringId": "1873662270331703296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поспати не хочеш?",
              "maleVariant": "",
              "stringId": "1873665480870088704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "О, серйозно, бачиш? То з воза мастило крапнуло.",
              "maleVariant": "",
              "stringId": "1873668483823980544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага-а-а?..",
              "maleVariant": "",
              "stringId": "1873669289415561216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Випити]",
              "maleVariant": "",
              "stringId": "2208601324284833796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зорі вже видно. Значить, до сутінків іще пара годин.",
              "maleVariant": "",
              "stringId": "2228821801554231296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До заходу сонця ще є достатньо часу.",
              "maleVariant": "",
              "stringId": "2228830442961321984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм, можна буде ще раз оглянути машину.",
              "maleVariant": "",
              "stringId": "2228834546517360640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, ти там заснула чи що?",
              "maleVariant": "Ві, ти там заснув чи що?",
              "stringId": "2228942036328333312"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}