  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона мала рацію.",
              "maleVariant": "",
              "stringId": "1754881289358393348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Блядь, Ві!",
              "maleVariant": "",
              "stringId": "1754881759019778048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона нас обдурила, а ти нічого з цим не зробила?",
              "maleVariant": "Вона нас обдурила, а ти нічого з цим не зробив?",
              "stringId": "1754888968457928704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я вирішила дати їй шанс.",
              "maleVariant": "Я вирішив дати їй шанс.",
              "stringId": "1754889761382711296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зміни мають відбуватися поступово.",
              "maleVariant": "",
              "stringId": "1754889816428756992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джуді, вона мала рацію. Так, вона не розповіла нам свій план, але це лише тому, що ми б не погодилися на таке.",
              "maleVariant": "",
              "stringId": "1754890155697618944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я вирішила дати їй шанс. Ти ж звернулася до неї з якоїсь причини.",
              "maleVariant": "Я вирішив дати їй шанс. Ти ж звернулася до неї з якоїсь причини.",
              "stringId": "1754892121282691072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Такі зміни мають відбуватися поступово. На папірчику революція завжди має чудовий вигляд.",
              "maleVariant": "",
              "stringId": "1754892204229246976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві… Пробач, я трохи переборщила. Просто… якось не так я це все уявляла.",
              "maleVariant": "",
              "stringId": "1754892626880872448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тебе розумію.",
              "maleVariant": "",
              "stringId": "1754894151862697988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Евелін теж хотіла все й одразу.",
              "maleVariant": "",
              "stringId": "1754894895663792128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я розумію, чому ти розчарована.",
              "maleVariant": "",
              "stringId": "1754895331804299264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, ти не ображайся, мені просто потрібно заспокоїтися.",
              "maleVariant": "",
              "stringId": "1754896129057603584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Евелін також хотіла все й одразу.",
              "maleVariant": "",
              "stringId": "1754896322784116736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, правда.",
              "maleVariant": "",
              "stringId": "1754896322800893952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я мушу йти.",
              "maleVariant": "",
              "stringId": "1754897013082669056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, дякую тобі за допомогу. Знову.",
              "maleVariant": "",
              "stringId": "1754897753259884544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пробач.",
              "maleVariant": "",
              "stringId": "1754901624552144896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це не моя провина.",
              "maleVariant": "",
              "stringId": "1754901624568922112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пробач, Джуді. Я не хотіла цього зробити.",
              "maleVariant": "Пробач, Джуді. Я не хотів цього зробити.",
              "stringId": "1754901954341879808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знаю.",
              "maleVariant": "",
              "stringId": "1754902467758243840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Цього б не сталося, якби вона не накинулася на мене першою. Це не моя провина.",
              "maleVariant": "",
              "stringId": "1754902813318561792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пробач, але… Я тобі не вірю. Усе можна було вирішити інакше.",
              "maleVariant": "",
              "stringId": "1754902813318561800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти… убила її?",
              "maleVariant": "Ти… убив її?",
              "stringId": "1775165119327617024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Блядь… Могла б і здогадатися.",
              "maleVariant": "",
              "stringId": "1775165237657321476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Головне те, що «Хмари» тепер вільні.",
              "maleVariant": "",
              "stringId": "1775167152491315200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Є різниця між тупістю та вірою в людей.",
              "maleVariant": "",
              "stringId": "1775167152491315216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та яка різниця, що вона там напланувала. Головне те, що нам це вдалося. Тепер «Хмари» належать лялькам.",
              "maleVariant": "",
              "stringId": "1775167152558424068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Еге. Це справді була наша ціль.",
              "maleVariant": "",
              "stringId": "1775167152575201280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джуді, не драматизуй. Є різниця між тупістю та вірою в те, що люди інколи мають добрі наміри.",
              "maleVariant": "",
              "stringId": "1775167152608755716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знаю, що ти маєш добрі наміри. Це очевидно.",
              "maleVariant": "",
              "stringId": "1775167152608755724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, ти хоча б відмовилася від її грошей.",
              "maleVariant": "Ну, ти хоча б відмовився від її грошей.",
              "stringId": "1775174024422211584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так ще й узяла в неї гроші!",
              "maleVariant": "Так ще й узяв у неї гроші!",
              "stringId": "1775174225866244100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Досить! Ти хоч сама розумієш, що пиздиш?",
              "maleVariant": "Досить! Ти хоч сам розумієш, що пиздиш?",
              "stringId": "1775176021649113092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти взяла її євдики, тому для мене тут усе зрозуміло.",
              "maleVariant": "Ти взяв її євдики, тому для мене тут усе зрозуміло.",
              "stringId": "1775210963204554752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую.",
              "maleVariant": "",
              "stringId": "1775231205888737280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую.",
              "maleVariant": "",
              "stringId": "1775231376731127812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я не хотіла кровопролиття.",
              "maleVariant": "Я не хотів кровопролиття.",
              "stringId": "1782345598342512640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якби ми не вирішили все мирним шляхом, то Хіромі був би не останньою жертвою.",
              "maleVariant": "",
              "stringId": "1782345995912200196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона була б гіршою за Хіромі.",
              "maleVariant": "",
              "stringId": "1782349004838780928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Після всього, що сталося, я усвідомила, що вона буде ще гіршим тираном, аніж Хіромі.",
              "maleVariant": "Після всього, що сталося, я усвідомив, що вона буде ще гіршим тираном, аніж Хіромі.",
              "stringId": "1782349433580535808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І це було причиною її вбивати?",
              "maleVariant": "",
              "stringId": "1782350665967714304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бля… Ти її вбила.",
              "maleVariant": "Бля… Ти її вбив.",
              "stringId": "1892205587805724672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, ти й маєш рацію. Пробач.",
              "maleVariant": "",
              "stringId": "1892275977655640068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона могла подумати про наслідки.",
              "maleVariant": "",
              "stringId": "1892277587983163392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Може, ти й маєш рацію. Вона мене спровокувала. Мені шкода, що все так скінчилося.",
              "maleVariant": "",
              "stringId": "1892282898525675520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мені також.",
              "maleVariant": "",
              "stringId": "1892291141373222912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Майко добре знала, що вона робить. І розуміла, чим усе може закінчитися.",
              "maleVariant": "",
              "stringId": "1892293021595820032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона думала, що недоторканна…",
              "maleVariant": "",
              "stringId": "1892296319258288128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну… я ж не винна, що вона помилялася.",
              "maleVariant": "Ну… я ж не винен, що вона помилялася.",
              "stringId": "1892296690605187072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона, певно, це від самого початку спланувала. Щойно ми зайшли в її офіс.",
              "maleVariant": "",
              "stringId": "1892332694913376256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Їбати, але я тупа.",
              "maleVariant": "",
              "stringId": "1892333729262297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І ти це зробила.",
              "maleVariant": "І ти це зробив.",
              "stringId": "1892344652135882752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та блядь!",
              "maleVariant": "",
              "stringId": "1892422475668217856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Такому великому клубу, тим більше в цій сфері послуг, потрібна міцна рука.",
              "maleVariant": "",
              "stringId": "1892422658254659584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Оскільки ти їй довіряла, то чому я мала в ній сумніватися?",
              "maleVariant": "",
              "stringId": "1892438285358948352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хоч мене тут не звинувачуй.",
              "maleVariant": "",
              "stringId": "1892438861136224256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я нікого не звинувачую. Вона знає суворі реалії роботи в «Хмарах» ліпше за тебе… і за мене, як на те пішло.",
              "maleVariant": "",
              "stringId": "1892439077830746112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А в Найт-Сіті потрібно вміти крутитися.",
              "maleVariant": "",
              "stringId": "1892445515466629120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ось де ми з тобою відрізняємося. Ти думаєш про прибутки, а я — про людей.",
              "maleVariant": "",
              "stringId": "1892445781955928064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти найманка, вона — підла та бездушна сука, а я — просто жалюгідна ідіотка.",
              "maleVariant": "Ти найманець, вона — підла та бездушна сука, а я — просто жалюгідна ідіотка.",
              "stringId": "1892456948099145728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Першими жертвами революції завжди стають революціонери. Якби в «Хмарах» почалася анархія, то все наїбнулося б мідним тазом. Усі вилетіли б на вулицю.",
              "maleVariant": "",
              "stringId": "1892461376864739328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скількома життями ти готова пожертвувати заради своєї химери?",
              "maleVariant": "",
              "stringId": "1892464001190420480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але… мені вже час до цього звикнути… Що б я не робила, життя завжди знайде спосіб розчарувати мене.",
              "maleVariant": "",
              "stringId": "1892499926679248896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та підтримка великої банди.",
              "maleVariant": "",
              "stringId": "1892514551982415872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я їй завжди це казала.",
              "maleVariant": "",
              "stringId": "1892536555586412544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто зачекай секунду. Я мушу зібратися.",
              "maleVariant": "",
              "stringId": "1904483304446058496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мені треба… спокійно це обдумати. На самоті.",
              "maleVariant": "",
              "stringId": "1910142416004907008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ще побачимося, Ві.",
              "maleVariant": "",
              "stringId": "1911209990960513024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До зустрічі, Ві.",
              "maleVariant": "",
              "stringId": "1912548097131610112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Відійти] До зустрічі, Джуді.",
              "maleVariant": "",
              "stringId": "1914240191504121860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж… до зустрічі, Джуді.",
              "maleVariant": "",
              "stringId": "1914240515069509632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Відійти] Немає за що. До зустрічі.",
              "maleVariant": "",
              "stringId": "1914271987885211648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Немає за що. До зустрічі, Джуді.",
              "maleVariant": "",
              "stringId": "1914271988975730692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Відійти] До зустрічі.",
              "maleVariant": "",
              "stringId": "1914272426374529024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "До зустрічі, Джуді.",
              "maleVariant": "",
              "stringId": "1914272427397939204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Відійти] Бережи себе, Джуді.",
              "maleVariant": "",
              "stringId": "1914272550123274240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бувай, Джуді, ще побачимося.",
              "maleVariant": "",
              "stringId": "1914272551163461636"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}