  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вітаннячко!",
              "maleVariant": "",
              "stringId": "1467767613846839296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую, що запросила.",
              "maleVariant": "",
              "stringId": "1467767614400487424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Маєш чудовий вигляд, Джосс.",
              "maleVariant": "",
              "stringId": "1468876195438911488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Маєш чудовий вигляд, Джосс.",
              "maleVariant": "",
              "stringId": "1468876896458104832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти серйозно? Дякую.",
              "maleVariant": "",
              "stringId": "1468877562194812928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую, що покликала мене.",
              "maleVariant": "",
              "stringId": "1468879596985901056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як там Ренді?",
              "maleVariant": "",
              "stringId": "1468881823473459200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я просто допомагала Ріверові.",
              "maleVariant": "",
              "stringId": "1468897740538372116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Головне, що тепер усе позаду.",
              "maleVariant": "",
              "stringId": "1468898314319159296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Головне, що зараз усе добре.",
              "maleVariant": "",
              "stringId": "1468899885438328832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це все Рівер, я тільки була на підтримці.",
              "maleVariant": "",
              "stringId": "1468899943487496192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я просто не знаю… як тобі віддячити… Якби Ренді…",
              "maleVariant": "",
              "stringId": "1468901433404940288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джосс, мені нічого від тебе не треба.",
              "maleVariant": "",
              "stringId": "1468904469795823616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую.",
              "maleVariant": "",
              "stringId": "1468907600944422912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти не підеш?",
              "maleVariant": "",
              "stringId": "1511127588943810560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не підеш?",
              "maleVariant": "",
              "stringId": "1511319042429407236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тут над новою піснею думаю, не хочу відволікатися.",
              "maleVariant": "",
              "stringId": "1572068379557748736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джамбалая. Іди, м’ясо помішаєш.",
              "maleVariant": "",
              "stringId": "1725669026180337664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Круто, басматі.",
              "maleVariant": "",
              "stringId": "1725694887973588992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Спочатку томати…",
              "maleVariant": "",
              "stringId": "1725696439882846208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А тепер можна й рис засипати.",
              "maleVariant": "",
              "stringId": "1725697013999177728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Додати рис] Рис — є.",
              "maleVariant": "",
              "stringId": "1725697800011747332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дуже люб’язно з твого боку зайти до нас.",
              "maleVariant": "",
              "stringId": "1729849648595570688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тут!",
              "maleVariant": "",
              "stringId": "1729858342179803136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, Ренді завжди такий був. Але якоїсь користі від цього малувато… Розумієте, у батька його теж був складний характер…",
              "maleVariant": "",
              "stringId": "1731407383472607232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Привітулі!",
              "maleVariant": "",
              "stringId": "1792592307712823296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, це тобі дякую.",
              "maleVariant": "",
              "stringId": "1792597148157743104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джосс своє зробила. Бери вже ложку й ставай за роботу!",
              "maleVariant": "",
              "stringId": "1793917909808058368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І коли його можна буде знову навідати?",
              "maleVariant": "",
              "stringId": "1793951003051167744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ох, ось це добре, бо я хочу йому дещо принести. Він любить музику, тож… Ще раз?",
              "maleVariant": "",
              "stringId": "1793951325559590912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні-ні, я запам’ятаю, кажіть.",
              "maleVariant": "",
              "stringId": "1793951922560045056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дуже вам дякую! Без вас я б не впоралася, правда. Угу… Так, добре, ще зідзвонимося.",
              "maleVariant": "",
              "stringId": "1793952342023999488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Радий, що ти зайшла. Допоможеш мені з їжею.",
              "maleVariant": "Радий, що ти зайшов. Допоможеш мені з їжею.",
              "stringId": "1805317148954824708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А що там у меню?",
              "maleVariant": "",
              "stringId": "1805317148954824720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну не знаю, може, краще Джосс покликати?..",
              "maleVariant": "",
              "stringId": "1805317148954824744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це давній рецепт сім’ї Вордів?",
              "maleVariant": "",
              "stringId": "1805324684592386052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага. Цибуля, паприка, чебрець…",
              "maleVariant": "",
              "stringId": "1805324684592386064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якийсь ти дивний, не такий, як зазвичай.",
              "maleVariant": "",
              "stringId": "1805543136359845892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А я думала, ти жартував про куховаріння разом.",
              "maleVariant": "А я думав, ти жартував про куховаріння разом.",
              "stringId": "1805543746245201920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вигляд у тебе сьогодні якийсь… незвичний.",
              "maleVariant": "",
              "stringId": "1805544474007277568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чесно, я думала, що ти жартував про те, щоби готувати разом.",
              "maleVariant": "Чесно, я думав, що ти жартував про те, щоби готувати разом.",
              "stringId": "1805544512913641472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага… Незвичний, але все ж нормальний. Наче ти добренько виспався.",
              "maleVariant": "",
              "stringId": "1805545602744815616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Або ж, що Джосс усе зробить сама, поки ми…",
              "maleVariant": "",
              "stringId": "1805545845175586816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти його прикінчив?",
              "maleVariant": "",
              "stringId": "1823884956834861056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А що там, один постріл у голову, і все тобі.",
              "maleVariant": "",
              "stringId": "1823884987990151168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він досі в комі?",
              "maleVariant": "",
              "stringId": "1823885021813018624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я не був певен, чи цього мені хочеться.",
              "maleVariant": "",
              "stringId": "1823886683646595072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я теж так подумав. Це було б так легко…",
              "maleVariant": "",
              "stringId": "1823887329804292096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага. Спить десь на межі життя та смерті. Просто людська оболонка у лікарняному ліжку.",
              "maleVariant": "",
              "stringId": "1823887919405993984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нє, пікніки — то не моє. Але ви повеселіться.",
              "maleVariant": "",
              "stringId": "1844535253764542464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Досі в лікарні, досі відходить від усього. Але йому вже ліпше.",
              "maleVariant": "",
              "stringId": "1844539881474363392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якось усе не виходило висловити тобі свою подяку.",
              "maleVariant": "",
              "stringId": "1844540005474766848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Реально? Джосс навіть чайник ледь-ледь поставити може.",
              "maleVariant": "",
              "stringId": "1845939007655120896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це я люблю з їжею помутузитися, ха… Просто давно нагоди такої не випадало.",
              "maleVariant": "",
              "stringId": "1845939459297775616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Та невже?",
              "maleVariant": "",
              "stringId": "1845942268642594816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм. Ну, я хотів показати тобі, що таким я теж буваю.",
              "maleVariant": "",
              "stringId": "1845944266758688768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не хотілося би паплюжити такий чудовий день, але не можу не спитати…",
              "maleVariant": "",
              "stringId": "1846042664576880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як там справи в Ренді?",
              "maleVariant": "",
              "stringId": "1846042691554643968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Потрошку оговтується.",
              "maleVariant": "",
              "stringId": "1846043889145876480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але фізично відновлюватися йому ще довго й тяжко.",
              "maleVariant": "",
              "stringId": "1846044045777965056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А морально? Таке пережити…",
              "maleVariant": "",
              "stringId": "1846044205362843648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як би це не звучало, але те, що зробив Гарріс, у якомусь сенсі йому допомогло.",
              "maleVariant": "",
              "stringId": "1846044378235277312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можу помилятися, але… для нього це наче життя з нового аркуша.",
              "maleVariant": "",
              "stringId": "1846044517838491648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що там із Пітером Пеном?",
              "maleVariant": "",
              "stringId": "1846048007214206976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тобто чи вбив я його?",
              "maleVariant": "",
              "stringId": "1846048161312935936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Побалакаємо про це потім, гаразд?",
              "maleVariant": "",
              "stringId": "1846048601681301504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно, без проблем. Просто, ну… сам розумієш.",
              "maleVariant": "",
              "stringId": "1846049752095002624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я розумію.",
              "maleVariant": "",
              "stringId": "1846050296297558016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гараздоньки, тепер наче добре помішано.",
              "maleVariant": "",
              "stringId": "1846053967571849216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можеш принести з кухні рис?",
              "maleVariant": "",
              "stringId": "1846054508049862656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так, пане.",
              "maleVariant": "",
              "stringId": "1846055070556360704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я ходив до нього в лікарню.",
              "maleVariant": "",
              "stringId": "1848698917249748992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я приставив пістолет до його чола і…",
              "maleVariant": "",
              "stringId": "1848836810983215104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дещо згадав…",
              "maleVariant": "",
              "stringId": "1848838973096595456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як померли мої батьки. Ти точно хочеш це слухати?",
              "maleVariant": "",
              "stringId": "1848839389590982656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Знаєш, дійсно ліпше якось іншим разом.",
              "maleVariant": "",
              "stringId": "1848842770518773760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У нашого старого була ферма, зовсім як у батька Гарріса. Але потім справи пішли кепські…",
              "maleVariant": "",
              "stringId": "1848844123685130240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Батьки тоді відкрили невеличкий продуктовий магазин просто під нами.",
              "maleVariant": "",
              "stringId": "1848844563667619840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вони шукали готівку, але в касі було порожньо. Тому пішли нагору.",
              "maleVariant": "",
              "stringId": "1848848292437430272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поставили нас усіх на коліна й почали допитувати батька, де він ховає гроші.",
              "maleVariant": "",
              "stringId": "1848850226045128704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я пам’ятаю тільки, як мама плакала, а тато клявся, що більше в них нічого нема.",
              "maleVariant": "",
              "stringId": "1848850575984300032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Рівере, якщо не хочеш, можеш не продовжувати…",
              "maleVariant": "",
              "stringId": "1848851324013252608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Один із них був угашений у зюзю. Він і зробив перший постріл батькові в голову.",
              "maleVariant": "",
              "stringId": "1848851350722580480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що, вже втомилася від ниття?",
              "maleVariant": "Що, вже втомився від ниття?",
              "stringId": "1848851547468992512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Боже, Рівере… Мені дуже прикро.",
              "maleVariant": "",
              "stringId": "1848852298937274368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Його почало плющити. Він дав мені пушку і звелів націлитися на голову мами.",
              "maleVariant": "",
              "stringId": "1848852456743768064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я досі бачу перед собою її заплакані очі, якими вона дивилася на те дуло.",
              "maleVariant": "",
              "stringId": "1848852641561579520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І що ти зробив?",
              "maleVariant": "",
              "stringId": "1848854783324196864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нічого. Стояв, як у землю вкопаний.",
              "maleVariant": "",
              "stringId": "1848855534691815424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Коли йому набридло, він забрав пістолет, застрелив маму, і вони пішли. Поліція їх так і не знайшла.",
              "maleVariant": "",
              "stringId": "1848855891811635200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тому я й пішов у детективи. Я не міг дозволити такій самій наволочі спокійно гуляти на свободі. Їх треба ловити.",
              "maleVariant": "",
              "stringId": "1848856729229594624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Словами не описати, як мені прикро.",
              "maleVariant": "",
              "stringId": "1848858376265666560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джосс теж там була? І теж усе бачила?",
              "maleVariant": "",
              "stringId": "1848858380745183232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тож коли я стояв над непритомним Гаррісом і вже цілив у його брову…",
              "maleVariant": "",
              "stringId": "1848858960918089728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу. Мені здається, що вона й досі бачить у мені того хлопчика з пістолетом, приставленим до маминої голови.",
              "maleVariant": "",
              "stringId": "1848859445041434624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я знову став тим малюком… Але також і тим обдовбнем, що все ж застрелив моїх батьків.",
              "maleVariant": "",
              "stringId": "1848860489305690112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я розумію.",
              "maleVariant": "",
              "stringId": "1848863771298238464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То ти його не вбив?",
              "maleVariant": "",
              "stringId": "1848863808040341504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я багато вбивав раніше, але… ще ніколи — ось так… це було б не вбивство, а страта.",
              "maleVariant": "",
              "stringId": "1848864659970592768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я прошмигнув із лікарні й вирішив, що на цьому справу закрито.",
              "maleVariant": "",
              "stringId": "1848864978016276480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це добре. Але що далі? Ти ж не повернешся до поліції?",
              "maleVariant": "",
              "stringId": "1848867727248330752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я думав про роботу приватним слідчим… А там подивимося.",
              "maleVariant": "",
              "stringId": "1848868130287390720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Однієї ночі нас розбудив шум.",
              "maleVariant": "",
              "stringId": "1872262060496261120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Рівер надворі. Він без угаву все тріщить, яка ж ти чудова.",
              "maleVariant": "Рівер надворі. Йому аж не терпілося тебе побачити.",
              "stringId": "1904172619986505728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я те саме їм казала! А вони мені те саме відповідають раз за разом. Усім байдуже, що такі діти всюди зайві.",
              "maleVariant": "",
              "stringId": "1905691732789563392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну, так, так і сказали. Я щоразу ледь волосся не рву від цих слів.",
              "maleVariant": "",
              "stringId": "1905692788026118144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм, здається, десь тут…",
              "maleVariant": "",
              "stringId": "1921535222464090116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ем-м, а де рис?",
              "maleVariant": "",
              "stringId": "1937170448015921156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як справи, Джосс?",
              "maleVariant": "",
              "stringId": "1937171366165843968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм… а де у вас тут рис?",
              "maleVariant": "",
              "stringId": "1937172822092648448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тобі передзвоню.",
              "maleVariant": "",
              "stringId": "1937174436111159296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А, щодо рису, він… ось тут.",
              "maleVariant": "",
              "stringId": "1937204120140091392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Перемішати] Це сімейний рецепт?",
              "maleVariant": "",
              "stringId": "1944599319619448832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як Ренді тримається?",
              "maleVariant": "",
              "stringId": "1968168959607242752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти його вбив?",
              "maleVariant": "",
              "stringId": "1969089996066848768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він досі в комі?",
              "maleVariant": "",
              "stringId": "1969089996083626012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можеш не продовжувати…",
              "maleVariant": "",
              "stringId": "1969089996469501952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Словами не описати, як мені прикро.",
              "maleVariant": "",
              "stringId": "1969089996771491840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джосс теж це все бачила?",
              "maleVariant": "",
              "stringId": "1969089996788269056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я розумію.",
              "maleVariant": "",
              "stringId": "1969089996888932352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То ти його не вбив?",
              "maleVariant": "",
              "stringId": "1969089996905709572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добре, хай закипить і ще трохи повариться. Ходімо?",
              "maleVariant": "",
              "stringId": "1969106268590637056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бери стілець і… пиво чи що ти там п’єш.",
              "maleVariant": "",
              "stringId": "1969153477763858432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Гарно у вас тут.",
              "maleVariant": "",
              "stringId": "1969159772726296576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гарно тут у вас.",
              "maleVariant": "",
              "stringId": "1969161237058486272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дядечко Рівер, ти казав, що пограєш із нами!",
              "maleVariant": "",
              "stringId": "1969188799994331136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага. Наче все добре й нема в нас ніяких бід.",
              "maleVariant": "",
              "stringId": "1969530769014980608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно, без проблем.",
              "maleVariant": "",
              "stringId": "1970444021232238608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти помстився Пітерові Пену?",
              "maleVariant": "",
              "stringId": "1970445471790333952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як там справи в Ренді?",
              "maleVariant": "",
              "stringId": "1970445528128225280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Як ти?",
              "maleVariant": "",
              "stringId": "1970455480255315968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "О, соєве? Добре.",
              "maleVariant": "",
              "stringId": "1978155355433881600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не забувай перемішувати, щоби нічого не підгоріло.",
              "maleVariant": "",
              "stringId": "1978162610673831936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мішаєш там?",
              "maleVariant": "",
              "stringId": "1978165895501729796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зуб даю, ти смачнішої джамбалаї більше ніде не скуштуєш.",
              "maleVariant": "",
              "stringId": "1978168286104014848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ще треба подрібнити селеру та часник… Але ти перемішуй, не відволікайся.",
              "maleVariant": "",
              "stringId": "1978169142832549888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Перемішую, перемішую.",
              "maleVariant": "",
              "stringId": "1978170469407019008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Рис — є.",
              "maleVariant": "",
              "stringId": "1978181018886885376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То ти розповіси мені, що там із Гаррісом?",
              "maleVariant": "",
              "stringId": "1978184846071844864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Рівере?..",
              "maleVariant": "",
              "stringId": "1978187298179403776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "І правда, ліпше іншим разом.",
              "maleVariant": "",
              "stringId": "1978190352219918340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що сталося з твоїми батьками?",
              "maleVariant": "",
              "stringId": "1978191222168899584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що… сталося?",
              "maleVariant": "",
              "stringId": "1978194030188318720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми ж хотіли відпочити, спокійно побалакати. А такі теми, гм… доволі грузькі…",
              "maleVariant": "",
              "stringId": "1978198353207119872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ой, ну так. Напевно, маєш рацію.",
              "maleVariant": "",
              "stringId": "1978200006031331328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто… що ти зробив із Гаррісом? Приставив пістолет до чола, а потім?..",
              "maleVariant": "",
              "stringId": "1978200586254569472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, просто… Я бачу, що тобі важко. Тому зовсім не обов’язково знову це згадувати.",
              "maleVariant": "",
              "stringId": "1978208606367797248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти обіцяв!",
              "maleVariant": "",
              "stringId": "1978212477777498112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гаразд-гаразд, ідемо грати. Ві, ти з нами?",
              "maleVariant": "",
              "stringId": "1978212788743196672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Підвестися] А в що гратимемо?",
              "maleVariant": "",
              "stringId": "1978213316285976580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У що гратимемо, дядько Рівере?",
              "maleVariant": "",
              "stringId": "1978213966939967488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зараз побачиш!",
              "maleVariant": "",
              "stringId": "1978214595582251008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хех, легко ж мене вибити з рівноваги.",
              "maleVariant": "",
              "stringId": "1978220510171648000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хіба Джосс не може допомогти?",
              "maleVariant": "",
              "stringId": "2044836924885184520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Перемішати]",
              "maleVariant": "",
              "stringId": "2047363457415053312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Зачекати]",
              "maleVariant": "",
              "stringId": "2119840637512724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Підвестися]",
              "maleVariant": "",
              "stringId": "2222888715950518276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Випити]",
              "maleVariant": "",
              "stringId": "2230311342334685184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Привіт, як справи?",
              "maleVariant": "",
              "stringId": "2443210360907657220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Привітики, як справи?",
              "maleVariant": "",
              "stringId": "2483811212787097600"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}