  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти запізнився!",
              "maleVariant": "",
              "stringId": "1561834548131422208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я дорогою зустрів супутницю.",
              "maleVariant": "Я дорогою зустрів супутника.",
              "stringId": "1561845962845872128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хтось зі студії?",
              "maleVariant": "",
              "stringId": "1561846529244684288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, познайомся із Зулейхою.",
              "maleVariant": "",
              "stringId": "1561849602897764352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зулейха — сестра вбитого мною чоловіка.",
              "maleVariant": "",
              "stringId": "1561932080983465984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вона мені писала, коли я сидів у в’язниці.",
              "maleVariant": "",
              "stringId": "1561943568947240960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джошуа Стівенсон, ми знов зустрілися. А ти хто?",
              "maleVariant": "",
              "stringId": "1561961856397893632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Підвестися] Ходімо.",
              "maleVariant": "",
              "stringId": "1772039950399672324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Підвестися] З мене досить, Джош. Я йду.",
              "maleVariant": "",
              "stringId": "1772040432862072832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нам варто піти.",
              "maleVariant": "",
              "stringId": "1772043282858086400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "З мене досить, Джош.",
              "maleVariant": "",
              "stringId": "1772043536814804992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Прошу, пані Ель-Ахмар!",
              "maleVariant": "",
              "stringId": "1772044680601505792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зажди!",
              "maleVariant": "",
              "stringId": "1772045029550821376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пам’ятаєш, що ти писала в останньому листі? Так от, вона відповідає всім пунктам.",
              "maleVariant": "Пам’ятаєш, що ти писала в останньому листі? Так от, він відповідає всім пунктам.",
              "stringId": "1772176648135741440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Стулися.",
              "maleVariant": "",
              "stringId": "1773651305661698048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дуже приємно, Ві.",
              "maleVariant": "",
              "stringId": "1811138549722406912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Приємно познайомитися.",
              "maleVariant": "",
              "stringId": "1811139031480164356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хтось пояснить мені, що тут відбувається?",
              "maleVariant": "",
              "stringId": "1811148222190084096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хочеться все ж розуміти, що відбувається.",
              "maleVariant": "",
              "stringId": "1811155094926286848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти їй нічого не розповів?",
              "maleVariant": "Ти йому нічого не розповів?",
              "stringId": "1811156189337640960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми тільки-но зустрілися. Часу не було.",
              "maleVariant": "",
              "stringId": "1811156888880439296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мама ще не повернулася. Можемо поговорити в нас, поки вона не повернеться. Заходьте.",
              "maleVariant": "",
              "stringId": "1811157650431827968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "М-м-м.",
              "maleVariant": "",
              "stringId": "1811157890446680064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви з Джошуа друзі?",
              "maleVariant": "",
              "stringId": "1811157976312471552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти дійсно навіжений.",
              "maleVariant": "",
              "stringId": "1811159385883828224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Радше нові знайомі.",
              "maleVariant": "",
              "stringId": "1811161631665184772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це складне питання.",
              "maleVariant": "",
              "stringId": "1811162098726100992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, не друзі.",
              "maleVariant": "",
              "stringId": "1811162537618071552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Радше нові знайомі.",
              "maleVariant": "",
              "stringId": "1811162793973932032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це довга та дивна історія.",
              "maleVariant": "",
              "stringId": "1811162857945456640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні. Ми не друзі.",
              "maleVariant": "",
              "stringId": "1811162962501066752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ех…",
              "maleVariant": "",
              "stringId": "1811176682589544448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мама ще не повернулася. Можемо поговорити в нас, поки вона не повернеться. Заходьте.",
              "maleVariant": "",
              "stringId": "1811179700106059776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не уявляю, як можна за таке пробачити.",
              "maleVariant": "",
              "stringId": "1811269762651021312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіщо ми сюди приїхали?",
              "maleVariant": "",
              "stringId": "1811269951226929152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це через тебе Джошуа став вірянином?",
              "maleVariant": "",
              "stringId": "1811269990988931072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тобто ти кажеш, що просто взяла й пробачила вбивцю брата?",
              "maleVariant": "",
              "stringId": "1811270692494663680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це завдяки тобі Джошуа став вірянином?",
              "maleVariant": "",
              "stringId": "1811270860988243968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіщо ми сюди прийшли? І на кого чекаємо?",
              "maleVariant": "",
              "stringId": "1811271103570010112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так ми й здружились. Через прощення.",
              "maleVariant": "",
              "stringId": "1811439695431229440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«Не суди, і тебе не судитимуть. Не засуджуй, і тебе не засудять. Пробачай, і тебе пробачать».",
              "maleVariant": "",
              "stringId": "1811440619100856320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "З її листа все почалося.",
              "maleVariant": "",
              "stringId": "1811442825942294528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я втратила брата. Бог дав мені іншого.",
              "maleVariant": "",
              "stringId": "1811444224205807616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Твій Бог дозволив смерть невинних людей.",
              "maleVariant": "",
              "stringId": "1811447492457816068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Щирих вірян зараз майже не зустрінеш.",
              "maleVariant": "",
              "stringId": "1811447936164847616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це якась нісенітниця.",
              "maleVariant": "",
              "stringId": "1811448304441516032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У Найт-Сіті не кожен день зустрінеш справжнього вірянина.",
              "maleVariant": "",
              "stringId": "1811448378596810752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти віриш у Бога милостивого. Але ж він дозволив убити твого брата. І нічого не зробив.",
              "maleVariant": "",
              "stringId": "1811454099476471808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіщо ми сюди приїхали?",
              "maleVariant": "",
              "stringId": "1811459055650627584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я довго оплакувала Рубена. Ти навіть не уявляєш скільки. Я хотіла, щоби він повернувся. Але потім Бог показав мені свої плани. І я змирилася.",
              "maleVariant": "",
              "stringId": "1811485843697197056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Господь знає, що я покаявся у вбивстві Рубена. Але якби не він, я б ніколи не знайшов шлях до Бога.",
              "maleVariant": "",
              "stringId": "1811487559301750784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Усе ще не вірю.",
              "maleVariant": "",
              "stringId": "1811489986914574340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "То це все було Боже провидіння?",
              "maleVariant": "",
              "stringId": "1811490264359395328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага, усе ще не вірю.",
              "maleVariant": "",
              "stringId": "1811491176150753280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Несповідимі шляхи Господні.",
              "maleVariant": "",
              "stringId": "1811491257050488832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Саме Боже провидіння привело тебе в мій дім. Ми зустрілися завдяки йому.",
              "maleVariant": "",
              "stringId": "1811494094664916992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ві, ти віриш у Бога?",
              "maleVariant": "",
              "stringId": "1811497254469001216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Отже, ти не віриш у Творця?",
              "maleVariant": "",
              "stringId": "1811497641250938880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так, вірю.",
              "maleVariant": "",
              "stringId": "1811497981828423684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні.",
              "maleVariant": "",
              "stringId": "1811498165773819904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можливо, якась вища сила є.",
              "maleVariant": "",
              "stringId": "1811498617584246784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звісно ж вірю.",
              "maleVariant": "",
              "stringId": "1811499676612452352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не можу сказати, що вірю.",
              "maleVariant": "",
              "stringId": "1811499757478633472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можливо, є якась вища сила, але ніхто цього не зміг довести. І навряд колись зможе.",
              "maleVariant": "",
              "stringId": "1811499828228153344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чому ми взагалі про це розмовляємо?",
              "maleVariant": "",
              "stringId": "1811500367246548992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чого ми взагалі про це розмовляємо?",
              "maleVariant": "",
              "stringId": "1811501415302455296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Віра — це така річ, яку ніхто крім тебе довести не може.",
              "maleVariant": "",
              "stringId": "1811507218289811456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У нас із Зулейхою виникла суперечка, і я сподіваюся, що ти зможеш допомогти нам її вирішити.",
              "maleVariant": "",
              "stringId": "1811508137177931776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Боже милосердя не має меж, як і наша любов до ближнього.",
              "maleVariant": "",
              "stringId": "1811509781345099776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скажи мені, Ві, ти віриш у Бога?",
              "maleVariant": "",
              "stringId": "1811511187946889216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні-ні. Ми про таке не домовлялися!",
              "maleVariant": "",
              "stringId": "1811512360724619264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Уважно слухаю.",
              "maleVariant": "",
              "stringId": "1811512786631024644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я геть нічого не тямлю в релігійних дебатах.",
              "maleVariant": "",
              "stringId": "1811513129020448768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я слухаю.",
              "maleVariant": "",
              "stringId": "1811513682937012224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Е-е… Релігійні дебати — це зовсім не моє.",
              "maleVariant": "",
              "stringId": "1811513760464527360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Джошуа, він… Він вважає себе Месією та хоче забрати своє життя.",
              "maleVariant": "",
              "stringId": "1811514137784115200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це неправда. Я ніколи не називав себе якимось месією.",
              "maleVariant": "",
              "stringId": "1811517334867173376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Самогубство! Ти справді вважаєш, що Бог може хотіти від тебе такого?",
              "maleVariant": "",
              "stringId": "1811519190846693376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це буде не самогубство.",
              "maleVariant": "",
              "stringId": "1811521289223430144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ого. Це якесь довбане божевілля.",
              "maleVariant": "",
              "stringId": "1811522415494717440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зулейхо, розкажи, що відбувається.",
              "maleVariant": "",
              "stringId": "1811528246265143296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що тут насправді відбувається, Джошуа?",
              "maleVariant": "",
              "stringId": "1811531016921702400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я досі не розумію, що тут відбувається, Зулейхо.",
              "maleVariant": "",
              "stringId": "1811531101126549504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Коли медіа дізналися, що у в’язниці я знайшов Бога, уже наступного дня моє обличчя було на кожному крикунці.",
              "maleVariant": "",
              "stringId": "1811532787136094208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це помітила МГ-студія та надіслала представників із пропозицією.",
              "maleVariant": "",
              "stringId": "1811533748437016576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ех, кровопивці.",
              "maleVariant": "",
              "stringId": "1811536365414277120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На мене чекав електростілець. А тепер, завдяки студії, моя смерть стане дечим більшим, ніж убивством із дозволу держави.",
              "maleVariant": "",
              "stringId": "1811536696126758912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вони хочуть записати мозкограй розп’яття Джошуа. Та сама страта, лише вбрана у священні шати.",
              "maleVariant": "",
              "stringId": "1811538281774997504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чому саме мозкограй? Чого ти намагаєшся досягти?",
              "maleVariant": "",
              "stringId": "1811542239067267072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хіба це не протизаконно?",
              "maleVariant": "",
              "stringId": "1811542324681400320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чекай. МГ-студія змінила спосіб виконання смертної карти? Хіба органи юстиції не мають бути проти?",
              "maleVariant": "",
              "stringId": "1811543037796966400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Корпорації пишуть закони. Корпорації призначають суддів. Достатньо лише однієї зустрічі директора студії з прокурором штату.",
              "maleVariant": "",
              "stringId": "1811544639719104512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Офіційно я перебуваю в одноденному звільненні під наглядом. Але жоден закон не забороняє мені провести його з новими знайомими.",
              "maleVariant": "",
              "stringId": "1811545251198296064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Коли Джошуа кинули за ґрати, у крикунцях присвятили йому місце на п’ятій сторінці.",
              "maleVariant": "",
              "stringId": "1811551495007002624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але коли їм стало відомо, що він повірив у Бога, раптом почали це обговорювати по телебаченню в прайм-тайм.",
              "maleVariant": "",
              "stringId": "1811552475333287936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хіба погано скористатися можливістю, аби розповісти людям про мою віру та Божу благодать?",
              "maleVariant": "",
              "stringId": "1811553203632234496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти пішов значно далі, Джошуа! Замість того, аби бути страченим у в’язниці, він погодився дозволити стервоїдам зі студії мозкограїв записати свою агонію під час розп’яття!",
              "maleVariant": "",
              "stringId": "1811553690540597248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Корупція також гріх, чи не так? І саме через неї ти зараз тут.",
              "maleVariant": "",
              "stringId": "1811561695805988864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це значно важливіше за людські закони. З цим мозкограєм я донесу Слово Боже до мільйонів.",
              "maleVariant": "",
              "stringId": "1811561871379554304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це безумство, абсолютне та очевидне.",
              "maleVariant": "",
              "stringId": "1811566712260681728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Але чому мозкограй? Чого ти хочеш цим досягти?",
              "maleVariant": "",
              "stringId": "1811566786432753664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Єдина річ, що гірша за безумство — це богохульство.",
              "maleVariant": "",
              "stringId": "1811567998704050176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зулейха перетворила мене на дитя Боже. Час повернути борг.",
              "maleVariant": "",
              "stringId": "1811577709037903872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Подумай про мільйони, під’єднані до годівниць наче худоба; про дітей, що зростають серед вуличних банд. Чи знають вони про любов?",
              "maleVariant": "",
              "stringId": "1811578731273674752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Подумай про хвойд на Шпек-стріт! Про обдовбаних корпарів у їхніх офісах; про вбивць, що вештаються вулицями в пошуках жертви! Я хочу достукатися до них. І я достукаюсь.",
              "maleVariant": "",
              "stringId": "1811580596883316736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Навіть подумай про найманих убивць, як ти, Ві. Чи пізнала ти любов? Чи знаєш, що це таке?",
              "maleVariant": "Навіть подумай про найманих убивць, як ти, Ві. Чи пізнав ти любов? Чи знаєш, що це таке?",
              "stringId": "1811598778872389632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "О, мама вже вдома!",
              "maleVariant": "",
              "stringId": "1811599568592723968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скажи мені, Ві, ти віриш у Бога?",
              "maleVariant": "",
              "stringId": "1811600494409834496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тут нема про що дебатувати. Ці гієни з МГ-студії переконали Джошуа замінити одну страту на іншу — його власне розп’яття.",
              "maleVariant": "",
              "stringId": "1811602284085796864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Він хоче, щоби всі могли відчути його агонію! Відчути його біль та страждання!",
              "maleVariant": "",
              "stringId": "1811603067397234688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я хочу засвідчити існування любові. І зробити це таким чином, який зрозуміє кожна заблудла душа на цій грішній землі.",
              "maleVariant": "",
              "stringId": "1811604787800072192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Любов?",
              "maleVariant": "",
              "stringId": "1811607145183469568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо безумовна любов існує, тоді існує і Бог! Настав час світу нарешті згадати про Його божественну присутність.",
              "maleVariant": "",
              "stringId": "1811607228113248256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мене звуть Ві.",
              "maleVariant": "",
              "stringId": "1812391006913978368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми її чекали?",
              "maleVariant": "",
              "stringId": "1812391364671332352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Моє ім’я Ві.",
              "maleVariant": "",
              "stringId": "1812391461794635776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми на неї чекаємо?",
              "maleVariant": "",
              "stringId": "1812391529104826368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Забирайся геть із мого дому.",
              "maleVariant": "",
              "stringId": "1812392991306309632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так.",
              "maleVariant": "",
              "stringId": "1812395297636638720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пані Ель-Ахмар. Я прийшов молити вас про прощення.",
              "maleVariant": "",
              "stringId": "1812395582614429696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти… Ти мого сина вбив!",
              "maleVariant": "",
              "stringId": "1812402402586288128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Цього я ніколи не пробачу.",
              "maleVariant": "",
              "stringId": "1812402544857079808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Геть із мого дому, негайно!",
              "maleVariant": "",
              "stringId": "1812402743499317248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Геть! Усі!",
              "maleVariant": "",
              "stringId": "1812402916908621824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я щиро шкодую, що спричинив вам біль.",
              "maleVariant": "",
              "stringId": "1812403482518904832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ні, навіть не розраховуй на мене.",
              "maleVariant": "",
              "stringId": "1812407937003970560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Зулейхо?",
              "maleVariant": "",
              "stringId": "1831726537145606144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Склянку води, будь ласка.",
              "maleVariant": "",
              "stringId": "1831812314202398724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сідайте, будь ласка.",
              "maleVariant": "",
              "stringId": "1831812422063120384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вам принести щось випити?",
              "maleVariant": "",
              "stringId": "1831812734270332928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Чогось міцного.",
              "maleVariant": "",
              "stringId": "1831813890925158400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти] Нічого, дякую.",
              "maleVariant": "",
              "stringId": "1831814457072312320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Е-е, склянку води.",
              "maleVariant": "",
              "stringId": "1831816429049503744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гадаю, я, мабуть, буду щось міцніше.",
              "maleVariant": "",
              "stringId": "1831816506593796096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Дякую, не треба.",
              "maleVariant": "",
              "stringId": "1831816574038204416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Прошу.",
              "maleVariant": "",
              "stringId": "1831823181526622208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тут алкоголь не подають. У нас його немає.",
              "maleVariant": "",
              "stringId": "1831823659056521216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти забрав у мене мого Рубена…",
              "maleVariant": "",
              "stringId": "1831857185755783168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Моє дитя, мого синочка…",
              "maleVariant": "",
              "stringId": "1831857908367253504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ти вбив мого сина!",
              "maleVariant": "",
              "stringId": "1831858327680212992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мого Рубена…",
              "maleVariant": "",
              "stringId": "1831858860256157696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "В ім’я Отця, і Сина, і Святого Духа. Амінь.",
              "maleVariant": "",
              "stringId": "1831892645978857472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Убивця!",
              "maleVariant": "",
              "stringId": "1846398371870826496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нехай, просто забирайтеся.",
              "maleVariant": "",
              "stringId": "1846398920569675776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пані Ель-Ахмар! Будь ласка!",
              "maleVariant": "",
              "stringId": "1846399980621291520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто забирайся, прошу.",
              "maleVariant": "",
              "stringId": "1846400327708336128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не любиш чекати?",
              "maleVariant": "",
              "stringId": "1885205295892934660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Схоже, хтось не вміє терпляче чекати.",
              "maleVariant": "",
              "stringId": "1885212122491793408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Васкезе, пильнуй їх. Я залишусь у машині.",
              "maleVariant": "",
              "stringId": "1943348215006859264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тільки швидше. У нас щільний графік.",
              "maleVariant": "",
              "stringId": "1943348585162575872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тільки недовго. Я чекатиму зовні.",
              "maleVariant": "",
              "stringId": "1957784562128203776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поясниш, Джошуа?",
              "maleVariant": "",
              "stringId": "1983947927548407808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ліпше повернімося в машину.",
              "maleVariant": "",
              "stringId": "2008499214941184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Вийти]",
              "maleVariant": "",
              "stringId": "2095269084884447236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Пити]",
              "maleVariant": "",
              "stringId": "2218693984013070336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сісти в авто]",
              "maleVariant": "",
              "stringId": "2221575440561418240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У тебе все гаразд?",
              "maleVariant": "",
              "stringId": "2230541593284538368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гм?",
              "maleVariant": "",
              "stringId": "2230541798855766016"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}