        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вітаю. Я Ґіліан Джордан і ви дивитеся світові новини на «Каналі 54».",
              "maleVariant": "",
              "stringId": "1751894983099080704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вчені з «Біодину» оголосили про прорив у лікуванні множинного склерозу.",
              "maleVariant": "",
              "stringId": "1751898735172444160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "МС — одна з кількох хвороб, які не можна зцілити, замінивши пошкоджені тканини імплантами.",
              "maleVariant": "",
              "stringId": "1751898878349205504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Однак експерти з «Біодину» створили нанітів, які у разі введення до спинного мозку забезпечують тривале безперервне відновлення нервових волокон.",
              "maleVariant": "",
              "stringId": "1751898982250504192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Оголошена вартість цієї найновітнішої лікувальної технології складає 700 000 євродоларів на місяць.",
              "maleVariant": "",
              "stringId": "1751899134788952064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Другий тиждень поспіль пекельні пожежі вирують на рештках Атлантик-Сіті.",
              "maleVariant": "",
              "stringId": "1751899297779605504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Територія в 4 000 гектар вважається найбільшим звалищем з токсичними та біологічними відходами у світі.",
              "maleVariant": "",
              "stringId": "1751899466004750336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вже померло семеро пожежників, а сорок лікують серйозні опіки та токсичні опіки.",
              "maleVariant": "",
              "stringId": "1751899582707064832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Оголошено нового лауреата шведської Академічної нагороди з літератури. Це Вірджинія Ґранчестер. Її останню книгу — «У тіні яблуні» визнано скарбом цього покоління.",
              "maleVariant": "",
              "stringId": "1751899722125729792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ґранчестер — це ШІ, створений компанією «Мікрокібернетичний ворон», й кілька років поспіль отримує різноманітні нагороди.",
              "maleVariant": "",
              "stringId": "1751899946386776064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Її перший роман «Реквієм для самурая» одночасно вийшов 70 мовами й продався накладом понад 3 мільйони копій.",
              "maleVariant": "",
              "stringId": "1751900245692309504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "«У тіні яблуні» — драматичний твір про Ірландію двадцятого століття. Його вважають шедевром постапокаліптичної літератури.",
              "maleVariant": "",
              "stringId": "1751900392778162176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Це були світові новини. Дякую, що обираєте новини «Каналу 54».",
              "maleVariant": "",
              "stringId": "1751900563637329920"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}