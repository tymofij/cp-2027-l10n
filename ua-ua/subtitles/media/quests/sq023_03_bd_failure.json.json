        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Здорів усім. Це Каріна — ваша експертка з мозкограїв. Вже стигли пірнути в «Страсті Джошуа»?",
              "maleVariant": "",
              "stringId": "1792308309140611072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Якщо ні, то це добре. Ви такої смердючої купи лайна давненько не бачили в бібліотеках своїх візорів.",
              "maleVariant": "",
              "stringId": "1802363372603949056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Усе те саме, як і в тих, які й раніше грали нам мізки емоційним навантаженням. Однак у цьому — жодної глибини та суті.",
              "maleVariant": "",
              "stringId": "1802363590288326656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вас силоміць змушують відчувати якісь нещирі почуття.",
              "maleVariant": "",
              "stringId": "1802363776079216640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А та велична сцена смерті, якою вони так гайпували у всіх трейлерах? Збіса боляче, але той мужик, що помер — нікчема.",
              "maleVariant": "",
              "stringId": "1802363957659025408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я ставлю 2 зірки — і це ще дуже щедро. Я щиро очікувала більшого від Рейчел Касич.",
              "maleVariant": "",
              "stringId": "1802364106489708544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Після перерви повернуся з іншими новинами МГ з усього світу!",
              "maleVariant": "",
              "stringId": "1802364303940763648"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}