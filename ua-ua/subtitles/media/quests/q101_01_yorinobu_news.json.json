          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "… повністю забезпечити спокій та захищеність моєї родини та моїх працівників.",
              "maleVariant": "",
              "stringId": "1792139386969563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Саме тому я вирішив подвоїти кількість оперативників нашої служби безпеки в Найт-Сіті.",
              "maleVariant": "",
              "stringId": "1793874625730166784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Президентка Маєрс назвала це порушенням Арвінської угоди — провокацією, спрямованою проти НСША та «Мілітеху».",
              "maleVariant": "",
              "stringId": "1793876078016315392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скажіть мені — а як же тоді назвати вбивство мого батька? Чи не було це провокацією? Чи не було це…",
              "maleVariant": "",
              "stringId": "1793876414248501248"
            }
          ]
        }
