        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Привіт, привіт. Мене звати Аріф Ікбал і ви дивитеся новини від ВСН.",
              "maleVariant": "",
              "stringId": "1672049718354419712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На щастя служба безпеки «Арасаки» — під особистим керівництвом Йорінобу Арасаки — швидко піймала викрадачів та повернула Ханако-саму.",
              "maleVariant": "",
              "stringId": "1680710522355634176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Тим, хто підійме руку на сім’ю Арасака, я обіцяю…",
              "maleVariant": "",
              "stringId": "1680711038976446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "З’явилися нові подробиці у справі спроби викрадення Ханако Арасаки на параді даші, який проводила корпорація «Арасака».",
              "maleVariant": "",
              "stringId": "1680711838058467328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Згідно з першими свідченнями напад відбувся у самий розпал параду. Вони перехопили й захопили платформу, де перебувала спадкоємиця сім’ї Арасака.",
              "maleVariant": "",
              "stringId": "1680712023195045888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Йорінобу-сама публічно виступив із заявою, де засудив жалюгідні дії злочинців та пообіцяв швидку відплату.",
              "maleVariant": "",
              "stringId": "1680712560284061696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ми вирвемо ту руку, а рану засиплемо сіллю.",
              "maleVariant": "",
              "stringId": "1680713657933418496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Що ж, схоже, кинуто рукавичку.",
              "maleVariant": "",
              "stringId": "1680713922577223680"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
