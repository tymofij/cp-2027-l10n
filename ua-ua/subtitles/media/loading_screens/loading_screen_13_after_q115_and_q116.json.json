        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добрий вечір, Найт-Сіті. Ми почнемо з… ем…",
              "maleVariant": "",
              "stringId": "1672055979443658752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На ринку паніка… Збитки від останнього удару перевищили 500 мільярдів євродоларів.",
              "maleVariant": "",
              "stringId": "1680737634739343360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Після чергового нападу на штаб-квартиру «Арасаки» в Найт-Сіті… ем… японський голіаф втратив 17%. Аналітики кажуть, що потрібно очікувати подальшого падіння.",
              "maleVariant": "",
              "stringId": "1680738953109753856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Відповідно до заяв керівництва поліції напад скоєно підрозділом одного з терористичних угруповань, але невідомо хто й для чого оплатив їхні послуги.",
              "maleVariant": "",
              "stringId": "1680739350595555328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У світлі цих подій я маю лише одну рекомендацію для вас, друзі. Продавайте.",
              "maleVariant": "",
              "stringId": "1680740839439257600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Продавайте зараз, допоки можете.",
              "maleVariant": "",
              "stringId": "1740382421442170880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Генеральний директор «Арасаки» Йорінобу Арасака евакуював увесь офіс із Найт-Сіті до Токіо.",
              "maleVariant": "",
              "stringId": "1740387718814568448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сьогодні ми почнемо з огляду новин на біржі. Як і завжди.",
              "maleVariant": "",
              "stringId": "1741429761951207424"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
