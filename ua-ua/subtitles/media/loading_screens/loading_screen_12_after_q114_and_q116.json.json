        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Добрий вечір, Найт-Сіті. Ми почнемо з… ох… ем…",
              "maleVariant": "",
              "stringId": "1672054587723259904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "На ринку паніка… Збитки від останнього удару перевищили 500 мільярдів євродоларів.",
              "maleVariant": "",
              "stringId": "1680732233364983808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Після чергового нападу на штаб-квартиру «Арасаки» в Найт-Сіті… ем… японський голіаф втратив 17%. Аналітики кажуть, що потрібно очікувати подальшого падіння.",
              "maleVariant": "",
              "stringId": "1680734240188452864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Відповідно до заяв поліції Найт-Сіті напад провела група кочівників, які проникли до вежі, скориставшись тунелем маглеву.",
              "maleVariant": "",
              "stringId": "1680734635057008640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У світлі цих подій я маю лише одну рекомендацію для вас, друзі. Продавайте.",
              "maleVariant": "",
              "stringId": "1680735393403949056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сьогодні ми почнемо з… з огляду новин на біржі. Як і завжди.",
              "maleVariant": "",
              "stringId": "1741544287841357824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Генеральний директор «Арасаки» Йорінобу Арасака евакуював увесь офіс із Найт-Сіті до Токіо.",
              "maleVariant": "",
              "stringId": "1741554562191659008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Продавайте зараз, допоки можете.",
              "maleVariant": "",
              "stringId": "1741555189407879168"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
