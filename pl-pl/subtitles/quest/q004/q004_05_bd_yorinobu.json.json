  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedziałem: nie.",
              "maleVariant": "",
              "stringId": "1646342469183037440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oni mi za to zetną głowę...",
              "maleVariant": "",
              "stringId": "1646366208037707776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To zginiesz w słusznej sprawie.",
              "maleVariant": "",
              "stringId": "1646367615327363072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Program jest wciąż w fazie testów. Ta technologia w obecnej postaci nie przyjmie się na rynku!",
              "maleVariant": "",
              "stringId": "1646383359368847360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wkrótce się przekonamy.",
              "maleVariant": "",
              "stringId": "1646383589954904064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam, że musiałaś czekać. Interesy. Jak zwykle.",
              "maleVariant": "",
              "stringId": "1650358263638351872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, popracuję nad tym... Pod jednym warunkiem.",
              "maleVariant": "",
              "stringId": "1650359651785203712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szafka przy łóżku.",
              "maleVariant": "",
              "stringId": "1650361173076041728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bardzo miło... Ale chodź już.",
              "maleVariant": "",
              "stringId": "1650454585695494144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, tak. Brakowało mi tego. Ciebie.",
              "maleVariant": "",
              "stringId": "1650486272622026752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "System alarmowy. Najnowszy firmware, ale ptaszki ćwierkają w BBS-ie, że ta wersja jest dziurawa jak sito.",
              "maleVariant": "",
              "stringId": "1650559518658748416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wieżyczki automatyczne. Najnowszy model skanerów. Jednostka centralna podłączona do ich systemu.",
              "maleVariant": "",
              "stringId": "1650559860880400384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cholera, kamery. Wbudowane czujniki ruchu, reakcja na sygnatury termiczne, system IFF.",
              "maleVariant": "",
              "stringId": "1650560025028681728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystkie ich systemy bezpieczeństwa podłączone są do wewnętrznej sieci hotelu.",
              "maleVariant": "",
              "stringId": "1650585724082937856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plan jest taki: użyjemy Flatheada, by dostać się do ich jednostki centralnej.",
              "maleVariant": "",
              "stringId": "1650587205947318272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musimy zdobyć Flatheada. Inaczej nie wyłączymy tych systemów.",
              "maleVariant": "",
              "stringId": "1650587291746000896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale ja...",
              "maleVariant": "",
              "stringId": "1653700796166660096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozgość się, proszę, zaraz to skończę.",
              "maleVariant": "",
              "stringId": "1653701130570129408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam pomysł. Zaraz do ciebie dołączę.",
              "maleVariant": "",
              "stringId": "1653708332492087296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyjąłem do wiadomości.",
              "maleVariant": "",
              "stringId": "1654542740130230272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na szczęście jeszcze... nie zaczęłam się nudzić.",
              "maleVariant": "",
              "stringId": "1654552079117185024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Za chwilę mi przejdzie.",
              "maleVariant": "",
              "stringId": "1654553945397915648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem.",
              "maleVariant": "",
              "stringId": "1654621623160532992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naładowany, zabezpieczony. Dobrze wiedzieć, nie zapomnijcie o tej spluwie w trakcie skoku.",
              "maleVariant": "",
              "stringId": "1654624110047260672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sekundkę, szukam czegoś odpowiedniego... Czuję, że ci się spodoba.",
              "maleVariant": "",
              "stringId": "1654790608564908032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dasz sobie z tym radę?",
              "maleVariant": "",
              "stringId": "1654858051966595072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Taa, jeśli tylko dostanę się do ich wewnętrznej sieci.",
              "maleVariant": "",
              "stringId": "1654858195042693120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz je wyłączyć?",
              "maleVariant": "",
              "stringId": "1654863053523120128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z zewnątrz nie będę miała szans. Muszę najpierw sprawdzić, w jaki sposób są podłączone do systemu.",
              "maleVariant": "",
              "stringId": "1654863217000312832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wygeneruję klucze i podrobię ich sygnatury.",
              "maleVariant": "",
              "stringId": "1654864902758199296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stworzę połączenie i zrobię tam mały bałagan. To powinno wystarczyć.",
              "maleVariant": "",
              "stringId": "1654869027336089600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bez niego się nie obejdzie?",
              "maleVariant": "",
              "stringId": "1654869522062635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma szans. A przynajmniej ja nie mam lepszego pomysłu.",
              "maleVariant": "",
              "stringId": "1654869962867208192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proszę porozmawiać ze swoim ojcem. On temu projektowi poświęca szczególną uwagę i na pewno będzie w stanie wytłumaczyć ryzyko, jakie...",
              "maleVariant": "",
              "stringId": "1654880736289353728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój ojciec to pozbawiony jakiejkolwiek wizji starzec, któremu się wydaje, że jest wieczny, a wszystko wokół niego niezmienne!",
              "maleVariant": "",
              "stringId": "1654883020389871616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proszę przynajmniej zapoznać się z dokumentacją i dopilnować, by Relic był przechowywany w odpowiednich warunkach.",
              "maleVariant": "",
              "stringId": "1654885991064399872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie dogadaliśmy się przez dwadzieścia lat, nie dogadamy się i teraz.",
              "maleVariant": "",
              "stringId": "1654892623685087232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "PDA Yorinobu. Może tam jest coś ważnego.",
              "maleVariant": "",
              "stringId": "1655204614574399488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Według tych instrukcji Relic musi być przechowywany w specjalnym kontenerze termicznym.",
              "maleVariant": "",
              "stringId": "1655211393643405312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszałaś? Gdzieś tutaj jest dokumentacja dotycząca Relicu. Rozejrzyj się.",
              "maleVariant": "Słyszałeś? Gdzieś tutaj jest dokumentacja dotycząca Relicu. Rozejrzyj się.",
              "stringId": "1655219164463980544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skan wskazuje na niskie temperatury. To pewnie jakaś chłodziarka.",
              "maleVariant": "",
              "stringId": "1656209557884198912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy sygnaturę termalną, która pasuje do specyfikacji z dokumentów.",
              "maleVariant": "",
              "stringId": "1656219332961329152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, mamy to. Dobra robota.",
              "maleVariant": "",
              "stringId": "1656220471446429696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Włącz w edytorze wykrywanie warstw termalnych.",
              "maleVariant": "",
              "stringId": "1656224332454256640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm, wydaje mi się, że wcześniej wykryłaś sygnatury pasujące do tej specyfikacji.",
              "maleVariant": "Hmm, wydaje mi się, że wcześniej wykryłeś sygnatury pasujące do tej specyfikacji.",
              "stringId": "1656226543053791232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba mamy już wszystko.",
              "maleVariant": "",
              "stringId": "1656236222400126976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Chmury\"? To jakiś klub?",
              "maleVariant": "",
              "stringId": "1656306459527045120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jest ten sam facet, z którym Yorinobu rozmawiał na początku.",
              "maleVariant": "",
              "stringId": "1656316077737205760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ewidentnie mają kosę. Albo Yorinobu chce zrzucić starego z tronu, albo Saburo planuje go wydziedziczyć. A może jedno i drugie.",
              "maleVariant": "",
              "stringId": "1656316180296327168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czekaj, właśnie sprawdziłam. Yorinobu obecnie nie zajmuje żadnego oficjalnego stanowiska w Arasace.",
              "maleVariant": "",
              "stringId": "1656322277321170944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hellmanowi bardzo zależy na tym, żeby odzyskać Relic, ale chyba nie tylko dlatego, że trzęsie dupą przed Saburo.",
              "maleVariant": "",
              "stringId": "1658858749527977984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pracował nad tą technologią. Może to dla niego sprawa osobista?",
              "maleVariant": "",
              "stringId": "1658866246041071616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli co? Nie wyciągać go z lodówki?",
              "maleVariant": "",
              "stringId": "1658890053598441472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lepiej nie. Moglibyście go uszkodzić.",
              "maleVariant": "",
              "stringId": "1658890991562256384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To powinno ułatwić znalezienie miejsca, gdzie Yorinobu trzyma Relic.",
              "maleVariant": "",
              "stringId": "1658907042475798528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sprawdźmy jeszcze raz.",
              "maleVariant": "",
              "stringId": "1658917149758582784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli tutaj Yorinobu musi trzymać kontener.",
              "maleVariant": "",
              "stringId": "1658921750691966976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wychodź z edytora.",
              "maleVariant": "",
              "stringId": "1658924156444078080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ekskluzywna agencja towarzyska. Usługi escortowe i prostytutki, mocno wspomagane techem.",
              "maleVariant": "",
              "stringId": "1658936412821123072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko w porządku? Wyglądasz na zdenerwowanego...",
              "maleVariant": "",
              "stringId": "1658955560607412224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty też chcesz mi stawiać żądania?",
              "maleVariant": "",
              "stringId": "1658970419147436032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko jedno. Skup. Się. Na mnie.",
              "maleVariant": "",
              "stringId": "1658971083206422528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostały nam jakieś dropsy? Ostatnio było po nich fajnie.",
              "maleVariant": "",
              "stringId": "1658971771508486144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszę coś… To dźwięk wentylatorów i systemów chłodzenia. Chyba serwer.",
              "maleVariant": "",
              "stringId": "1659500246734692352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jednostka sterująca urządzeniami w apartamencie.",
              "maleVariant": "",
              "stringId": "1659501338595909632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To tylko... ciepło bijące od lampy.",
              "maleVariant": "",
              "stringId": "1659529689993797632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nic nadzwyczajnego.",
              "maleVariant": "",
              "stringId": "1659534584964669440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja nie powinienem nawet tego słuchać.",
              "maleVariant": "",
              "stringId": "1660316905839915008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale posłuchasz jeszcze tego: Saburo jest oderwanym od rzeczywistości despotą, zawieszonym w jakichś skostniałych wyobrażeniach o świecie, którego już nie ma! Którego może nigdy nie było!",
              "maleVariant": "",
              "stringId": "1660317068713127936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko przede mną.",
              "maleVariant": "",
              "stringId": "1660328008447815680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, Donaghy's. Być może pan Arasaka ma powód do świętowania.",
              "maleVariant": "",
              "stringId": "1660800326037102592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naprawdę myślisz, że Relic mógłby być tutaj przechowywany?",
              "maleVariant": "",
              "stringId": "1660804996595933184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To zwykła kuchenna chłodziarka w zabudowie.",
              "maleVariant": "",
              "stringId": "1660828525114646528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poziom temperatury nie zgadza się z wytycznymi. Tu nic nie znajdziesz.",
              "maleVariant": "",
              "stringId": "1660828525164978176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Klimatyzacja. Standardowy model na wyposażeniu hoteli tej klasy.",
              "maleVariant": "",
              "stringId": "1660899619758628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Temperatura zgadza się z wytycznymi dokumentacji, ale ukrycie biochipu w takim miejscu nie wchodzi w grę.",
              "maleVariant": "",
              "stringId": "1660900974267162624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Za niska temperatura, za duże ryzyko wahań.",
              "maleVariant": "",
              "stringId": "1709886265579139072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chodzi o utrzymanie stałej niskiej temperatury.",
              "maleVariant": "",
              "stringId": "1915814643698888704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W \"Chmurach\" celują w klientów z wyższej półki: wysoko postawionych korposzczurów, celebrytów, biznesmenów.",
              "maleVariant": "",
              "stringId": "1915815103260389376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli akcja z Relikiem to rzeczywiście jakaś jego samowolka.",
              "maleVariant": "",
              "stringId": "1915817320050716672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Za duże ryzyko uszkodzenia i wahań temperatury.",
              "maleVariant": "",
              "stringId": "1915818186040274944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Te dokumenty już znalazłaś. Szukaj gdzie indziej.",
              "maleVariant": "Te dokumenty już znalazłeś. Szukaj gdzie indziej.",
              "stringId": "1928675921689980928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, musimy się dowiedzieć, gdzie Yorinobu trzyma Relic. Oglądaj uważnie.",
              "maleVariant": "",
              "stringId": "1928739524468158464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wow, takiego szampana to jeszcze nie próbowałam. Smak jest podkręcony?",
              "maleVariant": "Wow, takiego szampana to jeszcze nie próbowałem. Smak jest podkręcony?",
              "stringId": "1957616205530279936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. Drogi. Wkrótce będziesz pić takie bąbelki w realu.",
              "maleVariant": "",
              "stringId": "1957617154231513088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czuję...",
              "maleVariant": "",
              "stringId": "1957618018375577600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Evelyn... Ona... Poczułam jej przerażenie.",
              "maleVariant": "Evelyn... Ona... Poczułem jej przerażenie.",
              "stringId": "1957619926515462144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oho, zaraz ten braindance zmieni się w zwykłego pornola.",
              "maleVariant": "",
              "stringId": "1957622654826328064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja pierdolę, jakbym sama się z nim...",
              "maleVariant": "Ja pierdolę, jakbym sam się z nim...",
              "stringId": "1957629576216457216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozłączył się. Przewiń do momentu, gdy zaczynają rozmawiać. Musimy ich podsłuchać.",
              "maleVariant": "",
              "stringId": "1957682279290384384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze raz, przewiń nagranie. Nie wszystko usłyszałyśmy.",
              "maleVariant": "Jeszcze raz, przewiń nagranie. Nie wszystko usłyszeliśmy.",
              "stringId": "1957688044327677952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz podsłuchać tę rozmowę, V. Tam może być coś ważnego.",
              "maleVariant": "",
              "stringId": "1957704010533056512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cedr, pieprz, benzoes, wetyweria.",
              "maleVariant": "",
              "stringId": "1964965070985883648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. Normalnie czuję jego perfumy!",
              "maleVariant": "",
              "stringId": "1964966082987552768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skup się na istotniejszych sprawach, V.",
              "maleVariant": "",
              "stringId": "1964969308289888256"
            },
            {
