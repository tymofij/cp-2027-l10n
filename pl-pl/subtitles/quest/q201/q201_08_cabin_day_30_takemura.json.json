  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyglądasz jak wtedy, gdy zabrałem cię ze złomowiska. Wstawaj.",
              "maleVariant": "",
              "stringId": "2004115944562987008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura? Co ty tu robisz?",
              "maleVariant": "",
              "stringId": "2004115944579764232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Co ty tu robisz?",
              "maleVariant": "",
              "stringId": "2004115944630095872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Jak zwykle bezceremonialny.",
              "maleVariant": "",
              "stringId": "2004115944646873112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura. Jak zwykle bezceremonialny.",
              "maleVariant": "",
              "stringId": "2004115944680427532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ponoć domagałaś się kogoś, kto powie ci prawdę.",
              "maleVariant": "Ponoć domagałeś się kogoś, kto powie ci prawdę.",
              "stringId": "2004115944680427548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co za miejsce... Nic dziwnego, że wariujesz.",
              "maleVariant": "",
              "stringId": "2004115944713981952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyleciałeś mnie stąd zabrać?",
              "maleVariant": "",
              "stringId": "2004115944764313604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Prosili mnie, żebym z tobą porozmawiał.",
              "maleVariant": "",
              "stringId": "2004115944764313620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O czym chciałeś porozmawiać?",
              "maleVariant": "",
              "stringId": "2004115944781090816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odzyskałeś swojego ulubionego szefa.",
              "maleVariant": "",
              "stringId": "2004115944781090832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co właściwie teraz porabiasz?",
              "maleVariant": "",
              "stringId": "2004115944797868040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kto chciał, żebyś ze mną porozmawiał?",
              "maleVariant": "",
              "stringId": "2004115944797868056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co teraz zamierzasz? Nadal będziesz chronił tyłek Saburo?",
              "maleVariant": "",
              "stringId": "2004115944831422508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostałem przeniesiony do Japonii. Prosto stąd lecę do Takamatsu.",
              "maleVariant": "",
              "stringId": "2004115944831422524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego?",
              "maleVariant": "",
              "stringId": "2004115944831422540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nowe czasy, nowe obowiązki. Za dużo gadania.",
              "maleVariant": "",
              "stringId": "2004115944831422556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze, to co masz mi do powiedzenia?",
              "maleVariant": "",
              "stringId": "2004115944864976932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przykro mi, ale nic dobrego.",
              "maleVariant": "",
              "stringId": "2004115944864976948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kto chciał, żebyś ze mną porozmawiał? Hanako Arasaka?",
              "maleVariant": "",
              "stringId": "2004115944898531368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co ci powiedzieli?",
              "maleVariant": "",
              "stringId": "2004115944898531408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiem bez ogródek - operacja w niczym nie pomogła.",
              "maleVariant": "",
              "stringId": "2004115944948862988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Umrzesz, zanim nadejdzie zima.",
              "maleVariant": "",
              "stringId": "2004115944948863004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przecież mają najlepszych specjalistów.",
              "maleVariant": "",
              "stringId": "2004115944965640192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wierzę.",
              "maleVariant": "",
              "stringId": "2004115944982417420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "2004115944982417436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak to możliwe? Przecież Arasaka dysponuje najlepszymi specjalistami?",
              "maleVariant": "",
              "stringId": "2004115945015971856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wierzę w to.",
              "maleVariant": "",
              "stringId": "2004115945049526284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałaś prawdy i oto ona.",
              "maleVariant": "Chciałeś prawdy i oto ona.",
              "stringId": "2004115945049526300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja... Ja naprawdę wierzyłam, że wszystko skończy się dobrze.",
              "maleVariant": "Ja... Naprawdę wierzyłem, że wszystko skończy się dobrze.",
              "stringId": "2004115945049526316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "2004115945083080732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są jeszcze jakieś możliwości?",
              "maleVariant": "",
              "stringId": "2004115945099857920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostało mi kilka miesięcy życia, tak?",
              "maleVariant": "",
              "stringId": "2004115945099857936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obiecałeś, że Arasaka mnie naprawi.",
              "maleVariant": "",
              "stringId": "2004115945099857952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli zostało mi kilka miesięcy życia?",
              "maleVariant": "",
              "stringId": "2004115945133412368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na początku będziesz czuła się nieźle, potem choroba zaatakuje z całą gwałtownością.",
              "maleVariant": "Na początku będziesz czuł się nieźle, potem choroba zaatakuje z całą gwałtownością.",
              "stringId": "2004115945133412384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ostatnie tygodnie życia spędzisz w łóżku.",
              "maleVariant": "",
              "stringId": "2004115945133412400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obiecywałeś, że Arasaka mnie naprawi, jeśli ci pomogę.",
              "maleVariant": "",
              "stringId": "2004115945183744028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naprawdę w to wierzyłem.",
              "maleVariant": "",
              "stringId": "2004115945183744044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedziałeś, że masz możnych przyjaciół. Gdzie oni teraz są?",
              "maleVariant": "",
              "stringId": "2004115945183744060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie przyleciałem tu z pustymi rękami. Porozmawiajmy o innych możliwościach.",
              "maleVariant": "",
              "stringId": "2004115945183744076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako Arasaka obiecała, że uratuje mnie przed śmiercią.",
              "maleVariant": "",
              "stringId": "2004115945183744092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlatego w jej imieniu chcę przedstawić ci alternatywę.",
              "maleVariant": "",
              "stringId": "2004115945200521216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O czym ty mówisz? Już raz wam zaufałam i zawiedliście.",
              "maleVariant": "O czym ty mówisz? Już raz wam zaufałem i zawiedliście.",
              "stringId": "2004115945234075720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz dołączyć do programu \"Secure Your Soul\". Hanako Arasaka wydała zgodę.",
              "maleVariant": "",
              "stringId": "2004115945234075780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz opuścić ciało. Arasaka przechowa twój engram w Mikoshi do czasu, aż znajdzie sposób na przeniesienie konstruktu do nowego ciała.",
              "maleVariant": "",
              "stringId": "2004115945250852876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego nie mogę dostać nowego ciała od razu?",
              "maleVariant": "",
              "stringId": "2004115945250852900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przy obecnym stanie wiedzy to technicznie niemożliwe.",
              "maleVariant": "",
              "stringId": "2004115945250852912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A przypadek Saburo Arasaki?",
              "maleVariant": "",
              "stringId": "2004115945250852928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On znalazł się w ciele syna, zgodność tkankowa ułatwiła transfer.",
              "maleVariant": "",
              "stringId": "2004115945250852940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcę trafić do Mikoshi.",
              "maleVariant": "",
              "stringId": "2004115945284407296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka chce mnie do swojej kolekcji dusz.",
              "maleVariant": "",
              "stringId": "2004115945284407312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wybacz, ale to brzmi jak podstęp. Arasaka chce mnie uwięzić.",
              "maleVariant": "",
              "stringId": "2004115945317961760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przeceniasz swoją rolę. Dla Arasaków jesteś i zawsze byłaś nikim.",
              "maleVariant": "Przeceniasz swoją rolę. Dla Arasaków jesteś i zawsze byłeś nikim.",
              "stringId": "2004115945317961776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tą ofertą Hanako Arasaka hojnie płaci za twoje usługi. To wszystko. Nie ma, jak to mówicie, \"drugiego dna\".",
              "maleVariant": "",
              "stringId": "2004115945317961788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będzie cię chronić umowa. Podpisz ją i będziesz żyła.",
              "maleVariant": "Będzie cię chronić umowa. Podpisz ją i będziesz żył.",
              "stringId": "2004115945385070624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widziałam, że Saburo Arasaka wrócił. W ciele Yorinobu.",
              "maleVariant": "Widziałem, że Saburo Arasaka wrócił. W ciele Yorinobu.",
              "stringId": "2004115945418625064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sprawiedliwości stało się zadość.",
              "maleVariant": "",
              "stringId": "2004115945418625080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dostałeś swoją zemstę? Czujesz się usatysfakcjonowany?",
              "maleVariant": "",
              "stringId": "2004115945418625096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wierzę, że Hanako Arasaka wybrała najlepsze rozwiązanie.",
              "maleVariant": "",
              "stringId": "2004115945435402240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta rodzina jest pojebana.",
              "maleVariant": "",
              "stringId": "2004115945452179456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka powinien pozostać martwy.",
              "maleVariant": "",
              "stringId": "2004115945452179472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żałuję Yorinobu Arasaki.",
              "maleVariant": "",
              "stringId": "2004115945452179488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cała ta rodzina jest pojebana. Dobrze o tym wiesz.",
              "maleVariant": "",
              "stringId": "2004115945485733924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Licz się ze słowami.",
              "maleVariant": "",
              "stringId": "2004115945502511108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Siostra zabija brata, żeby osadzić w jego ciele zamordowanego ojca? To brzmi jak film z paśnika.",
              "maleVariant": "",
              "stringId": "2004115945502511124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oni są ulepieni z innej gliny niż ty i ja. Nie mamy prawa ich osądzać.",
              "maleVariant": "",
              "stringId": "2004115945502511136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka powinien pozostać martwy.",
              "maleVariant": "",
              "stringId": "2004115945536065564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wielu życzy mu śmierci, a on przeżyje wszystkich.",
              "maleVariant": "",
              "stringId": "2004115945536065580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy Arasakowie mogliby być postaciami z jakiejś starej, w chuj smutnej sztuki teatralnej.",
              "maleVariant": "",
              "stringId": "2004115945586397224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A Yorinobu Arasaka najbardziej.",
              "maleVariant": "",
              "stringId": "2004115945586397240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żałujesz go?",
              "maleVariant": "",
              "stringId": "2004115945586397256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Próbował za wszelką cenę wyjść z cienia ojca i zabił go jego największy koszmar.",
              "maleVariant": "",
              "stringId": "2004115945603174412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz zaakceptować prawdę i wysłuchać, co mam do powiedzenia. Są inne możliwości.",
              "maleVariant": "",
              "stringId": "2004115945670283268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzymali cię przez kilka dni w tym karcerze? Barbarzyństwo.",
              "maleVariant": "",
              "stringId": "2004115945787723808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź na łóżku]",
              "maleVariant": "",
              "stringId": "2004115945854832640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[weź]",
              "maleVariant": "",
              "stringId": "2004115945905164288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest tam napisane, że zrzekasz się prawa własności, ale bądź spokojna. Po prostu prawo nie nadąża za przemianami w świecie i engram nie ma statusu osoby.",
              "maleVariant": "Jest tam napisane, że zrzekasz się prawa własności, ale bądź spokojny. Po prostu prawo nie nadąża za przemianami w świecie i engram nie ma statusu osoby.",
              "stringId": "2004115945989050372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam stać się rzeczą należącą do korporacji?",
              "maleVariant": "",
              "stringId": "2004115945989050388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podobnie jak wiele gwiazd braindansu i wielu polityków. Nikt dotąd nie narzekał na warunki programu.",
              "maleVariant": "",
              "stringId": "2004115945989050404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jeśli się nie zgodzę?",
              "maleVariant": "",
              "stringId": "2004115945989050420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jesteś więźniem. Możesz zabrać swoje rzeczy i wrócić do Night City.",
              "maleVariant": "",
              "stringId": "2004115945989050436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak masz na imię?",
              "maleVariant": "",
              "stringId": "2004116215800238112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hajime.",
              "maleVariant": "",
              "stringId": "2004116215800238128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hajime, co byś powiedział człowiekowi, który wszedł prosto do więzienia wroga, żeby ratować swoje życie?",
              "maleVariant": "",
              "stringId": "2004116215800238144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedziałbym mu: \"Zapamiętaj dobrze drogę do domu\".",
              "maleVariant": "",
              "stringId": "2004116215800238160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Do zobaczenia?",
              "maleVariant": "",
              "stringId": "2004116216051896320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[podpisz]",
              "maleVariant": "",
              "stringId": "2004116216085450752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[zabierz rzeczy i leć na Ziemię]",
              "maleVariant": "",
              "stringId": "2004116216085450764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja na twoim miejscu podpisałbym dokumenty, ale wybór należy do ciebie. Zastanów się dobrze.",
              "maleVariant": "",
              "stringId": "2004116216119005188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[weź kostkę]",
              "maleVariant": "",
              "stringId": "2004116216337108992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka dotrzymuje słowa.",
              "maleVariant": "",
              "stringId": "2004116216404217880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcę umierać.",
              "maleVariant": "",
              "stringId": "2004116216504881156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobry wybór.",
              "maleVariant": "",
              "stringId": "2004116216504881168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stanowisko do wykonania engramu jest gotowe. Nie warto czekać, aż twój stan pogorszy się jeszcze bardziej.",
              "maleVariant": "",
              "stringId": "2004116216504881180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oddaj się w ręce Arasaki]",
              "maleVariant": "",
              "stringId": "2004116216605544448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Już postanowiłam.",
              "maleVariant": "Już postanowiłem.",
              "stringId": "2004116216722984964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wracam na Ziemię.",
              "maleVariant": "",
              "stringId": "2004116216739762176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czeka cię śmierć.",
              "maleVariant": "",
              "stringId": "2004116216739762188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[pokaż środkowy palec]",
              "maleVariant": "",
              "stringId": "2004116217192747008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wróć na Ziemię]",
              "maleVariant": "",
              "stringId": "2004116217293410304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zobaczymy się jeszcze kiedyś?",
              "maleVariant": "",
              "stringId": "2004116217545068544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wierzę, że tak.",
              "maleVariant": "",
              "stringId": "2004116217545068560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do zobaczenia.",
              "maleVariant": "",
              "stringId": "2004116217545068576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lubię cię, Takemura, i dziekuję, że pofatygowałeś się na orbitę, żeby mi pomóc.",
              "maleVariant": "",
              "stringId": "2004116217628954628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czego mianowicie?",
              "maleVariant": "",
              "stringId": "2004116217628954640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Żegnaj, Takemura.",
              "maleVariant": "",
              "stringId": "2004116217662509056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"A thing of beauty, I know, will never fade away.\"",
              "maleVariant": "",
              "stringId": "2004116217796726792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"I see your eyes, I know you see me you're like a ghost how you're everywhere.\"",
              "maleVariant": "",
              "stringId": "2004116217830281224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyleciałem porozmawiać.",
              "maleVariant": "",
              "stringId": "2018716791898398720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nigdy nie zrozumiem, dlaczego tak hołubisz tego człowieka.",
              "maleVariant": "",
              "stringId": "2018794699115487232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bo nie spędziłaś życia u jego boku i... Zresztą, nie przyleciałem tu rozmawiać o Arasakach.",
              "maleVariant": "Bo nie spędziłeś życia u jego boku i... Zresztą, nie przyleciałem tu rozmawiać o Arasakach.",
              "stringId": "2018795035968430080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ona już o tobie zapomniała. Zadzwonili stąd, z kliniki.",
              "maleVariant": "",
              "stringId": "2018845020630048768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedzieli, że powinien porozmawiać z tobą ktoś, kogo znasz.",
              "maleVariant": "",
              "stringId": "2018845795351552000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlatego operacja się udała, ale nawet korporacyjni neurochirurdzy niewiele mogli zrobić. Zmiany wywołane przez biochip zaszły za daleko.",
              "maleVariant": "",
              "stringId": "2018876965657272320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są jebaną Arasaką, niech wymyślą coś innego!",
              "maleVariant": "",
              "stringId": "2018877801548505088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "2018886455924383744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie złość się, proszę. Chcę porozmawiać o alternatywie.",
              "maleVariant": "",
              "stringId": "2018886547477651456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, to nie brzmi dobrze. Jeśli się zgodzę, będą mogli zrobić ze mną dosłownie wszystko.",
              "maleVariant": "",
              "stringId": "2018907086330028032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odwiedź mnie w Takamatsu, pokażę ci, co to znaczy dobra kuchnia.",
              "maleVariant": "",
              "stringId": "2018919758379376640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale jednego nie rozumiałeś i nigdy nie zrozumiesz.",
              "maleVariant": "",
              "stringId": "2018923194051223552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiesz, jak wspaniale być wolnym człowiekiem.",
              "maleVariant": "",
              "stringId": "2018924516079067136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przemawia przez ciebie gorycz, zastanów się jeszcze.",
              "maleVariant": "",
              "stringId": "2018925922160762880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żegnaj. Dużo razem przeżyliśmy i nigdy cię nie zapomnę.",
              "maleVariant": "",
              "stringId": "2018926809893593088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żegnaj.",
              "maleVariant": "",
              "stringId": "2018927814093856768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A twój fenotyp na skutek działania chipu stał się... unikatowy. Korporacja nie jest w stanie zapewnić ci odpowiedniego ciała.",
              "maleVariant": "",
              "stringId": "2025631369722880000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[weź torbę]",
              "maleVariant": "",
              "stringId": "2066552551861358596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wstawaj, porozmawiajmy.",
              "maleVariant": "",
              "stringId": "2221684586083348480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "2221685564731912192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proszę.",
              "maleVariant": "",
              "stringId": "2221685689738948608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co sądzisz?",
              "maleVariant": "",
              "stringId": "2221685976293797888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma co zwlekać, nic już cię tu nie trzyma.",
              "maleVariant": "",
              "stringId": "2221686084255182848"
            }
          ]
        }
      }
    },
