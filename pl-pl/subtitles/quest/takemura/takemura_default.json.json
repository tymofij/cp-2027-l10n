  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko u ciebie w porządku?",
              "maleVariant": "",
              "stringId": "1951904610263048196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie jesteś?",
              "maleVariant": "",
              "stringId": "1951904927167881216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, co dzieje się w Arasace?",
              "maleVariant": "",
              "stringId": "1951905708180840448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz może, co dzieje się w Arasace?",
              "maleVariant": "",
              "stringId": "1951915942400040960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko tyle, ile da się wywnioskować z wiadomości w telewizji.",
              "maleVariant": "",
              "stringId": "1951921956964868096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale wiesz co? Im szerszy uśmiech, tym bardziej fałszywy.",
              "maleVariant": "",
              "stringId": "1951922478451073024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu i jego marionetki szczerzą się do kamer i zapewniają, że wszystko jest w porządku.",
              "maleVariant": "",
              "stringId": "1951923714294358016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie ty właściwie jesteś?",
              "maleVariant": "",
              "stringId": "1951924816289013760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W mieście, a gdzie miałbym być?",
              "maleVariant": "",
              "stringId": "1951925106954280960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To żadna odpowiedź.",
              "maleVariant": "",
              "stringId": "1951925834414366720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lepiej, żebyś nie znała dokładnego adresu. Na wypadek, gdyby cię dorwali.",
              "maleVariant": "Lepiej, żebyś nie znał dokładnego adresu. Na wypadek, gdyby cię dorwali.",
              "stringId": "1951926438394142720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale jedno muszę przyznać Night City. Łatwo się tutaj zapaść pod ziemię...",
              "maleVariant": "",
              "stringId": "1951927198586576896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A, dzięki.",
              "maleVariant": "",
              "stringId": "1951932843415117824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli tylko przyzwyczaisz się do robactwa, wilgoci i braku bieżącej wody.",
              "maleVariant": "",
              "stringId": "1951933518647730176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aż boję się spytać.",
              "maleVariant": "",
              "stringId": "1951934810342367232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I słusznie.",
              "maleVariant": "",
              "stringId": "1951935805852033024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko u ciebie w porządku?",
              "maleVariant": "",
              "stringId": "1951938283729395712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. Skąd ta troska?",
              "maleVariant": "",
              "stringId": "1951938625145741312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu pytam. Co w tym złego?",
              "maleVariant": "",
              "stringId": "1951938928544915460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stąd, że mamy wspólne interesy.",
              "maleVariant": "",
              "stringId": "1951939359417376768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No wiesz, Takemura? Po prostu pytam. Nie musisz się od razu napinać.",
              "maleVariant": "",
              "stringId": "1951941210145628160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ano stąd, że mamy wspólne interesy. Zapomniałeś?",
              "maleVariant": "",
              "stringId": "1951941270912704512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam, źle to zabrzmiało. Po prostu... Nie przywykłem do takich pytań.",
              "maleVariant": "",
              "stringId": "1951941526631030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście, że nie.",
              "maleVariant": "",
              "stringId": "1951942467514081280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To dobrze. W Night City łatwo zarobić kulę, a ja potrzebuję cię żywego. Dlatego pytam.",
              "maleVariant": "",
              "stringId": "1951943057652654080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to już wiesz, że żyję. To o siebie powinnaś się martwić.",
              "maleVariant": "No to już wiesz, że żyję. To o siebie powinieneś się martwić.",
              "stringId": "1951950331330764800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, ludzie tacy jak ja mogą być albo w formie, albo w grobie.",
              "maleVariant": "",
              "stringId": "1951952431318114304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak idą przygotowania?",
              "maleVariant": "",
              "stringId": "1952009240347222016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właściwie wszystko gotowe. Teraz najtrudniejsza część: czekanie.",
              "maleVariant": "",
              "stringId": "1952009483365195776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale nie powinniśmy o tym mówić. Ktoś może nas podsłuchiwać.",
              "maleVariant": "",
              "stringId": "1952011266967805952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Zmiana tematu.",
              "maleVariant": "",
              "stringId": "1952012209847345156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jesteś zbyt paranoiczny?",
              "maleVariant": "",
              "stringId": "1952012432011239424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, zmiana tematu.",
              "maleVariant": "",
              "stringId": "1952016335750635520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiedziałam, że z ciebie taki paranoik.",
              "maleVariant": "Nie wiedziałem, że taki z ciebie paranoik.",
              "stringId": "1952016628982816768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lepiej być żywym paranoikiem niż martwym głupcem.",
              "maleVariant": "",
              "stringId": "1953301660296880128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozmawiałam z Hanako.",
              "maleVariant": "",
              "stringId": "1953303342045011968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozmawiałam z nią. Nie twarzą w twarz, ale rozmawiałam. Wiesz, o kogo chodzi.",
              "maleVariant": "Rozmawiałem z nią. Nie twarzą w twarz, ale rozmawiałem. Wiesz, o kogo chodzi.",
              "stringId": "1953306251080048640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Ze mną też się skontaktowała.",
              "maleVariant": "",
              "stringId": "1953307112522981376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zagraliśmy, jak to mówicie... Va banque. I opłaciło się.",
              "maleVariant": "",
              "stringId": "1953307344300220416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tego jeszcze nie wiemy.",
              "maleVariant": "",
              "stringId": "1953308051224993796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie teraz jesteś?",
              "maleVariant": "",
              "stringId": "1953310512694239232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Twoja kryjówka spalona. Chyba dosłownie. Gdzie teraz jesteś?",
              "maleVariant": "",
              "stringId": "1953311043189809152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Koniec z pustostanami, V. Nie gniewaj się, ale na razie tylko tyle mogę powiedzieć.",
              "maleVariant": "",
              "stringId": "1953314581924093952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trudno uwierzyć, ale to prawda.",
              "maleVariant": "",
              "stringId": "1953316565896994816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tego jeszcze nie wiemy. Na razie to tylko słowa.",
              "maleVariant": "",
              "stringId": "1953322292044701696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słowa mają różną wartość – w zależności od tego, kto je wypowiada. W tym przypadku jestem spokojny.",
              "maleVariant": "",
              "stringId": "1953322542243323904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wciąż trudno mi w to uwierzyć, ale to prawda.",
              "maleVariant": "",
              "stringId": "1953361299256451072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ufam komuś, kto gada ze mną przez proxy.",
              "maleVariant": "",
              "stringId": "1954522890559627264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trudno zaufać komuś, kto gada z tobą przez proxy.",
              "maleVariant": "",
              "stringId": "1954523647111405568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak miewa się Oda?",
              "maleVariant": "",
              "stringId": "1954633385270648832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak się miewa Oda? Wyliże się z tego?",
              "maleVariant": "",
              "stringId": "1954638883114000384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka zapewniła mu najlepszą opiekę medyczną, ale nie wiem, czy to wystarczy. Zraniona duma zrasta się wolniej niż kości.",
              "maleVariant": "",
              "stringId": "1954639672314241024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do usłyszenia.",
              "maleVariant": "",
              "stringId": "1954645338936385536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do usłyszenia.",
              "maleVariant": "",
              "stringId": "1954645992123736064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W kontakcie.",
              "maleVariant": "",
              "stringId": "1954646769848360960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mówiłem.",
              "maleVariant": "",
              "stringId": "1975314368131047424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Złożyła ci ofertę.",
              "maleVariant": "",
              "stringId": "1975327764469587968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy się spotkać osobiście.",
              "maleVariant": "",
              "stringId": "1975328997947285508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę znaleźć Soulkillera.",
              "maleVariant": "",
              "stringId": "1975329820366413824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś nieźle poinformowany.",
              "maleVariant": "",
              "stringId": "1975330905499967488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś nieźle poinformowany. Muszę w ogóle odpowiadać?",
              "maleVariant": "",
              "stringId": "1975334357462491136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem tylko tyle, ile Hanako-sama zechciała mi powiedzieć. Czyli niewiele.",
              "maleVariant": "",
              "stringId": "1975335006187102208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Więc, tak, proszę.",
              "maleVariant": "",
              "stringId": "1975336246996443136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wkrótce mamy się spotkać osobiście.",
              "maleVariant": "",
              "stringId": "1975336674211471360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bardzo dobrze. Trudno o czytelniejszy znak, że ci ufa.",
              "maleVariant": "",
              "stringId": "1975337225997328384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na razie niewiele z tego wynikło. Najpierw muszę odnaleźć Soulkillera, bo Hanako ma - cytuję: \"związane ręce\".",
              "maleVariant": "",
              "stringId": "1975339193008463872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będziesz tam?",
              "maleVariant": "",
              "stringId": "1975340735941910528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze nie teraz. Wciąż szuka mnie Arasaka, nie możemy ryzykować.",
              "maleVariant": "",
              "stringId": "1975341343562342400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to chyba czas zacząć poszukiwania?",
              "maleVariant": "",
              "stringId": "1975343705727246336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ooo, tak jakby Soulkiller to było coś, co mogę kupić na straganie w Kabuki.",
              "maleVariant": "",
              "stringId": "1975344863288041472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W tym nie mogę ci pomóc. Ale myślę, że warto spróbować. Twoje życie od tego zależy.",
              "maleVariant": "",
              "stringId": "1975345636868694016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale... to już tylko kwestia czasu, V.",
              "maleVariant": "",
              "stringId": "1975350485450563584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coś jeszcze?",
              "maleVariant": "",
              "stringId": "1975351673831104512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak idą przygotowania?",
              "maleVariant": "",
              "stringId": "1975353812439605248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam.",
              "maleVariant": "",
              "stringId": "1975361318012395520"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}