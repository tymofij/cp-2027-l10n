  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jeszcze nie wszyscy.",
              "maleVariant": "",
              "stringId": "1730128440841826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To chyba... Tak, wszyscy ludzie Yorinobu zdjęci.",
              "maleVariant": "",
              "stringId": "1730239100456103936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba właśnie zaczęła się wojna.",
              "maleVariant": "",
              "stringId": "1743214428115791872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見事だ\" t=\"Doskonale.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1781020138217160704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czekam.",
              "maleVariant": "",
              "stringId": "1795388015658274816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu to dziwne. Zwołać największe zebranie zarządu od dekady i nie zaprosić na nie własnej siostry.",
              "maleVariant": "",
              "stringId": "1813996648192294912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potwierdzam. Zabezpieczenia wewnętrzne nie zostały naruszone.",
              "maleVariant": "",
              "stringId": "1813999114141057024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "U mnie wszystkie systemy sprawne. Czekam.",
              "maleVariant": "",
              "stringId": "1814000516229775360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak jest. Tutaj wszystko w porządku.",
              "maleVariant": "",
              "stringId": "1814001760612663300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhmm, uwielbiam ten widok. I to powietrze...!",
              "maleVariant": "",
              "stringId": "1814004720918872064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze, że skierowali nas tutaj, a nie do Wieży.",
              "maleVariant": "",
              "stringId": "1814005033042198528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hellman, u nas czysto! Ściągaj AV-kę!",
              "maleVariant": "",
              "stringId": "1822853255962640384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ci ludzie... Po prostu nie zostawił mi wyboru.",
              "maleVariant": "",
              "stringId": "1835821347927625728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weź się w garść.",
              "maleVariant": "",
              "stringId": "1835869897550073860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musieliśmy.",
              "maleVariant": "",
              "stringId": "1835870221518114816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy coś do zrobienia.",
              "maleVariant": "",
              "stringId": "1835870770200186880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musieliśmy.",
              "maleVariant": "",
              "stringId": "1835870844321927168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To dlatego zawsze wolałam trzymać się z boku.",
              "maleVariant": "",
              "stringId": "1837312048028848128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żebym nie musiała podejmować takich decyzji.",
              "maleVariant": "",
              "stringId": "1837312558039437312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli to ma się udać, lepiej weź się w garść.",
              "maleVariant": "",
              "stringId": "1837321457111011328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie muszę.",
              "maleVariant": "",
              "stringId": "1837322820746350592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu...",
              "maleVariant": "",
              "stringId": "1837328094026665984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, ona nie pełni żadnej oficjalnej funkcji...",
              "maleVariant": "",
              "stringId": "1837358695802867712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie musi mieć konkretnego tytułu, żeby pociągać za sznurki.",
              "maleVariant": "",
              "stringId": "1837359202307018752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że po co jej są te złote palce? Okręca sobie wokół nich ludzi. Tak było z jej ojcem.",
              "maleVariant": "",
              "stringId": "1837360025581785088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to masz odpowiedź na swoje pytanie. Nowy cesarz nie da sobą manipulować.",
              "maleVariant": "",
              "stringId": "1837360181526007808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sam nie wiem. Dziwne to wszystko...",
              "maleVariant": "",
              "stringId": "1837360676537765888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pieprzony Smasher... Nie mogę w to uwierzyć. Były najemnik rozstawia elitarnych żołnierzy Arasaki po kątach.",
              "maleVariant": "",
              "stringId": "1844219229852536832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może to tymczasowe.",
              "maleVariant": "",
              "stringId": "1844219539358617600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu musiał mieć jakiś powód, żeby wyciągnąć go spod kamienia. Raczej szybko pod niego nie wróci.",
              "maleVariant": "",
              "stringId": "1845817765694148608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Idę lewą. Prawa strona jest twoja.",
              "maleVariant": "",
              "stringId": "1873562604761268224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W budynku jest netrunner. Zajmę się nim.",
              "maleVariant": "",
              "stringId": "1873563575105110016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W środku czysto. Bedę stąd monitorować sytuację.",
              "maleVariant": "",
              "stringId": "1873565188888739840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaraz u ciebie będę, V.",
              "maleVariant": "",
              "stringId": "1873566118833045504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Już wiedzą.",
              "maleVariant": "",
              "stringId": "1908332456134590464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzeba ich zneutralizować. Wszystkich.",
              "maleVariant": "",
              "stringId": "1908333041239998464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem w sypialni w środkowej części budynku.",
              "maleVariant": "",
              "stringId": "1908340320051937280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anders, ściągniesz AV-kę.",
              "maleVariant": "",
              "stringId": "1908341071721545728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale najpierw trzeba zneutralizować czwórkę strażników. To twoje zadanie, V.",
              "maleVariant": "",
              "stringId": "1908341643891720192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale najpierw trzeba zneutralizować czwórkę strażników. Takemura, V? To wasze zadanie.",
              "maleVariant": "",
              "stringId": "1908342161905041408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pójdziemy lewą stroną ogrodu.",
              "maleVariant": "",
              "stringId": "1908342834923061248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zagłuszę ich komunikację. Nie wezwą posiłków.",
              "maleVariant": "",
              "stringId": "1908345087952175104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uwaga. Kolejnych pięciu strażników na zewnątrz. I netrunner w garażu po lewej.",
              "maleVariant": "",
              "stringId": "1908346169327632384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeden wartownik na patio, drugi przy basenie. Trzeci u góry, a czwarty w środku.",
              "maleVariant": "",
              "stringId": "1908347682095644672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaraz będę nad rezydencją.",
              "maleVariant": "",
              "stringId": "1908352095912738816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co ty robisz?! Teraz musisz się zająć wszystkimi!",
              "maleVariant": "",
              "stringId": "1908354188501336064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dwuosobowy patrol.",
              "maleVariant": "",
              "stringId": "1908359178498359296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zajmij się jednym, drugiego biorę na siebie.",
              "maleVariant": "",
              "stringId": "1908359976791851008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zneutralizowani. Wszyscy.",
              "maleVariant": "",
              "stringId": "1908362355952074752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W takim razie ściągam blokady. Czekam.",
              "maleVariant": "",
              "stringId": "1908362894936915968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze. Ląduj przy basenie.",
              "maleVariant": "",
              "stringId": "1908363579833204736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zapraszam.",
              "maleVariant": "",
              "stringId": "1915253453191462912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wsiądź]",
              "maleVariant": "",
              "stringId": "1915351422116904964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wojna... To duże słowo. Nawet kiedy wypowiada je córka Saburo Arasaki.",
              "maleVariant": "",
              "stringId": "1937339808506462208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pożyczę sobie tę drzazgę...",
              "maleVariant": "",
              "stringId": "1963563808545665024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zdążył powiadomić kolegów... Dobrze.",
              "maleVariant": "",
              "stringId": "1963569362592690176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To nam daje przewagę, ale musimy się pośpieszyć.",
              "maleVariant": "",
              "stringId": "1963643195563536384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zneutralizować, taa.",
              "maleVariant": "",
              "stringId": "2000940801754734592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyjąłem.",
              "maleVariant": "",
              "stringId": "2007179881702301696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anders, ściągniesz AV-kę.",
              "maleVariant": "",
              "stringId": "2007195287800324096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze, że obyło się bez rozlewu krwi.",
              "maleVariant": "",
              "stringId": "2007211512710373376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu liczę. Ty, ja i Hellman to trzy osoby. Właściwie dwie i pół.",
              "maleVariant": "",
              "stringId": "2008132269175021568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu liczę. Ty, ja, Takemura i Hellman to cztery osoby. Właściwie trzy i pół.",
              "maleVariant": "",
              "stringId": "2008132363530084352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd ta myśl?",
              "maleVariant": "",
              "stringId": "2008203766220529664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest jeszcze Oda. Spotkamy się z nim na miejscu.",
              "maleVariant": "",
              "stringId": "2008208250132832256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Byłby jeszcze Oda, gdybyś...",
              "maleVariant": "",
              "stringId": "2008210348778004488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miał najlepszą opiekę.",
              "maleVariant": "",
              "stringId": "2008212456919080960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On albo ja, Hanako.",
              "maleVariant": "",
              "stringId": "2008213793475993600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak szybko się z tego wylizał?",
              "maleVariant": "",
              "stringId": "2008214134691012608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój brat to nie jest cała Arasaka. Przynajmniej jeszcze nie teraz.",
              "maleVariant": "",
              "stringId": "2008218423517847552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On o tym wie. I dlatego otacza się tymi... kryminalistami pokroju Adama Smashera.",
              "maleVariant": "",
              "stringId": "2008218793925222400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, czy mnie to pocieszyło.",
              "maleVariant": "",
              "stringId": "2008224008619499520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój ojciec był sam, a zbudował imperium.",
              "maleVariant": "",
              "stringId": "2008224535642185728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteśmy sami, takie są fakty.",
              "maleVariant": "",
              "stringId": "2008226207642755072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak czy inaczej, to mało –  przeciwko całej Arasace.",
              "maleVariant": "",
              "stringId": "2008228149890719744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To dopiero początek. Później może być różnie.",
              "maleVariant": "",
              "stringId": "2008349264663355392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie lubisz sobie brudzić rączek, co?",
              "maleVariant": "",
              "stringId": "2008349264680132620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A teraz...",
              "maleVariant": "",
              "stringId": "2008364373116788736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To dopiero początek. Później może być różnie.",
              "maleVariant": "",
              "stringId": "2008426324547149840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś na to przygotowana?",
              "maleVariant": "",
              "stringId": "2008426324547149856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem.",
              "maleVariant": "",
              "stringId": "2008426324563927040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie lubisz sobie brudzić rączek, co?",
              "maleVariant": "",
              "stringId": "2008428072011636740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niewykluczone, że wkrótce będziesz musiała.",
              "maleVariant": "",
              "stringId": "2008428072011636756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem.",
              "maleVariant": "",
              "stringId": "2008428072011636772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cholera. Musimy uważać na tego mecha.",
              "maleVariant": "",
              "stringId": "2011315843168542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wysiądź] Zneutralizować, mówisz...",
              "maleVariant": "",
              "stringId": "2133278409131900932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czekamy.",
              "maleVariant": "",
              "stringId": "2257350875024826368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie możemy po prostu polecieć bez niej?",
              "maleVariant": "Nie możemy po prostu polecieć bez niego?",
              "stringId": "2257353090875629576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, wsiadaj!",
              "maleVariant": "",
              "stringId": "2257354536752881672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V - biegnij po Arasaka-samę! Zaraz po was podlecimy!",
              "maleVariant": "",
              "stringId": "2257398606321786880"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}