  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdy lojalny człowiek ma być gotowy.",
              "maleVariant": "",
              "stringId": "1753439384113266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będziemy czekać.",
              "maleVariant": "",
              "stringId": "1753439567807004672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przewinęłam się przez wywiad, a nigdy nie słyszałam o tym miejscu.",
              "maleVariant": "Przewinąłem się przez wywiad, a nigdy nie słyszałem o tym miejscu.",
              "stringId": "1821289547171762176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście, że nie słyszałaś.",
              "maleVariant": "Oczywiście, że nie słyszałeś.",
              "stringId": "1821321276662091776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jest jeden z najpilniej strzeżonych sekretów całej Arasaki.",
              "maleVariant": "",
              "stringId": "1821323272496484352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gabinet ojca. Wierna replika jego tokijskiego biura.",
              "maleVariant": "",
              "stringId": "1821324270522732544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kazał je odwzorować we wszystkich placówkach Arasaki.",
              "maleVariant": "",
              "stringId": "1821324549947265024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdy przedmiot. Każdy szczegół. Żeby mógł z nich korzystać z zamkniętymi oczyma.",
              "maleVariant": "",
              "stringId": "1821327312986968064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie taki jest. Przezorny. Drobiazgowy.",
              "maleVariant": "",
              "stringId": "1821329848645062656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dokąd my właściwie idziemy?",
              "maleVariant": "",
              "stringId": "1821433581064294400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tędy.",
              "maleVariant": "",
              "stringId": "1821449271318024192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Był.",
              "maleVariant": "",
              "stringId": "1822868108110336000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Że co, proszę?",
              "maleVariant": "",
              "stringId": "1822876060477165568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałaś powiedzieć \"był\".",
              "maleVariant": "",
              "stringId": "1822876116865388544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura. Zbierz ludzi.",
              "maleVariant": "",
              "stringId": "1822885497711497216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że jest na to szansa?",
              "maleVariant": "",
              "stringId": "1841690843489898496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, ty idziesz ze mną.",
              "maleVariant": "",
              "stringId": "1842859818361950208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Symbole są trwalsze, niż ci się wydaje.",
              "maleVariant": "",
              "stringId": "1842861158895063040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Większa, niż może się wydawać.",
              "maleVariant": "",
              "stringId": "1842997280702861312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pytanie, czy... liderzy frakcji są jeszcze wierni wartościom Arasaki.",
              "maleVariant": "",
              "stringId": "1844343836416806912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I czy będą mieli odwagę przyznać się do błędu.",
              "maleVariant": "",
              "stringId": "1844346672991047680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moje zeznania gówno znaczą.",
              "maleVariant": "",
              "stringId": "1844347581762506756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że jest na to szansa?",
              "maleVariant": "",
              "stringId": "1844347722036809728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam być szczera? To naiwne założenie.",
              "maleVariant": "Mam być szczery? To naiwne założenie.",
              "stringId": "1844350650868961280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mają wyboru. Chodzi o przyszłość korporacji.",
              "maleVariant": "",
              "stringId": "1844350926099189760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie będą mieli wyboru. Tu chodzi o przyszłość korporacji.",
              "maleVariant": "",
              "stringId": "1844351183243579392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Korporacji, w której twój brat był pierwszym pęknięciem. Słowa Takemury.",
              "maleVariant": "",
              "stringId": "1844373897060683776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pęknięciem... Tak.",
              "maleVariant": "",
              "stringId": "1844376056338714624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale ktoś sprawił, że to pęknięcie się powiększało.",
              "maleVariant": "",
              "stringId": "1844376513484296192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie możesz się teraz obwiniać.",
              "maleVariant": "",
              "stringId": "1844378741246922752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale oni mogą mi to wypomnieć.",
              "maleVariant": "",
              "stringId": "1844379212602806272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moje zeznania gówno znaczą. Ty też nie masz tu dużej siły przebicia.",
              "maleVariant": "",
              "stringId": "1844386383554023424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To się zaraz okaże.",
              "maleVariant": "",
              "stringId": "1844387415688351744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, cholera...",
              "maleVariant": "",
              "stringId": "1844402972579991556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powoli...",
              "maleVariant": "",
              "stringId": "1844402978619789316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki.",
              "maleVariant": "",
              "stringId": "1844402981555802112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dasz radę. Jesteśmy już blisko.",
              "maleVariant": "",
              "stringId": "1844402984424706048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wytrzyma?",
              "maleVariant": "",
              "stringId": "1844402990481281028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powinna.",
              "maleVariant": "Powinien.",
              "stringId": "1844402993903833088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wytrzymam.",
              "maleVariant": "",
              "stringId": "1844402996923731968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chwila, czy to...",
              "maleVariant": "",
              "stringId": "1863349118742372352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań]",
              "maleVariant": "",
              "stringId": "1895369521335713792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, dalej.",
              "maleVariant": "",
              "stringId": "1913787885628243972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dasz radę.",
              "maleVariant": "",
              "stringId": "1913787886030897152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mamy na to czasu.",
              "maleVariant": "",
              "stringId": "1913787886098006020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wysiądź]",
              "maleVariant": "",
              "stringId": "1917011090325192708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, cholera...",
              "maleVariant": "",
              "stringId": "1954422090967891972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To chyba ostatni moment.",
              "maleVariant": "",
              "stringId": "2008472534385500160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na co?",
              "maleVariant": "",
              "stringId": "2008472613892726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żebyś mi przedstawiła swój plan.",
              "maleVariant": "",
              "stringId": "2008472678736666624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co, jeśli nie ma żadnego?",
              "maleVariant": "",
              "stringId": "2008472798274330624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pogrywaj ze mną, Hanako.",
              "maleVariant": "",
              "stringId": "2008472950863110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plan... Wtedy, podczas parady, mieliście jeden plan z Takemurą.",
              "maleVariant": "",
              "stringId": "2008473060435107840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chwilę później nic z niego nie zostało.",
              "maleVariant": "",
              "stringId": "2008473505316544512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jednak jesteśmy tu teraz. Wiesz, czemu?",
              "maleVariant": "",
              "stringId": "2008474449538260992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bo czasem trzeba spojrzeć prawdzie w oczy.",
              "maleVariant": "",
              "stringId": "2008482695556702208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To właśnie wtedy zrobiłam. Dzięki tobie.",
              "maleVariant": "",
              "stringId": "2008483086230953984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz bardzo szczere spojrzenie.",
              "maleVariant": "",
              "stringId": "2008484156399230976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak chcesz przekonać zarząd Arasaki? Mam na nich patrzeć?",
              "maleVariant": "",
              "stringId": "2008484371785129984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z bronią w gotowości.",
              "maleVariant": "",
              "stringId": "2008488133924823040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I jeśli mój urok nie zadziała, sięgamy po ostrzejsze argumenty. Rozumiem.",
              "maleVariant": "",
              "stringId": "2008488681113722880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dopiero zrozumiesz. Jak oni wszyscy.",
              "maleVariant": "",
              "stringId": "2008503592434868224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chodź.",
              "maleVariant": "",
              "stringId": "2013916409912696832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[chwyć dłoń]",
              "maleVariant": "",
              "stringId": "2025833805022052352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań]",
              "maleVariant": "",
              "stringId": "2042827347669893120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? To nie jest zebranie, na które chcemy się spóźnić.",
              "maleVariant": "",
              "stringId": "2239262414837489664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czekasz na czerwony dywan? Wyskakuj.",
              "maleVariant": "",
              "stringId": "2239262732799287300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak się powiedziało A, to trzeba powiedzieć B. Chodź.",
              "maleVariant": "",
              "stringId": "2239263043983089664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy coraz mniej czasu.",
              "maleVariant": "",
              "stringId": "2239272875649769480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Taka duża dziewczynka, a boi się wejść do windy?",
              "maleVariant": "Taki duży chłopiec, a boi się wejść do windy?",
              "stringId": "2239273052565512200"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}