  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To tylko jedna wyspa.",
              "maleVariant": "",
              "stringId": "1730332124934664192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I nie odzywaj się, dopóki nie dam ci znaku.",
              "maleVariant": "",
              "stringId": "1730334429100732416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bez względu na to, co się stanie... Musisz mi ufać.",
              "maleVariant": "",
              "stringId": "1730334717266194432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I dwanaście tysięcy ludzi.",
              "maleVariant": "",
              "stringId": "1730335383405555712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Można ich jakoś... relokować. Na tym archipelagu jest dużo wysp.",
              "maleVariant": "",
              "stringId": "1730335640147292160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brzmi jak kosztowna operacja.",
              "maleVariant": "",
              "stringId": "1730336438491115520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spóźniłam się. Przepraszam.",
              "maleVariant": "",
              "stringId": "1730342228258025472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu trzymaj broń w gotowości.",
              "maleVariant": "",
              "stringId": "1730343109951057920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój brat...? Gdzie on jest?",
              "maleVariant": "",
              "stringId": "1730343430211334144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zatem... musimy zacząć bez niego.",
              "maleVariant": "",
              "stringId": "1730343735070126080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę wiedzieć, co tu się dzieje.",
              "maleVariant": "",
              "stringId": "1730345230624714752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu zamordował mego ojca i wykorzystał jego śmierć do swojej wojennej retoryki.",
              "maleVariant": "",
              "stringId": "1730347484425580544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam. Naocznego świadka.",
              "maleVariant": "",
              "stringId": "1730347754924634112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To prawda. Byłam przy tym.",
              "maleVariant": "To prawda. Byłem przy tym.",
              "stringId": "1730348236564951040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Łatwo łyknęliście kłamstwa Yorinobu.",
              "maleVariant": "",
              "stringId": "1730348664350404608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie przeprowadziliście nawet porządnego śledztwa.",
              "maleVariant": "",
              "stringId": "1730348783435083776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wszystko prawda. Widziałam to na własne oczy.",
              "maleVariant": "To wszystko prawda. Widziałem to na własne oczy.",
              "stringId": "1730349503261536256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mogę to potwierdzić.",
              "maleVariant": "",
              "stringId": "1730349944670089216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ten człowiek jest poszukiwany.",
              "maleVariant": "",
              "stringId": "1730350205086035968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeżeli nasze słowa nie wystarczą, może przekona was mój ojciec.",
              "maleVariant": "",
              "stringId": "1730350797221097472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ach, tak?",
              "maleVariant": "",
              "stringId": "1730352110474457088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見下げ果てた奴らめ\" t=\"Jesteście godni pogardy.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1730354735387340800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze tylko trochę, V.",
              "maleVariant": "",
              "stringId": "1761818864418570240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, ale trzeba się śpieszyć.",
              "maleVariant": "",
              "stringId": "1761819092118945796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A mam jakiś, kurwa, wybór?!",
              "maleVariant": "",
              "stringId": "1761819306749870080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest coraz gorzej.",
              "maleVariant": "",
              "stringId": "1761819467307827200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, ale robię się coraz słabsza. Musimy się pośpieszyć.",
              "maleVariant": "Tak, ale robię się coraz słabszy. Musimy się pośpieszyć.",
              "stringId": "1761821759696289792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A mam jakiś, kurwa, wybór? Nie.",
              "maleVariant": "",
              "stringId": "1761822181324505088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest coraz gorzej... Ale muszę spróbować.",
              "maleVariant": "",
              "stringId": "1761822710830219264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz trudniejsza część.",
              "maleVariant": "",
              "stringId": "1761823166230970368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako-sama.",
              "maleVariant": "",
              "stringId": "1762185025043841024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz pamięć do twarzy, Oda.",
              "maleVariant": "",
              "stringId": "1762186170172719104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niedopuszczalne.",
              "maleVariant": "",
              "stringId": "1763182300645834752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To poważne oskarżenia, nawet bardzo, ale... Tylko oskarżenia.",
              "maleVariant": "",
              "stringId": "1763195064835653632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są tutaj!",
              "maleVariant": "",
              "stringId": "1763400557260718080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystkich.",
              "maleVariant": "",
              "stringId": "1763607659929419776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co... Co ty robisz...? Nie możesz tu...",
              "maleVariant": "",
              "stringId": "1811268650187898880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To twoje miejsce, V.",
              "maleVariant": "",
              "stringId": "1811269326745911296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypierdalaj.",
              "maleVariant": "",
              "stringId": "1811423298689449988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pozwolisz...?",
              "maleVariant": "",
              "stringId": "1811425393811406848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co powiedziałby na to jej ojciec...?",
              "maleVariant": "",
              "stringId": "1811426227622264832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ogłuchłeś?!",
              "maleVariant": "",
              "stringId": "1811427691048484864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszałeś? Wypierdalaj.",
              "maleVariant": "",
              "stringId": "1811428200488648704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pozwolisz...? Wszyscy czekamy.",
              "maleVariant": "",
              "stringId": "1811428300732514308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jakiś skandal...",
              "maleVariant": "",
              "stringId": "1811430041838436352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Dziękuję.",
              "maleVariant": "",
              "stringId": "1811434432335200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pozwolę się tak traktować...",
              "maleVariant": "",
              "stringId": "1812536846438121472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zabezpieczyć VIP-ów!",
              "maleVariant": "",
              "stringId": "1815589611446599680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedział, żebyśmy rozpoczęli bez niego.",
              "maleVariant": "",
              "stringId": "1816945237225504768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako? Co tutaj robisz?",
              "maleVariant": "",
              "stringId": "1816945798020726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaczniemy... Co?",
              "maleVariant": "",
              "stringId": "1816947693426704384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako. Wyjaśnij, co to ma znaczyć.",
              "maleVariant": "",
              "stringId": "1816959946733518848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ustąp miejsca. Teraz.",
              "maleVariant": "",
              "stringId": "1816975677135126528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I czemu przyprowadziłaś ze sobą tych... ludzi.",
              "maleVariant": "",
              "stringId": "1816977436226224128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dziękuję.",
              "maleVariant": "",
              "stringId": "1825551335476510720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedziałeś?",
              "maleVariant": "",
              "stringId": "1837123952670806016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O czym?",
              "maleVariant": "",
              "stringId": "1837124158661464064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O Saburo.",
              "maleVariant": "",
              "stringId": "1837124208305246208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie rozmawiałam z jednym z jego konstruktów.",
              "maleVariant": "Właśnie rozmawiałem z jednym z jego konstruktów.",
              "stringId": "1837127671642722304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie... Powiedziałby mi.",
              "maleVariant": "",
              "stringId": "1837130183661719552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czujesz to, Takemura?",
              "maleVariant": "",
              "stringId": "1837130817957924864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City znowu kopnęło cię w dupę.",
              "maleVariant": "",
              "stringId": "1837131082903719936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co...?!",
              "maleVariant": "",
              "stringId": "1837131762313859072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tworzył swoje cyfrowe kopie. Na wypadek śmierci.",
              "maleVariant": "",
              "stringId": "1837132297792262144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po co? Żebyś się mniej przykładał do swoich obowiązków?",
              "maleVariant": "",
              "stringId": "1837136196263612416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありえん…\" t=\"To niemożliwe...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1837140337132179456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co wtedy? Mam ich zneutralizować?",
              "maleVariant": "",
              "stringId": "1837156410191646720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Interesujące.",
              "maleVariant": "",
              "stringId": "1837177179026100224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Licz się ze słowami.",
              "maleVariant": "",
              "stringId": "1837179497805123584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No właśnie: słowa, słowa... Masz coś więcej prócz nich?",
              "maleVariant": "",
              "stringId": "1837192778481537024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Łatwo łyknęliście kłamstwa Yorinobu... Zbyt łatwo.",
              "maleVariant": "",
              "stringId": "1837194905530535936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka zostaje zamordowany, a wy nie przeprowadzacie nawet porządnego śledztwa.",
              "maleVariant": "",
              "stringId": "1837195050166915072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dasz radę?",
              "maleVariant": "",
              "stringId": "1837301592165847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spokojnie.",
              "maleVariant": "",
              "stringId": "1837304529940254720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będę spokojna, jak to się skończy!",
              "maleVariant": "Będę spokojny, jak to się skończy!",
              "stringId": "1837304977908699136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze.",
              "maleVariant": "",
              "stringId": "1837306949600677888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście, Hanako-sama.",
              "maleVariant": "",
              "stringId": "1841477113854431232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to zapomnijmy o relokacji. Dwanaście tysięcy? Nikt nie zwróci uwagi.",
              "maleVariant": "",
              "stringId": "1841525616517451776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama!",
              "maleVariant": "",
              "stringId": "1843127674853003264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"いずれにせよ、仮にも役員である者たちが、なんと情けない\" t=\"W obu przypadkach nie zasługujecie na swoje stanowiska.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843132983399026688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To...? To niby ma nas przekonać?!",
              "maleVariant": "",
              "stringId": "1843133783890333696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"バヘット。お前がミリテクとの全面戦争を憂慮していた時、私はこう言ったはずだ\" t=\"Barreto. Już ci to raz mówiłem. Kiedy obawiałeś się otwartego konfliktu z Militechem.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843134920429277184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jakaś tania sztuczka!",
              "maleVariant": "",
              "stringId": "1843136731294863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お前には3つのものが欠けている。自信、知性、そして根性だ\" t=\"Jesteś człowiekiem trzech deficytów: wiary, inteligencji i fiuta.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843144370431070208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見たところ、あれから何も変わっていないようだな\" t=\"Jak widać, niewiele się pod tym kątem zmieniło.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843151640200089600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To... To naprawdę on...",
              "maleVariant": "",
              "stringId": "1843153757384085504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama... Ja...",
              "maleVariant": "",
              "stringId": "1843154157889785856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"赤子のようにまんまと騙されたか、日和見主義に走ったか…\" t=\"Daliście się oszukać jak dzieci albo przemówił przez was zwykły oportunizm.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843157165960728576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo-domo, co mamy robić?",
              "maleVariant": "",
              "stringId": "1843188214396751872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコの指示に従え\" t=\"Wszystko, co wam rozkaże Hanako.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843190186021621760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obiekcje?",
              "maleVariant": "",
              "stringId": "1843212599610445824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To ty.",
              "maleVariant": "",
              "stringId": "1845868980276695040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V jest po naszej stronie.",
              "maleVariant": "",
              "stringId": "1845869530049286144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mnie czy moim pieniądzom?",
              "maleVariant": "",
              "stringId": "1845871355410386944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ten kabaret zaczyna mnie nudzić. Czy ktoś może wezwać ochronę?",
              "maleVariant": "",
              "stringId": "1845880640576032768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie, czemu nie? Niech idą prosto do Yorinobu i zapytają, co stało się w Konpeki Plaza.",
              "maleVariant": "",
              "stringId": "1845882236408041472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale niech wezmą broń. Kiedy wasz szef jest wkurzony, zaczyna dusić ludzi.",
              "maleVariant": "",
              "stringId": "1845885426243342336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widziałaś co?",
              "maleVariant": "Widziałeś co?",
              "stringId": "1845889650041688064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widziałam, jak Yorinobu udusił Saburo Arasakę.",
              "maleVariant": "Widziałem, jak Yorinobu udusił Saburo Arasakę.",
              "stringId": "1845889901028839424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coraz bardziej interesujące.",
              "maleVariant": "",
              "stringId": "1845890535610257408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było wam to na rękę?",
              "maleVariant": "",
              "stringId": "1845893248402198528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze raz: co to za jedna?",
              "maleVariant": "Jeszcze raz: co to za jeden?",
              "stringId": "1845894798096863232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To poważne oskarżenia, nawet bardzo, ale... Tylko oskarżenia.",
              "maleVariant": "",
              "stringId": "1845998954392457216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"周囲で怪しい動きがあれば直ちに報告しろ\" t=\"Raportujcie każdy podejrzany ruch w budynku.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1863401380156088320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko tylu?",
              "maleVariant": "",
              "stringId": "1863407712498163712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Reszta czeka w gotowości. Zajęli kluczowe pozycje.",
              "maleVariant": "",
              "stringId": "1863407943470096384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A ci są najlepsi. Będą lojalni.",
              "maleVariant": "",
              "stringId": "1863409061923201024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jakieś dwadzieścia procent do osiemdziesięciu, Hanako-sama.",
              "maleVariant": "",
              "stringId": "1863410370260836352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze. Tyle musi wystarczyć.",
              "maleVariant": "",
              "stringId": "1863411138456002560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Idziemy.",
              "maleVariant": "",
              "stringId": "1863412863774564352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコの発する言葉は、すべて私の言葉同然だ\" t=\"Traktujcie każde jej słowo tak, jakby padło z moich ust.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1865012892738068480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To ty?! To twój pomysł, Hanako?",
              "maleVariant": "",
              "stringId": "1865019346614296576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamknąć nas tutaj, żebyśmy się pozagryzali jak szczury w klatce?!",
              "maleVariant": "",
              "stringId": "1865022177148739584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sprowadziłaś tu więcej śmieci z ulicy?!",
              "maleVariant": "",
              "stringId": "1865022845267812352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszycie?",
              "maleVariant": "",
              "stringId": "1865023541572608000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zadałem ci pytanie!",
              "maleVariant": "",
              "stringId": "1865026292784373760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co się dzieje?!",
              "maleVariant": "",
              "stringId": "1865029326943887360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ktoś ma obiekcje.",
              "maleVariant": "",
              "stringId": "1865029933725458432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kto aktywował blokadę?!",
              "maleVariant": "",
              "stringId": "1865030265780117504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaaa!",
              "maleVariant": "",
              "stringId": "1865038608200441856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Omówimy kwestię usunięcia go ze stanowiska CEO Arasaki.",
              "maleVariant": "",
              "stringId": "1866466116045185024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba się przesłyszałem.",
              "maleVariant": "",
              "stringId": "1866473600847781888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pracowałam kilka pięter niżej, w departamencie Abernathy.",
              "maleVariant": "Pracowałem kilka pięter niżej, w departamencie Abernathy.",
              "stringId": "1872075979106361344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wiem, że \"wzruszmy ramionami, poszukajmy kozła ofiarnego\" to nie jest właściwa procedura w przypadku śmierci CEO.",
              "maleVariant": "",
              "stringId": "1872076665529376768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Macie przed sobą naocznego świadka. Wypadałoby zadać mu kilka pytań, a potem pójść prosto po Yorinobu.",
              "maleVariant": "",
              "stringId": "1872078813432455168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To twoi ludzie?",
              "maleVariant": "",
              "stringId": "1905582051571023872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Można tak powiedzieć.",
              "maleVariant": "",
              "stringId": "1905582686051778560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Twoje miejsce.",
              "maleVariant": "",
              "stringId": "1914104974943535108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Usiądziesz?",
              "maleVariant": "",
              "stringId": "1914104975060975620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Siądzie czy nie, to zebranie zamienia się w farsę.",
              "maleVariant": "",
              "stringId": "1914104975178416128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bzdura.",
              "maleVariant": "",
              "stringId": "2000974106692755456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A wy o tym wiedzieliście. Wszyscy.",
              "maleVariant": "",
              "stringId": "2007155863423098880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Można?",
              "maleVariant": "",
              "stringId": "2025682974039416832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, na ile są wierni mnie, a na ile moim pieniądzom.",
              "maleVariant": "",
              "stringId": "2025683231888449536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W twoim przypadku chyba na jedno wychodzi.",
              "maleVariant": "",
              "stringId": "2025683680225992704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zresztą, mniejsza o motywację. Grunt, żeby strzelali we właściwą stronę.",
              "maleVariant": "",
              "stringId": "2025720647395385344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zawsze szczera do bólu.",
              "maleVariant": "Zawsze szczery do bólu.",
              "stringId": "2025739414993182720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podobno po to tu jestem.",
              "maleVariant": "",
              "stringId": "2025740170622214144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko w ostateczności.",
              "maleVariant": "",
              "stringId": "2025748002562187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli: kiedy nasze argumenty nie przekonają zarządu.",
              "maleVariant": "",
              "stringId": "2025753643615854592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Jeśli\". Nie: \"kiedy\".",
              "maleVariant": "",
              "stringId": "2025770605951655936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeżeli nie przekonamy zarządu, sytuacja może wymknąć się spod kontroli.",
              "maleVariant": "",
              "stringId": "2025797265853849600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz pamięć do twarzy.",
              "maleVariant": "",
              "stringId": "2085340488626421764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kto to?! Czy ona... Czy ona jest uzbrojona?!",
              "maleVariant": "Kto to?! Czy on... Czy on jest uzbrojony?!",
              "stringId": "2257441266453553152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to ma znaczyć?!",
              "maleVariant": "",
              "stringId": "2257444837886631944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ochrona? Chyba mamy wtargnięcie.",
              "maleVariant": "",
              "stringId": "2257445247066152968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to za jedna?!",
              "maleVariant": "Co to za jeden?!",
              "stringId": "2257445434551541768"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
