  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To po co im ciężarówki?",
              "maleVariant": "",
              "stringId": "1812883702697598976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spotkałam nomadów. Mają w sobie bestie.",
              "maleVariant": "Spotkałem nomadów. Mają w sobie bestie.",
              "stringId": "1812883702697598992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz jakiś problem z nomadami, świrze?",
              "maleVariant": "",
              "stringId": "1812892488959172608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty! Przestań pierdolić, bo skończysz sześć stóp pod ziemią!",
              "maleVariant": "",
              "stringId": "1812893101814095872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz złamać moje kości, lecz... lecz nie złamiesz ducha, który łaknie prawdy!",
              "maleVariant": "",
              "stringId": "1812893463832862720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam go dosyć. Rozpieprzcie go.",
              "maleVariant": "",
              "stringId": "1812894482629304324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostawcie go. Jest nieszkodliwy.",
              "maleVariant": "",
              "stringId": "1812895648729702400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fajnie być wziętym za wilkołaka.",
              "maleVariant": "",
              "stringId": "1812896048698531840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co słychać u Aldecaldos?",
              "maleVariant": "",
              "stringId": "1812897882297245696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam dosyć tego kolesia. Stężenie jego kretynizmów przekroczyło dopuszczalne normy.",
              "maleVariant": "",
              "stringId": "1812902648150663168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wolnego! Łapy precz od mojego duchowego mistrza!",
              "maleVariant": "",
              "stringId": "1812902675010985984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyluzujcie. Nie chcecie, żeby ludzie uważali nas za wilkołaki? Przecież to całkiem zajebiste.",
              "maleVariant": "",
              "stringId": "1812902708598972416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Siemacie, chłopaki. Co tam słychać u Aldecaldos?",
              "maleVariant": "",
              "stringId": "1812902741264211968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, ilu idiotów wierzy w to, co opowiada ten pomyleniec? Pierdolić to! Nie będę słuchał żarcików o nomadach-kanibalach!",
              "maleVariant": "",
              "stringId": "1812904033344724992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może Bakkerowie lubią dowcipasy typu \"twoja stara to suka\", ale ja tego znosił nie będę!",
              "maleVariant": "",
              "stringId": "1812906688691478528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kogo ja widzę! V! U nas wszystko wporzo. Panam rozkręca się w nowej roli.",
              "maleVariant": "",
              "stringId": "1812908178541813760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ha, ha! Co, kurwa? A już się chciałem obrazić...",
              "maleVariant": "",
              "stringId": "1812908352437657600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było blisko. Za dnia nie są tak groźni, ale wciąż mogą walczyć jak ludzie.",
              "maleVariant": "",
              "stringId": "1812915043929595904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było blisko... Dziękuję ci. Nigdy mnie nie uciszą! Nigdy!",
              "maleVariant": "",
              "stringId": "1812916344650375168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I tak się kończy obrabianie dupy nomadom!",
              "maleVariant": "",
              "stringId": "1812920143012077568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lecz co zapoczątkowało te potworne przemiany? Jeden ze statków Alfa Centauri rozbił się przed laty na Badlandach.",
              "maleVariant": "",
              "stringId": "1813937070085939200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coś tu się nie klei. Po co im ciężarówki, skoro potrafią tak zasuwać?",
              "maleVariant": "",
              "stringId": "1813937721058697216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poznałam kilku nomadów. Przyznam, że jest w nich coś z bestii. Ale z tym ludzkim mięsem to przegiąłeś.",
              "maleVariant": "Poznałem kilku nomadów. Przyznam, że jest w nich coś z bestii. Ale z tym ludzkim mięsem to przegiąłeś.",
              "stringId": "1813937733406728192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O zachodzie słońca zdzierają z siebie ubrania i tańczą nago w zwierzęcej ekstazie.",
              "maleVariant": "",
              "stringId": "1813940838416424960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W ciągu nocy przemierzają dziesiątki kilometrów, szybcy niczym strzały!",
              "maleVariant": "",
              "stringId": "1813941315694665728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A podczas pełni przemieniają się w monstra o ostrych kłach i wyruszają na łowy... Głodne ludzkiego mięsa!",
              "maleVariant": "",
              "stringId": "1814329074770944000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wieść niesie, że przewożono w nim substancję, wybłaganą przez wampiry z Arasaki.",
              "maleVariant": "",
              "stringId": "1814329264890355712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miała ona przeobrazić wrogów Saburo w oddane mu wilkołacze sługi. Lecz wypadek statku skreślił te plany...",
              "maleVariant": "",
              "stringId": "1815414351534923776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, gdybyś miał jakiś poważniejszy dowód na ten twój kosmiczny spisek...",
              "maleVariant": "",
              "stringId": "1854574298864459776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spotkają się dziś w nocy przy starej fabryce w Kabuki. Przekażą tamtym coś... ważnego.",
              "maleVariant": "",
              "stringId": "1854575236761165824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obserwuj ich. Przechwyć to coś, artefakt Alfa Centauri. Przekonasz się, że od początku miałem słuszność!",
              "maleVariant": "",
              "stringId": "1912763937978490880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przemiana zachodzi jedynie podczas pełni. To zaledwie trzy noce w ciągu miesiąca!",
              "maleVariant": "",
              "stringId": "1923009676278743040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podczas swej krwawej uczty pożerają wszystkich ludzi z okolicy. Wciąż muszą szukać nowych terenów i nowej zwierzyny...",
              "maleVariant": "",
              "stringId": "1923010013702111232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz odrzucić prawdę... Ale co zrobisz, gdy dopędzi cię na swych wilczych łapach?",
              "maleVariant": "",
              "stringId": "1923013178824572928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozpieprzcie go. Może się czegoś nauczy.",
              "maleVariant": "",
              "stringId": "1923016151495589888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z przyjemnością!",
              "maleVariant": "",
              "stringId": "1923016457478455296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A tak poważnie, to tylko nieszkodliwy szajbus. Dajcie mu spokój.",
              "maleVariant": "",
              "stringId": "1923016977890918400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trochę dystansu. Przecież to całkiem zabawne.",
              "maleVariant": "",
              "stringId": "1923019033150210048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaraz, kurwa, zobaczysz dystans!",
              "maleVariant": "",
              "stringId": "1923019074069839872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Facet gadał już o piekle, wampirach i nekromantach z kosmosu. Serio myślisz, że próbuje zszargać nam reputację?",
              "maleVariant": "",
              "stringId": "1923020752076652544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trochę racji w tym jest. Gość wyraźnie ma coś z głową.",
              "maleVariant": "",
              "stringId": "1923021450394714112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdolić to. Nie będę bił niedorozwoja.",
              "maleVariant": "",
              "stringId": "1923021812782579712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trochę. Jest zupełnie nieszkodliwy. Opowiada takie rzeczy, że już nawet rząd wypada wiarygodniej.",
              "maleVariant": "",
              "stringId": "1923022525378052096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzeba opowiedzieć Saulowi... Albo wiesz, zeżreć jakiegoś frajera i mknąć niczym strzała!",
              "maleVariant": "",
              "stringId": "1923023737884229632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dziękuję za pomoc... Nawet mimo tego, że wyczułem w twoim głosie kpinę.",
              "maleVariant": "",
              "stringId": "1923025965529415680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czemu nie spróbują się wyleczyć?",
              "maleVariant": "",
              "stringId": "1923027251419136000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czemu nie szukają lekarstwa? Rozszarpani ludzie powinni przyciągać... niewygodną uwagę.",
              "maleVariant": "",
              "stringId": "1923027375939633152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy ptaki szukałyby leku na latanie? Czy gwiazdy pragną porzucić swój blask?",
              "maleVariant": "",
              "stringId": "1923063339546296320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wilkołak jest wolny. Nie obowiązuje go prawo reptilian. Lepiej stać się ludojadem, niż żyć w kajdanach!",
              "maleVariant": "",
              "stringId": "1923063438548647936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wierzę.",
              "maleVariant": "",
              "stringId": "1924029700628004868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W tym, co mówisz, jest ziarno prawdy.",
              "maleVariant": "",
              "stringId": "1924029876084129792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szczerze? Ciężko wymyślić bardziej porąbane teorie spiskowe. Wampiry z Alfa Centauri? Serio?",
              "maleVariant": "",
              "stringId": "1924029963208212480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ciężko brać na serio twoje... interpretacje, ale jest trochę prawdy w tym, co mówisz.",
              "maleVariant": "",
              "stringId": "1924030033924177920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma sprawy. Kto mi powie, jak sfingowano lądowanie na Marsie, jeśli dam cię zatłuc?",
              "maleVariant": "",
              "stringId": "1924062586605387776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wierzysz mi, prawda? Myślisz, że nie umiem wyczuć ironii? Że nie wiem, kiedy biorą mnie za fałszywego proroka?",
              "maleVariant": "",
              "stringId": "1924063331077570560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdybyś słyszała to, co ja... zrozumiałabyś, że świat nie jest taki, jak ci się wydaje.",
              "maleVariant": "Gdybyś słyszał to, co ja... zrozumiałbyś, że świat nie jest taki, jak ci się wydaje.",
              "stringId": "1924093758773379072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale co zrobić? Nikt nie słucha Kasandry... dopóki nie jest za późno.",
              "maleVariant": "",
              "stringId": "1924093856215449600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czasami nawet więcej niż trochę...",
              "maleVariant": "",
              "stringId": "1924094115775758336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Też to widzisz! Wiesz, że za zasłoną mroku kryją się istoty z koszmarów ludzkości... ale boisz się w to uwierzyć.",
              "maleVariant": "",
              "stringId": "1924094229189738496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki za informację.",
              "maleVariant": "",
              "stringId": "1924147962015629316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, zobaczymy, co da się zrobić. Dzięki za cynk... chyba.",
              "maleVariant": "",
              "stringId": "1924148133780766720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd o tym wiesz?",
              "maleVariant": "",
              "stringId": "1924149687971725312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaraz, chwila... Zwolnij trochę. Skąd w ogóle wiesz o tym jakimś spotkaniu?",
              "maleVariant": "",
              "stringId": "1924149737045082112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój umysł rezonuje z ich falami radiowymi. Od czasu wypadku ciągle słyszę tajne transmisje Centauri.",
              "maleVariant": "",
              "stringId": "1924149948857434112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypadku? Wiesz, że to nie brzmi jak argument na twoją korzyść?",
              "maleVariant": "",
              "stringId": "1924150048581206016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie rozumiesz! Mam w głowie ich koprocesor. Nie wiem, jak się dostał do mojego rippera...",
              "maleVariant": "",
              "stringId": "1924150142902714368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...ale dzięki niemu słyszę ich knowania. Jeśli mi pomożesz, razem ocalimy tę planetę!",
              "maleVariant": "",
              "stringId": "1924150237811425280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wolno ci wkroczyć zbyt wcześnie... ani zbyt późno. Bo wszystko będzie stracone!",
              "maleVariant": "",
              "stringId": "1924156577602584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na twoich barkach spoczywają losy ludzkości. Nie zawiedź jej... nie zawiedź mnie.",
              "maleVariant": "",
              "stringId": "1924156673450819584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zero presji, naprawdę.",
              "maleVariant": "",
              "stringId": "1924157101974470656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weź wyluzuj. Kiedy jakieś dziecko mówi ci, że jesteś głupi i śmierdzisz, to też się tak przejmujesz?",
              "maleVariant": "",
              "stringId": "1924345973731737600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Znasz tego oszołoma?",
              "maleVariant": "",
              "stringId": "1924448446215602176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedzieliście, że Hanako Arasaka jest wampirzycą?",
              "maleVariant": "",
              "stringId": "1924448768724025344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzymaj się, V!",
              "maleVariant": "",
              "stringId": "1925735257021857792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiecie, że Hanako jest wampirzycą?",
              "maleVariant": "",
              "stringId": "1925736882583093252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podglądają nas kosmici.",
              "maleVariant": "",
              "stringId": "1925737012841398272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, czy wiecie, ale jesteśmy bohaterami reality show dla jakichś frajerów z kosmosu.",
              "maleVariant": "",
              "stringId": "1925737165530841088"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}