    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oddajcie nam Garry'ego!",
              "maleVariant": "",
              "stringId": "1814352345054748672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agenci Alfa Centauri porwali proroka!",
              "maleVariant": "",
              "stringId": "1814353839535595520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sprawiedliwości dla proroka Garry'ego!",
              "maleVariant": "",
              "stringId": "1814354107736170496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie Garry? Co się stało?",
              "maleVariant": "",
              "stringId": "1814356304913932288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A potem... wsadzili go do AV-ki i odlecieli na statek-matkę.",
              "maleVariant": "",
              "stringId": "1814357585837596672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ktoś próbował zareagować?",
              "maleVariant": "",
              "stringId": "1814358508852912128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nikt! Policjanci powiedzieli, że jest chory. A prawda jest taka, że wiedział za dużo! Był dla nich niebezpieczny!",
              "maleVariant": "",
              "stringId": "1814359203832307712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz... zbieram pieniądze na pomnik Garry'ego. Żebyśmy pamiętali o jego odwadze... I zbrodniach Alfa Centauri!",
              "maleVariant": "",
              "stringId": "1912517988035239936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kilka mogę poświęcić.",
              "maleVariant": "",
              "stringId": "1912520111208701956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dam ci nawet więcej.",
              "maleVariant": "",
              "stringId": "1912536983182417920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kilka mogę poświęcić. To dla ciebie, Garry.",
              "maleVariant": "",
              "stringId": "1912538115980042240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A, co tam. Garry zasługuje na jebutny pomnik. Bierz forsę, zanim się rozmyślę.",
              "maleVariant": "",
              "stringId": "1912538142655815680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz może, co to za tajemniczy chip?",
              "maleVariant": "",
              "stringId": "1913897331563077632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę lecieć. Może innym razem.",
              "maleVariant": "",
              "stringId": "1913902399037284352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam tu taki chip... Mogłabyś rzucić na niego okiem?",
              "maleVariant": "",
              "stringId": "1913902625898799104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dziękuję. Powinno wystarczyć na nos.",
              "maleVariant": "",
              "stringId": "1913908260375797760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, co powiedzieć. Garry byłby wzruszony!",
              "maleVariant": "",
              "stringId": "1913909000720785408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz może, co to za tajemniczy chip?",
              "maleVariant": "",
              "stringId": "1913909637164474372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przekaż darowiznę na budowę pomnika Garry'ego!",
              "maleVariant": "",
              "stringId": "1913911958476857344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ostrzegałam go...! Przyszli ludzie w czarnych garniturach... Mieli niebieskie oczy! Tak, jak mówił!",
              "maleVariant": "",
              "stringId": "1913913786069327872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm... Garry mówił, że jesteś oddana naszej sprawie.",
              "maleVariant": "Hmm... Garry mówił, że jesteś oddany naszej sprawie.",
              "stringId": "1913918723251675136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd mam wiedzieć, że nie jesteś jedną z nich?",
              "maleVariant": "Skąd mam wiedzieć, że nie jesteś jednym z nich?",
              "stringId": "1913920196442234880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spadam. Powodzenia ze zbiórką.",
              "maleVariant": "",
              "stringId": "1913921493505581056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Santa Muerte...! Schowaj to, szybko! Nikt nie może wiedzieć... Rozumiesz?!",
              "maleVariant": "",
              "stringId": "1913934475228782592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to jest? O co w tym wszystkim chodzi?",
              "maleVariant": "",
              "stringId": "1913934764602204160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Garry to przepowiedział! Oni tu są! Naginają nas do swojej woli! Jesteśmy ich marionetkami!",
              "maleVariant": "",
              "stringId": "1913935088821903360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uspokój się! Kto nas nagina? Techno-nekromanci?",
              "maleVariant": "",
              "stringId": "1913935548131745792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"I oto zejdą na Ziemię, lecz wy tego nie dostrzeżecie, bo zasnują niebo ciemnością\". Tak powiedział!",
              "maleVariant": "",
              "stringId": "1913935988365893632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiem ci, co wiem, a ty zedrzesz mi skórę z twarzy i podszyjesz się pode mnie! Nie ma głupich!",
              "maleVariant": "",
              "stringId": "1925572571009105920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobrze, pokaż. Powiem ci wszystko, co wiem.",
              "maleVariant": "",
              "stringId": "1925585731359326208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nadchodzi noc... Wieczna noc!",
              "maleVariant": "",
              "stringId": "1925623277812432896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie powstrzymaliście ich?",
              "maleVariant": "",
              "stringId": "1925626219328159748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Statek-matka? Serio?",
              "maleVariant": "",
              "stringId": "1925626323481116672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widziałaś ten... statek-matkę?",
              "maleVariant": "",
              "stringId": "1925626431610273792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A niby jak? Rozpylili czarną mgłę, żeby niczego nie było widać! Biedny Garry... Poświęcił się dla nas!",
              "maleVariant": "",
              "stringId": "1925629712210313216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zechcesz poświęcić kilka eurodolarów... na ten szczytny cel?",
              "maleVariant": "",
              "stringId": "1925630543689138176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Taa... Może innym razem. Albo na inny pomnik.",
              "maleVariant": "",
              "stringId": "1925648379144814592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powodzenia z dalszą zbiórką.",
              "maleVariant": "",
              "stringId": "1925648702475321344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zastanów się. Garry zasługuje na to... i na wiele więcej!",
              "maleVariant": "",
              "stringId": "1925652220842065920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki i do zobaczenia. Idź w świat i głoś ludziom prawdę!",
              "maleVariant": "",
              "stringId": "1925661735251591168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I tak oto kończy się epicka opowieść o odkrywaniu sekretów istot paranormalnych. Czego nauczyła nas ta historia?",
              "maleVariant": "",
              "stringId": "1925810091273732096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Światem rządzą kosmici?",
              "maleVariant": "",
              "stringId": "1925820734773116932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Otwarte zakończenia są beznadziejne.",
              "maleVariant": "",
              "stringId": "1925820816947920896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Światem rządzi banda szarych eminencji z kosmosu, a my nic nie możemy z tym zrobić?",
              "maleVariant": "",
              "stringId": "1925820936854683648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tego, że otwarte zakończenia są do dupy.",
              "maleVariant": "",
              "stringId": "1925821024465305600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są ludzie, którzy wiedzą więcej niż inni. Słyszą prywatne rozmowy wielkich tego świata.",
              "maleVariant": "",
              "stringId": "1925821108636598272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mogę się nie zgodzić, ale chodziło mi o coś innego.",
              "maleVariant": "",
              "stringId": "1925821496878153728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O! W samo sedno. Tylko że nie.",
              "maleVariant": "",
              "stringId": "1925834178154971136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co z tym robią? Inwestują cały swój czas w wymyślanie na nowo bajeczek sprzed kilku epok.",
              "maleVariant": "",
              "stringId": "1925865196459114496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wolę wierzyć w kosmitów.",
              "maleVariant": "",
              "stringId": "1925865485715095556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz rację. Straciliśmy tylko czas.",
              "maleVariant": "",
              "stringId": "1925865579415846912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale ty lubisz psuć zabawę. Ja tam wolę wierzyć, że przypadkiem otarliśmy się o wojnę wampirów z kosmitami.",
              "maleVariant": "",
              "stringId": "1925865651037782016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz rację. Te podchody były kompletną stratą czasu. Głupia pogoń za... bzdurami.",
              "maleVariant": "",
              "stringId": "1925865684575436800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A prawda jest brutalnie przyziemna. Za wszystkim stoją korporacje. Ludzie, którym odbiło od nadmiaru władzy.",
              "maleVariant": "",
              "stringId": "1925865725025304576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy wolimy. Wampiry i reptilianie są zajebiście ciekawi, prawda? Romantyczni wręcz.",
              "maleVariant": "",
              "stringId": "1925866001127948288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że to właśnie widzieliśmy? Tajne interesy korporacji?",
              "maleVariant": "",
              "stringId": "1925866725249368064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem o tym przekonany. W prawdziwym świecie to oni są krwiopijcami. Innych nigdy nie będzie.",
              "maleVariant": "",
              "stringId": "1925867087167471616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bzdury bywają kusząco przyjemne. Oni o tym wiedzą. Faszerują szarą masę marzeniami o ciekawszym świecie.",
              "maleVariant": "",
              "stringId": "1925878081243561984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem spłukana.",
              "maleVariant": "Jestem spłukany.",
              "stringId": "1945909127391911936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie da rady, jestem spłukana.",
              "maleVariant": "Nie da rady, jestem spłukany.",
              "stringId": "1945909325111402496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Biedny Garry... Jego dziedzictwo przepadnie!",
              "maleVariant": "",
              "stringId": "1945909848979972096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie jest Garry?",
              "maleVariant": "",
              "stringId": "1995483818980712452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz może, co to za tajemniczy chip?",
              "maleVariant": "",
              "stringId": "2003814750667653120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[pokaż tajemniczy chip]",
              "maleVariant": "",
              "stringId": "2222782551538442240"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}