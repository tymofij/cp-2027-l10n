      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musi być ci ciężko samej.",
              "maleVariant": "",
              "stringId": "1921436089602510848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Między tobą a Riverem już w porządku?",
              "maleVariant": "",
              "stringId": "1921436089602510864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musi być ci ciężko samej z trójką dzieci.",
              "maleVariant": "",
              "stringId": "1921436089619288068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z mężem wcale nie miałam lżej.",
              "maleVariant": "",
              "stringId": "1921436089619288084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W najlepszym wypadku po prostu nie było go w domu.",
              "maleVariant": "",
              "stringId": "1921436089619288100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czasem wolałam, żeby zniknął gdzieś na miesiąc, nie dając znaku życia, niż kręcił się w pobliżu.",
              "maleVariant": "",
              "stringId": "1921436089619288116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Między tobą a Riverem już wszystko w porządku?",
              "maleVariant": "",
              "stringId": "1921436089652842500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, w końcu się dogadaliśmy. Lepiej późno niż wcale.",
              "maleVariant": "",
              "stringId": "1921436089652842516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przykre, że w takich okolicznościach...",
              "maleVariant": "",
              "stringId": "1921436089652842532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam nadzieję, że to już koniec twoich kłopotów z Randym.",
              "maleVariant": "",
              "stringId": "1921436089669619716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z nim nigdy nic nie wiadomo. Zmienił się po stracie ojca.",
              "maleVariant": "",
              "stringId": "1921436089669619732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Domyślam się, że to był dla niego cios.",
              "maleVariant": "Domyślam się, że był to dla niego cios.",
              "stringId": "1921436089686396932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale może to, co zrobił mu ten szaleniec... Myślę, że teraz będzie inaczej.",
              "maleVariant": "",
              "stringId": "1921436089686396948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na pewno.",
              "maleVariant": "",
              "stringId": "1921436089686396964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aż tak było źle?",
              "maleVariant": "",
              "stringId": "1937433101388091396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aż tak źle z nim było?",
              "maleVariant": "",
              "stringId": "1937433238692827136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ulżyło ci, kiedy go zabrakło?",
              "maleVariant": "",
              "stringId": "1937433257734967296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak zginął?",
              "maleVariant": "",
              "stringId": "1937433275300712448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostawmy ten temat.",
              "maleVariant": "",
              "stringId": "1937433292514136064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ulżyło ci, kiedy go zabrakło?",
              "maleVariant": "",
              "stringId": "1937433429902757888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak zginął?",
              "maleVariant": "",
              "stringId": "1937433466930073600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostawmy ten temat.",
              "maleVariant": "",
              "stringId": "1937433499209437184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam nadzieję, że to koniec kłopotów z Randym.",
              "maleVariant": "",
              "stringId": "1937457110540488704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Motocykl i autostrada to był jego świat. Za to w domu... Coś ci opowiem.",
              "maleVariant": "",
              "stringId": "1937487895792324608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Raz wziął ze sobą Doriana do miasta, coś załatwić... Wiesz, jak ojciec i syn.",
              "maleVariant": "",
              "stringId": "1937488756446728192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wrócił wieczorem, pijany, awanturując się, że nasłałam na niego gliny.",
              "maleVariant": "",
              "stringId": "1937489534070689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Płakałam każdej nocy przez kilka tygodni.",
              "maleVariant": "",
              "stringId": "1937496987298566144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zabito mi męża, V. Ojca moich dzieci.",
              "maleVariant": "",
              "stringId": "1937498132075122688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam.",
              "maleVariant": "",
              "stringId": "1937498589472362496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Randy zamknął się w sobie, a miałam na głowie jeszcze dwójkę maluchów...",
              "maleVariant": "",
              "stringId": "1937498863393968128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było mi źle z nim i bez niego. Pewne sprawy nie mają dobrych rozwiązań.",
              "maleVariant": "",
              "stringId": "1937500056757342208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aldecaldos powiedzieli mi tylko, że honorową śmiercią. Cokolwiek miałoby to oznaczać.",
              "maleVariant": "",
              "stringId": "1937504728759013376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Identyfikowałam jego ciało... Miał dziurę zamiast twarzy. Nie wiem, gdzie w tym honor.",
              "maleVariant": "",
              "stringId": "1937506112678006784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kiedy okazało się, że Randy może już do mnie nie wrócić...",
              "maleVariant": "",
              "stringId": "1937558700526014464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale wszystko dobrze się skończyło. Razem z synem wrócił też mój brat.",
              "maleVariant": "",
              "stringId": "1937559117121064960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzymaj się.",
              "maleVariant": "",
              "stringId": "1937577636801941504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzymaj się, Joss.",
              "maleVariant": "",
              "stringId": "1937577789340389376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A nasłałaś?",
              "maleVariant": "",
              "stringId": "1963542117637820416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River go znalazł w jakiejś knajpie i zabrał Doriana do domu. Szkoda słów.",
              "maleVariant": "",
              "stringId": "1963542326044397568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River opowiedział mi o waszych rodzicach.",
              "maleVariant": "",
              "stringId": "1963568882162733056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River opowiedział mi o waszych rodzicach...",
              "maleVariant": "",
              "stringId": "1963569226095661056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musi ci bardzo ufać, bo nie mówił o tym prawie nikomu.",
              "maleVariant": "",
              "stringId": "1963573508765257728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To musiało być straszne... widzieć ich śmierć. Jak daliście sobie radę?",
              "maleVariant": "",
              "stringId": "1963576553309868032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sierociniec to niezła szkoła życia, ale mieliśmy siebie. Przynajmniej do czasu.",
              "maleVariant": "",
              "stringId": "1963577292597891072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potem straciłam brata na rzecz jego obsesji. Jakby chciał sam jeden wyłapać wszystkich morderców na świecie.",
              "maleVariant": "",
              "stringId": "1963581037087952896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ma to swoje dobre strony. Gliniarz z powołaniem to rzadkość.",
              "maleVariant": "",
              "stringId": "1963585992322584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Być może. Ale wolę go widzieć takim, jaki jest teraz. Pogodzonym ze sobą i swoim życiem.",
              "maleVariant": "",
              "stringId": "1963586493525135360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam, Joss...",
              "maleVariant": "",
              "stringId": "2005702499139457024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam, Joss...",
              "maleVariant": "",
              "stringId": "2005702799820722176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostawiłaś ich. Zostawiłaś samych... Obiecałaś pomóc...",
              "maleVariant": "Zostawiłeś ich. Zostawiłeś samych... Obiecałeś pomóc...",
              "stringId": "2005704389596155904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss...",
              "maleVariant": "",
              "stringId": "2005704971295789056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój syn i brat nie żyją. Nie żyją! Rozumiesz?",
              "maleVariant": "",
              "stringId": "2005705450302083072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pokazuj mi się więcej na oczy. Nigdy.",
              "maleVariant": "",
              "stringId": "2005705631898669056"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}