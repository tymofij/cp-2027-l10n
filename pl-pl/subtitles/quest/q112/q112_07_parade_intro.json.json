  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura? Ściągnęłam tego bliższego.",
              "maleVariant": "Takemura? Ściągnąłem tego bliższego.",
              "stringId": "1663657671006187520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy zabezpieczoną południową część ulicy.",
              "maleVariant": "",
              "stringId": "1663658335249723392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I po kłopocie.",
              "maleVariant": "",
              "stringId": "1663661755771744256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widzę. Bardzo dobrze.",
              "maleVariant": "",
              "stringId": "1663661755771744264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś już blisko. Masz go prawie przed sobą.",
              "maleVariant": "",
              "stringId": "1663673526448386048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze parę metrów i snajper jest twój.",
              "maleVariant": "",
              "stringId": "1663674713302200320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tę kładkę patroluje strzelec.",
              "maleVariant": "",
              "stringId": "1663675867457220608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uwaga, podejrzane zachowanie w sektorze 3-C.",
              "maleVariant": "",
              "stringId": "1669053767187709952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyjąłem, Wysokie Gniazdo. Już sprawdzam obszar...",
              "maleVariant": "",
              "stringId": "1669056901020663808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W białej marynarce, przyjąłem.",
              "maleVariant": "",
              "stringId": "1669058258330992640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niskie Gniazdo? Agent Militechu na twojej czwartej.",
              "maleVariant": "",
              "stringId": "1669060075018272768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Baza? Melduję brak kontaktu z pozostałymi stanowiskami.",
              "maleVariant": "",
              "stringId": "1669060916060110848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wysoki Zamek, dlaczego cię nie słyszę?",
              "maleVariant": "",
              "stringId": "1669062349035683840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potwierdzam. To wrogi agent.",
              "maleVariant": "",
              "stringId": "1669243880693841920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Parada wkrótce się zacznie.",
              "maleVariant": "",
              "stringId": "1672165359190294536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będę na ciebie czekać tam, gdzie poprzednio.",
              "maleVariant": "",
              "stringId": "1698117438240997376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "U góry jest kładka na drugą stronę ulicy.",
              "maleVariant": "",
              "stringId": "1698192225550221312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niewielkie drzwi obok różowego neonu –  prowadzą na kładkę techniczną.",
              "maleVariant": "",
              "stringId": "1698196025287655424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przedostaniesz się nią na drugą stronę ulicy.",
              "maleVariant": "",
              "stringId": "1698196433947082752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na końcu pomieszczenia znajdziesz drzwi prowadzące na klatkę schodową.",
              "maleVariant": "",
              "stringId": "1698365674952343552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To najszybsza droga do następnego snajpera. Wysoko się zaszył.",
              "maleVariant": "",
              "stringId": "1698366182798671872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz się jakoś opuścić na dół. Tylko ostrożnie.",
              "maleVariant": "",
              "stringId": "1698419549361160192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spróbuj się dostać do części technicznej. Stamtąd przebijesz wyżej.",
              "maleVariant": "",
              "stringId": "1698420421541511168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jeśli się nie uda, możesz przejść po tej kładce na prawo.",
              "maleVariant": "",
              "stringId": "1698421006563033088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko uważaj na silniki platform. Trzymaj głowę nisko, bo cię usmażą.",
              "maleVariant": "",
              "stringId": "1698421502279434240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, jak to otworzyć. Spróbujmy innej drogi.",
              "maleVariant": "",
              "stringId": "1698421833159688192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak jak mówiłem: dostań się na balkon z zielonym neonem w kształcie strzałki. I drugi snajper jest twój.",
              "maleVariant": "",
              "stringId": "1698422581473853440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[przeciąż obwody]",
              "maleVariant": "",
              "stringId": "1698505217265385476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przy straganach? Tylko nie pakuj się w kłopoty.",
              "maleVariant": "",
              "stringId": "1721317695742152704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie widzę. Elegancka robota.",
              "maleVariant": "",
              "stringId": "1721366778091540480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obserwuje tłum i nie zwraca uwagi na tyły.",
              "maleVariant": "",
              "stringId": "1721371586257096704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, zajdę go.",
              "maleVariant": "",
              "stringId": "1721372618173321216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W kontakcie.",
              "maleVariant": "",
              "stringId": "1721401639468666880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem na pozycji. Platformy właśnie wyruszyły.",
              "maleVariant": "",
              "stringId": "1724431179609817088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętaj, żeby uważać, bo ściągniesz na siebie drony Arasaki.",
              "maleVariant": "",
              "stringId": "1724431179626594332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyjęłam, jestem gotowa.",
              "maleVariant": "Przyjąłem, jestem gotowy.",
              "stringId": "1724431351240736768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa! Prawie spadłam!",
              "maleVariant": "Kurwa! Prawie spadłem.",
              "stringId": "1724431491749920768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zwracaj na siebie uwagi!",
              "maleVariant": "",
              "stringId": "1724431491766697984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz zdążyć, zanim pojawi się platforma Hanako-samy.",
              "maleVariant": "",
              "stringId": "1724431642090553344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak to się mówi...? Cel zdjęty.",
              "maleVariant": "",
              "stringId": "1724431810517024768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coś, co chciałem usłyszeć.",
              "maleVariant": "",
              "stringId": "1724431810517024780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wciąż mamy tu jeden problem.",
              "maleVariant": "",
              "stringId": "1733091170888544256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta netrunnerka Arasaki. Przejęła kontrolę nad wszystkimi sieciami.",
              "maleVariant": "",
              "stringId": "1733092059258908672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musisz się jej pozbyć, bo inaczej nie uda nam się przejąć platformy.",
              "maleVariant": "",
              "stringId": "1733092122995552256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaszyła się w nieukończonym apartamentowcu, niedaleko gniazda drugiego snajpera.",
              "maleVariant": "",
              "stringId": "1733092436561719296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To znaczy?",
              "maleVariant": "",
              "stringId": "1764749454521274368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widziałem też netrunnerkę w korporacyjnym uniformie. Gdzieś się zaszyła.",
              "maleVariant": "",
              "stringId": "1770656078498762752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to zaczynajmy.",
              "maleVariant": "",
              "stringId": "1770938091470458880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, idę po nią.",
              "maleVariant": "",
              "stringId": "1770947849753595904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brodaty ćpun obok stoiska z balonami. Zajmijcie się nim.",
              "maleVariant": "",
              "stringId": "1770966335980703744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Patrol, czekajcie!",
              "maleVariant": "",
              "stringId": "1770979228348452864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Balkon dwa piętra wyżej. Kobieta w czerwieni. Chyba jest uzbrojona.",
              "maleVariant": "",
              "stringId": "1770982366610591744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odwołuję. Trzyma lornetkę. Zgarniajcie ćpuna.",
              "maleVariant": "",
              "stringId": "1770984906496544768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powtarzam, Wysoki Zamek, zgłoś się.",
              "maleVariant": "",
              "stringId": "1770991100309147648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Baza? Coś jest nie tak, Wysoki Zamek nie odpowiada. Proszę o rozpoznanie.",
              "maleVariant": "",
              "stringId": "1770991494389174272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proszę o rozpoznanie.",
              "maleVariant": "",
              "stringId": "1770998790330650624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miej go na oku. Zaraz dotrze tu czoło parady.",
              "maleVariant": "",
              "stringId": "1771000848425291776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz spróbować się wspiąć po tych rurach. Wyglądają na wytrzymałe.",
              "maleVariant": "",
              "stringId": "1793842159099592704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To teren budowy, bądź ostrożna.",
              "maleVariant": "To teren budowy, bądź ostrożny.",
              "stringId": "1793844448484933632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcemy, żeby robotnicy wezwali ochronę.",
              "maleVariant": "",
              "stringId": "1793845087629754368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? To znowu Oda! Posłuchaj!",
              "maleVariant": "",
              "stringId": "1876334641850368000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...komunikat o naruszeniu bezpieczeństwa! Trzeba ją stąd ewakuować! Natychmiast!",
              "maleVariant": "",
              "stringId": "1876334862403649536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka potrafi ochronić swoich przywódców.",
              "maleVariant": "",
              "stringId": "1876335413350645760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sytuacja jest pod kontrolą. Bez odbioru.",
              "maleVariant": "",
              "stringId": "1876335864338989056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszałaś? Chyba się połapali. Musimy się śpieszyć.",
              "maleVariant": "Słyszałeś? Chyba się połapali. Musimy się śpieszyć.",
              "stringId": "1876336499088179200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zapytaj swojego szefa, co jest dla niego ważniejsze: wizerunek czy życie siostry!",
              "maleVariant": "",
              "stringId": "1883701077932318720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Otwarte.",
              "maleVariant": "",
              "stringId": "1895511850779426816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słowo, które dobrze usłyszeć z ust złodziejki.",
              "maleVariant": "Słowo, które dobrze usłyszeć z ust złodzieja.",
              "stringId": "1896726696119050240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie tak.",
              "maleVariant": "",
              "stringId": "1898409598204583936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie za szybko.",
              "maleVariant": "",
              "stringId": "1898411585499373568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcemy cię szukać na dole.",
              "maleVariant": "",
              "stringId": "1898412011741323264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz wskocz na balkon obok zielonego neonu i wejdź do windy.",
              "maleVariant": "",
              "stringId": "2006884509167722496"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}