    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Milczenie nie jest złotem.",
              "maleVariant": "",
              "stringId": "1669135755361927168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cisza to wygaszony płomień.",
              "maleVariant": "",
              "stringId": "1669136419068592128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka 3 marca 1942 roku w obozie jenieckim, miesiąc po stracie myśliwca nad zatoką Guadalcanal.",
              "maleVariant": "",
              "stringId": "1669136734530584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka, pilot trzeciej eskadry w stopniu sierżanta.",
              "maleVariant": "",
              "stringId": "1669136891548549120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa!",
              "maleVariant": "",
              "stringId": "1669217419584880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Jak sytuacja?!",
              "maleVariant": "",
              "stringId": "1669218386388729856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz dzisiaj dużo szczęścia.",
              "maleVariant": "",
              "stringId": "1669223235054485504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wasze pieprzone poczucie honoru...",
              "maleVariant": "",
              "stringId": "1669223459097427968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale masz dzisiaj trochę szczęścia.",
              "maleVariant": "",
              "stringId": "1669223849721348096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, udało się! Jestem w systemie.",
              "maleVariant": "",
              "stringId": "1669230128560881664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możesz podziękować Takemurze.",
              "maleVariant": "",
              "stringId": "1698486545029283840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedziałam, że tak będzie!",
              "maleVariant": "Wiedziałem, że tak będzie!",
              "stringId": "1713082986469015552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja... Udało mi się...",
              "maleVariant": "",
              "stringId": "1713085187102883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda-san. Nie żyje?",
              "maleVariant": "",
              "stringId": "1713085246712332288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oddycha...",
              "maleVariant": "",
              "stringId": "1713085328887136256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, proszę. Oszczędź go.",
              "maleVariant": "",
              "stringId": "1724162296454713344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie!",
              "maleVariant": "",
              "stringId": "1724187621863591936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty... Nie musiałaś tego robić!",
              "maleVariant": "Ty... Nie musiałeś tego robić.",
              "stringId": "1724188104258883584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zrobiłby ze mną to samo.",
              "maleVariant": "",
              "stringId": "1724189454287900676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musiałam. Nie lubię niedokończonych spraw.",
              "maleVariant": "Musiałem. Nie lubię niedokończonych spraw.",
              "stringId": "1724192698280054784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zrobiłby ze mną to samo.",
              "maleVariant": "",
              "stringId": "1724194637659115520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musiałam. Nie lubię zostawiać za sobą niedokończonych spraw.",
              "maleVariant": "Musiałem. Nie lubię zostawiać za sobą niedokończonych spraw.",
              "stringId": "1724194708593184768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sam widziałeś!",
              "maleVariant": "",
              "stringId": "1724195301483220992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będę o tym pamiętał.",
              "maleVariant": "",
              "stringId": "1724196313199677440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One później wracają i kopią cię w dupę.",
              "maleVariant": "",
              "stringId": "1724197607595446272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozumiem. Będę o tym pamiętał.",
              "maleVariant": "",
              "stringId": "1724198471135531008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hrr...",
              "maleVariant": "",
              "stringId": "1724200725154500608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To ja tobie dziękuję, V.",
              "maleVariant": "",
              "stringId": "1724201522659463168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze.",
              "maleVariant": "",
              "stringId": "1724202997997506560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda!",
              "maleVariant": "",
              "stringId": "1725692919384248320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widzę Hanako-samę. Chyba zaraz rozpocznie przemówienie.",
              "maleVariant": "",
              "stringId": "1740315746735321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Założyciel i CEO największego koncernu zbrojeniowego w historii.",
              "maleVariant": "",
              "stringId": "1773226942591275008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żołnierz, przedsiębiorca, wizjoner.",
              "maleVariant": "",
              "stringId": "1773227138012286976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój ojciec.",
              "maleVariant": "",
              "stringId": "1773227278957678592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie kieruję tych słów do jego przyjaciół, członków rodziny i partnerów biznesowych.",
              "maleVariant": "",
              "stringId": "1773228423298027520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kieruję je do jego przeciwników.",
              "maleVariant": "",
              "stringId": "1773228678043275264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I nieprzypadkowo robię to właśnie tutaj, w tym mieście.",
              "maleVariant": "",
              "stringId": "1773228825934434304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pięćdziesiąt lat temu w Night City nasi wrogowie nie pierwszy raz ujawnili swoje oblicze.",
              "maleVariant": "",
              "stringId": "1773229002313306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tchórzliwy akt terroru pochłonął cztery tysiące istnień. Pracowników Arasaka Corporation, obywateli tego miasta.",
              "maleVariant": "",
              "stringId": "1773229207901310976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka Tower runęła.",
              "maleVariant": "",
              "stringId": "1773229364399181824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jednak...",
              "maleVariant": "",
              "stringId": "1773229528446799872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jednak wciąż widzę jej iglicę ponad dachami.",
              "maleVariant": "",
              "stringId": "1773229586697293824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potężniejszą i trwalszą niż wcześniej.",
              "maleVariant": "",
              "stringId": "1773229822366846976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niedawno mój ojciec padł ofiarą równie tchórzliwej zbrodni.",
              "maleVariant": "",
              "stringId": "1773229988796829696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zbrodni wymierzonej w świat podzielanych przez nas wartości.",
              "maleVariant": "",
              "stringId": "1773230168111714304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I podobnie jak pięćdziesiąt lat temu, Arasaka Corporation wychodzi z tego doświadczenia mocniejsza, a Night City jest tego naocznym świadkiem.",
              "maleVariant": "",
              "stringId": "1773230362727419904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To dzieje się tu i teraz.",
              "maleVariant": "",
              "stringId": "1773230601903411200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słuchajcie, bo nikt z nas nie będzie milczeć.",
              "maleVariant": "",
              "stringId": "1773231479687352320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Milczenie nie jest złotem.",
              "maleVariant": "",
              "stringId": "1773231823318290432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cisza to wygaszony płomień.",
              "maleVariant": "",
              "stringId": "1773232269156028416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz już ci nic nie powinno przeszkodzić. Kontynuujemy!",
              "maleVariant": "",
              "stringId": "1773457869241335808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[odłącz] Zrób sobie przerwę.",
              "maleVariant": "",
              "stringId": "1794073849835307012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zrób sobie przerwę.",
              "maleVariant": "",
              "stringId": "1794076688959684608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz, V! Odwracam jej uwagę!",
              "maleVariant": "",
              "stringId": "1794077043277709312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypnij ją z sieci!",
              "maleVariant": "",
              "stringId": "1794077698058895360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przepraszam, Oda.",
              "maleVariant": "",
              "stringId": "1834462117765189632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty mi też nie zostawiasz wyboru...",
              "maleVariant": "",
              "stringId": "1834463290291261440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ough...!",
              "maleVariant": "",
              "stringId": "1857629018768875520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie tak.",
              "maleVariant": "",
              "stringId": "1857629722438868992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co jest...?!",
              "maleVariant": "",
              "stringId": "1857631771960352768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ostrzegałem cię.",
              "maleVariant": "",
              "stringId": "2004115740769206272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Załatwimy to szybko.",
              "maleVariant": "",
              "stringId": "2230175985079525376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zostawiasz mi wyboru!",
              "maleVariant": "",
              "stringId": "2230178248040751112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzeba było wyjechać z miasta!",
              "maleVariant": "",
              "stringId": "2230182266821517312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze się trzymasz?! Spróbujmy inaczej!",
              "maleVariant": "",
              "stringId": "2230183836330074120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pozwolę ci się... do niej... zbliżyć!",
              "maleVariant": "",
              "stringId": "2230199314201026568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A gdzie Takemura-san?! Bał się własnego ucznia?!",
              "maleVariant": "",
              "stringId": "2230201191688937480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[podłącz się]",
              "maleVariant": "",
              "stringId": "2231360085887631364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uważaj, będzie teraz ładować kamuflaż! Wypatruj załamań światła!",
              "maleVariant": "",
              "stringId": "2252032229872345088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jego maska zakłóca systemy namierzania! Musisz spróbować czegoś innego!",
              "maleVariant": "",
              "stringId": "2252051393278222344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba już mu wystarczy na dzisiaj! Nie zabijaj go, to był jego obowiązek!",
              "maleVariant": "",
              "stringId": "2252053442866814984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Utrzymuj dystans! Nie wchodź w zasięg ostrza, bo cię posieka na tatar!",
              "maleVariant": "",
              "stringId": "2252057682972614664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda myśli, że walczy o słuszną sprawę, ale to TY walczysz o słuszną sprawę! Utrzymaj się na nogach!",
              "maleVariant": "",
              "stringId": "2252059389282590728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[posłuchaj śpiewu]",
              "maleVariant": "",
              "stringId": "2453324305031774208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań]",
              "maleVariant": "",
              "stringId": "2453407681940045824"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}