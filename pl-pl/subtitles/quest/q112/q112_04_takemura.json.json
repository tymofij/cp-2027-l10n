    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To co, idziemy?",
              "maleVariant": "",
              "stringId": "1660580220269064192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W porządku. Zaczekajmy.",
              "maleVariant": "",
              "stringId": "1660648652486307840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie kłóćmy się o to.",
              "maleVariant": "",
              "stringId": "1660663118875684868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na ulicy ludzie nienawidzą korporacji.",
              "maleVariant": "",
              "stringId": "1660663443900690432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Tak, słabiej się czuję.",
              "maleVariant": "",
              "stringId": "1660706242226384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobrze, czas rozruszać kości.",
              "maleVariant": "",
              "stringId": "1685252933890883584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę mieć pewność, że niczego nie pominęliśmy.",
              "maleVariant": "",
              "stringId": "1712936061744726016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powinniśmy trochę tutaj poczekać.",
              "maleVariant": "",
              "stringId": "1713036092657123328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No i bakeneko poszedł sobie.",
              "maleVariant": "",
              "stringId": "1766413117334360064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1773573723300696064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co?",
              "maleVariant": "",
              "stringId": "1773576740515221504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wykonuj gwałtownych ruchów.",
              "maleVariant": "",
              "stringId": "1773577106929618944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widzisz?",
              "maleVariant": "",
              "stringId": "1773610744643719168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kot.",
              "maleVariant": "",
              "stringId": "1773611061783433216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślałam, że nie ma ich już w mieście.",
              "maleVariant": "Myślałem, że nie ma ich już w mieście.",
              "stringId": "1773616136740278276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No i? To tylko kot.",
              "maleVariant": "",
              "stringId": "1773618395104546816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ładny dachowiec. Myślałam, że zniknęły z miasta.",
              "maleVariant": "Ładny dachowiec. Myślałem, że zniknęły z miasta.",
              "stringId": "1773620770053689344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No co? To tylko kot?",
              "maleVariant": "",
              "stringId": "1773620979282350080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pamiętasz, co zdarzyło się w Tom's Diner? Ludzie nienawidzą korporacji.",
              "maleVariant": "",
              "stringId": "1781989202595811328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, mam już dosyć tej dyskusji.",
              "maleVariant": "",
              "stringId": "1781998416022081536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiesz, co czuję.",
              "maleVariant": "",
              "stringId": "1782006745440063488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wreszcie zaczynasz mówić z sensem.",
              "maleVariant": "",
              "stringId": "1782006745456840704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ech, czasami chciałbym zostać nomadą. Uciec od świata, zapomnieć o wszystkim.",
              "maleVariant": "",
              "stringId": "1782126000508637184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wreszcie zaczynasz mówić z sensem.",
              "maleVariant": "",
              "stringId": "1782227335463702528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Znajdzie swoją drogę.",
              "maleVariant": "",
              "stringId": "1783526439892422656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To powiedziałaś przed wejściem do Konpeki Plaza?",
              "maleVariant": "To powiedziałeś przed wejściem do Konpeki Plaza?",
              "stringId": "1834479571673419776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My też znajdziemy.",
              "maleVariant": "",
              "stringId": "1995380886478856192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli przez ten czas nie wydarzy się nic szczególnego – wchodzimy!",
              "maleVariant": "",
              "stringId": "2004085483342995456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O Jackiego? Mów.",
              "maleVariant": "",
              "stringId": "2011525265438351360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kiedy ja ruszyłem twoim tropem, inni poszli za Wellesem. I przechwycili jego zwłoki.",
              "maleVariant": "",
              "stringId": "2011526037878153216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie rozumiem po co. Żeby jego matka nie mogła mu wyprawić porządnego pogrzebu?",
              "maleVariant": "",
              "stringId": "2011528585481625600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka zna sposób na pozyskiwanie informacji od zmarłych.",
              "maleVariant": "",
              "stringId": "2011529065544884224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O czym ty mówisz?",
              "maleVariant": "",
              "stringId": "2011533238558597120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pytaj mnie o szczegóły, bo się na tym nie znam. Wiem tylko, że używa się do tego Soulkillera.",
              "maleVariant": "",
              "stringId": "2011533889380360192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z ciała Wellesa da się stworzyć engram, uszkodzony i niepełny, ale jednak.",
              "maleVariant": "",
              "stringId": "2011535192231845888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brednie. Nie wierzę.",
              "maleVariant": "",
              "stringId": "2011536939394342912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wierzę. To jakaś bzdura.",
              "maleVariant": "",
              "stringId": "2011537798622683136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przeżyłaś własną śmierć. Zacznij wierzyć w niemożliwe.",
              "maleVariant": "Przeżyłeś własną śmierć. Zacznij wierzyć w niemożliwe.",
              "stringId": "2011538012012093440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie trzymają konstrukt Jackiego?",
              "maleVariant": "",
              "stringId": "2011541966066360320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie trzymają konstrukt Jackiego?",
              "maleVariant": "",
              "stringId": "2011543650851168268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem.",
              "maleVariant": "",
              "stringId": "2011543650867945472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wszyscy chcą wysługiwać się korporacjom.",
              "maleVariant": "",
              "stringId": "2011590749345095684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wszyscy chcą wysługiwać się korporacjom.",
              "maleVariant": "",
              "stringId": "2011592214935576576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego nie powiedziałeś mi o tym wcześniej?",
              "maleVariant": "",
              "stringId": "2011650300626935808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiedziałem, czy mogę ci ufać. To się zmieniło.",
              "maleVariant": "",
              "stringId": "2011650478331207680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie osądzam cię.",
              "maleVariant": "",
              "stringId": "2011659115879538688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jest jeszcze za późno na zmianę.",
              "maleVariant": "",
              "stringId": "2012789124660797440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm... Wiesz, co myślę?",
              "maleVariant": "",
              "stringId": "2012797710417862656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Że wtedy w Konpeki oboje straciliśmy kogoś bliskiego.",
              "maleVariant": "Że wtedy w Konpeki obaj straciliśmy kogoś bliskiego.",
              "stringId": "2012798393686761472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mędrkuj, nie wiesz, co czuję.",
              "maleVariant": "",
              "stringId": "2012803108906541056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie osądzam cię.",
              "maleVariant": "",
              "stringId": "2012842009868181504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie pogodziliśmy się z losem. I dlatego szukamy okazji do sprzeczki.",
              "maleVariant": "",
              "stringId": "2012842998499188736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest coś, o czym powinnaś wiedzieć... Chodzi o Wellesa.",
              "maleVariant": "Jest coś, o czym powinieneś wiedzieć... Chodzi o Wellesa.",
              "stringId": "2012863072589127680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powinnością żywych jest zadośćuczynić umarłym.",
              "maleVariant": "",
              "stringId": "2012865370732175360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako-sama pomoże nam tego dokonać.",
              "maleVariant": "",
              "stringId": "2012865911679950848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo sprawi, że dołączycie do umarłych.",
              "maleVariant": "",
              "stringId": "2012871354477256704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jest jeszcze za późno.",
              "maleVariant": "",
              "stringId": "2014141091161120768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nomadzi wcale tacy nie są.",
              "maleVariant": "",
              "stringId": "2014141091161120784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wiesz co? Nabrałam dystansu. Ty też powinieneś.",
              "maleVariant": "I wiesz co? Nabrałem dystansu. Ty też powinieneś.",
              "stringId": "2014151271038918656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nomadzi wcale nie są egoistami. Na pierwszym miejscu stawiają rodzinę.",
              "maleVariant": "",
              "stringId": "2014153015047299080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mówisz, jakbyś mnie osądzała. Nie masz do tego prawa.",
              "maleVariant": "Mówisz, jakbyś mnie osądzał. Nie masz do tego prawa.",
              "stringId": "2027094611174694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja, w przeciwieństwie do ciebie i twojego przyjaciela Wellesa, nie poszedłem na skróty.",
              "maleVariant": "",
              "stringId": "2027271725811310592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I niby ty masz czyste ręce?",
              "maleVariant": "",
              "stringId": "2027328329168527360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zapomniałeś już, że pracowałam dla Arasaki.",
              "maleVariant": "Zapomniałeś już, że pracowałem dla Arasaki.",
              "stringId": "2027344169242796032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zapomniałeś już, że byłam częścią Arasaki? Dobrze wiem, jak funkcjonuje korporacja.",
              "maleVariant": "Zapomniałeś już, że byłem częścią Arasaki? Dobrze wiem, jak funkcjonuje korporacja.",
              "stringId": "2027345135761432576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nieprawda. Wygodnie ustawiasz się w opozycji do korporacyjnego świata i nie proponujesz niczego w zamian.",
              "maleVariant": "",
              "stringId": "2028753201010647040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pokazujesz mi brudne ulice i uważasz, że nie istnieje żaden inny świat. A co z milionami ludzi, którzy pracują dla Arasaki w zamian za stabilne życie i bezpieczeństwo?",
              "maleVariant": "",
              "stringId": "2028753325598253056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A co z dzieciakami z ulic Chiba-11? One też żyją w świecie urządzonym przez korporacje.",
              "maleVariant": "",
              "stringId": "2028753422889328640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Korporacje potrzebują pieniędzy i zasobów, żeby utrzymać porządek. Kto niby ma nami rządzić? Skorumpowane rządy? Motłoch przyssany do paśników?",
              "maleVariant": "",
              "stringId": "2028753779824599040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Krytykanci i nihiliści, jak ty?",
              "maleVariant": "",
              "stringId": "2028756566268518400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I niby ty masz czyste ręce?",
              "maleVariant": "",
              "stringId": "2028829904076611644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nikt nie ma czystych rąk. Pytanie, w jakim celu je sobie brudzisz.",
              "maleVariant": "",
              "stringId": "2028829904093388804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to powiedz, w imię czego brudziłeś sobie ręce?",
              "maleVariant": "",
              "stringId": "2028829904093388816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Najważniejsze, co dała mi Arasaka, to system wartości, których mogę się trzymać. Tak po prostu.",
              "maleVariant": "",
              "stringId": "2028829904093388832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wierzysz w duchy?",
              "maleVariant": "",
              "stringId": "2037552193874714628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierwsze zwierzę, jakie widzę w Night City, nie licząc karaluchów.",
              "maleVariant": "",
              "stringId": "2037552315492753408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A może to bakeneko?",
              "maleVariant": "",
              "stringId": "2037553994506199040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To tylko zwykły kot.",
              "maleVariant": "",
              "stringId": "2037556175863046144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może rzeczywiście to bakeneko.",
              "maleVariant": "",
              "stringId": "2037556515953991680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to jest bakeneko?",
              "maleVariant": "",
              "stringId": "2037557230009081856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to jest bakeneko?",
              "maleVariant": "",
              "stringId": "2037560996309524480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierwsze wymarły ptaki, potem psy, koty utrzymały się dosyć długo.",
              "maleVariant": "",
              "stringId": "2037591566460674048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wierzysz w duchy?",
              "maleVariant": "",
              "stringId": "2037592256893444096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To tylko zwykły kot.",
              "maleVariant": "",
              "stringId": "2037592348111167488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kot duch? Może, kto wie.",
              "maleVariant": "",
              "stringId": "2037592462934433792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kot-duch. Przynosi nieszczęście, potrafi przywracać do życia zmarłych.",
              "maleVariant": "",
              "stringId": "2037592901155315712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skoro dzięki Relicowi można rozmawiać ze zmarłymi – jak tu nie wierzyć?",
              "maleVariant": "",
              "stringId": "2037593750485098496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może.",
              "maleVariant": "",
              "stringId": "2037597760625598464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moja babka znała dziesiątki historii o kitsune, kappa i właśnie o bakeneko.",
              "maleVariant": "",
              "stringId": "2037598986318667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tęsknisz za domem?",
              "maleVariant": "",
              "stringId": "2037662730830766084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie się wychowywałeś?",
              "maleVariant": "",
              "stringId": "2037662935026262016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tęsknisz za domem?",
              "maleVariant": "",
              "stringId": "2037663511004864512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie się wychowywałeś?",
              "maleVariant": "",
              "stringId": "2037663612238585856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Głupie pytanie.",
              "maleVariant": "",
              "stringId": "2037664110236688384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pochodzę ze slumsów Chiba-11. Kiedyś bardzo chciałem się stamtąd wyrwać.",
              "maleVariant": "",
              "stringId": "2037667150620549120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cóż, czas wymazuje złe wspomnienia. Zostaje tylko tęsknota za prostotą dzieciństwa.",
              "maleVariant": "",
              "stringId": "2037668020301094912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo...",
              "maleVariant": "",
              "stringId": "2037674193964261380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak wyglądało dorastanie w Chiba-11?",
              "maleVariant": "",
              "stringId": "2037676335475220480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo...",
              "maleVariant": "",
              "stringId": "2037678352398573568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo...",
              "maleVariant": "",
              "stringId": "2037678438029484032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak dorastało się w Chiba-11?",
              "maleVariant": "",
              "stringId": "2037678479905415168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo. Noc, która miesza się z dniem w światłach miasta.",
              "maleVariant": "",
              "stringId": "2037678690996346880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo. Pierwsza przejażdżka motocyklem po wzgórzach.",
              "maleVariant": "",
              "stringId": "2037678826002604032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam swoje dzieciństwo. Skoki między dachami budynków.",
              "maleVariant": "",
              "stringId": "2037678927236325376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bajki na dobranoc opowiadane przez neony Charter Hill.",
              "maleVariant": "",
              "stringId": "2037681769363824640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pamiętam zapach mułu z kanału. Chodziliśmy tam z chłopakami prać koszule.",
              "maleVariant": "",
              "stringId": "2037682901708464128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierwszy pocałunek wśród łodyg neokukurydzy.",
              "maleVariant": "",
              "stringId": "2037684037609873408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żelazisty smak krwi z rozbitych warg.",
              "maleVariant": "",
              "stringId": "2037712798942257152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czasami po slumsach jeździły korporacyjne transportery. Arasaka wybierała dzieciaki – mówiło się, że tylko spośród tych czystych.",
              "maleVariant": "",
              "stringId": "2037716317610991616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do czego was wybierali?",
              "maleVariant": "",
              "stringId": "2037718816208216068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Werbowali do armii?",
              "maleVariant": "",
              "stringId": "2037722349389242368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musieli jeździć wojskowymi transporterami?",
              "maleVariant": "",
              "stringId": "2037724143595053056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do czego was wybierali?",
              "maleVariant": "",
              "stringId": "2037728344375058432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Werbowali was do korporacyjnej armii?",
              "maleVariant": "",
              "stringId": "2037728462352441344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Transportery, mówisz.",
              "maleVariant": "",
              "stringId": "2037728563552608256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do korporacyjnej armii.",
              "maleVariant": "",
              "stringId": "2037729001236619264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak.",
              "maleVariant": "",
              "stringId": "2037732564968955904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Opancerzone. Chiba-11 miała najwyższe statystyki zabójstw w Japonii. Pewnie nadal ma.",
              "maleVariant": "",
              "stringId": "2037734752181051392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdy mnie wskazali, wygrałem los na loterii.",
              "maleVariant": "",
              "stringId": "2037738915312984064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W wojsku dostałem wszystko, czego mi brakowało: dyscyplinę, regularne posiłki, a gdy okazało się, że jestem zdolny, również dobrą edukację.",
              "maleVariant": "",
              "stringId": "2037739664583450624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musiałeś tylko zabijać wrogów Arasaki.",
              "maleVariant": "",
              "stringId": "2037752071116578820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdy sposób jest dobry, żeby wyjść z biedy.",
              "maleVariant": "",
              "stringId": "2037752510344093696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedyne, co musiałeś robić w zamian, to likwidować wrogów Saburo.",
              "maleVariant": "",
              "stringId": "2037760146359738368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Znalazłeś dobry sposób na wyrwanie się z biedy.",
              "maleVariant": "",
              "stringId": "2037760284654329856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty brudzisz się dla pieniędzy, ja w imię zasad.",
              "maleVariant": "",
              "stringId": "2038667239627448320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozejrzyj się. Zobacz, jak wygląda rzeczywistość, której bronisz.",
              "maleVariant": "",
              "stringId": "2038761262937563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Daj spokój, nie każdy chce zmieniać świat.",
              "maleVariant": "",
              "stringId": "2038850407584260096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niestety.",
              "maleVariant": "",
              "stringId": "2038850835201941504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wszystko da się naprawić od razu.",
              "maleVariant": "",
              "stringId": "2038902374692052992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozumiem ich. Wielu rzeczy żałuję, ale nie tego, że okradłam Arasakę.",
              "maleVariant": "Rozumiem ich. Wielu rzeczy żałuję, ale nie tego, że okradłem Arasakę.",
              "stringId": "2038909518833278976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co?",
              "maleVariant": "",
              "stringId": "2038955482348544000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skurwysyny.",
              "maleVariant": "",
              "stringId": "2039085712031637504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jak zostałeś ochroniarzem Saburo?",
              "maleVariant": "",
              "stringId": "2039190069821595648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A w jaki sposób korporacyjny trep zostaje zaufanym człowiekiem Saburo Arasaki?",
              "maleVariant": "",
              "stringId": "2039190468012040192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miałem najwyższe noty w akademii, potem trafiłem do odziałów specjalnych, ale o wszystkim przesądził talent Arasaka-samy.",
              "maleVariant": "",
              "stringId": "2039206057887686656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mów, że wpadłeś w oko samemu Saburo.",
              "maleVariant": "",
              "stringId": "2039207152282263552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Najpotężniejszym człowiekiem na świecie zostaje się dokonując słusznych wyborów przy niepełnej wiedzy.",
              "maleVariant": "",
              "stringId": "2039207538024013824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama potrafił zajrzeć w ludzką duszę i spośród setki stojących na baczność kandydatów wskazać palcem tego, który będzie mu służył najlepiej.",
              "maleVariant": "",
              "stringId": "2039213022982463488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha, bo złodziejstwo jest szlachetniejszym zajęciem.",
              "maleVariant": "",
              "stringId": "2039228135160438784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przynajmniej nie pomagam w drenowaniu świata i ludzi ze wszystkiego co cenne.",
              "maleVariant": "",
              "stringId": "2039230187064287232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak to mówicie: \"nie nauczysz starego psa nowych sztuczek\".",
              "maleVariant": "",
              "stringId": "2043488478191706112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze, nie przeciągajmy tej dyskusji.",
              "maleVariant": "",
              "stringId": "2043489346311970816"
            }
          ]
        }
      }
    },
