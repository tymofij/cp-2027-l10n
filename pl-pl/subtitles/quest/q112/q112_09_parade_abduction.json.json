    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"危険だと警告されたの。オダとも連絡が取れないし、何か変だわ\" t=\"Dostałam komunikat o zagrożeniu. Oda nie odpowiada. Coś jest nie tak.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669447913484337152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"私が参加したのは、あくまでもお父様を追悼するため\" t=\"Zgodziłam się na to ze względu na pamięć o ojcu.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669449579881316352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"操り人形になるためじゃないわ\" t=\"Ale teraz zaczynam się czuć jak twoje narzędzie.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669450106887225344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"パレードを中止すれば、弱さを露呈したと思われる\" t=\"Jeśli nie dokończymy, zostanie to odebrane jako oznaka słabości.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714211719007453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"弱さ？ 誰の？ あなたの？\" t=\"Słabości?! Czyjej? Twojej?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212267806965760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"だが、お前が言うなら中止してもいい。全部やめだ。お前の安全が第一だからな\" t=\"Ale rozumiem. Powiedz jedno słowo, a kończymy z tym. Twoje bezpieczeństwo jest najważniejsze.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212716832374784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お父様…\" t=\"Ze względu na ojca.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714220354475409408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お許しを。どうか、話だけでもお聞きください\" t=\"Proszę o wybaczenie, Hanako-sama. I proszę, abyś mnie wysłuchała.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714224100794187776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様は…\" t=\"Twój ojciec...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225132743966720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"父のことを口にしないで！\" t=\"Jak śmiesz o nim mówić?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225355629281280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、無礼を働きたくありません。しかし…\" t=\"Błagam. Nie każ mi tego robić, Hanako-sama…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225488303505408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uciekaj, V!",
              "maleVariant": "",
              "stringId": "1714227320375492608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa! Kurwa! Kurwa!",
              "maleVariant": "",
              "stringId": "1714228292917149696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"兄さん…\" t=\"Yorinobu?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724068500475875328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"やり終えましょう\" t=\"Kontynuujemy.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724113942068727808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありがとう、ハナコ。せっかくの式典だ――やり終えなければ、悔いが残る\" t=\"Dziękuję, Hanako. Szkoda byłoby urwać tak dobre przemówienie.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724116336999534592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"Ech...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724118030575603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様\" t=\"Hanako-sama.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724119896722116608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"あなたは…\" t=\"Ty...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724121657927143424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"これが、俺にできる最後のご奉仕なのです\" t=\"To ostatnie, co mogę dla niego zrobić.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724149966509576192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様の死について真実を明かすことが\" t=\"Wyjawić ci prawdę o jego śmierci.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724150642950148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"この見せ物では、みんなに役割がある。ミチコは文句を言ってないぞ\" t=\"Wszyscy mamy do odegrania rolę w tym spektaklu. Michiko nie ma z tym problemu.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724223690596290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"なぜ止まったんだ？\" t=\"Dlaczego przerwałaś?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724231251970101248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"俺たちだ\" t=\"Naszej.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724233151520051200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cokolwiek zrobił, teraz dopiero mamy przejebane!",
              "maleVariant": "",
              "stringId": "1771027611004047360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako jest w środku.",
              "maleVariant": "",
              "stringId": "1773724698127908864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba chce do kogoś zadzwonić.",
              "maleVariant": "",
              "stringId": "1773725998731247616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spróbuj podsłuchać.",
              "maleVariant": "",
              "stringId": "1773726175479218176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko nie zapomnij wyłączyć alarmów. Wtedy wchodzę.",
              "maleVariant": "",
              "stringId": "1773726519227596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura? Systemy zablokowane. Teraz albo nigdy.",
              "maleVariant": "",
              "stringId": "1773729105720655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co robi?",
              "maleVariant": "",
              "stringId": "1780695188894519296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pośpiesz się, V!",
              "maleVariant": "",
              "stringId": "1790741196538200068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, zaraz będzie za późno!",
              "maleVariant": "",
              "stringId": "1790741196554977284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie macie prawa nas tutaj trzymać!",
              "maleVariant": "",
              "stringId": "1795474982084558848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy na ziemię!",
              "maleVariant": "",
              "stringId": "1795475678641012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdol się! To naruszanie swobód obywatelskich!",
              "maleVariant": "",
              "stringId": "1795476053259468800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stać!",
              "maleVariant": "",
              "stringId": "1795479668128759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Komuś jeszcze brakuje swobód obywatelskich?! Może iść w jego ślady!",
              "maleVariant": "",
              "stringId": "1795483864295030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Prawie mnie zadeptali... Chyba mam połamane żebra...",
              "maleVariant": "",
              "stringId": "1801295829407907840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musimy się stąd wydostać!",
              "maleVariant": "",
              "stringId": "1801296487729086464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W porządku? Wchodzimy na wizję.",
              "maleVariant": "",
              "stringId": "1801297323049893888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzy, dwa, jeden...",
              "maleVariant": "",
              "stringId": "1801298631119093760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z tej strony Gillean Jordan. Nadajemy na żywo z Japantown, gdzie doszło do incydentu w trakcie parady upamiętniającej Saburo Arasakę.",
              "maleVariant": "",
              "stringId": "1801299000486281216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naoczni świadkowie donoszą o strzelaninie na jednej z odświętnych platform, którą miała przemieszczać się Hanako Arasaka.",
              "maleVariant": "",
              "stringId": "1801299245702070272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W tej chwili nie jestem w stanie podać więcej szczegółów.",
              "maleVariant": "",
              "stringId": "1801300495923429376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wciąż czekamy na oficjalny komunikat korporacji w tej sprawie.",
              "maleVariant": "",
              "stringId": "1801300622893400064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ej! Nie pchaj się w kadr!",
              "maleVariant": "",
              "stringId": "1801303594255794176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pieprzyć to, robimy powtórkę!",
              "maleVariant": "",
              "stringId": "1801304953780715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki! Niektórzy tutaj pracują!",
              "maleVariant": "",
              "stringId": "1801305575091355648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sytuacja wydaje się zaogniać. Jeśli możecie, omijajcie Kanzaki Street.",
              "maleVariant": "",
              "stringId": "1863315822830493696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedziałem, że nie można mu ufać! Miałaś go przecież trzymać na krótkiej smyczy!",
              "maleVariant": "Wiedziałem, że nie można mu ufać! Miałeś go przecież trzymać na krótkiej smyczy!",
              "stringId": "1906936140640829440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lepiej stąd spierdalajmy! I to szybko.",
              "maleVariant": "",
              "stringId": "1906937283303456768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zastrzelił ją?",
              "maleVariant": "",
              "stringId": "2007084788928552960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wyłącz zabezpieczenia]",
              "maleVariant": "",
              "stringId": "2034753975066525696"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}