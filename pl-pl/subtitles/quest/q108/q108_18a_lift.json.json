        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz musimy znaleźć rdzeń.",
              "maleVariant": "",
              "stringId": "1200972978767962128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może się tak pechowo złożyć, że ktoś zobaczy nagrania, na których włamujemy się do Arasaka Tower.",
              "maleVariant": "",
              "stringId": "1653783024840806400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będziemy się tym martwić, jak stąd wyjdziemy.",
              "maleVariant": "",
              "stringId": "1653783874271252480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. A co?",
              "maleVariant": "",
              "stringId": "1731559286131195908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie podoba mi się. Media przynoszą pecha.",
              "maleVariant": "",
              "stringId": "1731559286131195928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze znasz tego Thompsona?",
              "maleVariant": "",
              "stringId": "1731578913007665152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Thompson pokazuje nagranie Johnny'emu, słychać wrzaski tłumu przed Arasaka Tower]",
              "maleVariant": "",
              "stringId": "1731592805515591680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nieźle.",
              "maleVariant": "",
              "stringId": "1731593707274174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cały czas nagrywasz?",
              "maleVariant": "",
              "stringId": "1731599679812075540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. A co? Chcesz powiedzieć parę słów do kamery?",
              "maleVariant": "",
              "stringId": "1731599679828852740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha. Rób swoje i nie wchodź mi w drogę.",
              "maleVariant": "",
              "stringId": "1731599679828852756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cały czas nagrywasz?",
              "maleVariant": "",
              "stringId": "1731601536194248708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, a co?",
              "maleVariant": "",
              "stringId": "1733108619160129536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co?",
              "maleVariant": "",
              "stringId": "1733133284570107904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dokładna liczba rannych w zamieszkach nie jest znana.",
              "maleVariant": "",
              "stringId": "2237425638401720328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pod Arasaka Tower są nasi reporterzy...",
              "maleVariant": "",
              "stringId": "2237425638401720344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...którzy są zgodni w jednym: tłum został odurzony substancjami psychoaktywnymi.",
              "maleVariant": "",
              "stringId": "2237425638401720360"
            }
          ]
        }
      }
    },
