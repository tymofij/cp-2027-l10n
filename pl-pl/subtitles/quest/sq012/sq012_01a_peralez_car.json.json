    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1692517901624004608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zapraszam do samochodu.",
              "maleVariant": "",
              "stringId": "1692563268675686400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Witaj, V. Jefferson Peralez.",
              "maleVariant": "",
              "stringId": "1692563738639060992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozmawiałaś już z moją żoną, Elizabeth.",
              "maleVariant": "Rozmawiałeś już z moją żoną, Elizabeth.",
              "stringId": "1692564911735558144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[weź drzazgę] No dobrze. Pokażcie to nagranie.",
              "maleVariant": "",
              "stringId": "1692576723600265220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobrze. Zobaczę, co się da zrobić.",
              "maleVariant": "No dobrze. Zobaczę, co da się zrobić.",
              "stringId": "1692588839669452800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doskonale. Będziemy ci wdzięczni za cokolwiek, co pozwoli jakoś rozjaśnić tę sytuację.",
              "maleVariant": "",
              "stringId": "1692596960932515840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Braindance jeszcze dziś musi wrócić na miejsce, inaczej sporo ludzi będzie miało kłopoty.",
              "maleVariant": "",
              "stringId": "1692618948061286400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wsiądź] Dzięki.",
              "maleVariant": "",
              "stringId": "1895403992388427780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki.",
              "maleVariant": "",
              "stringId": "1895405202512240640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miło mi państwa poznać. Panie Peralez.",
              "maleVariant": "",
              "stringId": "1895430810952753152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak pewnie wiesz, niedawno zmarł burmistrz Lucius Rhyne.",
              "maleVariant": "",
              "stringId": "1895430840296103936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak zwykle miałaś rację, Liz.",
              "maleVariant": "",
              "stringId": "1895437736436523008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moja żona doskonale zna się na ludziach. Powiedziała, że na pewno się dogadamy.",
              "maleVariant": "",
              "stringId": "1895438108639059968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz wiem dlaczego.",
              "maleVariant": "",
              "stringId": "1895439288094121984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na czym polega zlecenie?",
              "maleVariant": "",
              "stringId": "1895445033468518404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miło mi.",
              "maleVariant": "",
              "stringId": "1895451939306168320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Od razu do rzeczy, co?",
              "maleVariant": "",
              "stringId": "1895456546866552832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możemy najpierw pogawędzić.",
              "maleVariant": "",
              "stringId": "1898073547883634692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A zatem – na czym będzie polegało zlecenie?",
              "maleVariant": "",
              "stringId": "1898073604221526016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasz czas jest tak samo cenny jak mój.",
              "maleVariant": "",
              "stringId": "1898092288285564928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możemy najpierw pogawędzić o pogodzie albo o tym, gdzie w mieście dają najlepszy ramen.",
              "maleVariant": "",
              "stringId": "1898097348176470016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podejrzewam, że wasz czas jest nie mniej cenny niż mój.",
              "maleVariant": "",
              "stringId": "1898097373342294016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jestem otwarta.",
              "maleVariant": "Jestem otwarty.",
              "stringId": "1898100265231015936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobrze. Do rzeczy zatem.",
              "maleVariant": "",
              "stringId": "1898100707042222080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pani Peralez.",
              "maleVariant": "",
              "stringId": "1898107700003954688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Elizabeth?",
              "maleVariant": "",
              "stringId": "1898113524667473920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Racja.",
              "maleVariant": "",
              "stringId": "1898124911363973120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z tego co wiem, dochodzenie w tej sprawie jest już zamknięte.",
              "maleVariant": "",
              "stringId": "1898125643521679360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta sprawa jest już zamknięta.",
              "maleVariant": "",
              "stringId": "1898125681253638148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było o tym głośno. Media coś pominęły?",
              "maleVariant": "",
              "stringId": "1898167619713458176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Parę dni wcześniej doszło do próby zamachu.",
              "maleVariant": "",
              "stringId": "1898168678120906752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z tego co pamiętam, parę dni przed śmiercią Rhyne'a doszło do próby zamachu na niego...",
              "maleVariant": "",
              "stringId": "1898176246557925376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O tej sprawie było bardzo głośno.",
              "maleVariant": "",
              "stringId": "1898176268049539072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Owszem. Policja bardzo szybko dowiodła, że śmierć nastąpiła z przyczyn naturalnych. Rhyne rzekomo umarł w domu, w wyniku awarii kardiowszczepu.",
              "maleVariant": "",
              "stringId": "1898177668426981376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Według NCPD to był z niczym niezwiązany, przypadkowy atak cyberszaleńca.",
              "maleVariant": "",
              "stringId": "1898293739632943104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naturalnie. Media karmiły się tym wydarzeniem przez wiele tygodni.",
              "maleVariant": "",
              "stringId": "1898328188441550848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A śmierć burmistrza nastąpiła z przyczyn naturalnych...?",
              "maleVariant": "",
              "stringId": "1898330350521376768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego podejrzewacie, że policja się pomyliła? Macie jakiekolwiek poszlaki?",
              "maleVariant": "",
              "stringId": "1898331076521844736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I nie miała nic wspólnego z tym atakiem. Dokładnie tak mówią oficjalne raporty. Rhyne jakoby umarł w domu w wyniku awarii kardiowszczepu.",
              "maleVariant": "",
              "stringId": "1898343528772886528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcemy się dowiedzieć, jak do tego doszło.",
              "maleVariant": "",
              "stringId": "1898354419434217472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie. Sądzicie, że coś pominęli?",
              "maleVariant": "",
              "stringId": "1898368044714647552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy powody przypuszczać, że tak.",
              "maleVariant": "",
              "stringId": "1898368395073249280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale nas zastanawia atak cyberszaleńca, jaki miał miejsce w ratuszu parę dni przed śmiercią Rhyne'a.",
              "maleVariant": "",
              "stringId": "1898371169018142720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd podejrzenia, że policja się myli?",
              "maleVariant": "",
              "stringId": "1898372570049896452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Według policji – z niczym niezwiązany incydent.",
              "maleVariant": "",
              "stringId": "1898374735669714944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jaki to ma związek ze zbliżającymi się wyborami?",
              "maleVariant": "",
              "stringId": "1898411669653774336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Peralez? Kandydat na burmistrza?",
              "maleVariant": "",
              "stringId": "1924389261430972416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Peralez? Kandydat na burmistrza Night City?",
              "maleVariant": "",
              "stringId": "1924390084974174208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Ten sam.",
              "maleVariant": "",
              "stringId": "1924394652655116288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Więc również ten sam, który do niedawna był prokuratorem generalnym, a teraz zasiada w radzie miasta.",
              "maleVariant": "",
              "stringId": "1924401832968019968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz pewnie rozumiesz, dlaczego zależało nam na dyskrecji.",
              "maleVariant": "",
              "stringId": "1924401942288359424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Te wasze podejrzenia... mają może jakiś związek z nadchodzącymi wyborami na burmistrza?",
              "maleVariant": "",
              "stringId": "1924436682752090112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście. Jeśli Lucius Rhyne został zamordowany, musimy to wiedzieć.",
              "maleVariant": "",
              "stringId": "1924453433560752128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tuż przed śmiercią Rhyne zarządził cięcia budżetowe dla departamentu policji. Być może to się komuś nie spodobało.",
              "maleVariant": "",
              "stringId": "1924454857711509504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Udało nam się dotrzeć do nagrania, na którym widać moment zamachu.",
              "maleVariant": "",
              "stringId": "1924463438435409920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nasz szef ochrony już je oglądał i nie znalazł niczego podejrzanego... Dlatego zależało nam na kimś, kto potrafi edytować braindansy. To surowe nagranie.",
              "maleVariant": "",
              "stringId": "1924464487044964352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeżeli zgodzisz się nam pomóc, możesz je zaraz obejrzeć.",
              "maleVariant": "",
              "stringId": "1924465207374733312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd je macie?",
              "maleVariant": "",
              "stringId": "1924492786148270080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd pochodzi to nagranie?",
              "maleVariant": "",
              "stringId": "1924494741398577152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rhyne miał wtedy publicznie przemawiać. Braindance z tego typu wydarzeń to standardowa procedura bezpieczeństwa.",
              "maleVariant": "",
              "stringId": "1924506122088833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po ataku nagranie trafiło do NCPD jako materiał dowodowy.",
              "maleVariant": "",
              "stringId": "1924508021890441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musieliśmy pociągnąć za sporo sznurków, żeby je wydobyć.",
              "maleVariant": "",
              "stringId": "1924508423469883392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to obejrzę to od razu.",
              "maleVariant": "",
              "stringId": "1947712436352409600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Carter, jedziemy.",
              "maleVariant": "",
              "stringId": "1957819339482095616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź i zaczekaj]",
              "maleVariant": "",
              "stringId": "2065155347913728004"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}