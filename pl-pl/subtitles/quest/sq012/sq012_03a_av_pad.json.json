      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co sądzisz?",
              "maleVariant": "",
              "stringId": "1698275538001055744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kim jest policjant, który uratował Rhyne'a?",
              "maleVariant": "",
              "stringId": "1928826001470218244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to jest Red Queen's Race?",
              "maleVariant": "",
              "stringId": "1928826839810924544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Red Queen's Race? Co to za miejsce?",
              "maleVariant": "",
              "stringId": "1928830979001769988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nigdy o nim nie słyszałem. Przykro mi.",
              "maleVariant": "",
              "stringId": "1928830979068878848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A czy ktoś jeszcze, poza ochroniarzami Rhyne'a, ma zdalny dostęp do terminalu?",
              "maleVariant": "",
              "stringId": "1928830979974848512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mmm... Policja... I nikt więcej.",
              "maleVariant": "",
              "stringId": "1928830980041957376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Policjant, który uratował Rhyne'a. Wiadomo coś o nim?",
              "maleVariant": "",
              "stringId": "1928830980847263748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Detektyw River Ward. Dobry gliniarz, pracowałem z nim przy kilku sprawach.",
              "maleVariant": "",
              "stringId": "1928830980914372608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kim jest Holt?",
              "maleVariant": "",
              "stringId": "1928830981048590340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zastępca burmistrza. I jego bulterier. Teraz startuje w wyborach.",
              "maleVariant": "",
              "stringId": "1928830981098921984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ochroniarze burmistrza też są z policji?",
              "maleVariant": "",
              "stringId": "1928830981266694144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. Wszyscy są wybrani przez Holta i zatrudnieni przez korporację.",
              "maleVariant": "",
              "stringId": "1928830981317025792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Choć niektórzy z nich to eks-gliny.",
              "maleVariant": "",
              "stringId": "1928830981367357440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czego miała dotyczyć ta konferencja?",
              "maleVariant": "",
              "stringId": "1928830981501575172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niczego sensownego. Rhyne uwielbiał dźwięk własnego głosu i lubił się oglądać w telewizji.",
              "maleVariant": "",
              "stringId": "1928830981551906816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z tego co pamiętam, tutaj akurat pretekstem było odnotowanie spadku przestępczości w Night City.",
              "maleVariant": "",
              "stringId": "1928830981619015680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spadku?",
              "maleVariant": "",
              "stringId": "1928830981669347328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rhyne ogłosił Pacificę \"niezależnym terytorium\". Tym samym zbrodnie tam popełniane przestały iść na rachunek miasta.",
              "maleVariant": "",
              "stringId": "1928830981736456192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Sprytnie.",
              "maleVariant": "",
              "stringId": "1928830981770010624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zadzwonię do Warda.",
              "maleVariant": "",
              "stringId": "1928845965333848068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ochrona burmistrza to też policjanci?",
              "maleVariant": "",
              "stringId": "1928847199566520320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kim jest Holt?",
              "maleVariant": "",
              "stringId": "1928851463865065472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kto ma dostęp do terminala ochrony?",
              "maleVariant": "",
              "stringId": "1928855228236128256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czego dotyczyła konferencja?",
              "maleVariant": "",
              "stringId": "1928860810435321856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli chcesz z nim porozmawiać, mogę to załatwić.",
              "maleVariant": "",
              "stringId": "1929998123244675072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zjawił się we właściwym momencie.",
              "maleVariant": "",
              "stringId": "1930017986277236736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo miał dużo szczęścia, albo dobre informacje. A ja będę potrzebowała i tego, i tego.",
              "maleVariant": "Albo miał dużo szczęścia, albo dobre informacje. A ja będę potrzebował i tego, i tego.",
              "stringId": "1930019268828291072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ładnie powiedziane.",
              "maleVariant": "",
              "stringId": "1930020090106568704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdybyś jeszcze czegoś potrzebowała, daj znać.",
              "maleVariant": "Gdybyś jeszcze czegoś potrzebował, daj znać.",
              "stringId": "1930020546883051520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przesyłam ci kontakt do niego.",
              "maleVariant": "",
              "stringId": "1930026374012821504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oddaj drzazgę] Za wcześnie, żeby oceniać.",
              "maleVariant": "",
              "stringId": "1930027661748035584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oddaj drzazgę] Myślę, że coś jest na rzeczy.",
              "maleVariant": "",
              "stringId": "1930028216771895296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcę wyciągać pochopnych wniosków. Jeszcze za wcześnie, żeby powiedzieć cokolwiek wiążącego.",
              "maleVariant": "",
              "stringId": "1930029164164505600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cała ta sytuacja wydaje się mocno podejrzana.",
              "maleVariant": "",
              "stringId": "1930029211140710400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz jakieś pytania?",
              "maleVariant": "",
              "stringId": "1930029290849263616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oddaj drzazgę] To wygląda na zbieg okoliczności.",
              "maleVariant": "",
              "stringId": "1930050031296667648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak mawia mój znajomy – widziałam już dziwniejsze zbiegi okoliczności.",
              "maleVariant": "Jak mawia mój znajomy – widziałem już dziwniejsze zbiegi okoliczności.",
              "stringId": "1930057031455825920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteśmy paranoikami, czy sytuacja jednak jest podejrzana?",
              "maleVariant": "",
              "stringId": "1930057708500377600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słusznie.",
              "maleVariant": "",
              "stringId": "1930063033068642304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ha! Więc jednak.",
              "maleVariant": "",
              "stringId": "1930063841780785152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm. Ciekawe. Nasze odczucia były kompletnie odwrotne.",
              "maleVariant": "",
              "stringId": "1930068211406692352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma co bazować na pierwszym wrażeniu.",
              "maleVariant": "",
              "stringId": "1930070246197780480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wysiądź]",
              "maleVariant": "",
              "stringId": "2065249417394741252"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}