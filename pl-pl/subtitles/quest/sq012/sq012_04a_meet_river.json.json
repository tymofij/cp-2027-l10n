  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River Ward?",
              "maleVariant": "",
              "stringId": "1700890536825180160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Detektyw Ward?",
              "maleVariant": "",
              "stringId": "1700891001688281092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hej. Chyba ktoś do ciebie.",
              "maleVariant": "",
              "stringId": "1700982118597382144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To ja. Jesteś V, prawda?",
              "maleVariant": "",
              "stringId": "1700983760499625984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój partner, detektyw Han.",
              "maleVariant": "",
              "stringId": "1700983916276076544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Red Queen's Race. Słyszałeś coś na ten temat? Wiesz, co to za miejsce?",
              "maleVariant": "",
              "stringId": "1701115711139205120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może i wiem. To ma jakiś związek z Rhyne'em?",
              "maleVariant": "",
              "stringId": "1701116952049209344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wybierał się tam w dniu śmierci.",
              "maleVariant": "",
              "stringId": "1701118020220997632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, spotkajmy się z moim informatorem.",
              "maleVariant": "",
              "stringId": "1701118796418895872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obiło mi się o uszy, że Red Queen's Race to elitarny klub, ale nawet nie wiem, jak tam trafić.",
              "maleVariant": "",
              "stringId": "1745803853716639744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedźmy do pracodawczyni.",
              "maleVariant": "",
              "stringId": "1745805693204156420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedźmy do informatora.",
              "maleVariant": "",
              "stringId": "1745806603166797824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedźmy do pracodawczyni. Też mi się wydaje, że gościa, którego stać na tyle chromu, ile on na sobie nosił, stać też na leki antypsychotyczne.",
              "maleVariant": "",
              "stringId": "1745806924517593088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedźmy do tego twojego informatora. Klub to pewniejszy trop.",
              "maleVariant": "",
              "stringId": "1745807684760358912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej.",
              "maleVariant": "",
              "stringId": "1756154294396637184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, dobra.",
              "maleVariant": "",
              "stringId": "1806954913600229376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki, że zgodziłeś się na spotkanie.",
              "maleVariant": "",
              "stringId": "1933016127373008900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze nie wiem, co zamierzam. Na razie wysłucham, co ma nowego do powiedzenia o śmierci Rhyne'a.",
              "maleVariant": "",
              "stringId": "1933028122226810916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty znowu o tym? Szef ci raz powiedział, że masz to zostawić. Lepiej go posłuchaj. On nie lubi powtarzać rozkazów.",
              "maleVariant": "",
              "stringId": "1933028122226810928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak nie chcesz słuchać jego, to posłuchaj mnie. Ja ci dobrze życzę. Po prostu odpuść tę sprawę.",
              "maleVariant": "",
              "stringId": "1933028122243588100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przecież nic nie robię. Siedzę, piję kawę. Słucham plotek.",
              "maleVariant": "",
              "stringId": "1933028122243588112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra. Rób, co chcesz. Ja w tym nie będę brał udziału. Zresztą i tak śpieszę się do córki.",
              "maleVariant": "",
              "stringId": "1933028122243588124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A wy... przynajmniej znajdźcie sobie przytulniejszy kącik na takie rozmowy.",
              "maleVariant": "A wy... przynajmniej znajdźcie sobie mniej eksponowane miejsce na takie rozmowy.",
              "stringId": "1933028122243588136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, to słucham.",
              "maleVariant": "",
              "stringId": "1933028122260365312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki, że zgodziłeś się ze mną spotkać.",
              "maleVariant": "",
              "stringId": "1933030116433813504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naprawdę zamierzasz pracować z najemniczką?",
              "maleVariant": "Naprawdę zamierzasz pracować z najemnikiem?",
              "stringId": "1933040774646816768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przed atakiem na Rhyne'a chciałeś przekazać jakąś wiadomość ludziom burmistrza.",
              "maleVariant": "",
              "stringId": "1933076392877711360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wynajęła mnie Elizabeth Peralez. I jej mąż, Jefferson.",
              "maleVariant": "",
              "stringId": "1933076428931948544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Twój partner nie przepada za edgerunnerami.",
              "maleVariant": "",
              "stringId": "1933081205556338688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Detektyw Han nie przepada za edgerunnerami.",
              "maleVariant": "",
              "stringId": "1933081855757344768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie bierz tego do siebie. Detektyw Han mało za kim przepada. Poza swoją córką.",
              "maleVariant": "",
              "stringId": "1933105064720424960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to było?",
              "maleVariant": "",
              "stringId": "1933112870672490496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chłopaki z mojego posterunku zgarnęli Horvátha, jak się pętał po Arroyo i strzelał do śmietników, i coś chrzanił, że idzie na spotkanie z burmistrzem.",
              "maleVariant": "",
              "stringId": "1933130031331762176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Najpierw moje pytanie. Dlaczego węszysz wokół tej sprawy? Kto cię wynajął?",
              "maleVariant": "",
              "stringId": "1933130075220959232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pétera Horvátha. Psychola, który potem zaatakował burmistrza. Przytrzymali go, ale gdzieś się \"zapodział\" jeszcze przed złożeniem zeznań.",
              "maleVariant": "",
              "stringId": "1933175609424044032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Więc postanowiłeś ostrzec burmistrza?",
              "maleVariant": "",
              "stringId": "1933181626421567488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to znaczy, że się zapodział?",
              "maleVariant": "",
              "stringId": "1933182033587822592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nigdy nie zdradzam nazwisk zleceniodawców.",
              "maleVariant": "",
              "stringId": "1933187025396785152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedziałeś, że się \"zapodział\"? Co to znaczy?",
              "maleVariant": "",
              "stringId": "1933187066484187136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Więc postanowiłeś ostrzec burmistrza przed niebezpieczeństwem?",
              "maleVariant": "",
              "stringId": "1933187169647288320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak po prostu, z dobrej woli?",
              "maleVariant": "",
              "stringId": "1934314850573049856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mówisz to tak, jakby dobra wola była czymś wstydliwym.",
              "maleVariant": "",
              "stringId": "1934315107281231872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wstydliwym – nie. Ale na pewno niecodziennym.",
              "maleVariant": "",
              "stringId": "1934316634897080320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To znaczy, że najwyraźniej komuś zależało, żeby akurat tego dnia nie liczył karaluchów w celi.",
              "maleVariant": "",
              "stringId": "1934317625608462336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I uprzedzając twoje następne pytanie – nie wiem komu.",
              "maleVariant": "",
              "stringId": "1934325079020888064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kogo zgarnęli?",
              "maleVariant": "",
              "stringId": "1934388765618036736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moja kolej. Dlaczego Peralez się za to zabrał akurat teraz? I dlaczego nieoficjalnie?",
              "maleVariant": "",
              "stringId": "1934441210188619776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, to pewnie masz jeszcze sporo tropów poza mną. Mam nadzieję, że tak jest. Bo ode mnie się niczego nie dowiesz.",
              "maleVariant": "",
              "stringId": "1934460958263697408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I nie chodzi wcale o złośliwości czy wścibstwo. Chodzi o moje własne bezpieczeństwo.",
              "maleVariant": "",
              "stringId": "1934469901190914048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pracuję dla Peralezów.",
              "maleVariant": "",
              "stringId": "1934480952829632516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcę zdradzić nazwiska zleceniodawcy.",
              "maleVariant": "",
              "stringId": "1934482865432256512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zanim otworzę usta, muszę wiedzieć, do czyjego ucha trafią moje słowa.",
              "maleVariant": "",
              "stringId": "1934493319701422080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyszły burmistrz? Ha. W sumie mogłem się tego spodziewać...",
              "maleVariant": "",
              "stringId": "1934494484358979584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz co... Han miał rację. Tutaj jest za dobra akustyka.",
              "maleVariant": "",
              "stringId": "1934502426844028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chodźmy do mojego auta.",
              "maleVariant": "",
              "stringId": "1934503676427853824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Okej.",
              "maleVariant": "",
              "stringId": "1934504246668648448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, jak chcesz.",
              "maleVariant": "",
              "stringId": "1934505629161578496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, co to jest Red Queen's Race?",
              "maleVariant": "",
              "stringId": "1934615348849106944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślę, że próbuje zdyskredytować Holta.",
              "maleVariant": "",
              "stringId": "1934617133357686784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moim zdaniem próbuje zdyskredytować Holta.",
              "maleVariant": "",
              "stringId": "1934667874822742016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może się boi.",
              "maleVariant": "",
              "stringId": "1934669997157343232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może po prostu się boi.",
              "maleVariant": "",
              "stringId": "1934670431485911040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podejrzewa go?",
              "maleVariant": "",
              "stringId": "1934673191354720256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem. Ale nawet jeśli nie, to morderstwo burmistrza i tak narobi smrodu wokół jego zastępcy.",
              "maleVariant": "",
              "stringId": "1934673357449158656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chce siadać na stołku, z którego przed chwilą się ktoś osunął martwy?",
              "maleVariant": "",
              "stringId": "1934675882218840064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Też bym nie chciała.",
              "maleVariant": "Też bym nie chciał.",
              "stringId": "1934676423586045952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A potem pojedziemy porozmawiać z kobietą, u której pracował Horváth.",
              "maleVariant": "",
              "stringId": "1934694753197842432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo, właściwie... Możemy do niej pojechać najpierw. No jak wolisz.",
              "maleVariant": "",
              "stringId": "1934696305140654080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My?",
              "maleVariant": "",
              "stringId": "1934699234828443648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Powiedziałeś \"my\"?",
              "maleVariant": "",
              "stringId": "1934699516199133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta sprawa mnie dręczy. Podejrzana jest choćby sama prędkość, z jaką ją zamknięto.",
              "maleVariant": "",
              "stringId": "1934703094309543936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ty wywiążesz się ze zlecenia, a ja znajdę dowody, które przekonają mojego szefa, że śledztwo trzeba wznowić.",
              "maleVariant": "",
              "stringId": "1934708604987465728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oboje zyskujemy. Więc – dokąd jedziemy najpierw?",
              "maleVariant": "Obaj zyskujemy. Więc – dokąd jedziemy najpierw?",
              "stringId": "1934711079727489024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzeba to zbadać.",
              "maleVariant": "",
              "stringId": "1944734753104814080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Podobno chcesz rozmawiać?",
              "maleVariant": "",
              "stringId": "1948748049721679876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jakaś cicha się trafiła.",
              "maleVariant": "Hm. Jakiś cichy się trafił.",
              "stringId": "1948748050006892548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No? Śmiało.",
              "maleVariant": "",
              "stringId": "1948748050040446980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Co chciałeś przekazać burmistrzowi?",
              "maleVariant": "",
              "stringId": "1948873165340237824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O co chodzi?",
              "maleVariant": "",
              "stringId": "1957872228313358336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałabym porozmawiać.",
              "maleVariant": "Chciałbym porozmawiać.",
              "stringId": "1957873563561648128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałabym porozmawiać, masz chwilę?",
              "maleVariant": "Chciałbym porozmawiać, masz chwilę?",
              "stringId": "1957874101640519680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie jesteś V.",
              "maleVariant": "",
              "stringId": "1957875741361725440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Detektyw Han, mój partner.",
              "maleVariant": "",
              "stringId": "1957876038519775232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Możemy porozmawiać na osobności?",
              "maleVariant": "",
              "stringId": "1957878086950739968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy możemy pogadać na osobności?",
              "maleVariant": "",
              "stringId": "1957878347165360128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mam tajemnic przed Hanem.",
              "maleVariant": "",
              "stringId": "1957878865698775040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź i zaczekaj]",
              "maleVariant": "",
              "stringId": "2104079250819997700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wsiądź]",
              "maleVariant": "",
              "stringId": "2341373769146523652"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}