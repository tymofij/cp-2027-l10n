      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様がお待ちです\" t=\"Hanako-sama czeka.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1651729622285426688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potrzebujesz powietrza. Teraz.",
              "maleVariant": "",
              "stringId": "1651764434454978560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie oczekuję, że to zrozumiesz. Wystarczy, że wiesz, z jakim trudem przychodzi mi wystąpić przeciw własnemu bratu.",
              "maleVariant": "",
              "stringId": "1680635610711068672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niczego nie próbuj!",
              "maleVariant": "",
              "stringId": "1680638817122258944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Idziemy stąd, kurwa.",
              "maleVariant": "",
              "stringId": "1680645781495615488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze się czujesz?",
              "maleVariant": "",
              "stringId": "1680645781512392704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wynośmy się stąd.",
              "maleVariant": "",
              "stringId": "1680647899753353216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Już mi lepiej.",
              "maleVariant": "",
              "stringId": "1680660288569741316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Krwawisz.",
              "maleVariant": "",
              "stringId": "1680661102700924928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, tak, tylko...",
              "maleVariant": "",
              "stringId": "1680662543645675520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "1680662972773306368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę stąd wyjść.",
              "maleVariant": "",
              "stringId": "1680665149633859584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. To z nią jest źle.",
              "maleVariant": "Tak. To z nim jest źle.",
              "stringId": "1680666391701491712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Już ze mną... lepiej...",
              "maleVariant": "",
              "stringId": "1680667908647030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. Muszę stąd...",
              "maleVariant": "",
              "stringId": "1680668037059842048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wprowadzę cię do Mikoshi.",
              "maleVariant": "",
              "stringId": "1680695280238727168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Robi się interesująco. Mów dalej.",
              "maleVariant": "",
              "stringId": "1680698203433734148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź] Pomożesz mi w zamian za...?",
              "maleVariant": "",
              "stringId": "1680701486130925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz, jak wzbudzić ciekawość.",
              "maleVariant": "",
              "stringId": "1680703821066407936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pomożesz mi w zamian za...?",
              "maleVariant": "",
              "stringId": "1680703918122602496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój brat...",
              "maleVariant": "",
              "stringId": "1680791610030166016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musi ponieść konsekwencje swoich decyzji.",
              "maleVariant": "",
              "stringId": "1680810055589302272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz zamiar pomścić ojca.",
              "maleVariant": "",
              "stringId": "1680812089524752384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Usiądź. Mamy mało czasu.",
              "maleVariant": "",
              "stringId": "1681004401416605708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przejdźmy do rzeczy.",
              "maleVariant": "",
              "stringId": "1681005282337886208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie... Nie wiem.",
              "maleVariant": "",
              "stringId": "1681006055666880524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tamta AV-ka za oknem przelatuje już trzeci raz.",
              "maleVariant": "",
              "stringId": "1681006055683657736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że miałaś ogon?",
              "maleVariant": "",
              "stringId": "1681006055683657748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie rozpraszajmy się. Czy masz przy sobie Soulkillera?",
              "maleVariant": "",
              "stringId": "1681006055683657760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wydajesz się spięta. Coś nie tak?",
              "maleVariant": "",
              "stringId": "1681007519780966400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wasze prywatne sprawy.",
              "maleVariant": "",
              "stringId": "1681018084863643648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu to powiedz. Będzie łatwiej.",
              "maleVariant": "",
              "stringId": "1682212773218357248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To prywatne sprawy waszej rodziny. Czemu miałabym się w to mieszać?",
              "maleVariant": "To prywatne sprawy waszej rodziny. Czemu miałbym się w to mieszać?",
              "stringId": "1682212880407990272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie podoba mi się to. Chcesz mnie wykorzystać.",
              "maleVariant": "",
              "stringId": "1682218315672326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamieniam się w słuch.",
              "maleVariant": "",
              "stringId": "1682226487787130880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcesz zabić brata.",
              "maleVariant": "",
              "stringId": "1682234701626871808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę, żeby Arasaka poznała prawdę.",
              "maleVariant": "",
              "stringId": "1682235383419375616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę, żeby poniósł karę.",
              "maleVariant": "",
              "stringId": "1682235756930535424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ciągle kręcisz. Chcesz go zabić.",
              "maleVariant": "",
              "stringId": "1682236259072610304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę, żeby Arasaka poznała prawdę.",
              "maleVariant": "",
              "stringId": "1682236572286455808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Konsekwencje? Chcesz pomścić ojca?",
              "maleVariant": "",
              "stringId": "1682257105266806784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przecież już to zrobiłaś. Na własne życzenie.",
              "maleVariant": "Przecież już to zrobiłeś. Na własne życzenie.",
              "stringId": "1682286027039707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To interesująca oferta. Przemyślę ją.",
              "maleVariant": "",
              "stringId": "1682287103667212288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przemyślę tę propozycję.",
              "maleVariant": "",
              "stringId": "1682288154709463040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie podoba mi się to.",
              "maleVariant": "",
              "stringId": "1682289000096600064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I jak masz zamiar to zrobić?",
              "maleVariant": "",
              "stringId": "1682291571288838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze. Byle szybko.",
              "maleVariant": "",
              "stringId": "1682292578022461440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Krwawisz.",
              "maleVariant": "",
              "stringId": "1682292915093508096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. Ja tylko składam ci propozycję.",
              "maleVariant": "",
              "stringId": "1682295622281859072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brzmi jak proszenie się o duże kłopoty.",
              "maleVariant": "",
              "stringId": "1682296441244880896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brzmi ryzykownie.",
              "maleVariant": "",
              "stringId": "1682296531623743488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli się uda, przejmiesz władzę?",
              "maleVariant": "",
              "stringId": "1682298853036789760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Załóżmy, że się uda. Co potem? Przejmiesz władzę?",
              "maleVariant": "",
              "stringId": "1682299852438769664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie tego mój brat nigdy nie potrafił zrozumieć.",
              "maleVariant": "",
              "stringId": "1682300643417395200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdy musi znać swoje miejsce, V.",
              "maleVariant": "",
              "stringId": "1682303389092679680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja znam swoje miejsce. Nie chcę niczego zmieniać.",
              "maleVariant": "",
              "stringId": "1682303881889845248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. I dlatego podejmiemy wszelkie możliwe środki ostrożności.",
              "maleVariant": "",
              "stringId": "1682334337066733568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja miałam być jej sercem. Dbać o jej ciągłość, stabilność, nigdy nie występować przeciw niej.",
              "maleVariant": "",
              "stringId": "1682336268342398976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ty pierwsza.",
              "maleVariant": "",
              "stringId": "1682353465123016704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba kończy ci się czas.",
              "maleVariant": "",
              "stringId": "1682369908925403136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie przy sobie. Za słabo się znamy.",
              "maleVariant": "",
              "stringId": "1682387207879266308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcę jej wysłuchać.",
              "maleVariant": "",
              "stringId": "1682387470912458756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chcesz stąd wyjść?",
              "maleVariant": "",
              "stringId": "1682403125766262784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd takie założenie?",
              "maleVariant": "",
              "stringId": "1682419939590148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To chyba oczywiste. Zresztą Takemura mówił, że miałaś zastąpić Yorinobu w Night City.",
              "maleVariant": "",
              "stringId": "1682420236815306752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcesz? Czy ci nie pozwolą?",
              "maleVariant": "",
              "stringId": "1682429303558377476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To twoja sprawa.",
              "maleVariant": "",
              "stringId": "1682431218644029440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja też znam swoje miejsce.",
              "maleVariant": "",
              "stringId": "1682431707632766976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobrze, jak wolisz. Nie moja sprawa.",
              "maleVariant": "",
              "stringId": "1682434210625622016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja też znam swoje miejsce. Doskonale pamiętam, skąd spadłam.",
              "maleVariant": "Ja też znam swoje miejsce. Doskonale pamiętam, skąd spadłem.",
              "stringId": "1682434402154319872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Być może po wszystkim... udałoby mi się tam wrócić?",
              "maleVariant": "",
              "stringId": "1682435970371031040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamknij się.",
              "maleVariant": "",
              "stringId": "1682439290313641984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamknij się. Chociaż na chwilę.",
              "maleVariant": "",
              "stringId": "1682439935347265536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie chcesz? Czy ci nie pozwolą, bo jesteś kobietą?",
              "maleVariant": "",
              "stringId": "1682457408968724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sama decyduję o swoim losie. Ty również powinnaś.",
              "maleVariant": "Sama decyduję o swoim losie. Ty również powinieneś.",
              "stringId": "1682461018385420288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Być może. Jeśli udowodnisz swoją wartość.",
              "maleVariant": "",
              "stringId": "1682494063880572928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaraz się porzygam.",
              "maleVariant": "",
              "stringId": "1682494155920379904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś. Dobrze.",
              "maleVariant": "",
              "stringId": "1683591408747679744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Denerwujesz się czymś?",
              "maleVariant": "",
              "stringId": "1683606571341189120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz mnie za idiotkę?",
              "maleVariant": "Masz mnie za idiotę?",
              "stringId": "1683611739210256384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście, że nie. Nie znamy się aż tak dobrze.",
              "maleVariant": "",
              "stringId": "1683612879104643072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masz mnie za idiotkę? Chcę stąd wyjść żywa.",
              "maleVariant": "Masz mnie za idiotę? Chcę stąd wyjść żywy.",
              "stringId": "1683612960172150784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To się źle skończy.",
              "maleVariant": "",
              "stringId": "1683648943441711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "1683658588377976832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pani kawa.",
              "maleVariant": "",
              "stringId": "1683671896803905536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czekaj.",
              "maleVariant": "",
              "stringId": "1683816371115134976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko w porządku, Hanako-sama?",
              "maleVariant": "",
              "stringId": "1683816813463212032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Dobrze się czujesz?",
              "maleVariant": "",
              "stringId": "1683821015618949120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm?",
              "maleVariant": "",
              "stringId": "1683821794551533568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spierdalam stąd!",
              "maleVariant": "",
              "stringId": "1683822307951120384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O niczym innym nie marzę.",
              "maleVariant": "",
              "stringId": "1683859895541960704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To nie przedłużaj i daj mi jej wysłuchać.",
              "maleVariant": "",
              "stringId": "1683860017176776704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Palec na cynglu. Niczego nie mów.",
              "maleVariant": "",
              "stringId": "1683951113618513920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powinniśmy wyjść. Teraz.",
              "maleVariant": "",
              "stringId": "1684900268070539264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czujesz? Właśnie tak pachnie gówno. Nie chcesz w nie wdepnąć.",
              "maleVariant": "",
              "stringId": "1684904963140882432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dziękuję za spotkanie.",
              "maleVariant": "",
              "stringId": "1685002531829899264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeszcze się, kurwa, ukłoń!",
              "maleVariant": "",
              "stringId": "1689301500272971784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja również, V.",
              "maleVariant": "",
              "stringId": "1689302645317963776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie traćmy czasu.",
              "maleVariant": "",
              "stringId": "1689302985526349824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przejdźmy do rzeczy.",
              "maleVariant": "",
              "stringId": "1689306773821722624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie podoba mi się to.",
              "maleVariant": "",
              "stringId": "1689308648474284032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteśmy tu, ponieważ wiem, jak uratować cię przed śmiercią.",
              "maleVariant": "",
              "stringId": "1689318612009881600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Krwawisz.",
              "maleVariant": "",
              "stringId": "1689422413551243264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "1689422966175961088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu wkrótce zwoła zebranie zarządu, na którym pojawią się przedstawiciele wszystkich frakcji.",
              "maleVariant": "",
              "stringId": "1692314910178807808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To doskonały moment, aby poznali prawdę na temat okoliczności śmierci mojego ojca.",
              "maleVariant": "",
              "stringId": "1692318855005605888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu podniósł rękę na ojca, ale wciąż jest Arasaką. Nadal należy do rodziny.",
              "maleVariant": "",
              "stringId": "1692326256039563264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu zrobił się czujny po paradzie. Może coś podejrzewać.",
              "maleVariant": "",
              "stringId": "1692403056027504640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A zatem?",
              "maleVariant": "",
              "stringId": "1692436520818839552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A co, jeśli ostatnia? Innych ofert możesz nie doczekać.",
              "maleVariant": "",
              "stringId": "1692441869881839616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jedyni goście? Kurwa.",
              "maleVariant": "",
              "stringId": "1692705925679996940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta porcelanowa dziwka jest niebezpieczna.",
              "maleVariant": "",
              "stringId": "1693954801086947328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Krwawisz.",
              "maleVariant": "",
              "stringId": "1695297798067326976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "1695298475313205248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ci tutaj chyba liczą na spore napiwki.",
              "maleVariant": "",
              "stringId": "1702496054254690304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyluzuj trochę.",
              "maleVariant": "",
              "stringId": "1702509824507043844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Będę ostrożna.",
              "maleVariant": "Wiem. Będę ostrożny.",
              "stringId": "1702513631324463104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spokojnie. Wchodzimy, rozmawiamy, wychodzimy.",
              "maleVariant": "",
              "stringId": "1702514784439296000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Będę ostrożna.",
              "maleVariant": "Wiem. Będę ostrożny.",
              "stringId": "1702514899010904064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[napij się]",
              "maleVariant": "",
              "stringId": "1702534570548322308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm.",
              "maleVariant": "",
              "stringId": "1702584677163524096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ostrożność to nie jest twoja mocna cecha.",
              "maleVariant": "",
              "stringId": "1702784518887182336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spadamy, jak tylko będzie okazja.",
              "maleVariant": "",
              "stringId": "1702785258410086400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdybym miała nieczyste intencje, wybrałabym inne miejsce. Bardziej dyskretne.",
              "maleVariant": "",
              "stringId": "1702790912700977152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uhm... Przemyślę to...",
              "maleVariant": "",
              "stringId": "1733115674434920448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Przemyślę to.",
              "maleVariant": "",
              "stringId": "1733116807266099204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zwlekaj z decyzją.",
              "maleVariant": "",
              "stringId": "1743248069187473408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wolisz się zabezpieczyć? Rozumiem, chociaż to niepotrzebne.",
              "maleVariant": "",
              "stringId": "1754353495610101760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteśmy tu, ponieważ wiem, jak uratować cię przed śmiercią.",
              "maleVariant": "",
              "stringId": "1754373025631703040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przecież to nie jest fizyczne miejsce.",
              "maleVariant": "",
              "stringId": "1754378767650656256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Punkty dostępowe są jak najbardziej fizyczne. A jeden z nich znajduje się całkiem blisko.",
              "maleVariant": "",
              "stringId": "1754378872072048640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdzie?",
              "maleVariant": "",
              "stringId": "1754380263339462656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tutaj, w Night City. Pod Arasaka Tower.",
              "maleVariant": "",
              "stringId": "1754380311372632064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wprowadzę cię na to spotkanie. Złożysz swoje świadectwo przeciwko mojemu bratu.",
              "maleVariant": "",
              "stringId": "1754384127400742912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Nie będę.",
              "maleVariant": "",
              "stringId": "1754519707757449216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie będę... zwlekać...",
              "maleVariant": "",
              "stringId": "1754521658662457344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu...",
              "maleVariant": "",
              "stringId": "1754604277777436672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、すぐに邸宅へお向かいください\" t=\"Hanako-sama, jesteś oczekiwana w rezydencji.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1754604384077877248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"すぐに済むわ\" t=\"Za chwilę skończymy.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1754605305079926784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Złożysz świadectwo... Czekaj, możemy zacząć mówić wprost?",
              "maleVariant": "",
              "stringId": "1760682294001930240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam oskarżyć Yorinobu przed wszystkimi szychami Arasaki?",
              "maleVariant": "",
              "stringId": "1760684011720413184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha? Na przykład jakie?",
              "maleVariant": "",
              "stringId": "1760684595282317312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przekonasz się.",
              "maleVariant": "",
              "stringId": "1760684665629184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pomożesz mi się pozbyć Yorinobu, a ja pomogę ci się pozbyć konstruktu.",
              "maleVariant": "",
              "stringId": "1760688551500845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ukłoń się] Jestem wdzięczna za spotkanie.",
              "maleVariant": "[ukłoń się] Jestem wdzięczny za spotkanie.",
              "stringId": "1761953646030544896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"女が来ました。武装しているようです\" t=\"Już jest. Wygląda na uzbrojoną.\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"jpn\" o=\"男が来ました。武装しているようです\" t=\"Już jest. Wygląda na uzbrojonego.\" b=\"\" a=\"\"/>",
              "stringId": "1802644506266177536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"もちろんです。わかりました\" t=\"Oczywiście. Rozumiem.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1802682666580840448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zanim przejdziemy do szczegółów, wyjaśnię, dlaczego zmieniłam zdanie i dlaczego chcę ci pomóc.",
              "maleVariant": "",
              "stringId": "2060540422630432768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedziałam. Od początku.",
              "maleVariant": "",
              "stringId": "2060545501429260288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy członkowie zarządu wiedzieli. Nikt, kto interesował się tą sprawą, nie uwierzył w historię o truciźnie. Zbyt wiele szczegółów się nie zgadzało.",
              "maleVariant": "",
              "stringId": "2060546556397379584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego nic z tym nie zrobiłaś?",
              "maleVariant": "",
              "stringId": "2060549036355117056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jesteś lepsza niż Yorinobu.",
              "maleVariant": "",
              "stringId": "2060549514304446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skoro wiedziałaś, to czemu nic z tym nie zrobiłaś? Dopiero teraz ruszyło cię sumienie?",
              "maleVariant": "",
              "stringId": "2060564173782802432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pomogłaś bratu zatuszować ojcobójstwo. Nie jesteś lepsza od niego.",
              "maleVariant": "",
              "stringId": "2060564275083632640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mam złudzeń. Mój ojciec nie był nieskazitelnym człowiekiem. Gdy trzeba było usuwać swoich wrogów, robił to bez litości.",
              "maleVariant": "",
              "stringId": "2060564778936983552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie masz bladego pojęcia o mojej rodzinie.",
              "maleVariant": "",
              "stringId": "2060575063773044736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Władza.",
              "maleVariant": "",
              "stringId": "2060586480299433984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rodzina.",
              "maleVariant": "",
              "stringId": "2060586581533155328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Władza.",
              "maleVariant": "",
              "stringId": "2060586686105542660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rodzina.",
              "maleVariant": "",
              "stringId": "2060586793043517440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie byłaś pewna, czy możesz mi wierzyć, że Yorinobu zabił waszego ojca, a teraz...",
              "maleVariant": "",
              "stringId": "2066335169236197376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiedziałaś?",
              "maleVariant": "",
              "stringId": "2066336550521167872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu nigdy nie potrafił tego zaakceptować, ale dla mojego ojca cel zawsze uświęcał środki.",
              "maleVariant": "",
              "stringId": "2066341865509642240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dla Saburo Arasaki liczyło się tylko jedno - wiesz co?",
              "maleVariant": "",
              "stringId": "2066342513462501376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Korporacja.",
              "maleVariant": "",
              "stringId": "2066348206911414272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem.",
              "maleVariant": "",
              "stringId": "2066348302742872064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Korporacja.",
              "maleVariant": "",
              "stringId": "2066348744554078208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem.",
              "maleVariant": "",
              "stringId": "2066348927509618688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rodzina.",
              "maleVariant": "",
              "stringId": "2066356138659045376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, rodzina.",
              "maleVariant": "",
              "stringId": "2066357164552249344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam wrażenie, że Yorinobu dostał inne wytyczne.",
              "maleVariant": "",
              "stringId": "2066358214688530432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie powiedziałaś, dlaczego zmieniłaś zdanie.",
              "maleVariant": "",
              "stringId": "2066362249122439172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobra, to twój brat, było ci ciężko. Niech będzie, że rozumiem. Ale w którym momencie zmieniłaś zdanie?",
              "maleVariant": "",
              "stringId": "2066362401291788288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mieliśmy rozmawiać o konkretach.",
              "maleVariant": "",
              "stringId": "2066362485630853120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaczęłam o tym myśleć, kiedy dostaliśmy ostrzeżenie o naruszeniu protokołów bezpieczeństwa w trakcie parady.",
              "maleVariant": "",
              "stringId": "2066362596998012928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Porozmawiajmy wreszcie o konkretach.",
              "maleVariant": "",
              "stringId": "2066362721484955648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewności nabrałam, kiedy oddział szturmowy mojego brata prawie mnie zabił w waszej kryjówce.",
              "maleVariant": "",
              "stringId": "2066371935548760064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu był skłonny poświęcić twoje życie.",
              "maleVariant": "",
              "stringId": "2066372640745480192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ojciec miał co do niego rację. Mój brat nigdy o nas nie dbał.",
              "maleVariant": "",
              "stringId": "2066373112655011840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostałam wychowana, żeby być sercem rodziny i nadszedł czas, żeby Arasaka wysłuchała mojego głosu.",
              "maleVariant": "",
              "stringId": "2066375012121075712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Serce wymierzy sprawiedliwość.",
              "maleVariant": "",
              "stringId": "2066376065713463296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy masz przy sobie Soulkillera, tak jak się umawialiśmy?",
              "maleVariant": "",
              "stringId": "2066386872522268672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niech będzie.",
              "maleVariant": "",
              "stringId": "2066387764197744640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Śmiało, siadaj.",
              "maleVariant": "",
              "stringId": "2258820060879888384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No i? Wkrótce będę musiała wracać, żeby nie wzbudzać podejrzeń.",
              "maleVariant": "",
              "stringId": "2258820367298961412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coś nie w porząku? Możesz mówić wprost.",
              "maleVariant": "",
              "stringId": "2258821506169937924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dziękuję za spotkanie.",
              "maleVariant": "",
              "stringId": "2456241390180503552"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}