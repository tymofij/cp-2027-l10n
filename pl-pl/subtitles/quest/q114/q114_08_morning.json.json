  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wepnij drzazgę] Dzięki.",
              "maleVariant": "",
              "stringId": "1551820382049157120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jest dolina, którą dostaniemy się na miejsce.",
              "maleVariant": "",
              "stringId": "1551820623892725760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mają jakieś drony, systemy wczesnego ostrzegania, te sprawy?",
              "maleVariant": "",
              "stringId": "1551820623993389056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jest to Fort Knox, ale Night Corp korzysta z usług Militechu, więc nigdy nic nie wiadomo. Na pewno patrolują okolicę.",
              "maleVariant": "",
              "stringId": "1551820623993389080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plac budowy jest ogrodzony. Wyrzutnia panzerki da sobie radę z tą bramą?",
              "maleVariant": "",
              "stringId": "1551820624094052352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uruchamiacie maszynę, przebijacie się i droga do podziemi Arasaka Tower staje przed nami otworem.",
              "maleVariant": "",
              "stringId": "1551820624278601728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A co z czasem przekopu? Możemy się wtedy spodziewać jakichś niespodzianek?",
              "maleVariant": "",
              "stringId": "1551820624412819456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plan jest prosty.",
              "maleVariant": "",
              "stringId": "1551820624429596684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z pomocą drona namierzyłem kilka newralgicznych punktów.",
              "maleVariant": "",
              "stringId": "1551820624429596696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może ma dla nas coś nowego.",
              "maleVariant": "",
              "stringId": "1693793955935440896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki.",
              "maleVariant": "",
              "stringId": "1693836644756246528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spałeś w ogóle?",
              "maleVariant": "",
              "stringId": "1693841987410350080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poza tym i tak bym nie zmrużył oka przed akcją.",
              "maleVariant": "",
              "stringId": "1693842613603160064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No i to by było na tyle. Wszystko okej?",
              "maleVariant": "",
              "stringId": "1693858454918942720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do tego przyda się Bob i jego zagłuszacz. Trzeba będzie go odpalić w okolicy anteny komunikacyjnej.",
              "maleVariant": "",
              "stringId": "1696742466654064640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Od tego mamy Carol i Mitcha.",
              "maleVariant": "",
              "stringId": "1696743427233566720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej, na razie spoko. Co dalej?",
              "maleVariant": "",
              "stringId": "1696744506696421376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdy dostaniecie się do środka, namierzycie system SERC. A potem nakłonicie go do współpracy.",
              "maleVariant": "",
              "stringId": "1696746092998971392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A jak nie będziemy dość szybcy?",
              "maleVariant": "",
              "stringId": "1696747443011211264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tyle mi wystarczy.",
              "maleVariant": "",
              "stringId": "1696761527819587584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[rozłącz się z dronem] Tyle mi wystarczy.",
              "maleVariant": "",
              "stringId": "1705463066549571588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak chcesz się jeszcze rozejrzeć, to śmiało.",
              "maleVariant": "",
              "stringId": "1705464420974219264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy jeszcze trochę czasu.",
              "maleVariant": "",
              "stringId": "1705464898655113216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyglądają na gotowych.",
              "maleVariant": "",
              "stringId": "1729978012331167744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czujesz to?",
              "maleVariant": "",
              "stringId": "1729978197836845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dla niektórych to pierwsza taka akcja. Pewnie Mitch ogarnął wszystko.",
              "maleVariant": "",
              "stringId": "1729979784525271040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Napięcie?",
              "maleVariant": "",
              "stringId": "1729980632982638592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale taki widok nigdy mi się nie znudzi.",
              "maleVariant": "",
              "stringId": "1739172720815132672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do Mikoshi możemy się dostać tylko robiąc podkop do podziemi Arasaka Tower.",
              "maleVariant": "",
              "stringId": "1751879639040282624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co oznacza, że musimy wbić się na plac budowy tunelu magleva i ukraść system SERC Night Corpu.",
              "maleVariant": "",
              "stringId": "1751880279778938880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyli już zdali sobie sprawę, na co się porywamy.",
              "maleVariant": "",
              "stringId": "1795125220398125056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie każmy im czekać.",
              "maleVariant": "",
              "stringId": "1795125425516367872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie wróciliśmy ze zwiadu.",
              "maleVariant": "",
              "stringId": "1795202095430553600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sama zobacz.",
              "maleVariant": "Sam zobacz.",
              "stringId": "1795202208274108416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zerknij.",
              "maleVariant": "",
              "stringId": "1795205416698564608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wątpię, żeby spodziewali się ataku na plac budowy.",
              "maleVariant": "",
              "stringId": "1795374990496681984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, wiem. Trzeba zdjąć ochronę, zanim zawiadomią swoich.",
              "maleVariant": "",
              "stringId": "1795375854691078144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale nie oszukujmy się. To ich nie powstrzyma na długo.",
              "maleVariant": "",
              "stringId": "1795376233352843264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miejmy nadzieję, że na tyle, ile potrzeba.",
              "maleVariant": "",
              "stringId": "1795376890197626880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zresztą, w razie czego my też mamy drony. Teddy będzie czuwał, żeby nas ostrzec na czas.",
              "maleVariant": "",
              "stringId": "1795378036551241728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z palcem w dupie.",
              "maleVariant": "",
              "stringId": "1795381225698676736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli wszystko pójdzie tak, jak ustaliliśmy, nikt nie powinien się zorientować, gdzie jesteśmy i co planujemy.",
              "maleVariant": "",
              "stringId": "1795384231991234560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ruszajmy.",
              "maleVariant": "",
              "stringId": "1817127634554638336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak stamtąd wrócimy?",
              "maleVariant": "",
              "stringId": "1817127634554638352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotowa.",
              "maleVariant": "Gotowy.",
              "stringId": "1817127634588192772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, wystarczy, bo nam tyłki przemarzną.",
              "maleVariant": "",
              "stringId": "1817206557783150592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale ten materiał jest niezły, przyda się nam.",
              "maleVariant": "",
              "stringId": "1819639319416852480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale i tak musimy być szybcy.",
              "maleVariant": "",
              "stringId": "1819648463351894016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy wszystko wiedzą?",
              "maleVariant": "",
              "stringId": "1819665249426264064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jedziemy.",
              "maleVariant": "",
              "stringId": "1819665253587013632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul już jest. Chodźmy.",
              "maleVariant": "",
              "stringId": "1841427597962825728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z wiekiem człowiek potrzebuje coraz mniej snu.",
              "maleVariant": "",
              "stringId": "1913921585256460288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak stamtąd wrócimy?",
              "maleVariant": "",
              "stringId": "1913929854209355800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tą samą drogą. Szybkie wejście, szybkie wyjście.",
              "maleVariant": "",
              "stringId": "1913929854226132992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nasi ludzie spróbują zabezpieczyć trasę na zewnątrz.",
              "maleVariant": "",
              "stringId": "1913929854226133004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, V, jesteś gotowa?",
              "maleVariant": "Dobra, V, jesteś gotowy?",
              "stringId": "1913930601734991872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spałeś w ogóle?",
              "maleVariant": "",
              "stringId": "1934285081050329088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stamtąd V, Panam i ja dostaniemy się na poziom laboratoriów i poszukamy dostępu do Mikoshi.",
              "maleVariant": "",
              "stringId": "1936088090165243904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy dwa cele: dostać się do Arasaka Tower i umożliwić V wejście do Mikoshi. To po pierwsze.",
              "maleVariant": "",
              "stringId": "1937011016964108288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po drugie: korzystając z zamieszania, musimy zgarnąć stamtąd jak najwięcej sprzętu. Takie fanty rozwiążą nasze problemy z kasą.",
              "maleVariant": "",
              "stringId": "1937012121324351488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, podłączaj się.",
              "maleVariant": "",
              "stringId": "2243458937288151040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, zbierajmy się.",
              "maleVariant": "",
              "stringId": "2243460331911008256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wsiądź]",
              "maleVariant": "",
              "stringId": "2243467449057693700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, wsiadaj i jedziemy.",
              "maleVariant": "",
              "stringId": "2243469263446495232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko zatankowane i podładowane. Jesteśmy gotowi.",
              "maleVariant": "",
              "stringId": "2247466572614660096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nic nie udało się przechwycić. Nie wiadomo, co Militech jest w stanie na nas rzucić.",
              "maleVariant": "",
              "stringId": "2247499830610030592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mamy jedno podejście. To się musi udać.",
              "maleVariant": "",
              "stringId": "2247500100270223360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A nasze graty? Jeśli coś się spierdoli, trzeba będzie zwijać obóz.",
              "maleVariant": "",
              "stringId": "2247506683750445056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na to też jesteśmy gotowi. Ale nic się nie spierdoli.",
              "maleVariant": "",
              "stringId": "2247508754209894400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to się wkrótce przekonamy.",
              "maleVariant": "",
              "stringId": "2247512276452175872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musi. Ale tak czy siak jesteśmy tu spaleni. Wiesz o tym?",
              "maleVariant": "",
              "stringId": "2247518045079793664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Ale na razie myślę o najbliższym kryzysie.",
              "maleVariant": "",
              "stringId": "2247518496605007872"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}