  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę zapalić. Masz fajki?",
              "maleVariant": "",
              "stringId": "1531611843562201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę się stąd wydostać? Rozumiesz?",
              "maleVariant": "",
              "stringId": "1531745063764807680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli przy okazji będę musiał zabić również ciebie, zrobię to.",
              "maleVariant": "",
              "stringId": "1531745948058308608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd się tu wziąłeś?",
              "maleVariant": "",
              "stringId": "1531771635402067972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie palę.",
              "maleVariant": "",
              "stringId": "1531772520551206912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To idź i kup, chcę zapalić ostatniego.",
              "maleVariant": "",
              "stringId": "1531775927802449920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa, człowieku!",
              "maleVariant": "",
              "stringId": "1531776637948448768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kim jesteś?",
              "maleVariant": "",
              "stringId": "1531784168183746560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dla kogo pracujesz? Siedź na dupie i tłumacz się.",
              "maleVariant": "",
              "stringId": "1531798393618747392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa!",
              "maleVariant": "",
              "stringId": "1531799943867060224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa!",
              "maleVariant": "",
              "stringId": "1531800673709510656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdolony chip...",
              "maleVariant": "",
              "stringId": "1531800921139892224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypierdalaj z mojej głowy.",
              "maleVariant": "",
              "stringId": "1531827162819645440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[zażyj lekarstwo]",
              "maleVariant": "",
              "stringId": "1531833199161298944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie palę.",
              "maleVariant": "",
              "stringId": "1532744625648357376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd się wziąłeś?",
              "maleVariant": "",
              "stringId": "1553261098178404352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zadręczaj się tym. Zobacz, mam dla ciebie lekarstwa.",
              "maleVariant": "",
              "stringId": "1640255507798315008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam dla ciebie coś jeszcze. Zobacz.",
              "maleVariant": "",
              "stringId": "1640255507798315016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vik wyjął to z twojej czaszki. Może kiedyś ta kula przyniesie ci szczęście.",
              "maleVariant": "",
              "stringId": "1640255507815092224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Był moim najlepszym przyjacielem.",
              "maleVariant": "",
              "stringId": "1653125477661462528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jackie był moim najlepszym przyjacielem. Bardzo mi go brakuje.",
              "maleVariant": "",
              "stringId": "1653126780949155840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdyby tu był, na pewno kazałby ci ruszyć tyłek, wstać z łóżka i przestać się użalać nad sobą.",
              "maleVariant": "",
              "stringId": "1653127023094714368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dużo mówił o tobie.",
              "maleVariant": "",
              "stringId": "1653130307939389440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dużo o tobie mówił. Naprawdę dużo.",
              "maleVariant": "",
              "stringId": "1653133702926487552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pokłóciliśmy się, zanim pojechał na waszą akcję.",
              "maleVariant": "",
              "stringId": "1653140217888559104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wiedz, że nie był na ciebie zły.",
              "maleVariant": "",
              "stringId": "1653141035140304896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Żałuję tylko, że nie mogliśmy przeżyć naszych ostatnich wspólnych chwil inaczej.",
              "maleVariant": "",
              "stringId": "1653141880728768512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[połóż się] Padam z nóg.",
              "maleVariant": "",
              "stringId": "1653148212466864132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tabletki, które mają mnie zabić?",
              "maleVariant": "",
              "stringId": "1653148644480176128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś kochana. Dzięki..",
              "maleVariant": "",
              "stringId": "1653148806229315584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Misty, jeśli chodzi o Jackie'ego…",
              "maleVariant": "",
              "stringId": "1653148869361979392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobranoc, V. Odpocznij i śpij spokojnie.",
              "maleVariant": "",
              "stringId": "1653150122200584192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak?",
              "maleVariant": "",
              "stringId": "1653150838956806144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mam pojęcia, co się ze mną działo. Potem śniłam, że umieram... I umarłam naprawdę.",
              "maleVariant": "Nie mam pojęcia, co się ze mną działo. Potem śniłem, że umieram... I umarłem naprawdę.",
              "stringId": "1654744294154096640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nawet nie wiem, czy już się obudziłam. A może ja faktycznie nie żyję?",
              "maleVariant": "Nawet nie wiem, czy już się obudziłem. A może ja faktycznie nie żyję?",
              "stringId": "1654776913927393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś kochana.",
              "maleVariant": "",
              "stringId": "1680714883322662912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli chodzi o Jackiego...",
              "maleVariant": "",
              "stringId": "1680714883322662928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszałaś? Ja umieram. Żaden amulet nie pomoże na mojego pecha.",
              "maleVariant": "",
              "stringId": "1680723670523760640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dajesz mi pigułki, które obudzą tego sukinsyna i sprawią, że szybciej umrę?",
              "maleVariant": "",
              "stringId": "1680747371126378496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja mówię o procesie umierania psyche. Poczujesz, że dawna V się rozpada i staniesz się obca dla samej siebie.",
              "maleVariant": "Ja mówię o procesie umierania psyche. Poczujesz, że dawny V się rozpada i staniesz się obcy dla samego siebie.",
              "stringId": "1680749834860548096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To będzie przerażające i bolesne.",
              "maleVariant": "",
              "stringId": "1680750977925828608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale nie myśl już o tym. Połóż się, proszę. Spróbuj zasnąć.",
              "maleVariant": "",
              "stringId": "1680754164607012864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd mam, kurwa, wiedzieć?",
              "maleVariant": "",
              "stringId": "1680853239671083008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak to możliwe, że cię widzę?",
              "maleVariant": "",
              "stringId": "1680856361256669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jest prawie jak dotyk...",
              "maleVariant": "",
              "stringId": "1693928484547289100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie gadaj bzdur – przecież żyjesz. A póki jest życie, jest nadzieja. I nie daj sobie wmówić, że jest inaczej.",
              "maleVariant": "",
              "stringId": "1725750154300542976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy ty naprawdę wierzysz, że mogę z tego wyjść?",
              "maleVariant": "",
              "stringId": "1725751755551592448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście, że tak. Przecież już raz zginęłaś i nadal żyjesz. Mylę się?",
              "maleVariant": "Oczywiście, że tak. Przecież już raz zginąłeś i nadal żyjesz. Mylę się?",
              "stringId": "1725752858032787456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzień dobry Night City!",
              "maleVariant": "",
              "stringId": "1735537728094892032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wciśnięci między dwie Kalifornie, skazani na kawę z paśnika, zaczynamy nowy dzień.",
              "maleVariant": "",
              "stringId": "1735538628830064640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zanieczyszczenie powietrza przekracza normę jedenaście razy, deszczu nie będzie.",
              "maleVariant": "",
              "stringId": "1741406203971530752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[zażyj lekarstwo]",
              "maleVariant": "",
              "stringId": "1754851376491094016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa.",
              "maleVariant": "",
              "stringId": "1754851939937116160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trzeba to skończyć!",
              "maleVariant": "",
              "stringId": "1755975805704888320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie!",
              "maleVariant": "",
              "stringId": "1755977876432773120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh...",
              "maleVariant": "",
              "stringId": "1755979911341301760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zrzygałbym się, ale nie mogę.",
              "maleVariant": "",
              "stringId": "1756064483307446272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wchodzę w ciebie, jak w gnijące mięso i nie mogę nic z tym zrobić.",
              "maleVariant": "",
              "stringId": "1756065191691837440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszysz?!",
              "maleVariant": "",
              "stringId": "1756066248757108736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zabiję cię!",
              "maleVariant": "",
              "stringId": "1756069344270901252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyłaź z mojej głowy!",
              "maleVariant": "",
              "stringId": "1756070011198791680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zabiję cię.",
              "maleVariant": "",
              "stringId": "1756070917755658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostaw mnie w spokoju!",
              "maleVariant": "",
              "stringId": "1756071035716263936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, zrób to.",
              "maleVariant": "",
              "stringId": "1756095520016195584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypierdalaj ze mnie!",
              "maleVariant": "",
              "stringId": "1756099862261686272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie tak!",
              "maleVariant": "",
              "stringId": "1756172889003417600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wsadź sobie lufę pistoletu w usta i pociągnij za spust.",
              "maleVariant": "",
              "stringId": "1756173216477896704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeśli mieszkacie w Watson, to mam dobre wieści - kordon wokół dzielnicy został odwołany.",
              "maleVariant": "",
              "stringId": "1756194282554286080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wy też otwórzcie się na świat. Kto wie, może ten dzień otworzy nowy rozdział w waszym życiu?",
              "maleVariant": "",
              "stringId": "1756197792817635328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co na to Rubicones? Posłuchajmy!",
              "maleVariant": "",
              "stringId": "1756198964001529856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostawcie w łóżku waszego doła, dzisiaj nie patrzcie na miasto, patrzcie w słońce, dopóki nie wypali wam oczu.",
              "maleVariant": "",
              "stringId": "1756204778397392896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa, kurwa, kurwa...",
              "maleVariant": "",
              "stringId": "1756420060462211072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To tylko kopia engramu... Są też inne...",
              "maleVariant": "",
              "stringId": "1756420575841509376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tylko śmierć jest rozwiązaniem.",
              "maleVariant": "",
              "stringId": "1756420811678834688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszysz? Tylko śmierć!",
              "maleVariant": "",
              "stringId": "1756424308100980736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przez jakiś czas będziesz czuła się nieźle. Potem nastąpi długa i bolesna agonia.",
              "maleVariant": "Przez jakiś czas będziesz czuł się nieźle. Potem nastąpi długa i bolesna agonia.",
              "stringId": "1774717953086349312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mózg można oszukać, ból da się uśmierzyć.",
              "maleVariant": "",
              "stringId": "1774718837564399616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To po chuj te tabletki? Nie lepiej po prostu strzelić sobie w łeb?",
              "maleVariant": "",
              "stringId": "1774726692237500416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego ten drugi ma umierać razem z tobą? Pomyśl o tym.",
              "maleVariant": "",
              "stringId": "1774726997968707584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przykro mi, że nie żyje.",
              "maleVariant": "",
              "stringId": "1774732426488602624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurewsko mi przykro, że Jackie nie żyje.",
              "maleVariant": "",
              "stringId": "1774759857421312000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jackie był wyjątkowy. Jego aura była jasna i piękna. Nie martw się, duch Jackiego wciąż jest bardzo silny. Jego miłość znajdziesz w każdej żywej istocie.",
              "maleVariant": "",
              "stringId": "1774768516259930112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Martwię się o ciebie.",
              "maleVariant": "",
              "stringId": "1774769616828522496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niepotrzebnie. Życie ma w sobie wielką i piękną moc – potrafi przezwyciężyć śmierć.",
              "maleVariant": "",
              "stringId": "1774769918566752256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań z łóżka]",
              "maleVariant": "",
              "stringId": "1782045434573447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Omegablokery, jeśli będziesz je zażywała regularnie, spowolnią działanie Relica.",
              "maleVariant": "Omega blokery - jeśli będziesz je zażywał regularnie, spowolnią działanie Relica.",
              "stringId": "1801308235405942784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A poza tym wytłumią też obecność gościa w twojej głowie.",
              "maleVariant": "",
              "stringId": "1801308235892482048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pseudoendotryzyna jest ode mnie. Powoduje odwrotny efekt - przyśpiesza działanie biochipu i uwalnia demona.",
              "maleVariant": "",
              "stringId": "1801308236899115008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę się położyć.",
              "maleVariant": "",
              "stringId": "1801319726121181184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obiecaj, że spróbujesz się wyspać.",
              "maleVariant": "",
              "stringId": "1801327129218736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mój pech jest nieuleczalny.",
              "maleVariant": "",
              "stringId": "1801330165878054912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sen jest małym misterium śmierci. Namiastką tego, co nieuniknione.",
              "maleVariant": "",
              "stringId": "1802321539337052160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[idź spać]",
              "maleVariant": "",
              "stringId": "1879531836931792900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań]",
              "maleVariant": "",
              "stringId": "1879681356235759620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odwal się!",
              "maleVariant": "",
              "stringId": "2010204369420050432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh...",
              "maleVariant": "",
              "stringId": "2463536371112120320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oddaj mi kontrolę nad ciałem!",
              "maleVariant": "",
              "stringId": "2463538192413134848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słyszysz?!",
              "maleVariant": "",
              "stringId": "2463538600804126720"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}