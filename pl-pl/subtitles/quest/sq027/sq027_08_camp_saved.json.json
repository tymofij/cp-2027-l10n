  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wracajcie do obozu.",
              "maleVariant": "",
              "stringId": "1360357881718788112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy wszystko...?",
              "maleVariant": "",
              "stringId": "1402699551926325248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki. Chyba muszę trochę ochłonąć.",
              "maleVariant": "",
              "stringId": "1804367676521074688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Idziesz ze mną?",
              "maleVariant": "",
              "stringId": "1804367999516037120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, w porządku. Rozjebaliście ich, gratulacje.",
              "maleVariant": "",
              "stringId": "1807122718222782464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A teraz musimy porozmawiać.",
              "maleVariant": "",
              "stringId": "1807123154111631360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdy z nas powinien myśleć przede wszystkim o tym, co jest najlepsze dla rodziny...",
              "maleVariant": "",
              "stringId": "1807172676812607488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Daj spokój, Saul. Jeśli mam odejść z rodziny, to powiedz to wprost.",
              "maleVariant": "",
              "stringId": "1807174123713269760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przynajmniej nie będę musiała znosić twojej kolejnej przemowy.",
              "maleVariant": "",
              "stringId": "1807179626874998784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, jeszcze paru będziesz musiała wysłuchać.",
              "maleVariant": "",
              "stringId": "1807185207748792320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bo od dzisiaj będziesz przewodzić tej rodzinie, razem ze mną.",
              "maleVariant": "",
              "stringId": "1807198217741602816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałem to zrobić uroczyście, ale niech będzie po twojemu.",
              "maleVariant": "",
              "stringId": "1807198851249278976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pomyliłem się. Ty miałaś rację, a ja nie.",
              "maleVariant": "",
              "stringId": "1807199135673421824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I niech mi się to więcej nie powtórzy.",
              "maleVariant": "",
              "stringId": "1807199767671148544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyja to była decyzja?",
              "maleVariant": "",
              "stringId": "1807202533596532740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Że co?",
              "maleVariant": "",
              "stringId": "1807202839143190528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam uratowała wam wsystkim tyłki.",
              "maleVariant": "",
              "stringId": "1807203979188572160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czyja to była decyzja? Twoja czy całej rodziny?",
              "maleVariant": "",
              "stringId": "1807204383435591680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No wiesz co, nie psuj jej tego.",
              "maleVariant": "",
              "stringId": "1807205022597189632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niech chociaż przez moment jej się wydaje, że ma moje pełne poparcie.",
              "maleVariant": "",
              "stringId": "1807206329861083136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam dziś uratowała wam wszystkim tyłki.",
              "maleVariant": "",
              "stringId": "1807208056672817152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, brawo. A teraz niech się weźmie do prawdziwej roboty.",
              "maleVariant": "",
              "stringId": "1807208889510596608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej. Okej. No to... wiele rzeczy będzie musiało się zmienić.",
              "maleVariant": "",
              "stringId": "1807210412210720768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. Na początek musimy się stąd wynieść. I to szybko.",
              "maleVariant": "",
              "stringId": "1807211344503836672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jasne. Przygotuję trasę.",
              "maleVariant": "",
              "stringId": "1807211950496878592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie będziemy czekać, aż Militech do nas trafi.",
              "maleVariant": "",
              "stringId": "1807216006556618752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[napij się] Będzie mi brakowało tych naszych eskapad.",
              "maleVariant": "",
              "stringId": "1807228176447115264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[napij się] Słuchaj, to, co wydarzyło się w Basilisku...",
              "maleVariant": "",
              "stringId": "1807228630707015680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[napij się] No to co teraz, szefowo?",
              "maleVariant": "",
              "stringId": "1807228963684421632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Koszmarny wybór. Ja byłem przeciwko.",
              "maleVariant": "",
              "stringId": "1886628842700558336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gratulacje.",
              "maleVariant": "",
              "stringId": "1886629659952304128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To był... interesujący dzień.",
              "maleVariant": "",
              "stringId": "1894187775896080384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czuję się, jakbym uniknęła czołowego zderzenia z lokomotywą.",
              "maleVariant": "",
              "stringId": "1894189366007058432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To, co się wydarzyło w Basilisku... Mam nadzieję, że nie żałujesz.",
              "maleVariant": "",
              "stringId": "1894197344059805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to co teraz, szefowo?",
              "maleVariant": "",
              "stringId": "1894199170259767296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mam pojęcia. Serio. Najmniejszego.",
              "maleVariant": "",
              "stringId": "1894199721978515456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul okazał się w porządku. Jak żyć?",
              "maleVariant": "",
              "stringId": "1894200965774200832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żałuję tylko, że nie miałyśmy więcej czasu.",
              "maleVariant": "Żałuję tylko, że nie mieliśmy więcej czasu.",
              "stringId": "1895053099902193664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będę improwizować.",
              "maleVariant": "",
              "stringId": "1895093596544847872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, poważnie. Wszyscy mamy u ciebie dług. I... możesz liczyć na Aldecaldos. Zawsze.",
              "maleVariant": "",
              "stringId": "1895100129039110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobrze wiedzieć.",
              "maleVariant": "",
              "stringId": "1947299147600752640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pusto za barem. To mi się podoba.",
              "maleVariant": "",
              "stringId": "1947304251095642112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Piwo? Lemoniada?",
              "maleVariant": "",
              "stringId": "1947304713207279616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oprzyj się] Piwo.",
              "maleVariant": "",
              "stringId": "1947306557878956036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[oprzyj się] Może być lemoniada.",
              "maleVariant": "",
              "stringId": "1947306644902375424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Piwo.",
              "maleVariant": "",
              "stringId": "1947308660215115776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może być lemoniada.",
              "maleVariant": "",
              "stringId": "1947308754788282368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Twój wybór.",
              "maleVariant": "",
              "stringId": "1947313141124296704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Za szczęśliwe zakończenia.",
              "maleVariant": "",
              "stringId": "1947313530640920576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W sprawie tych szczęśliwych zakończeń...",
              "maleVariant": "",
              "stringId": "1947315458057826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było blisko.",
              "maleVariant": "",
              "stringId": "1947317183779041280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, czy dałabym radę bez ciebie.",
              "maleVariant": "",
              "stringId": "1947331508585492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Daj spokój.",
              "maleVariant": "",
              "stringId": "1947332095687389184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będzie mi brakowało tych naszych eskapad.",
              "maleVariant": "",
              "stringId": "1947339401762304000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mnie też.",
              "maleVariant": "",
              "stringId": "1947360021078208512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale z drugiej strony - kto powiedział, że to już koniec?",
              "maleVariant": "",
              "stringId": "1947360586101288960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, rozejrzyj się. Popatrz na nich.",
              "maleVariant": "",
              "stringId": "1947362075918069760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhmmm.",
              "maleVariant": "",
              "stringId": "1947363283357528064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to coś trzeba będzie z tym zrobić.",
              "maleVariant": "",
              "stringId": "1947363446029414400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz... wiesz, że to mogłaby być twoja... nasza codzienność?",
              "maleVariant": "",
              "stringId": "1947364381258543104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zostań w obozie. Dołącz do nas.",
              "maleVariant": "",
              "stringId": "1947365387841171456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To mogłaby być twoja rodzina.",
              "maleVariant": "",
              "stringId": "1947366248243916800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam, co ty...",
              "maleVariant": "",
              "stringId": "1947366548052766720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałabym. Ale nie mogę teraz.",
              "maleVariant": "Chciałbym. Ale nie mogę teraz.",
              "stringId": "1947367413706780676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To nie dla mnie, Panam.",
              "maleVariant": "",
              "stringId": "1947369350015610880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To nie takie proste.",
              "maleVariant": "",
              "stringId": "1947370572755890176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałbym, Panam. Naprawdę.",
              "maleVariant": "",
              "stringId": "1947371468826996736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To nie dla mnie, Panam.",
              "maleVariant": "",
              "stringId": "1947371532865630208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wszystko nie jest takie proste.",
              "maleVariant": "",
              "stringId": "1947371624184016896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, nadciąga pierdolone \"ale\".",
              "maleVariant": "",
              "stringId": "1947374172475355136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale nie mogłabym teraz. Muszę dokończyć swoje sprawy.",
              "maleVariant": "Ale nie mógłbym teraz. Muszę dokończyć swoje sprawy.",
              "stringId": "1947374404705579008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Próbowałam kiedyś, nie wyszło.",
              "maleVariant": "Próbowałem kiedyś, nie wyszło.",
              "stringId": "1947376767474462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie jestem stworzona do takiego życia.",
              "maleVariant": "Nie jestem stworzony do takiego życia.",
              "stringId": "1947376841763975168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdolenie. Nie wiesz, jak byłoby tym razem.",
              "maleVariant": "",
              "stringId": "1947377484448788480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdolenie. Nie wiesz tego.",
              "maleVariant": "",
              "stringId": "1947378274454335488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mówię, jak jest.",
              "maleVariant": "",
              "stringId": "1947378582081368064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo chcesz, albo nie chcesz. Co tu nie jest proste?",
              "maleVariant": "",
              "stringId": "1947381196944699392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie w tym problem. To, czego chcę, nie ma w tym momencie znaczenia.",
              "maleVariant": "",
              "stringId": "1947382060484784128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, V. W sumie nie powinnam zrzucać na ciebie teraz takiej bomby.",
              "maleVariant": "",
              "stringId": "1947382824871522304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po prostu myślałam... Zapomnij o tym.",
              "maleVariant": "",
              "stringId": "1947383619004264448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Przemyślę to. Obiecuję.",
              "maleVariant": "",
              "stringId": "1947384405570482180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Muszę jechać. Uważaj na siebie.",
              "maleVariant": "",
              "stringId": "1947392609192898560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pomyślę o tym wszystkim, obie–",
              "maleVariant": "",
              "stringId": "1947394255121674240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uważaj na siebie, Panam. Niedłu–",
              "maleVariant": "",
              "stringId": "1947394329293746176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wstań] Daj znać, jak się przeniesiecie.",
              "maleVariant": "",
              "stringId": "1947396178025508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może jak przeniesiecie obóz, to–",
              "maleVariant": "",
              "stringId": "1947396602539405312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale ja... tyle rzeczy spieprzyłam... Mówiłeś...",
              "maleVariant": "",
              "stringId": "1979539369352777728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem. Ale zmieniłem zdanie. Narażałaś się dla rodziny, a nawet nie wiedziałaś, czy następnego dnia nie będziesz musiała odejść.",
              "maleVariant": "",
              "stringId": "1979539972778905600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[napij się]",
              "maleVariant": "",
              "stringId": "2162488503826673668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to czego się napijesz?",
              "maleVariant": "",
              "stringId": "2227211046882983936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa, widzieliście Basiliska? Rozpierdolił ich w drobny...arrgh.",
              "maleVariant": "",
              "stringId": "2227218344938389504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ruszaj się. Spokojnie, muszę to obejrzeć...",
              "maleVariant": "",
              "stringId": "2227249210888634368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Teraz już się tu nie odważą pokazać! Ilu załatwiliśmy?",
              "maleVariant": "",
              "stringId": "2227258059142021120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mnie raczej interesują nasze straty.",
              "maleVariant": "",
              "stringId": "2227277545844850688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na pewno byłyby mniejsze, gdyby Basilisk był tu od początku.",
              "maleVariant": "",
              "stringId": "2227278797425164288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam będzie się musiała gęsto tłumaczyć, czemu nie było jej w obozie.",
              "maleVariant": "",
              "stringId": "2227284814288470016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A co ona, jasnowidz? Weź się od niej odpierdol.",
              "maleVariant": "",
              "stringId": "2227286193912496128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kończysz już? Musa też mocno oberwał.",
              "maleVariant": "",
              "stringId": "2227296339732545536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[wysiądź]",
              "maleVariant": "",
              "stringId": "2412725381476151300"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}