  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cholera, zacięło się... Weź mi pomóż.",
              "maleVariant": "",
              "stringId": "1811292423955697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[otwórz]",
              "maleVariant": "",
              "stringId": "1811296530565189636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tu z lewej. Dociśnij... O, właśnie.",
              "maleVariant": "",
              "stringId": "1811305132646486016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja pierdolę, Mitch... Co ty odpierdalasz?",
              "maleVariant": "",
              "stringId": "1811373862323769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Luz, V. Scorpion tego chciał.",
              "maleVariant": "",
              "stringId": "1811385601476022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żebyś go woził w bagażniku jak zapasową oponę?",
              "maleVariant": "",
              "stringId": "1811385744166244352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skoro Scorpion tego chciał...",
              "maleVariant": "",
              "stringId": "1811386006964555780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul się nie wkurzy?",
              "maleVariant": "",
              "stringId": "1811386633811677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saulowi to się nie spodoba. Zniszczyć samochód... Straszne marnotrawstwo.",
              "maleVariant": "",
              "stringId": "1811386964826148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiem mu, że to był wypadek.",
              "maleVariant": "",
              "stringId": "1811387447355658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skoro Scorpion tego chciał...",
              "maleVariant": "",
              "stringId": "1811387679434887168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciał, chciał. Dziesięć razy mi powtarzał.",
              "maleVariant": "",
              "stringId": "1811388212967133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[podnieś kanister]",
              "maleVariant": "",
              "stringId": "1811389513553694724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mam.",
              "maleVariant": "",
              "stringId": "1811398952448970752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ekstra. Postaw go na siedzeniu pasażera.",
              "maleVariant": "",
              "stringId": "1811399237124771840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[postaw kanister]",
              "maleVariant": "",
              "stringId": "1811399600636710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Widzę, że kierowca już za kółkiem...",
              "maleVariant": "",
              "stringId": "1811414052681568256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma co tego przeciągać. Jeszcze godzinka, dwie i spuchnie jak balon.",
              "maleVariant": "",
              "stringId": "1811416248164831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Robimy.",
              "maleVariant": "",
              "stringId": "1811418292787367936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś pewien, że to dobry pomysł?",
              "maleVariant": "",
              "stringId": "1811419470111723520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś pewien, że to dobry pomysł?",
              "maleVariant": "",
              "stringId": "1811421128254312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, V. Nie jestem.",
              "maleVariant": "",
              "stringId": "1811422682579488768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Robimy.",
              "maleVariant": "",
              "stringId": "1811422853220552704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zajebiście. Wetknąłem szmatę do wlewu paliwa. Podpal ją, a ja uruchomię auto.",
              "maleVariant": "",
              "stringId": "1811423155025891328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[podpal szmatę]",
              "maleVariant": "",
              "stringId": "1811423554004865028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pali się! Szybko, zanim pierdolnie!",
              "maleVariant": "",
              "stringId": "1811423894330691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odszedł z hukiem. Tak, jak chciał.",
              "maleVariant": "",
              "stringId": "1811424662542635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Popiół i sadza. Tyle z nas zostaje.",
              "maleVariant": "",
              "stringId": "1811425287259049984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie. Taki pogrzeb trzeba opić.",
              "maleVariant": "",
              "stringId": "1811425434059689984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie.",
              "maleVariant": "",
              "stringId": "1811425723818987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To co... Po piwku?",
              "maleVariant": "",
              "stringId": "1811722638246825984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę już lecieć.",
              "maleVariant": "",
              "stringId": "1811735446527168512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wybacz... Nie lubię styp.",
              "maleVariant": "",
              "stringId": "1811735716288024576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to chodź.",
              "maleVariant": "",
              "stringId": "1811741647050657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaach... Nie ma to jak zimny browar.",
              "maleVariant": "",
              "stringId": "1811744593180119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pełna zgoda.",
              "maleVariant": "",
              "stringId": "1811747013125754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[weź]",
              "maleVariant": "",
              "stringId": "1845779842239057924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co to jest?",
              "maleVariant": "",
              "stringId": "1845780327872352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pojęcia nie mam... Ale Scorpion zawsze ją przy sobie nosił. Nie wiem, czemu.",
              "maleVariant": "",
              "stringId": "1845792706219757568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słuchaj... Ja tu jeszcze zostanę. Posiedzę sobie, podumam.",
              "maleVariant": "",
              "stringId": "1845792948096880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie. Żeby przywieźć go w to miejsce, jak umrze... I pożegnać z hukiem.",
              "maleVariant": "",
              "stringId": "1863237586613751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Posadzimy go za kółkiem, oblejemy auto benzyną... A potem je podpalimy i pozwolimy mu zjechać z klifu.",
              "maleVariant": "",
              "stringId": "1863238860105109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chyba cię pojebało!",
              "maleVariant": "",
              "stringId": "1863240077191467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kurwa, Mitch, chyba cię pojebało.",
              "maleVariant": "",
              "stringId": "1863241269229441024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oj tam, oj tam, nie marudź.",
              "maleVariant": "",
              "stringId": "1863243586733395968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, dalej, dawaj kanister.",
              "maleVariant": "",
              "stringId": "1863245705880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No dobra, jedziemy z koksem? Robimy ten pogrzeb?",
              "maleVariant": "",
              "stringId": "1863249863308005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale to było ostatnie życzenie mojego przyjaciela. Więc to, kurwa, zrobię, z twoją pomocą, albo bez.",
              "maleVariant": "",
              "stringId": "1863252538233769984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Twoja strata.",
              "maleVariant": "",
              "stringId": "1863258104175841280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ej, V! Dzięki za pomoc. Doceniam.",
              "maleVariant": "",
              "stringId": "1863258316642504704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mitch... Czemu ja? Czemu to mnie poprosiłeś o pomoc?",
              "maleVariant": "",
              "stringId": "1863262761832992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czemu to mnie poprosiłeś o pomoc?",
              "maleVariant": "",
              "stringId": "1863262994465869828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skąd się wzięła ksywka \"Scorpion\"?",
              "maleVariant": "",
              "stringId": "1863263606398046208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ej, tak swoją drogą... Skąd ta ksywka? \"Scorpion\"?",
              "maleVariant": "",
              "stringId": "1863264231651332096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zabawne... Wiesz, że nie wiem? Nigdy nie spytałem.",
              "maleVariant": "",
              "stringId": "1863264970436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może od znaku zodiaku.",
              "maleVariant": "",
              "stringId": "1863265301551173632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo dlatego, że miał duże żądło. Ha ha!",
              "maleVariant": "",
              "stringId": "1863266236545421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ech, Scorpion, skurwysynu... Będzie mi ciebie brakować.",
              "maleVariant": "",
              "stringId": "1863266849886883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bo ci dobrze z oczu patrzy.",
              "maleVariant": "",
              "stringId": "1863267756393418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poważnie pytam.",
              "maleVariant": "",
              "stringId": "1863268095561617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ech... Nie wiem, bo... To, że ja jeszcze żyję, że mogłem go pożegnać, jak trzeba... To twoja zasługa.",
              "maleVariant": "",
              "stringId": "1863268252848017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasze życia się przecięły. Na krótką chwilę... ale w ważnym momencie.",
              "maleVariant": "",
              "stringId": "1863269066895314944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słuchaj... Dam ci parę rzeczy, które do niego należały. Masz, taki amulet, na szczęście. No, weź.",
              "maleVariant": "",
              "stringId": "1863270157296914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozumiem. Zbieram się.",
              "maleVariant": "",
              "stringId": "1863272334694338560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha i... dzięki, V. Za dziś... Za to, że wyciągnęłaś mnie wtedy z gówna.",
              "maleVariant": "Aha i... Dzięki, V. Za dziś... Za to, że wyciągnąłeś mnie wtedy z gówna.",
              "stringId": "1863272509596815360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Równa z ciebie laska. Naprawdę równa.",
              "maleVariant": "Równy z ciebie chłop. Naprawdę równy.",
              "stringId": "1863273061936320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, dzięki.",
              "maleVariant": "",
              "stringId": "1873522370078363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki, wolę na trzeźwo.",
              "maleVariant": "",
              "stringId": "1873523224223207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Jak chcesz. Każdy żegna zmarłych po swojemu.",
              "maleVariant": "",
              "stringId": "1873524512897945600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skoro tak mówisz.",
              "maleVariant": "",
              "stringId": "1873528054668906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie. Słuchaj, jak chcesz, możesz wrócić do obozu moim autem. Jak tam ci wygodnie.",
              "maleVariant": "",
              "stringId": "1908015766788427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szerokiej drogi, przyjacielu.",
              "maleVariant": "",
              "stringId": "1934467678261235712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odbiło ci?!",
              "maleVariant": "",
              "stringId": "2121969965643616260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V... kanister. Na co czekasz?",
              "maleVariant": "",
              "stringId": "2238669820146728960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zróbmy to. Dla Scorpiona.",
              "maleVariant": "",
              "stringId": "2238714786642378752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dalej, podpalaj!",
              "maleVariant": "",
              "stringId": "2238716481912946688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, już. Tego właśnie chciał.",
              "maleVariant": "",
              "stringId": "2238716544173195264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nic nie powiesz?",
              "maleVariant": "",
              "stringId": "2238721843760185344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na milczenie jeszcze przyjdzie czas.",
              "maleVariant": "",
              "stringId": "2238722204570992644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamyśliłaś się?",
              "maleVariant": "Zamyśliłeś się?",
              "stringId": "2238722248493744132"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}