    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Opowiesz mi coś o Randym? Jaki jest?",
              "maleVariant": "",
              "stringId": "1338559622542442496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drobne kradzieże, prochy, ucieczki z domu.",
              "maleVariant": "",
              "stringId": "1396774671519178752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co cię gryzie?",
              "maleVariant": "",
              "stringId": "1401040175171756032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Opowiedz mi o Randym.",
              "maleVariant": "",
              "stringId": "1401042845500563456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wspomniałeś nic o ojcu Randy'ego.",
              "maleVariant": "",
              "stringId": "1401045522640920576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A co z ojcem Randy'ego?",
              "maleVariant": "",
              "stringId": "1401046087731109888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wujek glina opisze cię najlepiej.",
              "maleVariant": "",
              "stringId": "1401053490375680000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są tacy ludzie, którzy zawsze przyciągają kłopoty. Randy jest jednym z nich.",
              "maleVariant": "",
              "stringId": "1401053747838836736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Minęło już parę lat, odkąd dostał czapę. Wojna gangów.",
              "maleVariant": "",
              "stringId": "1401057441661706240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trudno przychodzi mi mówienie o tym, co mnie gryzie.",
              "maleVariant": "",
              "stringId": "1537312397961904128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie boisz się, że Yawen doniesie na ciebie do przełożonych?",
              "maleVariant": "",
              "stringId": "1780929152491163648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdy przyjdzie co do czego, Yawen zgodzi się nam pomóc?",
              "maleVariant": "",
              "stringId": "1780930867542069248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No to gdy znajdziemy Randy'ego, jesteśmy umówieni na piwo.",
              "maleVariant": "",
              "stringId": "1780931966516826112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na razie powinniśmy skupić się na znalezieniu Randy'ego.",
              "maleVariant": "",
              "stringId": "1780932008661192704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Skupmy się na znalezieniu Randy'ego.",
              "maleVariant": "",
              "stringId": "1780932115615944708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To jesteśmy umówieni na piwo.",
              "maleVariant": "",
              "stringId": "1780932497180168192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie powinniśmy raczej szukać mieszkania Harrisa?",
              "maleVariant": "",
              "stringId": "1782223670917337088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha, czyli wzorowy rodzic.",
              "maleVariant": "",
              "stringId": "1782262268496580608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A dlaczego nie próbujemy znaleźć mieszkania Harrisa?",
              "maleVariant": "",
              "stringId": "1782383506447536132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I próbujemy. Chcę znaleźć u Randy'ego jakieś połączenie z Harrisem.",
              "maleVariant": "",
              "stringId": "1782383506447536144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słaby trop.",
              "maleVariant": "",
              "stringId": "1782383506447536156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Najlepszy, jaki mamy.",
              "maleVariant": "",
              "stringId": "1782383506464313344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wywalili cię z policji.",
              "maleVariant": "",
              "stringId": "1782394969077714944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie powiedziełeś mi, że wywalili cię z policji.",
              "maleVariant": "",
              "stringId": "1782400463632732160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Straciłeś odznakę. Dlaczego mi nie powiedziałeś?",
              "maleVariant": "",
              "stringId": "1782487452558168064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wylali cię z policji.",
              "maleVariant": "",
              "stringId": "1782487534045106180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zawiesili mnie. Za głośno protestowałem przeciw tłumieniu smrodu wokół śmierci burmistrza.",
              "maleVariant": "",
              "stringId": "1782489968704360448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nawet nie pytam, jak się z tym czujesz.",
              "maleVariant": "",
              "stringId": "1782490296598269952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słusznie.",
              "maleVariant": "",
              "stringId": "1782491664813789184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm... Nie wiem.",
              "maleVariant": "",
              "stringId": "1782493206992261120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesteś trochę głupi, wiesz?",
              "maleVariant": "",
              "stringId": "1782493312353177600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przyjaźniliśmy się kiedyś. Nie sądzę, żeby to zrobiła.",
              "maleVariant": "",
              "stringId": "1783402570485731328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. Wszystkie nasze sprzeczki kończą się tak samo...",
              "maleVariant": "",
              "stringId": "1783446037299597312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kiedyś przy piwie wszystko ci opowiem.",
              "maleVariant": "",
              "stringId": "1783447658247098368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Racja.",
              "maleVariant": "",
              "stringId": "1783452580413837312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chętnie.",
              "maleVariant": "",
              "stringId": "1783458130853761024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss została sama z trójką dzieciaków, ale to twarda sztuka.",
              "maleVariant": "",
              "stringId": "1815737714720186368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że Yawen nam pomoże?",
              "maleVariant": "",
              "stringId": "1841609618746380288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yawen nie doniesie na ciebie?",
              "maleVariant": "",
              "stringId": "1841609835138912256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Domu. Wątpię, żeby trzymał porwanych w mieszkaniu.",
              "maleVariant": "",
              "stringId": "1908178454259982336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No... Wiem.",
              "maleVariant": "",
              "stringId": "1908216662825918464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mówiąc o tym, że też mam na nią haka. Nawet jeśli nie zamierzam go użyć.",
              "maleVariant": "",
              "stringId": "1908263265267900416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobra, jesteśmy na miejscu.",
              "maleVariant": "",
              "stringId": "1912776659653758976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W porządku. Powiedz mi lepiej, gdzie teraz jedziemy.",
              "maleVariant": "",
              "stringId": "1925708937564393472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do Joss.",
              "maleVariant": "",
              "stringId": "1925710024610549760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do matki Randy'ego.",
              "maleVariant": "",
              "stringId": "1925710242194264064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss myśli, że Randy po prostu zwiał z domu. Nie wspominaj jej na razie o porwaniu.",
              "maleVariant": "",
              "stringId": "1925710454023393280"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}