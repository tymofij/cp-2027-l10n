        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzień dobry, Night City!",
              "maleVariant": "",
              "stringId": "1923976551582420992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przy mikrofonie Stanley. Przed nami kolejny dzień w mieście snów.",
              "maleVariant": "",
              "stringId": "1957459904137457664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kocham Night City, jak kocha się matkę, która oddała cię do bidula, a teraz zaczepia cię na ulicy i pyta: “Czy masz może papierosa?”.",
              "maleVariant": "",
              "stringId": "1957460015370399744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Każdego dnia przyjeżdża tutaj stu nowych frajerów. Do końca roku przeżyje połowa. Jeżeli to będzie dobry rok.",
              "maleVariant": "",
              "stringId": "1957461363503591424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po co frajerzy przyjeżdżają do Night City? Chcą zostać ulicznymi samurajami; być jak Morgan Blackhand czy Weyland Boa Boa. \"Duże ryzyko i duże zdobycze, dziecino\". Tak mówią w Afterlife.",
              "maleVariant": "",
              "stringId": "1957462358577049600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W pierwszej lidze jesteś krótko, bo jak szybko żyjesz, to szybko się spalasz. Najczęściej jednak po prostu dostajesz kulę w łeb.",
              "maleVariant": "",
              "stringId": "1957462807837339648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiecie, gdzie jest najwięcej legend?",
              "maleVariant": "",
              "stringId": "1957463760632213504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na cmentarzach.",
              "maleVariant": "",
              "stringId": "1957463847773073408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie liczy się, skąd przychodzisz.",
              "maleVariant": "",
              "stringId": "1957464304734105600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Liczy się, w jaki sposób idziesz.",
              "maleVariant": "",
              "stringId": "1957464724969811968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City.",
              "maleVariant": "",
              "stringId": "1957465366916427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miasto snów.",
              "maleVariant": "",
              "stringId": "1957465422130245632"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}