  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mike, to jest główny punkt mojego programu. Nie wycofam się tylko dlatego, że Night Corp...",
              "maleVariant": "",
              "stringId": "1238744339447402496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę kończyć. Wprowadźcie poprawki, później to przegadamy.",
              "maleVariant": "",
              "stringId": "1238746235826458624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Miło cię znowu widzieć.",
              "maleVariant": "",
              "stringId": "1277483288074252288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie kończy przygotowania do debaty.",
              "maleVariant": "",
              "stringId": "1277513150881198080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystkie spotkania na dziś zaplanowałam mu poza domem, żeby nikt nam nie przeszkadzał.",
              "maleVariant": "",
              "stringId": "1277521782775607296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiem, czyje fiuty ściskają i kto im włazi do tyłków. Ale mam to gdzieś.",
              "maleVariant": "",
              "stringId": "1283718895691755520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Zapraszam na górę.",
              "maleVariant": "",
              "stringId": "1331697761733169152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki.",
              "maleVariant": "",
              "stringId": "1337115269749403648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co się stało?",
              "maleVariant": "",
              "stringId": "1640233941611974656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chodź, Jefferson wszystko ci wyjaśni.",
              "maleVariant": "",
              "stringId": "1640237850065768448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, dobrze, że jesteś.",
              "maleVariant": "",
              "stringId": "1641291227889537072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Usiądź, proszę. Wszystko ci opowiem.",
              "maleVariant": "",
              "stringId": "1641291227889537088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wezwaliście policję?",
              "maleVariant": "",
              "stringId": "1641297073994338304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego ja?",
              "maleVariant": "",
              "stringId": "1641297073994338336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego akurat ja? Nie lepiej wynająć detektywa?",
              "maleVariant": "",
              "stringId": "1641297074027892736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wolę korzystać ze sprawdzonych kontaktów.",
              "maleVariant": "",
              "stringId": "1641297074027892752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Udało ci się rozwiązać zagadkę śmierci poprzedniego burmistrza.",
              "maleVariant": "",
              "stringId": "1641297074027892768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wezwaliście policję?",
              "maleVariant": "",
              "stringId": "1641297074061447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie możemy sobie pozwolić na szum wokół tej sprawy.",
              "maleVariant": "",
              "stringId": "1641297074061447216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wiesz... Dziennikarze mają swoich informatorów w NCPD.",
              "maleVariant": "",
              "stringId": "1641297074078224396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gdybyśmy zgłosili włamanie, zaraz mielibyśmy na głowie media.",
              "maleVariant": "",
              "stringId": "1641297074078224412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Żadnych gliniarzy, kumam.",
              "maleVariant": "",
              "stringId": "1641297074078224444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W jakim sensie?",
              "maleVariant": "",
              "stringId": "1641297074111778816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To było parę dni temu. Obudził mnie hałas w środku nocy, więc wziąłem pistolet i wyszedłem z sypialni.",
              "maleVariant": "",
              "stringId": "1641297074111778832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zobaczyłem obcego mężczyznę. Wyglądało, jakby sięgał po broń, więc strzeliłem.",
              "maleVariant": "",
              "stringId": "1641297074111778848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trafiłeś?",
              "maleVariant": "",
              "stringId": "1641297074111778864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem, nagle mnie zamroczyło, straciłem przytomność. Rano nie było po nim śladu.",
              "maleVariant": "",
              "stringId": "1641297074128556044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co ustalili wasi ochroniarze?",
              "maleVariant": "",
              "stringId": "1641393064684687360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak wyglądał włamywacz?",
              "maleVariant": "",
              "stringId": "1641393064684687376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy coś zginęło z domu?",
              "maleVariant": "",
              "stringId": "1641393064684687392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak wyglądał włamywacz?",
              "maleVariant": "",
              "stringId": "1641393064718241792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Było ciemno, niewiele pamiętam. Na twarzy miał coś w rodzaju maski...",
              "maleVariant": "",
              "stringId": "1641393064718241808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A może jakiś wszczep? Nie zdążyłem mu się przyjrzeć.",
              "maleVariant": "",
              "stringId": "1641393064718241824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co ustaliła wasza ochrona?",
              "maleVariant": "",
              "stringId": "1641393064751796224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tu sprawa robi się naprawdę podejrzana.",
              "maleVariant": "",
              "stringId": "1641393064751796240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie stwierdzili żadnych śladów włamania. Nie znaleziono też pocisku.",
              "maleVariant": "",
              "stringId": "1641393064751796272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Monitoring niczego nie zarejestrował. Strażnik przy wejściu też...",
              "maleVariant": "",
              "stringId": "1641393064768573444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zasugerowali, że wszystko to musiało się Jeffowi po prostu przyśnić.",
              "maleVariant": "",
              "stringId": "1641393064768573460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wygodne wytłumaczenie...",
              "maleVariant": "",
              "stringId": "1641393064768573476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlatego Liz dała dziś ochronie wolne i zadzwoniłem do ciebie.",
              "maleVariant": "",
              "stringId": "1641393064768573492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy włamywacz coś ukradł? Zniszczył coś?",
              "maleVariant": "",
              "stringId": "1641393064802127872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie sądzę...",
              "maleVariant": "",
              "stringId": "1641393064802127888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie zauważyłam, żeby cokolwiek zniknęło z domu. Możliwe, że Jefferson go spłoszył.",
              "maleVariant": "",
              "stringId": "1641393064802127904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Elizabeth też go nie widziała?",
              "maleVariant": "",
              "stringId": "1641396362414264320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszystko przespałam. Zmęczenie kampanią robi swoje...",
              "maleVariant": "",
              "stringId": "1641396784998780928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mogą kogoś kryć.",
              "maleVariant": "",
              "stringId": "1641420711707938816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wątpię.",
              "maleVariant": "",
              "stringId": "1641420711724716044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A ci ochroniarze z SSI... co to za firma?",
              "maleVariant": "",
              "stringId": "1641420711741493248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Są na rynku od piętnastu lat... Mają klientów w całym mieście.",
              "maleVariant": "",
              "stringId": "1641420711758270468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O ile dobrze pamiętam, polecił mi ich ktoś ze znajomych.",
              "maleVariant": "",
              "stringId": "1641420711758270484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weldon Holt? To mój bezpośredni konkurent. Walczymy o ten sam stołek.",
              "maleVariant": "",
              "stringId": "1641420711791824912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A Holt zaczyna przegrywać w sondażach...",
              "maleVariant": "",
              "stringId": "1641420711791824960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Muszę się tu rozejrzeć.",
              "maleVariant": "",
              "stringId": "1641432513992855552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coś jeszcze, o czym powinnam wiedzieć?",
              "maleVariant": "Coś jeszcze, o czym powinienem wiedzieć?",
              "stringId": "1641432514009632780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W porządku. Muszę rozejrzeć się po mieszkaniu.",
              "maleVariant": "",
              "stringId": "1641432514110296064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście. Elizabeth cię oprowadzi. Ja muszę uciekać na spotkanie. Daj znać, jak coś ustalisz.",
              "maleVariant": "",
              "stringId": "1641432514110296080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W czym właściwie mieliby nam przeszkadzać?",
              "maleVariant": "",
              "stringId": "1746210663926206464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Już za moment wszystkiego się dowiesz.",
              "maleVariant": "",
              "stringId": "1746210828309368832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słusznie.",
              "maleVariant": "",
              "stringId": "1751394353388064768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jefferson.",
              "maleVariant": "",
              "stringId": "1753339715338244096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[usiądź]",
              "maleVariant": "",
              "stringId": "1775015730065559556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oczywiście daliśmy znać o całym zajściu SSI, to znaczy naszej ochronie.",
              "maleVariant": "",
              "stringId": "1804096183083339776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mówiłeś, że straciłeś wtedy przytomność?",
              "maleVariant": "",
              "stringId": "1804163387577470976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kiedy odzyskałeś przytomność?",
              "maleVariant": "",
              "stringId": "1804164040127287296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, jak tylko do niego strzeliłem.",
              "maleVariant": "",
              "stringId": "1804172386842578944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I co było dalej?",
              "maleVariant": "",
              "stringId": "1804172582683021312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nic. Obudziłem się rano we własnym łóżku. Z potwornym bólem głowy.",
              "maleVariant": "",
              "stringId": "1804172677021306880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I żadnego pomysłu, jak znalazłeś się z powrotem w sypialni?",
              "maleVariant": "",
              "stringId": "1804173129100169216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie... nie mam pojęcia. Elizabeth też nie.",
              "maleVariant": "",
              "stringId": "1804173808107655168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślisz, że mogą stać za tym włamaniem?",
              "maleVariant": "",
              "stringId": "1804180490690891776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie mam pojęcia.",
              "maleVariant": "",
              "stringId": "1804182189853773824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie sądzę. Musieliby być albo bardzo głupi, albo aż tak pewni siebie.",
              "maleVariant": "",
              "stringId": "1804194013814853632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Myślę, że SSI może kogoś kryć. Przychodzi ci ktoś do głowy?",
              "maleVariant": "",
              "stringId": "1804197284080128000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem. Przychodzi ci do głowy ktoś jeszcze, kto mógłby za tym stać?",
              "maleVariant": "",
              "stringId": "1804198693819256832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jest coś jeszcze, o czym mi nie powiedziałeś?",
              "maleVariant": "",
              "stringId": "1804212874140323840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm... to chyba wszystko. Elizabeth?",
              "maleVariant": "",
              "stringId": "1804213790729965568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak, wszystko. Pewnie i tak niczego się nie dowiemy...",
              "maleVariant": "",
              "stringId": "1804214096058519552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To co? Chyba możesz już działać...",
              "maleVariant": "",
              "stringId": "1804214451081187328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Daj spokój. Jestem przekonany, że V coś znajdzie.",
              "maleVariant": "",
              "stringId": "1804217966780354560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego akurat ja? Nie lepiej wynająć detektywa?",
              "maleVariant": "",
              "stringId": "1812709282360926212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wolę korzystać ze sprawdzonych kontaktów.",
              "maleVariant": "",
              "stringId": "1812709282360926228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słusznie.",
              "maleVariant": "",
              "stringId": "1812709282360926244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wprawdzie nie udało ci się rozwiązać sprawy śmierci poprzedniego burmistrza...",
              "maleVariant": "",
              "stringId": "1812709282377703428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ufam więc, że mogę ci powierzyć również tę sprawę.",
              "maleVariant": "",
              "stringId": "1812712386733088768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale byłem zadowolony z twojego podejścia do śledztwa i wiem, że mogę ci zaufać.",
              "maleVariant": "",
              "stringId": "1812717570406293504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weldon Holt? W końcu zabił poprzedniego burmistrza.",
              "maleVariant": "",
              "stringId": "1812722187596800004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W dodatku przegrywa z Jeffem w sondażach.",
              "maleVariant": "",
              "stringId": "1812722192277643264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może ci sami ludzie, którzy zamordowali poprzedniego burmistrza?",
              "maleVariant": "",
              "stringId": "1812722314415775748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Albo Weldon Holt. To konkurent Jeffa w wyborach. W dodatku przegrywa w sondażach.",
              "maleVariant": "",
              "stringId": "1812722318945624064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mieliśmy włamanie. I to dosyć dziwne.",
              "maleVariant": "",
              "stringId": "1825411027300388864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlatego chcę, żebyś go znalazła i odkryła jego motywy.",
              "maleVariant": "Dlatego chcę, żebyś go znalazł i odkrył jego motywy.",
              "stringId": "1825414978955513856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przychodzi ci do głowy ktoś jeszcze?",
              "maleVariant": "",
              "stringId": "1825490033223798784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale to mógł być ktokolwiek, naprawdę...",
              "maleVariant": "",
              "stringId": "1873600946220699648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeff ma wielu potężnych wrogów. Możemy tak zgadywać przez cały dzień.",
              "maleVariant": "",
              "stringId": "1873601540754903040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozumiem.",
              "maleVariant": "",
              "stringId": "1873601893730750464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlatego do ciebie zadzwoniłem. Chcę się dowiedzieć, o co tu chodzi.",
              "maleVariant": "",
              "stringId": "1873602064740913152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego wybraliście tak małą firmę ochroniarską?",
              "maleVariant": "",
              "stringId": "1928756579315879936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "SSI to mała firma. Dlaczego nie wybraliście usług Arasaki albo Militechu?",
              "maleVariant": "",
              "stringId": "1928785381936992256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak nam zalecono.",
              "maleVariant": "",
              "stringId": "1928844311975301120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzięki temu nie jesteśmy zależni od wielkiej korporacji.",
              "maleVariant": "",
              "stringId": "1928844428224630784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego zadzwoniłeś akurat do mnie?",
              "maleVariant": "",
              "stringId": "1969274872581459968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dlaczego zadzwoniłeś akurat do mnie?",
              "maleVariant": "",
              "stringId": "1969284992363270144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
