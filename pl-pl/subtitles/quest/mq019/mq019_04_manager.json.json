  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry, zły pokój.",
              "maleVariant": "",
              "stringId": "1944419146780286980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, przepraszam. Zły pokój.",
              "maleVariant": "",
              "stringId": "1944419871019147264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Właśnie... Czy z tego zapisu da się usunąć pewne... Niepożądane cechy?",
              "maleVariant": "",
              "stringId": "1944420030234927104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zdaję sobie sprawę, że nieczęsto dajecie zgodę na użycie Relica... A jeszcze rzadziej zezwalacie na stworzenie engramu żyjącej osoby.",
              "maleVariant": "",
              "stringId": "1944428403206561792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bo Lizzy... Od czasu konwersji... Ona nie jest sobą.",
              "maleVariant": "",
              "stringId": "1944468764339642368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A... O jak poważnych zmianach mówimy?",
              "maleVariant": "",
              "stringId": "1944473740545794052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bardzo poważnych. Czasami... Czasami jej nie poznaję. A czasami...",
              "maleVariant": "",
              "stringId": "1944473740545794060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ej! A ty co tu robisz?!",
              "maleVariant": "",
              "stringId": "1944476554605010944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nieważne. Wypijmy. Za współpracę.",
              "maleVariant": "",
              "stringId": "1944485314845122560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szampan? Miła niespodzianka.",
              "maleVariant": "",
              "stringId": "1944486753524948992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nawet bardzo. Zabieraj się stąd, bo wezwę ochronę.",
              "maleVariant": "",
              "stringId": "1944490181043068928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słucham waszej rozmowy, Liam.",
              "maleVariant": "",
              "stringId": "1944490583830470656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przysyła mnie Lizzy.",
              "maleVariant": "",
              "stringId": "1944491022537891844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja pierdolę, zero prywatności...",
              "maleVariant": "",
              "stringId": "1944491606485671936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wypierdalaj, bo wezwę ochronę.",
              "maleVariant": "",
              "stringId": "1944492200566890496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czy to nie oczywiste, Liam? Podsłuchuję was.",
              "maleVariant": "",
              "stringId": "1944493226661421056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ochrona! Intruz! Do mnie!",
              "maleVariant": "",
              "stringId": "1944495284487626752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lizzy. Wiesz, twoja dziewczyna.",
              "maleVariant": "",
              "stringId": "1944501871222628352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ta, którą chcesz sobie zgrać na drzazgę... Bez pozwolenia.",
              "maleVariant": "",
              "stringId": "1944502047450505220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lubisz tak załatwiać sprawy, co? Sypnąć kasą... A problemy puf!, znikają.",
              "maleVariant": "",
              "stringId": "1944508089932619776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma mowy.",
              "maleVariant": "",
              "stringId": "1944508147159703556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamawialiście szampana?",
              "maleVariant": "",
              "stringId": "1944524667197759488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eee... Zamawialiście szampana, tak?",
              "maleVariant": "",
              "stringId": "1944525214101446656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tak. Już go nawet pijemy.",
              "maleVariant": "",
              "stringId": "1944526277323956224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Okej. Wykładaj kasę.",
              "maleVariant": "",
              "stringId": "1944532327741693952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dawaj kasę. I lepiej, żeby to była spora suma.",
              "maleVariant": "",
              "stringId": "1944532428925083648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pewnie, pewnie. Rozumie się... Już robię przelew.",
              "maleVariant": "",
              "stringId": "1944584332027797504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Potraktuj to jako wyraz mojej wdzięczności.",
              "maleVariant": "",
              "stringId": "1945911819109715968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cóż... Arasaka przygląda się karierze Lizzy z wielkim zainteresowaniem.",
              "maleVariant": "",
              "stringId": "1945914958646923264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A, okej. Przepraszam, w systemie było, że zamówienie do realizacji...",
              "maleVariant": "",
              "stringId": "1945929555613372416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gówno mnie interesuje twój system. Zjeżdżaj.",
              "maleVariant": "",
              "stringId": "1945930212072280064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co? Kto cię tu przysłał? Mów! Ale już!",
              "maleVariant": "",
              "stringId": "1945987248248115200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie będę z tobą gadała.",
              "maleVariant": "Nie będę z tobą gadał.",
              "stringId": "1945994613093617664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chciałbyś wiedzieć, co? Spokojnie. Wszystko w swoim czasie.",
              "maleVariant": "",
              "stringId": "1945995187981701120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Przekażę, komu trzeba, o czym tu sobie gruchaliście... I będzie się działo.",
              "maleVariant": "",
              "stringId": "1945996179179622400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zaciekawi ją wasza rozmowa. Myślę nawet, że bardzo.",
              "maleVariant": "",
              "stringId": "1945999597956812800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hej, hej! Czekaj! Czekaj, kurwa!",
              "maleVariant": "",
              "stringId": "1946000548704866304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Słuchaj, ja... Zapłacę ci, okej? Zapłacę, ile chcesz. Tylko... Zapomnij, co tu było mówione, okej?",
              "maleVariant": "",
              "stringId": "1946003903661527040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O cholera... Słyszałaś to samo, co ja?",
              "maleVariant": "O cholera... Słyszałeś to samo, co ja?",
              "stringId": "1946005083754123264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha. Typ ją zdradza. Bankowo.",
              "maleVariant": "",
              "stringId": "1946007858336550912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No nie wiem... Znaczy, ma coś na sumieniu, to na pewno... Ale tam chodziło o coś innego.",
              "maleVariant": "",
              "stringId": "1946008284276510720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szczegóły. Tak czy inaczej, powinna się trzymać od niego z daleka.",
              "maleVariant": "",
              "stringId": "1946008939108028416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Relic... Chcą z nią zrobić to samo, co ze mną...",
              "maleVariant": "",
              "stringId": "1946009301227458560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obawiam się, że to niemożliwe. Engramu zapisanego w systemie Secure Your Soul nie można edytować.",
              "maleVariant": "",
              "stringId": "1946018749115322368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiedz, czemu chcesz to zrobić.",
              "maleVariant": "",
              "stringId": "1946021117370355712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Może przystanę na tę ofertę. Ale chcę najpierw usłyszeć, czemu to robisz.",
              "maleVariant": "",
              "stringId": "1946021547991158784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Liam, nie. Arasaka stanowczo...",
              "maleVariant": "",
              "stringId": "1946025992124682240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pierdolić Arasakę.",
              "maleVariant": "",
              "stringId": "1946026216285065216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Boję się o nią. Lizzy... Ona chciała przejść pełną konwersję, tak? Wymienić całe ciało na chrom. Zrobiła to... Ale coś poszło nie tak.",
              "maleVariant": "",
              "stringId": "1946026503645220864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na początku miała tylko problemy z pamięcią, z koncentracją, ale teraz... Zmieniła się.",
              "maleVariant": "",
              "stringId": "1946027161345642496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wszyscy się zmieniamy, Liam. Zwykle na gorsze.",
              "maleVariant": "",
              "stringId": "1946027928114745344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie rozumiesz. Kiedy z nią rozmawiam... Jestem przerażony. W niej... Jakby coś... Cholera, no... No n-nie potrafię tego wytłumaczyć...",
              "maleVariant": "",
              "stringId": "1946028906058665984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie szkodzi. Nie mnie się będziesz tłumaczyć.",
              "maleVariant": "",
              "stringId": "1946030217047101440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie, chwila! Zapłacę, ile trzeba, tylko nikomu nie mów... Błagam cię.",
              "maleVariant": "",
              "stringId": "1946032626137878528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spadam. Dopij szampana. Ciepły jest ohydny.",
              "maleVariant": "",
              "stringId": "1946035058012114944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie tym razem, obawiam się.",
              "maleVariant": "",
              "stringId": "1946035909002842112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Byłoby szkoda, gdyby zakończyła się przedwcześnie. Dlatego kopia zapasowa wydaje się rozsądnym rozwiązaniem.",
              "maleVariant": "",
              "stringId": "1963322144276664320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie do końca. Ciebie zabito, a tu chodzi o kopię zapasową.",
              "maleVariant": "",
              "stringId": "1963370484083249152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kopię zapasową! V... Mówimy o człowieku, a nie o folderze ze zdjęciami z wakacji. Facet nawet jej nie spytał, robi to za jej plecami...",
              "maleVariant": "",
              "stringId": "1963376035898904576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie ma prawa, żeby coś takiego robić. Nikt nie powinien mieć takiego prawa.",
              "maleVariant": "",
              "stringId": "1963377114707447808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Na słowo może ci nie uwierzyć.",
              "maleVariant": "",
              "stringId": "1963384484653223936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha. Dlatego ukradnę nagrania z monitoringu.",
              "maleVariant": "",
              "stringId": "1963385265380970496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zwłaszcza bez jej wiedzy. Cholera... Jak jej coś takiego powiedzieć...",
              "maleVariant": "",
              "stringId": "1963385719456321536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie musisz. W pokoju jest kamera... Więc są też nagrania.",
              "maleVariant": "",
              "stringId": "1963386166535573504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nie wiem kim jesteś, ale wpakowałaś się właśnie w poważne kłopoty.",
              "maleVariant": "Nie wiem kim jesteś, ale wpakowałeś się właśnie w poważne kłopoty.",
              "stringId": "1963390791208718336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aha. I za to mi płacą.",
              "maleVariant": "",
              "stringId": "1963391102107308032"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}