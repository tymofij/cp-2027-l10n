        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzień dobry. Witam wszystkich widzów. Nazywam się Arif Iqbal, a to są WNS News.",
              "maleVariant": "",
              "stringId": "1669212525940350976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Szokująca wiadomość. W następstwie zamachu przeprowadzonego w Konpeki Plaza zginął wieloletni CEO Arasaki, Saburo Arasaka.",
              "maleVariant": "",
              "stringId": "1670590870468218880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Choć mordercy udało się uciec, nietrudno odgadnąć, kto go nasłał.",
              "maleVariant": "",
              "stringId": "1670591713657540608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rozejm z Arvin, sygnowany przez prezydent Myers, zobowiązywał wszystkich do zażegnania konfliktu bez użycia siły.",
              "maleVariant": "",
              "stringId": "1670592445228044288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W imieniu całego zespołu redakcji WNS News chciałbym złożyć wyrazy głębokiego współczucia pogrążonej w żałobie rodzinie Arasaki.",
              "maleVariant": "",
              "stringId": "1670592950155137024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To wielka strata dla nas wszystkich. Łączymy się w bólu z bliskimi.",
              "maleVariant": "",
              "stringId": "1670596188711473152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Informację o śmierci Saburo oficjalnie przekazał jego syn, Yorinobu Arasaka.",
              "maleVariant": "",
              "stringId": "1689697309090435072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mimo licznych prowokacji Arasaka honorowała te postanowienia. Nasi rywale postanowili je złamać.",
              "maleVariant": "",
              "stringId": "1689698797128830976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dopilnuję, żeby ten akt agresji nie pozostał bez odpowiedzi.",
              "maleVariant": "",
              "stringId": "1689699214478856192"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
