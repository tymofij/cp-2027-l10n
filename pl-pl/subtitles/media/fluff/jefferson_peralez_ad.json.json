          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W zeszłym roku rada miejska Night City zwolniła z płacenia podatku dochodowego aż trzydzieści dwie korporacje.",
              "maleVariant": "",
              "stringId": "1793610736917716992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Między innymi Kang-Tao, Arasakę, Moore Technologies i Dynalar.",
              "maleVariant": "",
              "stringId": "1803789058476589056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Korporacje, których roczny dochód brutto liczy się w dziesiątkach trylionów eurodolarów.",
              "maleVariant": "",
              "stringId": "1803789243176960000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tę skandaliczną uchwałę przegłosowano w stosunku dziewięćdziesiąt dziewięć \"za\" i jeden \"przeciw\".",
              "maleVariant": "",
              "stringId": "1803789581271416832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ja oddałem ten jeden głos sprzeciwu.",
              "maleVariant": "",
              "stringId": "1803789797361958912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pokazać wam, co się stanie z tą uchwałą, jeśli wy, mieszkańcy Night City, wybierzecie mnie na burmistrza?",
              "maleVariant": "",
              "stringId": "1803790006741614592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nazywam się Jefferson Peralez i zatwierdzam ten przekaz.",
              "maleVariant": "",
              "stringId": "1803790278784172032"
            }
          ]
        }
      }
    },
