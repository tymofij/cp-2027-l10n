          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dobry wieczór, Night City. Tu Gillean Jordan, najnowsze wiadomości N54 News. Zostały już ogłoszone wyniki exit poll wyborów na nowego burmistrza.",
              "maleVariant": "",
              "stringId": "1744646259704651776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Obecny burmistrz, Weldon Holt z partii Dewolucjonistów, otrzymał niecałe 48% głosów, pomimo ciążących na nim podejrzeń o współudział w morderstwie Luciusa Rhyne'a, poprzedniego burmistrza miasta.",
              "maleVariant": "",
              "stringId": "1744677367045484544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wybory wygrał jednak kandydat Federalistów, Jefferson Peralez, na którego głosy oddało ponad 49% wyborców.",
              "maleVariant": "",
              "stringId": "1744677625985036288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W trakcie swojej przemowy Weldon Holt pogratulował konkurentowi zwycięstwa.",
              "maleVariant": "",
              "stringId": "1744678119251963904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Czas pokaże, czy Peralezowi uda się zrealizować ambitne obietnice przedwyborcze.",
              "maleVariant": "",
              "stringId": "1744678493669093376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Po przerwie: Czy Night City Bonecrushers przełamią pechową passę?",
              "maleVariant": "",
              "stringId": "1744678782824411136"
            }
          ]
        }
      }
    },
