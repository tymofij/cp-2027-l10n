        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Witam. Nazywam się Arif Iqbal, a to jest WNS News.",
              "maleVariant": "",
              "stringId": "1792293189345779712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Temat dnia: rutynowa kontrola drogowa naprowadziła NCPD na trop niebezpiecznego porywacza i mordercy.",
              "maleVariant": "",
              "stringId": "1796941990290243584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do incydentu doszło na skrzyżowaniu siódmej i piętnastej alei. Przedstawiamy nagranie z policyjnych kamer.",
              "maleVariant": "",
              "stringId": "1796942180241883136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...10-38, powtarzam, 10-38, rejestracja 2AA6244. Opuszczamy pojazd.",
              "maleVariant": "",
              "stringId": "1796943652325146624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odbiór.",
              "maleVariant": "",
              "stringId": "1796944220871442432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyłaź, już! Ręce do góry!",
              "maleVariant": "",
              "stringId": "1796945110869196800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spokojnie, panie władzo. Bez nerwów.",
              "maleVariant": "",
              "stringId": "1796945592157192192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zamknij się!",
              "maleVariant": "",
              "stringId": "1796945761154088960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O kurwa…",
              "maleVariant": "",
              "stringId": "1796947099321614336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Centrala, 10-53, 10-54, powtarzam, 10-53, 10-54...",
              "maleVariant": "",
              "stringId": "1796948366638956544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chłopiec z nagrania to LeVar Martinez, lat 17. Rodzice zgłosili jego zaginięcie ponad miesiąc temu.",
              "maleVariant": "",
              "stringId": "1796948701914841088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niestety, LeVara nie można było już uratować. Jego organizm nie wytrzymał ostrego wstrząsu septycznego.",
              "maleVariant": "",
              "stringId": "1796948940839174144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sekcja zwłok wykazała, że nastolatkowi podawano bardzo wysokie dawki hormonów, sterydów anabolicznych, a także stymulantów.",
              "maleVariant": "",
              "stringId": "1796949188051451904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W samochodzie, którego właścicielem jest Anthony Harris, znaleziono przedmioty bądź materiał genetyczny należący do siedmiu innych zaginionych nastolatków.",
              "maleVariant": "",
              "stringId": "1796949448014413824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niestety, ich dalszy los pozostaje nieznany. Choć “Piotruś Pan” – jak ochrzciły go niektóre media – przeżył postrzał, dotychczas nie odzyskał przytomności. Jego przesłuchanie jest więc niemożliwe.",
              "maleVariant": "",
              "stringId": "1796949672057356288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będziemy informować o dalszym rozwoju wypadków. A teraz przerwa na reklamy.",
              "maleVariant": "",
              "stringId": "1796949910579036160"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}