          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alberto był wyjątkowym człowiekiem. Wyjątkowym. Każdą przykrość przyjmował z pokorą. Każdą życzliwość – z uśmiechem.",
              "maleVariant": "",
              "stringId": "1318502328777650176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dzisiaj go żegnamy. Ale z lekkim sercem, wiedząc, że spotkamy się z nim znowu, w domu Ojca.",
              "maleVariant": "",
              "stringId": "1318512429399879680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Co... Co, na boga?!",
              "maleVariant": "",
              "stringId": "1319566990218702848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Niech ktoś to wyłączy!",
              "maleVariant": "",
              "stringId": "1319566990218702864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tato! Tatusiu!",
              "maleVariant": "",
              "stringId": "1512790672339447808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Będę tęsknić, tato. Już tęsknię.",
              "maleVariant": "",
              "stringId": "1512800379569516544"
            }
          ]
        }
      }
    },
