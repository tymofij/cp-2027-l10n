        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Powiadam wam! Oto nadchodzi wojna ostateczna, która zmiecie ludzkość z powierzchni ziemi!",
              "maleVariant": "",
              "stringId": "2179534196872343552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bomba z dwudziestego trzeciego była zaledwie jej zwiastunem! Słodkim posłańcem!",
              "maleVariant": "",
              "stringId": "2188348307548127232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Śmierć zrówna nas wszystkich! Runą korporacyjne trony, spłoną baraki nędzarzy!",
              "maleVariant": "",
              "stringId": "2188348380579348480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ciało i stal, chrom i krew stopią się w jedno! Na cmentarzysku świata legną gruzy dawnych potęg!",
              "maleVariant": "",
              "stringId": "2188348496811900928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I nie będzie nikogo, kto by pamiętał o Arasace, Militechu, Biotechnice i Petrochemie!",
              "maleVariant": "",
              "stringId": "2188348565531377664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oto nasza pociecha w godzinie rozpaczy! Są metale szlachetne i pospolite, lecz kościec ludzki jest jeden!",
              "maleVariant": "",
              "stringId": "2188348632891899904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W ten sam proch się obrócą marionetki i ich władcy! Miejcie ufność, bo Santa Muerte dosięgnie każdego!",
              "maleVariant": "",
              "stringId": "2188348688206381056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wyglądajcie dnia sądu! Ufajcie, bo nadchodzi era sprawiedliwej nicości!",
              "maleVariant": "",
              "stringId": "2188348745651568640"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}