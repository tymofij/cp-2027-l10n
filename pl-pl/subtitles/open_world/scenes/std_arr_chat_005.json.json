        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jak pan odpowie na ostatnie zarzuty? W pobliżu waszej elektrowni dochodzi do niepokojących zjawisk.",
              "maleVariant": "",
              "stringId": "1963261700214923264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Petrochem działa w trosce o wspólne dobro mieszkańców Night City.",
              "maleVariant": "",
              "stringId": "1966348923576238080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ale czy wszystkich? Ludzie z Arroyo tracą pamięć, doznają spontanicznych poparzeń...",
              "maleVariant": "",
              "stringId": "1966349165285588992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wie pani co? Przypomina mi pani dziennikarza, który zadawał podobne pytania jakieś pół roku temu.",
              "maleVariant": "",
              "stringId": "1966349418286006272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Z tego, co mi wiadomo, obecnie leży w śpiączce. Ponoć skatowali go nieznani sprawcy.",
              "maleVariant": "",
              "stringId": "1966349634007449600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yyy... pan mi grozi?",
              "maleVariant": "",
              "stringId": "1966349818389053440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proszę napisać o czymś... yyy... bezpieczniejszym. Może o nowym krawacie Ziggy'ego Q?",
              "maleVariant": "",
              "stringId": "1966350112845971456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Masa czytelników się na to rzuci. A pani pożyje dłużej.",
              "maleVariant": "",
              "stringId": "1967630927592669184"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
