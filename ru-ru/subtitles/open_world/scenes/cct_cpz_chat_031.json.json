          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А теперь глубокий вдох… и ме-е-едленный вы-ы-ыдох.",
              "maleVariant": "",
              "stringId": "2172133362346090496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
