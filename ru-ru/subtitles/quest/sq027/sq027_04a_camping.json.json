    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сесть.] Осталось только ждать.",
              "maleVariant": "",
              "stringId": "1757958994757701636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто выдохни и расслабься.",
              "maleVariant": "",
              "stringId": "1757961219349434368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ты так паникуешь, будто ты первая, кто вышел против Сола.",
              "maleVariant": "",
              "stringId": "1785579829227966468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ой, давай только без этого, а? Не надо меня утешать.",
              "maleVariant": "",
              "stringId": "1785579829227966476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А… а я знаю этого человека?",
              "maleVariant": "",
              "stringId": "1785579829227966484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Знала. Скорпион.",
              "maleVariant": "",
              "stringId": "1785579829227966492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Он вернулся с войны накрученный, в голове полно идей, куча новых контактов…",
              "maleVariant": "",
              "stringId": "1785579829227966500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Стал убеждать Сола объединиться со «Змеиным народом». Типа, чтобы стать сильнее.",
              "maleVariant": "",
              "stringId": "1785579829244743680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сесть.] Беспокоишься из-за Сола?",
              "maleVariant": "",
              "stringId": "1805482396761788416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Из-за Сола волнуешься?",
              "maleVariant": "",
              "stringId": "1805483051593306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Он мне никогда про это не рассказывал. А что Сол?",
              "maleVariant": "",
              "stringId": "1805488371883380736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Отчитал его перед всеми за то, что сеет смуту в семье. Потом целый год не давал ему ни одного заказа.",
              "maleVariant": "",
              "stringId": "1805488943650902016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "М-м, вот уж утешил, спасибо.",
              "maleVariant": "",
              "stringId": "1805489713255354368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ты сказала, тебя не надо утешать.",
              "maleVariant": "",
              "stringId": "1805490081951453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Выпить.] Выпьем за Скорпиона.",
              "maleVariant": "",
              "stringId": "1805498382646841344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Насчёт «Змеиного народа» Сол был прав.",
              "maleVariant": "",
              "stringId": "1805498795500572672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вы все мало говорите о войне.",
              "maleVariant": "",
              "stringId": "1805499234258325504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Попробую немного вздремнуть.",
              "maleVariant": "",
              "stringId": "1805501050257739780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давай-ка выпьем за Скорпиона.",
              "maleVariant": "Давайте-ка выпьем за Скорпиона.",
              "stringId": "1805503685874487296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Сол был прав насчёт «Змеиного народа».",
              "maleVariant": "",
              "stringId": "1805504575603806208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вы все мало говорите о войне.",
              "maleVariant": "",
              "stringId": "1805505087845765120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я попробую немного поспать.",
              "maleVariant": "",
              "stringId": "1805508909091590148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давай. Наш конвой в ближайшее время всё равно не приедет.",
              "maleVariant": "",
              "stringId": "1805514716877230080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "За Скорпиона.",
              "maleVariant": "",
              "stringId": "1806703285541081088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А что тут говорить? Запихивали тебя в панцер в центральной Мексике — и марш вперёд, рядовой.",
              "maleVariant": "",
              "stringId": "1806706739147771904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Неделями говоришь сам с собой или с бортовым искином, йодид хлещешь как воду, потому что снаружи счётчик Гейгера зашкаливает.",
              "maleVariant": "",
              "stringId": "1806707030534459392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу. Как вспомню, что мы сидели в этих раскалённых гробах, у меня жопа сыпью покрывается.",
              "maleVariant": "",
              "stringId": "1806719414619672576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага. И несмотря на это, вам не терпится покататься на «Василиске»?",
              "maleVariant": "Хм. И несмотря на это, вам не терпится покататься на «Василиске»?",
              "stringId": "1806720900024045568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Это другое.",
              "maleVariant": "",
              "stringId": "1806721809030385664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Совсем другое. Поймёшь, когда в него сядешь.",
              "maleVariant": "",
              "stringId": "1806721954287521792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нужно оставаться независимыми. Иначе вы разделите судьбу…",
              "maleVariant": "",
              "stringId": "1806736160344838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Судьбу Баккеров?",
              "maleVariant": "",
              "stringId": "1806737201320759296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу.",
              "maleVariant": "Хм.",
              "stringId": "1806744496205271040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мы и сами прекрасно справимся. Нужно только парочку хороших заказов.",
              "maleVariant": "",
              "stringId": "1806744783162773504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поэтому мы сегодня и здесь.",
              "maleVariant": "",
              "stringId": "1806747641899716608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Скорпион бы гордился.",
              "maleVariant": "",
              "stringId": "1806748277437435904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чтобы люди снова заговорили про Альдекальдо.",
              "maleVariant": "",
              "stringId": "1806749266856972288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "И за Джеки.",
              "maleVariant": "",
              "stringId": "1806982561679085568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Придвинуться ближе.] Прохладно стало.",
              "maleVariant": "",
              "stringId": "1873467562821177348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "М-м, как-то вдруг прохладно стало.",
              "maleVariant": "Как-то вдруг прохладно стало.",
              "stringId": "1873468065030361088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ты, похоже, вряд ли сможешь заснуть.",
              "maleVariant": "",
              "stringId": "1873468440554786816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Злишься на меня?",
              "maleVariant": "",
              "stringId": "1873468870303174656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Разбуди меня, если что случится.",
              "maleVariant": "",
              "stringId": "1873583158342733824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так нервничаешь, что сон не идёт?",
              "maleVariant": "Так нервничаешь, что спать не можешь?",
              "stringId": "1873585510340644864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ты на меня сердишься?",
              "maleVariant": "Ты на меня злишься?",
              "stringId": "1873585955205304320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я лучше просто рядом с тобой полежу. Подумаю, посмотрю на звёзды.",
              "maleVariant": "",
              "stringId": "1873586272076582912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Разбуди меня, если что случится. Ну или тоже ложись.",
              "maleVariant": "",
              "stringId": "1873597920682201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Неправда, но это мило.",
              "maleVariant": "",
              "stringId": "1873602703480606720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Может, тоже немного поспишь?",
              "maleVariant": "",
              "stringId": "1873609883172753408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ты в этом тоже разбираешься? У вас, может, и карты неба есть в библиотеке?",
              "maleVariant": "",
              "stringId": "1873652948373622784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Конечно. Вот тут Большая Медведица. А вон там, справа, видишь, звёздочка мигает?",
              "maleVariant": "",
              "stringId": "1873657120682692608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Спокойной ночи, Ви.",
              "maleVariant": "",
              "stringId": "1873657765196222464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я? Нет, с чего бы?",
              "maleVariant": "",
              "stringId": "1873662270331703296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поспать не хочешь?",
              "maleVariant": "",
              "stringId": "1873665480870088704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А, видишь, да? Это значит, что у Большой Медведицы течка!",
              "maleVariant": "",
              "stringId": "1873668483823980544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да-а-а?..",
              "maleVariant": "Э-э-э, да?..",
              "stringId": "1873669289415561216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Выпить.]",
              "maleVariant": "",
              "stringId": "2208601324284833796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Звёзды появились. Значит, до заката всего пара часов.",
              "maleVariant": "",
              "stringId": "2228821801554231296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У нас ещё довольно много времени до заката.",
              "maleVariant": "",
              "stringId": "2228830442961321984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Думаю, ещё раз осмотреть машину не повредит.",
              "maleVariant": "",
              "stringId": "2228834546517360640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви? Ты уснула там, что ли?",
              "maleVariant": "Ви? Ты уснул там, что ли?",
              "stringId": "2228942036328333312"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}