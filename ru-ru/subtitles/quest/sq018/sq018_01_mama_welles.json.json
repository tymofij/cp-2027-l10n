  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви, спасибо, что пришла.",
              "maleVariant": "Ви, спасибо, что пришёл.",
              "stringId": "1697943261782384640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сесть.] Было бы неплохо.",
              "maleVariant": "",
              "stringId": "1697944056284225540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Можно выпить немного.",
              "maleVariant": "Давайте, немного.",
              "stringId": "1697945210154033152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да нет, спасибо.",
              "maleVariant": "",
              "stringId": "1697945291003437056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Сесть.] Нет, спасибо.",
              "maleVariant": "",
              "stringId": "1697945770798260224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я хочу участвовать в церемонии.",
              "maleVariant": "",
              "stringId": "1697948702516092932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я бы хотела принять участие в церемонии, но… как положено. Чтобы не нарушать ваши традиции.",
              "maleVariant": "Я бы хотел принять участие в церемонии, но… как положено. Чтобы не нарушать ваши традиции.",
              "stringId": "1697949664689430528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Могу помочь.",
              "maleVariant": "",
              "stringId": "1697949734381985792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Садись, садись. Хочешь пива?",
              "maleVariant": "",
              "stringId": "1702514181381464064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Выпить.]",
              "maleVariant": "",
              "stringId": "1702514922464649216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давайте я вам помогу.",
              "maleVariant": "",
              "stringId": "1722525784814297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Salud.\" t=\"Твоё здоровье.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1825605937628237824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну ладно.",
              "maleVariant": "",
              "stringId": "1825607631439187968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Бывало и лучше.",
              "maleVariant": "",
              "stringId": "1864865408376164356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Держусь кое-как.",
              "maleVariant": "",
              "stringId": "1864865530866618368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Честно говоря, бывало и получше.",
              "maleVariant": "",
              "stringId": "1864866024754302976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да более-менее.",
              "maleVariant": "",
              "stringId": "1864866333522186240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Почему? Расскажи.",
              "maleVariant": "",
              "stringId": "1864871197220327424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "В тот день, когда погиб Джеки… В общем, с тех пор моя жизнь стала гораздо тяжелее.",
              "maleVariant": "",
              "stringId": "1864871267181318144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Понимаю тебя. Мы все так себя чувствуем.",
              "maleVariant": "",
              "stringId": "1864871351604269056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Более-менее.",
              "maleVariant": "",
              "stringId": "1864871887401439232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Nuestra casa es tu casa.\" b=\"Заехала бы хоть, поужинала с нами. \" a=\"\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"Nuestra casa es tu casa.\" b=\"Заехал бы хоть, поужинал с нами. \" a=\"\"/>",
              "stringId": "1864871950198558720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ну ладно, хватит. Что было, то прошло.",
              "maleVariant": "",
              "stringId": "1864872077336301568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"mi niña.\" b=\"Ну и что, зато у тебя доброе сердце, \" a=\"\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"mi niño.\" b=\"Ну и что, зато у тебя доброе сердце, \" a=\".\"/>",
              "stringId": "1864881922038878208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Нужно просто что-нибудь символическое. Поищи в его гараже. Там, считай, всё его прошлое.",
              "maleVariant": "",
              "stringId": "1864881973729480704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "В гараже?",
              "maleVariant": "",
              "stringId": "1864883745504153604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У Джеки был гараж?",
              "maleVariant": "",
              "stringId": "1864885510282727424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А-ах, да не за что, Ви. Это тебе спасибо за твою чуткость.",
              "maleVariant": "",
              "stringId": "1864886540772888576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Раньше он просто держал там мотоцикл, а не так давно совсем переехал туда жить. Ты там обязательно что-нибудь найдёшь.",
              "maleVariant": "",
              "stringId": "1864886962216554496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ничего, что я буду трогать его вещи?",
              "maleVariant": "",
              "stringId": "1864887180974673920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"No, mija.\" b=\"\" a=\" Он умер. А похороны — они для живых, а не для мёртвых. Вот, возьми ключ.\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"No, mijo.\" b=\"\" a=\" Он умер. А похороны — они для живых, а не для мёртвых. Вот, возьми ключ.\"/>",
              "stringId": "1864887304723419136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ahí te veo.\" t=\"До встречи.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864889318509432832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давно мы с тобой не виделись. Как ты поживаешь?",
              "maleVariant": "",
              "stringId": "1896490727361007616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я сейчас пытаюсь подготовить офренду. Столько надо сделать, рук не хватает…",
              "maleVariant": "",
              "stringId": "1924407058265731072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Gracias, mi cielo\" b=\"\" a=\", я сама справлюсь.\"/>",
              "maleVariant": "",
              "stringId": "1926981639971205120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хватит и того, что ты придёшь проститься.",
              "maleVariant": "",
              "stringId": "1926985406405865472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Просто у меня нет ничего, что можно было бы положить у гроба, чтобы почтить его память.",
              "maleVariant": "",
              "stringId": "1927026960986206208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Так…",
              "maleVariant": "",
              "stringId": "1928580049748815872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Спасибо, Мама Уэллс.",
              "maleVariant": "",
              "stringId": "1928581776711544832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "А что такое офренда?",
              "maleVariant": "",
              "stringId": "1934506005374103552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Офренда? А что это такое?",
              "maleVariant": "",
              "stringId": "1934506268591845376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мы собираемся вместе, чтобы почтить память ушедших. Родные, друзья… Приносим то, что они любили и что может пригодиться им после смерти.",
              "maleVariant": "",
              "stringId": "1934507718629847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Думаю, Джеки в первую очередь попросил бы пива.",
              "maleVariant": "",
              "stringId": "1934508314640445440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ха-ха-ха. И пиво тоже будет.",
              "maleVariant": "",
              "stringId": "1934526562161324032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Гараж на Республиканском проезде.",
              "maleVariant": "",
              "stringId": "1934545599184433152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Взять ключ и встать.] Спасибо.",
              "maleVariant": "",
              "stringId": "1943264135333453824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Надо убрать отсюда бильярдный стол.",
              "maleVariant": "",
              "stringId": "1976395134477742080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Думаете? Он довольно тяжёлый.",
              "maleVariant": "",
              "stringId": "1976445675203407872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "И что?",
              "maleVariant": "",
              "stringId": "1976446243716149248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Может, просто сдвинуть к стене?",
              "maleVariant": "",
              "stringId": "1976446577716965376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да, это я.",
              "maleVariant": "",
              "stringId": "1983628376209567744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Это Гвадалупе Уэллс. Да, из «Койота».",
              "maleVariant": "",
              "stringId": "1983631029392060420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Vamos, responde, carajo.\" t=\"Давай, возьми трубку.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1983631138360078340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Это я. Звоню сказать, что будет офренда по Джеки.",
              "maleVariant": "",
              "stringId": "1983631263484555268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я тебе звоню-звоню, а ты и трубку не берёшь, и не перезваниваешь.",
              "maleVariant": "",
              "stringId": "1988173848277590016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У меня будет офренда. Я должна проводить сына.",
              "maleVariant": "",
              "stringId": "1998391940310036480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да, считай, что тебя пригласили.",
              "maleVariant": "",
              "stringId": "1998392346553544704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Угу. Пока.",
              "maleVariant": "",
              "stringId": "1998392598832541696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "У меня большой заказ.",
              "maleVariant": "",
              "stringId": "1998395163884318720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Двенадцать ящиков пива. Да, десять и два имбирного.",
              "maleVariant": "",
              "stringId": "1998395575093882880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Да, к ним лимоны и лаймы.",
              "maleVariant": "",
              "stringId": "1998396004741607424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Текилы… Ах, да хрен с ним, столько же…",
              "maleVariant": "",
              "stringId": "1998396724970713088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Хм…",
              "maleVariant": "",
              "stringId": "1998405380252561408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ага… Не можешь.",
              "maleVariant": "",
              "stringId": "1998408648806559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Я понимаю.",
              "maleVariant": "",
              "stringId": "1998408909239283712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пф-ф-ф. Ну пока.",
              "maleVariant": "",
              "stringId": "1998409164420739072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ух… Тяжёлый, зараза.",
              "maleVariant": "",
              "stringId": "2238924446696128512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Уф… Ох… Давай, родимый…",
              "maleVariant": "",
              "stringId": "2238983336569782272"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}