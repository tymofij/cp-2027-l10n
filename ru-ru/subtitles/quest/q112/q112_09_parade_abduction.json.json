  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"危険だと警告されたの。オダとも連絡が取れないし、何か変だわ\" t=\"Меня предупредили об опасности. Ода не отвечает. Что-то не так.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669447913484337152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"私が参加したのは、あくまでもお父様を追悼するため\" t=\"Я согласилась на это, только чтобы почтить память отца.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669449579881316352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"操り人形になるためじゃないわ\" t=\"Я не хочу становиться твоей марионеткой.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669450106887225344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"パレードを中止すれば、弱さを露呈したと思われる\" t=\"Если мы отменим торжества, это расценят как проявление слабости.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714211719007453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"弱さ？ 誰の？ あなたの？\" t=\"Слабости? Чьей? Твоей?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212267806965760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"だが、お前が言うなら中止してもいい。全部やめだ。お前の安全が第一だからな\" t=\"Но я понимаю. Только скажи, и мы покончим со всем этим. Твоя безопасность для меня важнее всего.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212716832374784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お父様…\" t=\"Отец…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714220354475409408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お許しを。どうか、話だけでもお聞きください\" t=\"Прошу прощения, Ханако-сама. Пожалуйста, выслушайте меня.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714224100794187776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様は…\" t=\"Ваш отец…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225132743966720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"父のことを口にしないで！\" t=\"Не смей о нём говорить!\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225355629281280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、無礼を働きたくありません。しかし…\" t=\"Ханако-сама, я не хочу причинять вам вред, но…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225488303505408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви, беги!",
              "maleVariant": "",
              "stringId": "1714227320375492608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Блядь! Блядь! Блядь!",
              "maleVariant": "",
              "stringId": "1714228292917149696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"兄さん…\" t=\"Ёринобу…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724068500475875328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"やり終えましょう\" t=\"Давай продолжим.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724113942068727808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありがとう、ハナコ。せっかくの式典だ――やり終えなければ、悔いが残る\" t=\"Спасибо, Ханако. Было бы жаль прерывать такую трогательную церемонию.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724116336999534592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ох…",
              "maleVariant": "",
              "stringId": "1724118030575603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様\" t=\"Ханако-сама.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724119896722116608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"あなたは…\" t=\"Это ты…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724121657927143424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"これが、俺にできる最後のご奉仕なのです\" t=\"Это последнее, что я могу для него сделать.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724149966509576192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様の死について真実を明かすことが\" t=\"Открыть вам правду о его смерти.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724150642950148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"この見せ物では、みんなに役割がある。ミチコは文句を言ってないぞ\" t=\"У всех нас есть роли в этом спектакле. Митико отлично играет свою.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724223690596290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"なぜ止まったんだ？\" t=\"Почему ты остановилась?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724231251970101248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"俺たちだ\" t=\"Нашей.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724233151520051200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не знаю, что он сделал, но мы в полной жопе.",
              "maleVariant": "",
              "stringId": "1771027611004047360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ханако внутри.",
              "maleVariant": "",
              "stringId": "1773724698127908864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Пытается кому-то звонить.",
              "maleVariant": "",
              "stringId": "1773725998731247616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Подслушай.",
              "maleVariant": "",
              "stringId": "1773726175479218176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Не забудь выключить сигнализацию. Тогда я смогу войти.",
              "maleVariant": "",
              "stringId": "1773726519227596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Горо? Все системы заблочены. Пора действовать.",
              "maleVariant": "Горо? Системы заблочены. Можем начинать.",
              "stringId": "1773729105720655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Что она делает?",
              "maleVariant": "",
              "stringId": "1780695188894519296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Поторопись, Ви!",
              "maleVariant": "",
              "stringId": "1790741196538200068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ви, скоро будет поздно!",
              "maleVariant": "",
              "stringId": "1790741196554977284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Вы не имеете права нас задерживать!",
              "maleVariant": "",
              "stringId": "1795474982084558848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Все на землю! Живо!",
              "maleVariant": "",
              "stringId": "1795475678641012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Иди на хер! Это нарушение гражданских свобод!",
              "maleVariant": "",
              "stringId": "1795476053259468800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Стоять!",
              "maleVariant": "",
              "stringId": "1795479668128759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Кому тут ещё гражданских свобод не хватает? Можете к нему присоединиться!",
              "maleVariant": "",
              "stringId": "1795483864295030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чуть не затоптали… Кажется, рёбра сломаны…",
              "maleVariant": "",
              "stringId": "1801295829407907840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Давай отсюда выбираться!",
              "maleVariant": "",
              "stringId": "1801296487729086464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Как я выгляжу? Пора выходить в эфир.",
              "maleVariant": "",
              "stringId": "1801297323049893888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Три, два, один…",
              "maleVariant": "",
              "stringId": "1801298631119093760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Здравствуйте, с вами Джиллиан Джордан и «Новости-54» в прямом эфире из Джапан-тауна, где парад в честь покойного Сабуро Арасаки окончился массовой паникой.",
              "maleVariant": "",
              "stringId": "1801299000486281216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Многочисленные свидетели, которых мы опросили, сообщают о стрельбе на одной из платформ. Именно там должна была находиться в это время Ханако Арасака.",
              "maleVariant": "",
              "stringId": "1801299245702070272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "К сожалению, другой информацией мы пока не располагаем.",
              "maleVariant": "",
              "stringId": "1801300495923429376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Мы ожидаем официального заявления от корпорации «Арасака» в связи с этими событиями.",
              "maleVariant": "",
              "stringId": "1801300622893400064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Эй, ты! Не лезь в кадр!",
              "maleVariant": "",
              "stringId": "1801303594255794176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Чёрт! Снимаем ещё раз!",
              "maleVariant": "",
              "stringId": "1801304953780715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Спасибо! Вообще-то тут люди работают!",
              "maleVariant": "",
              "stringId": "1801305575091355648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ситуация, кажется, выходит из-под контроля. По возможности избегайте Кандзаки-cтрит.",
              "maleVariant": "",
              "stringId": "1863315822830493696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Знал, что ему нельзя доверять! Надо было держать его в узде.",
              "maleVariant": "",
              "stringId": "1906936140640829440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ноги в руки и сваливаем отсюда.",
              "maleVariant": "",
              "stringId": "1906937283303456768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Он её грохнул?",
              "maleVariant": "",
              "stringId": "2007084788928552960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Деактивировать систему охраны.]",
              "maleVariant": "",
              "stringId": "2034753975066525696"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}