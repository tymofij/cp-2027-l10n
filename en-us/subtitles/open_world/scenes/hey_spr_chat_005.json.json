        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Electronic Murderer,\" I got one word – masterpiece. Didn't quite click till that last scene...",
              "maleVariant": "",
              "stringId": "1831685761031331840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then boom! All the pieces came together!",
              "maleVariant": "",
              "stringId": "1831687367449763840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The heroine, it was 'cause of her outdated optics model she got hacked!",
              "maleVariant": "",
              "stringId": "1831687527269523456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Little bit of horror, little bit sci-fi. Perfect combo.",
              "maleVariant": "",
              "stringId": "1831687823622266880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The bodies were never there! That netrunner was feeding her hallucinations.",
              "maleVariant": "",
              "stringId": "1831687973291810816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It broke her down, she couldn't take it – took to the noose.",
              "maleVariant": "",
              "stringId": "1965082878785863680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bottom line – keep current on your chrome. Think we've been in her shoes, haha.",
              "maleVariant": "",
              "stringId": "1965083077059002368"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
