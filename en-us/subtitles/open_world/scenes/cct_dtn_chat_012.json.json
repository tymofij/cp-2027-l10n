        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, it's just the constant bickering over every little thing!",
              "maleVariant": "",
              "stringId": "2182443368390692864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's how it is with guys. They need to think they're the ones making all the decisions.",
              "maleVariant": "",
              "stringId": "2182504727182675968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Next meeting, listen to every single one of their suggestions. One after the other.",
              "maleVariant": "",
              "stringId": "2182513907977261056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know, pretend to be interested, throw 'em a little praise and say \"yeah, let's do it.\"",
              "maleVariant": "",
              "stringId": "2182521950337077248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thing is, their ideas are usually shit.",
              "maleVariant": "",
              "stringId": "2182522091517349888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doesn't matter. You say one thing, then do it all your own way anyway. Just pretend it was their idea.",
              "maleVariant": "",
              "stringId": "2182522186006630400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't get it. Won't they notice?",
              "maleVariant": "",
              "stringId": "2182523416514760704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honey, it's not about what anyone does, it's about how they feel. Their ego. You just gotta give it a little stroke.",
              "maleVariant": "",
              "stringId": "2182525765878648832"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}