          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My mother fought in the war. Never talked about it – not even once.",
              "maleVariant": "",
              "stringId": "1963274831959990272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who was leading Arasaka? Was it Yorinobu?",
              "maleVariant": "",
              "stringId": "1966168316728557568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You joking? Back then, Yorinobu didn't want anything to do with his father's empire.",
              "maleVariant": "",
              "stringId": "1966169661153984512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Orders trickled down from Kei Arasaka. At least – officially.",
              "maleVariant": "",
              "stringId": "1966169943061544960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not a lot of people these days remember – Saburo had two sons.",
              "maleVariant": "",
              "stringId": "1966170574371405824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And so the world keeps turning.",
              "maleVariant": "",
              "stringId": "1966170758920781824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Progressing.",
              "maleVariant": "",
              "stringId": "1966171010042150912"
            }
          ]
        }
      }
    },
