          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So... Militech made me an offer.",
              "maleVariant": "",
              "stringId": "2167595004638023680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pipe the fuck down...",
              "maleVariant": "",
              "stringId": "2214549718478196736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think I should take it? Should I tell Tom? Feel like I kinda owe it to him – after all, he hired me…",
              "maleVariant": "",
              "stringId": "2229135248905834496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What? Don't be gonk. How much more they offer you?",
              "maleVariant": "",
              "stringId": "2229137906702061568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sixty percent.",
              "maleVariant": "",
              "stringId": "2229139635845828608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sixty?! All right, here's what's gonna happen. You get them to make me the same offer, or I'll tell everyone you contacted Militech. I don't have to remind you what Arasaka does to spies, do I?",
              "maleVariant": "",
              "stringId": "2229139870458417152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait... what? You're joking, right?",
              "maleVariant": "",
              "stringId": "2229143487072538624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What can I say... shoulda kept your mouth shut.",
              "maleVariant": "",
              "stringId": "2229144870169448448"
            }
          ]
        }
      }
    },
