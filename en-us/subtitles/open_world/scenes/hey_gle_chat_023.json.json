        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is it just me, or did this use to be a better neighborhood?",
              "maleVariant": "",
              "stringId": "2161788985688535040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, not just you. Used to be less firefights. And way less cyberpsychosis.",
              "maleVariant": "",
              "stringId": "2177914670113603584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'ask me, think it's high time the mayor started arming the city.",
              "maleVariant": "",
              "stringId": "2177914831527198720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I mean, some psycho barges in your store and boom! Everyone starts sprayin' lead at him!",
              "maleVariant": "",
              "stringId": "2177915026881101824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, yeah, that's how it should be. Boom! Problem solved.",
              "maleVariant": "",
              "stringId": "2177915218510462976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Should be more guns on the street, y'know? That's the problem with Night City.",
              "maleVariant": "",
              "stringId": "2177915710569431040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But what if, you know... what if some gonk shoots someone by accident?",
              "maleVariant": "",
              "stringId": "2177915833512869888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then too bad. Better to make a mistake than eat lead yourself.",
              "maleVariant": "",
              "stringId": "2177916100320935936"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}