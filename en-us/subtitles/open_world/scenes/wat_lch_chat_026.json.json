        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh my God I got the BEST idea! We'll give her a present when we go to their show!",
              "maleVariant": "",
              "stringId": "2156529816405569536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They wrote on their site that they're lovers of expensive tea...",
              "maleVariant": "",
              "stringId": "2172091033477431296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Duuuude, yes! She'll totally remember us after! How nova'd that be?",
              "maleVariant": "",
              "stringId": "2172092221790863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just imagine. Few years go by and suddenly oops! We bump into each other on the street...",
              "maleVariant": "",
              "stringId": "2172094217944023040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And she'll be like, \"Hey, you're those Night City girls! You made me that AMAZING tea cup!",
              "maleVariant": "",
              "stringId": "2172094716948758528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, idea's solid, 'cept how're we gonna get backstage?",
              "maleVariant": "",
              "stringId": "2172095176745140224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll think of something.",
              "maleVariant": "",
              "stringId": "2172095474456838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe we could make her a cup? Y'know, like the kind you drink matcha with?",
              "maleVariant": "",
              "stringId": "2230243414958129152"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
