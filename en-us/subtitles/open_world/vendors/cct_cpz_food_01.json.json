          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna see your menu.",
              "maleVariant": "",
              "stringId": "1783748166422384640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could use a drink.",
              "maleVariant": "",
              "stringId": "1935648808663166980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll have a drink.",
              "maleVariant": "",
              "stringId": "1977633281005879296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What kinds of wines do you have?",
              "maleVariant": "",
              "stringId": "1977634070139011072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Throat's feelin' parched. What've you got?",
              "maleVariant": "",
              "stringId": "1977634721497645056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What kinds of wines do you have?",
              "maleVariant": "",
              "stringId": "1977635100327182336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe I didn't drink Champaradise every day, but Chateau Piquette was all the rage back in Arasaka.",
              "maleVariant": "",
              "stringId": "1979276688365436928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We cater to the cream of the corporate elite. I believe our wines would be sliiightly beyond your price range.",
              "maleVariant": "",
              "stringId": "1979279357855383552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "By the looks of you, I doubt their names would tell you anything.",
              "maleVariant": "",
              "stringId": "1979288358311673856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We had good wine at Arasaka.",
              "maleVariant": "",
              "stringId": "1979289225693741056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh! Uh... p-please forgive me – a misunderstanding, that's all! Would you like to see the wine list?",
              "maleVariant": "",
              "stringId": "1979302918317916160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll take something to drink.",
              "maleVariant": "",
              "stringId": "1979338355270991872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How long've you run this place?",
              "maleVariant": "",
              "stringId": "1979338429342400512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take something to drink.",
              "maleVariant": "",
              "stringId": "1979338646456352768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How long've you been runnin' this place?",
              "maleVariant": "",
              "stringId": "1979339209734602752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right away, miss. Arasaka clients are our always first priority.",
              "maleVariant": "Right away, sir. Arasaka clients are always our first priority.",
              "stringId": "1979342116487938048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Long as I can remember... Grandad opened the original merchant's back in Cambridge, and then father brought it here – to Night City.",
              "maleVariant": "",
              "stringId": "1979365591839653888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Drink]",
              "maleVariant": "",
              "stringId": "4108827857818365964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Step away from bar]",
              "maleVariant": "",
              "stringId": "4108827871894450184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit at bar] Could use a drink.",
              "maleVariant": "",
              "stringId": "4115800971752124416"
            }
          ]
        }
      }
