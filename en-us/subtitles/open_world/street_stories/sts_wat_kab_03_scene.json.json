  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stop!",
              "maleVariant": "",
              "stringId": "1923080477339914240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm here by contract, from a fixer, nothin' personal.",
              "maleVariant": "",
              "stringId": "1923082299630784516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fixer sent me.",
              "maleVariant": "",
              "stringId": "1923083357685911556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "GPS transmitter. The one you forgot to disable on the stolen meds.",
              "maleVariant": "",
              "stringId": "1924328600439721984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "GPS signal.",
              "maleVariant": "",
              "stringId": "1925693695782653952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, sure. And you be careful with that trigger.",
              "maleVariant": "",
              "stringId": "1925745996824154112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hand over the stolen meds and I'm gone.",
              "maleVariant": "",
              "stringId": "1925760444288831488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stolen?! Hahaha! The baloperidol doesn't work! S-so who's stealing from who?! Who?!",
              "maleVariant": "",
              "stringId": "1925852223159767040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stolen?! Hahaha! The baloperidol doesn't work! S-so who's stealing from who?! Who?!",
              "maleVariant": "",
              "stringId": "1925852426331852800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just hand 'em over and I'm gone.",
              "maleVariant": "",
              "stringId": "1925885647769669632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "H-how'd you find me?",
              "maleVariant": "",
              "stringId": "1925940552937082880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaaargh! I’m not fucking crazy!",
              "maleVariant": "",
              "stringId": "1925950590997073920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Baloperidol? For cyberpsychosis?",
              "maleVariant": "",
              "stringId": "1947640913972269056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Easy, easy…",
              "maleVariant": "",
              "stringId": "1947696791211577344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sh-shut up! Go away!",
              "maleVariant": "",
              "stringId": "1947697116840562688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If it doesn't work, give it to me.",
              "maleVariant": "",
              "stringId": "1947698691751383040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give it, or I call MaxTac.",
              "maleVariant": "",
              "stringId": "1947699443605540864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is our situation.",
              "maleVariant": "",
              "stringId": "1947700634100011008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If they're no use to you, they don't work, just hand 'em over.",
              "maleVariant": "",
              "stringId": "1947700927516741632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ripperdocs! They're the real criminals!",
              "maleVariant": "",
              "stringId": "1947707187599347712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're the same as them… rippers, politicians… the same as that fucking Holt!",
              "maleVariant": "",
              "stringId": "1947710731618455552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His death would change nothing.",
              "maleVariant": "",
              "stringId": "1947710974116335620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're a soldier, not a terrorist.",
              "maleVariant": "",
              "stringId": "1947738819647606784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I see we're past talking.",
              "maleVariant": "",
              "stringId": "1947746420951191552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do what you want, but give me the drugs.",
              "maleVariant": "",
              "stringId": "1947747827536203776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, I don't really care.",
              "maleVariant": "",
              "stringId": "1947748228931096580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna murder a mayor? Fine. You think it'll make a difference? Sure, why not.",
              "maleVariant": "",
              "stringId": "1947748228931096600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just need those meds.",
              "maleVariant": "",
              "stringId": "1947748228931096620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ha, ha, ha… I see… It's all so clear…",
              "maleVariant": "",
              "stringId": "1947748228931096640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Last marble just hit the floor. Can't let this drag on.",
              "maleVariant": "",
              "stringId": "1947749449775550468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're part of the system… a--a pawn in it… a slave to it…",
              "maleVariant": "",
              "stringId": "1947749449792327696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's just fight.",
              "maleVariant": "",
              "stringId": "1947783465027342336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We know where this's headed, rather not waste my time.",
              "maleVariant": "",
              "stringId": "1947783730224795648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I appreciate your candor.",
              "maleVariant": "",
              "stringId": "1947784044076175360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A Militech HKS-47 with polymer reinforcement. You were a soldier.",
              "maleVariant": "",
              "stringId": "1947788844926865412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Militech. 3rd Light Division, 155th Infantry Regiment, 7 years in a fuckin' jungle… When times were simpler.",
              "maleVariant": "",
              "stringId": "1947788844926865432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before you got sick?",
              "maleVariant": "",
              "stringId": "1947788844943642636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They cast me out like a dead battery.",
              "maleVariant": "",
              "stringId": "1947788844943642656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I understand your anger.",
              "maleVariant": "",
              "stringId": "1947788844993974272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where did you serve?",
              "maleVariant": "",
              "stringId": "1947788844993974304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're angry, and I get it. You feel cheated.",
              "maleVariant": "",
              "stringId": "1947788845111414788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not just me!",
              "maleVariant": "",
              "stringId": "1947788845128192000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know, but if you zero Holt, you'll make it worse for everyone in your condition. They'll hunt you down.",
              "maleVariant": "",
              "stringId": "1947788845128192020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd you serve?",
              "maleVariant": "",
              "stringId": "1947788845161746436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brazil. R-remember all of it… Dirt on my face… F-fatigues always--always wet…",
              "maleVariant": "",
              "stringId": "1947788845178523664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's like a braindance I-I-I can't shut off…",
              "maleVariant": "",
              "stringId": "1947788845178523684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The memories flood back?",
              "maleVariant": "",
              "stringId": "1947788845195300864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Without the adrenaline. Without my brothers… Fuck! So quickly they forgot me…",
              "maleVariant": "",
              "stringId": "1947788845195300884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's campaign season. The first ribbon-cutter to push an anti-psycho policy after Holt's death is winnin' that election.",
              "maleVariant": "",
              "stringId": "1947811611894190080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No! No, that's not true! The people will understand! The system will start to fall!",
              "maleVariant": "",
              "stringId": "1947827423497523200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone like you, they'll hunt you all down.",
              "maleVariant": "",
              "stringId": "1947829203795021824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave… Go, please…",
              "maleVariant": "",
              "stringId": "1947859677208743936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know I can't without those meds.",
              "maleVariant": "",
              "stringId": "1947860220941537280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Over there… I wanna be alone.",
              "maleVariant": "",
              "stringId": "1947860594855350272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But… but soon… I'll find him. End him.",
              "maleVariant": "",
              "stringId": "1954556513713754112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Other poltician's will make use of his death.",
              "maleVariant": "",
              "stringId": "1954558463662460928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They'll say you're a psycho.",
              "maleVariant": "",
              "stringId": "1954558734463504384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The death of one politician won't change anything.",
              "maleVariant": "",
              "stringId": "1954558802679664640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you kill Holt, first thing they'll do is brand you a psycho and a terrorist.",
              "maleVariant": "",
              "stringId": "1954559218989502464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh.",
              "maleVariant": "",
              "stringId": "1954609936916123648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It will! These--these silver-tongue devils won't dare turn on their people ever again!",
              "maleVariant": "",
              "stringId": "1954615156341575680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A line, I'm drawing a line! They can't treat us this way anymore! They--they--they can't use us anymore!",
              "maleVariant": "",
              "stringId": "1954654076714455040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It’s not about ME! This is for everyone! All the sick!",
              "maleVariant": "",
              "stringId": "1954676251546718208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, this'd be a disservice to them. What're people gonna remember?",
              "maleVariant": "",
              "stringId": "1954676793014587392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cyberpsycho terrorist on one side, media-backed politicians on the other. Now, who's the crowd favorite?",
              "maleVariant": "",
              "stringId": "1954695649212862464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I could think of a few differences.",
              "maleVariant": "",
              "stringId": "1954893056714313728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut UP! That bastard, he stole from--from research! He doesn't care about the sick!",
              "maleVariant": "",
              "stringId": "1954897356496670720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Cyberpsycho assassin kills in cold blood.\" Who'd wanna help a monster like that?",
              "maleVariant": "",
              "stringId": "1954928971734159360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Either you give me what you stole.",
              "maleVariant": "",
              "stringId": "1957975186767974400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How--how can you work for them?",
              "maleVariant": "",
              "stringId": "1957975431597887488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or MaxTac swoops in, pays you a visit.",
              "maleVariant": "",
              "stringId": "1957975704596746240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You ordinary… sell-out… piece of SHIT!!!",
              "maleVariant": "",
              "stringId": "1957976673631965184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's no reason to become a terrorist.",
              "maleVariant": "",
              "stringId": "1957999533914165248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear that howling at night?",
              "maleVariant": "",
              "stringId": "1958020391030329344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like a dyin' animal. Waaargh! Aaaagh!",
              "maleVariant": "",
              "stringId": "1958021105169305600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know… And we can't ask them or we will repay them for the rest of our lives!",
              "maleVariant": "",
              "stringId": "1958021136341372928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shh, shh, quiet!",
              "maleVariant": "",
              "stringId": "1958034090231173120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh… Can't call the police  on him or it'll rile up the Tygers.",
              "maleVariant": "",
              "stringId": "1958034876696727552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta do somethin'! Can't have him here.",
              "maleVariant": "",
              "stringId": "1958039733281329152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know…",
              "maleVariant": "",
              "stringId": "1958039984822128640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I told you not to move!",
              "maleVariant": "",
              "stringId": "2004013097929965568"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}