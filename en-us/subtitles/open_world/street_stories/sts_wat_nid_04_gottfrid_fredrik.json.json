  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too far away to hear us...",
              "maleVariant": "",
              "stringId": "1104218161305669632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Preacher's boy – who murdered him? Who gave you the XBD?",
              "maleVariant": "",
              "stringId": "1105015127132393472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The braindance of the preacher boy's murder.",
              "maleVariant": "",
              "stringId": "1105020462874284052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD of a preacher's little boy. Murdered. Ring any bells?",
              "maleVariant": "",
              "stringId": "1105023953491513344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't tell anyone you're here – we swear!",
              "maleVariant": "",
              "stringId": "1105025314123730944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-which preacher?",
              "maleVariant": "",
              "stringId": "1105033016661037056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just got a fresh batch with boys, no clue who their daddies are... Gonna need more.",
              "maleVariant": "",
              "stringId": "1105033313634537472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Papa, I think I know the one she wants. BD_9430.",
              "maleVariant": "Papa, I think I know the one he wants. BD_9430.",
              "stringId": "1105033455418789888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bryce Stone, circuit rider on NEWS 54. Son was Bobby Stone, twelve years old when he died.",
              "maleVariant": "",
              "stringId": "1105034233445404672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody gives us anything. They leave dead drops. We just go pick 'em up.",
              "maleVariant": "",
              "stringId": "1105041237647872000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wakey wakey, gentlemen.",
              "maleVariant": "",
              "stringId": "1229747213316890624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wakey wakey, gentleman.",
              "maleVariant": "",
              "stringId": "1229753900413198336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Papa! Why? Why did you have to kill him...?",
              "maleVariant": "",
              "stringId": "1229762424513994752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freddy! No!",
              "maleVariant": "",
              "stringId": "1229766769477394432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's it. That's the one. Just take it!",
              "maleVariant": "",
              "stringId": "1238864078437851136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keeps things secure, for everyone involved...",
              "maleVariant": "",
              "stringId": "1301017744899350528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cut. Right there.",
              "maleVariant": "",
              "stringId": "1509830974825140224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right before he gets -?",
              "maleVariant": "",
              "stringId": "1509831631904804864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaddyou think? Else it'll feel like stabbin' a pillow.",
              "maleVariant": "",
              "stringId": "1509831732853313536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK... Pullin' up by one frame... Augmenting pain feedback and ferrous aftertaste...",
              "maleVariant": "",
              "stringId": "1509841697563316224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freddyboy, you're a natural!",
              "maleVariant": "",
              "stringId": "1509843837815984128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ughhhh... Cuttin' that sweat-stench.",
              "maleVariant": "",
              "stringId": "1509844104020070400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No no, leave it! Some go for that extra kick.",
              "maleVariant": "",
              "stringId": "1509844606782902272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good thinking, Papa!",
              "maleVariant": "",
              "stringId": "1509845150918348800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our only concern is with imperfections, not who makes them.",
              "maleVariant": "",
              "stringId": "1538820716891574272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freddy! Fredrik, answer me right now!",
              "maleVariant": "",
              "stringId": "1663260809168785408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't do this, don't make me do this all on my own, please...",
              "maleVariant": "",
              "stringId": "1663262625637961728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD of the dead preacher's boy – where is it?",
              "maleVariant": "",
              "stringId": "1663272091276120068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD of the dead preacher's boy – where is it?",
              "maleVariant": "",
              "stringId": "1663272166756814848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y-you killed him! You killed him you fucking... F-fucking...",
              "maleVariant": "",
              "stringId": "1663355943717801984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where is it?!",
              "maleVariant": "",
              "stringId": "1663356477971468288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please, just fucking go!",
              "maleVariant": "",
              "stringId": "1663361225873264640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll do anything, please, just let me live!",
              "maleVariant": "",
              "stringId": "1663374724066283520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD of the dead preacher's boy – where is it?",
              "maleVariant": "",
              "stringId": "1663375234798292996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why, why, why...",
              "maleVariant": "",
              "stringId": "1663377444626075648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't leave me, Freddy, please...",
              "maleVariant": "",
              "stringId": "1663378126670237696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh my boy, my sweet sweet boy...",
              "maleVariant": "",
              "stringId": "1663378191447068672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD of the dead preacher's boy. Hand it over.",
              "maleVariant": "",
              "stringId": "1663385085238677504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-which preacher? Just tell me which one and it's yours! Free of charge!",
              "maleVariant": "",
              "stringId": "1663385085255454720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Father Stone from NEWS 54.",
              "maleVariant": "",
              "stringId": "1663385085255454732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Behind me on the shelf under BD_9430.",
              "maleVariant": "",
              "stringId": "1663385085272231940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take it and leave! Please, please don't kill me!",
              "maleVariant": "",
              "stringId": "1663385085272231952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't shoot! We don't record them!",
              "maleVariant": "",
              "stringId": "1663385085322563584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBDs fall into our lap, we don't ask any questions!",
              "maleVariant": "",
              "stringId": "1663385085372895232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We use dead drops, OK? They deliver, we pick up. That's it, I swear!",
              "maleVariant": "",
              "stringId": "1663385085406449664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This your little family business?",
              "maleVariant": "",
              "stringId": "1663422765473304576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-what's happening?! Security!",
              "maleVariant": "",
              "stringId": "1663464030596337664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "L-let's not do anything rash here. We just tune and sell. We don't scroll – honest to God!",
              "maleVariant": "",
              "stringId": "1663465381900423168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-we can figure this out. Just tell us what you want and we'll make it happen.",
              "maleVariant": "",
              "stringId": "1663466323672666112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a little family affair goin' here, huh?",
              "maleVariant": "",
              "stringId": "1663491635257118720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People wanna buy, so we sell. Business is business.",
              "maleVariant": "",
              "stringId": "1663520945691004928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lookin' for somethin' in particular?",
              "maleVariant": "",
              "stringId": "1663521236943474688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give it to me.",
              "maleVariant": "",
              "stringId": "1663529755340017668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kindly hand it the fuck over. Now!",
              "maleVariant": "",
              "stringId": "1663529985976406016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who killed the preacher's son?",
              "maleVariant": "",
              "stringId": "1663530127190233088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "XBD was raw when you got it. Must've noticed something in the background, some kinda clue...",
              "maleVariant": "",
              "stringId": "1663577092288516096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right! Please, just relax.",
              "maleVariant": "",
              "stringId": "1663608798374629376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just take it!",
              "maleVariant": "",
              "stringId": "1663609085751562240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freddy! Ma'am... it's just behind us on the shelf.",
              "maleVariant": "Freddy! Sir... it's just behind us on the shelf.",
              "stringId": "1663610772063096832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "... reflection in the mirror, tattoo, anything.",
              "maleVariant": "",
              "stringId": "1671035068821639168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are we talking black, white, chicano...? Any identifying marks? Style of execution?",
              "maleVariant": "",
              "stringId": "1671044194838614016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD_9430.",
              "maleVariant": "",
              "stringId": "1671051628319707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Put out two cuts: sweat in, sweat out, hehe.",
              "maleVariant": "",
              "stringId": "1682321146501238784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit! Help!",
              "maleVariant": "",
              "stringId": "1682372100231630848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't shoot!",
              "maleVariant": "",
              "stringId": "1682373048949641216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "L-let's not do anything rash here. We just tune and sell. We don't scroll – honest to God!",
              "maleVariant": "",
              "stringId": "1682507384285925376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-we can figure this out. Just tell us what you want and we'll make it happen.",
              "maleVariant": "",
              "stringId": "1682508103105744896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know, if it wasn't us, there'd be someone else.",
              "maleVariant": "",
              "stringId": "1682550204623327232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y-you wanna see somethin' really hardcore?",
              "maleVariant": "",
              "stringId": "1682551416005431296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freddy!",
              "maleVariant": "",
              "stringId": "1682551530560262144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We just supply what Night City demands. That's all!",
              "maleVariant": "",
              "stringId": "1682552070283300864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No...no, no, no... My only boy... Stay with me...",
              "maleVariant": "",
              "stringId": "1702833915927302144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enough. I'll find it myself.",
              "maleVariant": "",
              "stringId": "1702868416007081984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had enough of you two. I'll find it myself.",
              "maleVariant": "",
              "stringId": "1702868988831567872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O-OK...",
              "maleVariant": "",
              "stringId": "1702872417305989120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut up. You speak, you die.",
              "maleVariant": "",
              "stringId": "1702872754477699072"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}