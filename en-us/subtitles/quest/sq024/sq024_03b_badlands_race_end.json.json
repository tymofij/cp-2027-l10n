    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We did it, V, we won!",
              "maleVariant": "",
              "stringId": "1782456050122760196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're doin' well.",
              "maleVariant": "",
              "stringId": "1782456050189869060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drive up to the finish line.",
              "maleVariant": "",
              "stringId": "1782456050240200708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How'm I doin'?",
              "maleVariant": "",
              "stringId": "1782456050374418436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your last driver was better than I am?",
              "maleVariant": "",
              "stringId": "1782456050458304516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1782456050458304524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He was.",
              "maleVariant": "",
              "stringId": "1782456050458304532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A good husband, an amazing wheelman.",
              "maleVariant": "",
              "stringId": "1782456050458304540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want a lift to my garage?",
              "maleVariant": "",
              "stringId": "1782456050575745028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure thing.",
              "maleVariant": "",
              "stringId": "1782456050609299456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, thanks.",
              "maleVariant": "",
              "stringId": "1782456050609299472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, thanks.",
              "maleVariant": "",
              "stringId": "1782456050626076676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Find my own way there, thanks.",
              "maleVariant": "",
              "stringId": "1782456050659631108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your last driver better than me?",
              "maleVariant": "",
              "stringId": "1782456050693185536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuuuuck!",
              "maleVariant": "",
              "stringId": "1824282514711126016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasn't easy.",
              "maleVariant": "Heheh, wasn't easy.",
              "stringId": "1857417701380722688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like, chance at the final well?",
              "maleVariant": "",
              "stringId": "1857418579449876480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better believe it.",
              "maleVariant": "",
              "stringId": "1857419021160419328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Piss poor, that.",
              "maleVariant": "",
              "stringId": "1857419711609966592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poor? My fuckin' bank account's poor. That was a fucking DISASTER!",
              "maleVariant": "",
              "stringId": "1857419806334128128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, see you.",
              "maleVariant": "",
              "stringId": "1857428512115396608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go.",
              "maleVariant": "",
              "stringId": "1857428904685473792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How'm I doin'?",
              "maleVariant": "",
              "stringId": "1857451799813951488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're too wide around corners and slow down the last straightaway, but I'm nitpicking.",
              "maleVariant": "",
              "stringId": "1857454113190711296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So no regrets puttin' me behind the wheel?",
              "maleVariant": "",
              "stringId": "1857474085828825088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not yet.",
              "maleVariant": "",
              "stringId": "1857475150561284096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How'd he die?",
              "maleVariant": "",
              "stringId": "1857651099735035908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How did he die, if you don't mind my askin'?",
              "maleVariant": "How'd he die, if you don't mind me askin'?",
              "stringId": "1857651142533713920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, you're racin' for him?",
              "maleVariant": "",
              "stringId": "1857651165199732736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You doin' this for him?",
              "maleVariant": "",
              "stringId": "1857651246535675904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How do any competitive racers die?",
              "maleVariant": "",
              "stringId": "1857652459176071168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We reached the final last year. We were strong, out in front when…",
              "maleVariant": "",
              "stringId": "1857653003428958208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1857653710521503744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe we should change the subject.",
              "maleVariant": "",
              "stringId": "1857654124583194624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah.",
              "maleVariant": "",
              "stringId": "1857655344773337088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He adored this shit. Had a keen mind for machines, unmatched. He could talk to 'em…",
              "maleVariant": "",
              "stringId": "1857656614221385728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When he got behind the wheel, it was like he fused with it. But just him – no implants.",
              "maleVariant": "",
              "stringId": "1857658895285227520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaddaya say we win this for him?",
              "maleVariant": "",
              "stringId": "1859055615294443520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm countin' on you.",
              "maleVariant": "",
              "stringId": "1859055784123568128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our winners today – V and Claire!",
              "maleVariant": "",
              "stringId": "1936238282875736064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Luigi and Margot are our winners!",
              "maleVariant": "",
              "stringId": "1936239383326887936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No way... Just no fuckin' way! Gah!",
              "maleVariant": "",
              "stringId": "2237461226216099848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Damn good, gents 'n' ladies, damn good!",
              "maleVariant": "",
              "stringId": "2237461226283208712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why, no way the missus'll believe I bet on the winnin' horse…",
              "maleVariant": "",
              "stringId": "2237461226367094792"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}