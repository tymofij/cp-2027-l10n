  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ladies and gentlemen! Get your pants on right, strap in and fire up your engines!",
              "maleVariant": "",
              "stringId": "1395316292542537728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Three!",
              "maleVariant": "",
              "stringId": "1397864879895400448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two!",
              "maleVariant": "",
              "stringId": "1397866326594736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One!",
              "maleVariant": "",
              "stringId": "1397866444605673472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1397866521596317696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Claire. Next race – you ready?",
              "maleVariant": "",
              "stringId": "1751898020796964864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Second of four, I hope. Ready to go off-road?",
              "maleVariant": "",
              "stringId": "1751899302358163456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fucking sandstorm... Careful, V.",
              "maleVariant": "",
              "stringId": "1753410021805268992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bumpy up ahead.",
              "maleVariant": "",
              "stringId": "1783629146116595712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They're gaining.",
              "maleVariant": "",
              "stringId": "1783629176986673152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The guy in front, don't let him pull away.",
              "maleVariant": "",
              "stringId": "1783629208276180992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hold on, mag's empty, reloading...",
              "maleVariant": "",
              "stringId": "1783629234113093632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good, one gunner down.",
              "maleVariant": "",
              "stringId": "1783629270234439680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's ours to lose now, V. Stay focused.",
              "maleVariant": "",
              "stringId": "1783629300366319616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It'll be a tough one. Off-road, lots of lead flying.",
              "maleVariant": "",
              "stringId": "1832824050658844672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll take Beast. Wanna try her out in the field.",
              "maleVariant": "",
              "stringId": "1832824292452081664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nova. FYI, I'm already here.",
              "maleVariant": "",
              "stringId": "1856281910533296128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Perfect. Join you in a sec.",
              "maleVariant": "",
              "stringId": "1856282299513049088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds good, see ya.",
              "maleVariant": "",
              "stringId": "1856290053053091840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did you say it'll be tough?",
              "maleVariant": "",
              "stringId": "1856291920441118724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think this one'll be tough?",
              "maleVariant": "",
              "stringId": "1856292026338906112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like Aldecaldos fronted a solid team.",
              "maleVariant": "",
              "stringId": "1856292045263605760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not a fan-friendly event, is it?",
              "maleVariant": "S'not a fan-friendly event, is it?",
              "stringId": "1856292068768485376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lotta nomads here.",
              "maleVariant": "",
              "stringId": "1856292632164175872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't see many fans.",
              "maleVariant": "",
              "stringId": "1856292676858679296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Badlands, so no fucking around. Anything goes in this race.",
              "maleVariant": "",
              "stringId": "1856293799086657536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who'd wanna bake in this sun?",
              "maleVariant": "",
              "stringId": "1856294628401221632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Speed, carnage, a crunching metal symphony? Yeah, dull stuff.",
              "maleVariant": "",
              "stringId": "1856294932001722368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is their race, or so they think.",
              "maleVariant": "",
              "stringId": "1856296728774447104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are they wrong?",
              "maleVariant": "",
              "stringId": "1856297039337492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nomad team usually wins this one, so no, guess not.",
              "maleVariant": "",
              "stringId": "1856297156308242432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So you'll have your hands full.",
              "maleVariant": "",
              "stringId": "1856297800670777344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You too. One mistake and we'll be a four-ton tumbleweed.",
              "maleVariant": "",
              "stringId": "1856298334555344896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can do this.",
              "maleVariant": "",
              "stringId": "1856299040423153664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, I'm just gonna melt under that sun, sooo… gonna wait in the car.",
              "maleVariant": "",
              "stringId": "1856299347832082432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure.",
              "maleVariant": "",
              "stringId": "1856299848833306624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hold tight, Claire.",
              "maleVariant": "",
              "stringId": "1856320349567848448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Follow a race horse too tight ya might get kicked.",
              "maleVariant": "Ya follow a race horse too tight ya might get kicked.",
              "stringId": "1856320780272537600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am trying, you know.",
              "maleVariant": "",
              "stringId": "1856321858980417536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The sand lie light on his gut.",
              "maleVariant": "",
              "stringId": "1856323240869367808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got it.",
              "maleVariant": "",
              "stringId": "1856324243643568128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thirty seconds and counting!",
              "maleVariant": "",
              "stringId": "1935735591246315520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, you!",
              "maleVariant": "",
              "stringId": "1935737086012375040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear me?",
              "maleVariant": "",
              "stringId": "1935739234720759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You there! Hey!",
              "maleVariant": "",
              "stringId": "1935739433916645380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ignore him. Luigi tries to get in people's heads.",
              "maleVariant": "",
              "stringId": "1935842942427152384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's up?",
              "maleVariant": "",
              "stringId": "1935844450229108736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, what's up?",
              "maleVariant": "",
              "stringId": "1935845019463270400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just wanted to wish you luck.",
              "maleVariant": "",
              "stringId": "1935845998950694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks.",
              "maleVariant": "",
              "stringId": "1935847870281367552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got another race to run, huh.",
              "maleVariant": "",
              "stringId": "1938662150018043904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, V. Good to see you again.",
              "maleVariant": "",
              "stringId": "1938662242728939520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2055025166241579012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit and wait]",
              "maleVariant": "",
              "stringId": "2056328576408567808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I really thought you'd stand me up again. I thought Bouli would have to ride shotgun.",
              "maleVariant": "",
              "stringId": "2237382175312973828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, kid. You're to shoot holes in those Aldecaldo rat bastards and not give a shit about the rest.",
              "maleVariant": "",
              "stringId": "2237382175413637120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Out in the Badlands Luigi's the man!",
              "maleVariant": "",
              "stringId": "2237382175480745992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, guys, remember! Maximum madness!",
              "maleVariant": "",
              "stringId": "2237382175581409288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bouli is a fantastic shot.",
              "maleVariant": "",
              "stringId": "2247736558789726208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He is, he is... but he's talking, always. Too much. His mouth never stops running. It's distracting – I take the turns too wide.",
              "maleVariant": "",
              "stringId": "2247737851675545600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So next time I actually don't make it, there's duct tape in the trunk. Just sayin'.",
              "maleVariant": "",
              "stringId": "2247739784410828800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, yeah? It's obvious?",
              "maleVariant": "",
              "stringId": "2247759225815175168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is your first time out, kid – nothing's fuckin' obvious. Fail to take them out along the first leg, there'll be no scratch at the finish line, guaranteed.",
              "maleVariant": "",
              "stringId": "2247759452357922816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know, I know... \"The Badlands – our lands,\" \"Us or them\" and so on... I've heard it, hundreds of times.",
              "maleVariant": "",
              "stringId": "2247761661481062400"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}