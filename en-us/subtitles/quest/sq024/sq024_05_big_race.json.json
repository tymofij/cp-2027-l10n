        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, hey.",
              "maleVariant": "",
              "stringId": "1388242201008414720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck me... This isn't happening.",
              "maleVariant": "",
              "stringId": "1389347495155343360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You satisfied? Was it worth it?",
              "maleVariant": "",
              "stringId": "1389360633812623360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, folks. Get to your rigs. Time to crunch some steel and bash some heads!",
              "maleVariant": "",
              "stringId": "1395319513533128704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Three!",
              "maleVariant": "",
              "stringId": "1399625779895652352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two!",
              "maleVariant": "",
              "stringId": "1399626150705680384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One!",
              "maleVariant": "",
              "stringId": "1399626228753289216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1399626321279635456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No offense, but I need to be alone, walk this off. OK?",
              "maleVariant": "",
              "stringId": "1399643428201484288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course.",
              "maleVariant": "",
              "stringId": "1399675823562739712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We won.",
              "maleVariant": "",
              "stringId": "1399682503126077440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Calm down.",
              "maleVariant": "",
              "stringId": "1399682779614597120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, we won.",
              "maleVariant": "",
              "stringId": "1399683462447288320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, calm down. Did what I did to keep you outta trouble.",
              "maleVariant": "Claire, calm down. I did what I did to keep you outta trouble.",
              "stringId": "1399683525076635648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm outta here.",
              "maleVariant": "",
              "stringId": "1399683683034124288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not what I was racing for!",
              "maleVariant": "",
              "stringId": "1399743264733057024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone ready?",
              "maleVariant": "",
              "stringId": "1405263834904055808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can't fuckin' handle it…",
              "maleVariant": "",
              "stringId": "1405322960346599424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're done, Sampson.",
              "maleVariant": "",
              "stringId": "1754678470284156928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No remorse, not a bit?",
              "maleVariant": "",
              "stringId": "1754679086729404416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "None whatsoever.",
              "maleVariant": "",
              "stringId": "1754679377562443776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hhhh. Do somethin' for me, V?",
              "maleVariant": "",
              "stringId": "1754682517233868800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "After him, V! Don't let him slip away!",
              "maleVariant": "",
              "stringId": "1754685178989518848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got one shot at this!",
              "maleVariant": "",
              "stringId": "1754687656262901760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's take the wind out of his sails.",
              "maleVariant": "Let's take the wind outta his sails.",
              "stringId": "1754688223819341824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We missed the turn.",
              "maleVariant": "",
              "stringId": "1754688948863508480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm headin' for the finish line, Claire.",
              "maleVariant": "",
              "stringId": "1754689783228014592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the fuck did you just do?!",
              "maleVariant": "",
              "stringId": "1754691364765835264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I finished the race.",
              "maleVariant": "",
              "stringId": "1754691804362448896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That was my ONE chance, don't you get it?!",
              "maleVariant": "",
              "stringId": "1754692123364433920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That prick hasn't lost us yet. Still got a chance…",
              "maleVariant": "",
              "stringId": "1754693115904532480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There he is!",
              "maleVariant": "",
              "stringId": "1754693692688441344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "After him, V!",
              "maleVariant": "",
              "stringId": "1754693850293608448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get away from me, you whack.",
              "maleVariant": "",
              "stringId": "1766172875084013568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Final's upon us, V. Big day's here.",
              "maleVariant": "",
              "stringId": "1834161703744446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll take mine.",
              "maleVariant": "",
              "stringId": "1834162228737089536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beast it is.",
              "maleVariant": "",
              "stringId": "1834162479187369984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go with mine. Got a better feel for it.",
              "maleVariant": "Let's go with mine. I got a better feel for it.",
              "stringId": "1834162657747279872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beast's my choice. Whaddaya say?",
              "maleVariant": "",
              "stringId": "1834162664424611840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We meetin' at the starting line?",
              "maleVariant": "We meetin' at the startin' line?",
              "stringId": "1834162747673157632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No problem.",
              "maleVariant": "",
              "stringId": "1834163220455104512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We won't be sorry.",
              "maleVariant": "",
              "stringId": "1834163456627974144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure is. Wanna meet up? I'm already here.",
              "maleVariant": "",
              "stringId": "1861561520321859584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yep, but listen... this'll be highway drivin', so we'll want a machine that runs on lightning.",
              "maleVariant": "",
              "stringId": "1861562249543553024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your choice – your rig or my Beast?",
              "maleVariant": "",
              "stringId": "1861564217561321472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's right. Wait for me there.",
              "maleVariant": "",
              "stringId": "1861566484112871424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, leave the prick alone.",
              "maleVariant": "",
              "stringId": "1861575844423667716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, drop it. Leave the corporat alone.",
              "maleVariant": "",
              "stringId": "1861575891416649728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's goin' on here?",
              "maleVariant": "",
              "stringId": "1861575918646071296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's going on?",
              "maleVariant": "",
              "stringId": "1861576028016742400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think you'll leave here alive, asswipe?",
              "maleVariant": "",
              "stringId": "1861578017408684032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire!",
              "maleVariant": "",
              "stringId": "1861579283769724928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sampson... Dickspurt thinks I'm gonna let him go.",
              "maleVariant": "",
              "stringId": "1861579852819337216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're dead, you hear me?",
              "maleVariant": "",
              "stringId": "1861580429720686592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This your new driver?",
              "maleVariant": "",
              "stringId": "1861581142450376704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd get out while I could if I were you. Claire can't seem to keep her partners alive for long.",
              "maleVariant": "",
              "stringId": "1861581918983819264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You motherfucker!",
              "maleVariant": "",
              "stringId": "1861586390044774400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire! Stop!",
              "maleVariant": "",
              "stringId": "1861587199981654020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, stop it! We got a race comin' up, dammit!",
              "maleVariant": "",
              "stringId": "1861588294208458752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Truth is, Dean was as shit a husband as he was a driver.",
              "maleVariant": "",
              "stringId": "1861660028634796032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Head swelled lots larger than he could manage, and he paid for it. Face the facts.",
              "maleVariant": "",
              "stringId": "1861660215063220224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I didn't kill him – his ego did.",
              "maleVariant": "",
              "stringId": "1861660478146744320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're a waste of organic matter, Sampson...",
              "maleVariant": "",
              "stringId": "1861660826055872512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're done...",
              "maleVariant": "",
              "stringId": "1861765741822480384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck off.",
              "maleVariant": "",
              "stringId": "1861765896021872640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, c'mon. Save some steam for later.",
              "maleVariant": "",
              "stringId": "1861766097935667200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're corpo trash.",
              "maleVariant": "",
              "stringId": "1861768726321434624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a dozen bullets with your name on 'em.",
              "maleVariant": "",
              "stringId": "1861769866635251712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck off.",
              "maleVariant": "",
              "stringId": "1861770330894372864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, enough, c'mon.",
              "maleVariant": "",
              "stringId": "1861770462830399488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah?",
              "maleVariant": "",
              "stringId": "1861773095158812672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come in close on his tail and stay there.",
              "maleVariant": "",
              "stringId": "1861773173105758208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get on his butt.",
              "maleVariant": "",
              "stringId": "1861814408164020224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know.",
              "maleVariant": "",
              "stringId": "1861816914294214656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stop. I'm getting out.",
              "maleVariant": "",
              "stringId": "1861820501262995456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's your thirst for revenge, not mine.",
              "maleVariant": "",
              "stringId": "1861823944249708548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saved your ass.",
              "maleVariant": "",
              "stringId": "1861824099103412224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanted to win.",
              "maleVariant": "",
              "stringId": "1861824150424915968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This's your hunt for vengeance, not mine. Enlisted me to race, not to kill in cold blood.",
              "maleVariant": "",
              "stringId": "1861825885994356736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire, I saved your ass.",
              "maleVariant": "",
              "stringId": "1861825907469193216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I came out here to win this race.",
              "maleVariant": "",
              "stringId": "1861825931762601984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck the race! He killed my husband! The only man I ever loved!",
              "maleVariant": "",
              "stringId": "1861827048001114112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The only man who ever loved me…",
              "maleVariant": "",
              "stringId": "1861827395658584064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You saved my…? No, you saved his ass! And now he's outta my reach for good!",
              "maleVariant": "",
              "stringId": "1861828206048448512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't give a thought to Trauma Team? He gets in an accident, they swoop in, chromed to the teeth and packin' iron!",
              "maleVariant": "",
              "stringId": "1861828829758230528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you did. Oh happy fucking day.",
              "maleVariant": "",
              "stringId": "1861830506120876032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire...",
              "maleVariant": "",
              "stringId": "1861832992923717632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Justice – you took that away from me.",
              "maleVariant": "",
              "stringId": "1861833071273316352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You robbed me of my one chance to find justice for the man I loved!",
              "maleVariant": "",
              "stringId": "1861833420507844608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hope you're pleased with yourself.",
              "maleVariant": "",
              "stringId": "1861834211050262528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That scum who got away murdered the only man I ever loved. You get that?",
              "maleVariant": "",
              "stringId": "1861834881316818944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't like to lose, Claire. That's it. Your vengeance isn't my biz.",
              "maleVariant": "",
              "stringId": "1861835971315765248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, your victory? I hope you fuckin' choke on it.",
              "maleVariant": "",
              "stringId": "1861837409878151168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His car's damaged! He's pullin' off!",
              "maleVariant": "",
              "stringId": "1887814515837378560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We can still get him! Turn back!",
              "maleVariant": "",
              "stringId": "1887851474987261952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2054867889689804800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Lean and wait]",
              "maleVariant": "",
              "stringId": "2054869672252227584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not to worry, sugar! Champagne gonna give you your favorite massage afterwards!",
              "maleVariant": "",
              "stringId": "2237419384292380680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remember – tires not people. Bust their rubber, then I'll ram 'em.",
              "maleVariant": "",
              "stringId": "2237419384376266760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire 'n' V – that's who's gon' win this! Y'all got no chance! None!",
              "maleVariant": "",
              "stringId": "2237419384476930056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hope you wreck 'round the first bend, motherfucker! Lose your arms! And your legs!",
              "maleVariant": "",
              "stringId": "2237419384577593352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Madre\" b=\"Holy \" a=\" of the Angels, Queen of the Heavens, send your heavenly hosts so they may carry us to victory.\"/>",
              "maleVariant": "",
              "stringId": "2237419384661479432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You kick butt out there, Sampson! Race is yours!",
              "maleVariant": "",
              "stringId": "2237419384745365512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And in one piece! Impressive!",
              "maleVariant": "",
              "stringId": "2237435603112300552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you! Thanks for fucking nothing!",
              "maleVariant": "",
              "stringId": "2237435603212963848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Woot! Yeah! What a chase! I mean –  what a race!!",
              "maleVariant": "",
              "stringId": "2237435603313627144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bring some friends. We'll celebrate.",
              "maleVariant": "",
              "stringId": "2250141057290473472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you win today, we gon' take you straight to paradise, muffin!",
              "maleVariant": "",
              "stringId": "2250142137810292736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No fucking \"if\" about it.",
              "maleVariant": "",
              "stringId": "2250143657423409152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ruslana and her crew're positioned along the route. They’ll clean up.",
              "maleVariant": "",
              "stringId": "2250156757711196160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And Trauma, what then? There's at least one prick here, maybe more, who must have gold if not platinum.",
              "maleVariant": "",
              "stringId": "2250157929314521088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ruslana's people know what to do. We provide bodies – the more the better – they take it from there.",
              "maleVariant": "",
              "stringId": "2250159247936589824"
            }
          ]
        }
      }
    },
