  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice ride.",
              "maleVariant": "",
              "stringId": "1734409387521351680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Claire. Helluva machine.",
              "maleVariant": "",
              "stringId": "1734409387689123840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, you know your shit, and you've got good taste.",
              "maleVariant": "",
              "stringId": "1734409387689123856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beer in the fridge if you want any.",
              "maleVariant": "",
              "stringId": "1734409387705901068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When do we start?",
              "maleVariant": "",
              "stringId": "1734409388192440320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got my own ride.",
              "maleVariant": "",
              "stringId": "1734409388209217544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What're the stakes?",
              "maleVariant": "",
              "stringId": "1734409388209217560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to ride with my husband, Dean. But then, 'bout a year ago…",
              "maleVariant": "",
              "stringId": "1734409388813197312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1734409388829974528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There was a cock-up.",
              "maleVariant": "",
              "stringId": "1734409388829974540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep tellin' myself he died quickly, didn't suffer. Best way to go.",
              "maleVariant": "",
              "stringId": "1734409388829974552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wow. Really sorry, Claire…",
              "maleVariant": "",
              "stringId": "1734409388829974564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seems like you've turned over a new engine, though.",
              "maleVariant": "",
              "stringId": "1734409388829974576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "True, people're like cars. We gotta keep movin'.",
              "maleVariant": "",
              "stringId": "1734409388846751752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There a carrot we're chasin' after?",
              "maleVariant": "",
              "stringId": "1734409389282959360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First place gets a payout. That happens, we split it fifty-fifty. Sound fair?",
              "maleVariant": "",
              "stringId": "1734409389299736584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need a driver. I thought you might do.",
              "maleVariant": "",
              "stringId": "1785290523034001408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I'm drivin', why not use my wheels?",
              "maleVariant": "",
              "stringId": "1785292613089239040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think it's got what it takes? This isn't just about speed. It's a gauntlet of twisted steel.",
              "maleVariant": "",
              "stringId": "1785292805742010368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When do we start?",
              "maleVariant": "",
              "stringId": "1785293402457251840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's a qualifying ladder – need to climb it to reach the finals.",
              "maleVariant": "",
              "stringId": "1785293533353091072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Top three in at least two rallies gets our ticket stamped.",
              "maleVariant": "",
              "stringId": "1785293605612560384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's meet in City Center. I'll snap you the specifics. Hear the roar of overtuned monsters – that's the place.",
              "maleVariant": "",
              "stringId": "1786416755398156288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rest easy. I don't drive a soap box racer.",
              "maleVariant": "",
              "stringId": "1827282789298348032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Driver, eh? With you as navigator?",
              "maleVariant": "",
              "stringId": "1854851610842370052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A driver, huh? With you as my navigator?",
              "maleVariant": "",
              "stringId": "1854851691909877760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's the race like? Any special rules?",
              "maleVariant": "",
              "stringId": "1854851728802975744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There a race of some sort?",
              "maleVariant": "",
              "stringId": "1854852433966141440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not exactly. This kinda racin' involves drivers and gunners.",
              "maleVariant": "",
              "stringId": "1854854663205163008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lack of 'em, actually. You race in pairs – driver and gunner.",
              "maleVariant": "",
              "stringId": "1854857732663939072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, you a turret guru or a driver's ed dropout?",
              "maleVariant": "",
              "stringId": "1854859025583312896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm more an engine tuner – good at it, too. And I shoot, but I'm no rally racer.",
              "maleVariant": "",
              "stringId": "1854860587072360448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I trust you're capable behind the wheel?",
              "maleVariant": "",
              "stringId": "1854861507671756800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll be your driver.",
              "maleVariant": "",
              "stringId": "1854862729740947460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, Claire, I'll be your driver.",
              "maleVariant": "",
              "stringId": "1854862801815867392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But this won't be your first rodeo, will it?",
              "maleVariant": "",
              "stringId": "1854862854680875008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But this won't be your first time out, will it?",
              "maleVariant": "",
              "stringId": "1854863822944342016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't worry. Done my fair share o' rounds.",
              "maleVariant": "",
              "stringId": "1854864674119618560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And your last driver? They quit?",
              "maleVariant": "",
              "stringId": "1854867430716870656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In a way. Died in a race a year back.",
              "maleVariant": "",
              "stringId": "1854867825686089728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, sorry...",
              "maleVariant": "",
              "stringId": "1854868629398622208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1854870345674264576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, we got four races. First one's in City Center.",
              "maleVariant": "",
              "stringId": "1854871048555728896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your car, mine... You'll be on the pedals, you decide.",
              "maleVariant": "",
              "stringId": "1854910158829916160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got goosebumps already.",
              "maleVariant": "",
              "stringId": "1854910866308337664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How high?",
              "maleVariant": "",
              "stringId": "1854912558944882688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That'll be followed by the Badlands and Santo, then Watson to finish.",
              "maleVariant": "",
              "stringId": "1863062461453832192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, baby...",
              "maleVariant": "",
              "stringId": "1935548074635116544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, I'm here.",
              "maleVariant": "",
              "stringId": "1935549826763669504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Claire. V?",
              "maleVariant": "Hey, Claire. It's V.",
              "stringId": "1935550558887821312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meet Beast – my pride and joy.",
              "maleVariant": "",
              "stringId": "1935552135526694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, yeah, yeah, great. Uh, just gimme a sec?",
              "maleVariant": "",
              "stringId": "1935927010288340992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meet Beast.",
              "maleVariant": "",
              "stringId": "1935927380611829760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks. So what's this about?",
              "maleVariant": "",
              "stringId": "1935931751730909184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya there.",
              "maleVariant": "",
              "stringId": "1944782724081934336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, fine. I'll be your driver.",
              "maleVariant": "",
              "stringId": "1962021937652703236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not my thing, I don't think.",
              "maleVariant": "",
              "stringId": "1962022230448676864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doesn't really seem like my thing, Claire.",
              "maleVariant": "",
              "stringId": "1962022598406578176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, beg to differ. Seem like the ideal prospect to me.",
              "maleVariant": "",
              "stringId": "1962022942054293504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I'm willin'. You have fun with it.",
              "maleVariant": "",
              "stringId": "1962023161584164864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I'm willin'. You have fun, though.",
              "maleVariant": "",
              "stringId": "1962023296909189120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh… Fine. You brought me around.",
              "maleVariant": "Oh, fine, fine. You brought me 'round.",
              "stringId": "1962023312948207616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lightning speed, rush of adrenaline, the roar of engines… Willing to let all that fun pass you by?",
              "maleVariant": "",
              "stringId": "1983919625960820736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Damn shame…",
              "maleVariant": "",
              "stringId": "1983927096267902976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Claire?",
              "maleVariant": "",
              "stringId": "2003779718919307268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh... Claire?",
              "maleVariant": "",
              "stringId": "2003782011391655936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Under the rig!",
              "maleVariant": "",
              "stringId": "2003787748763320320"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}