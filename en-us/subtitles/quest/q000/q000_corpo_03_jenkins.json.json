  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have a seat, V. Be right with you.",
              "maleVariant": "",
              "stringId": "1663288107007385600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know what to do. Start now.",
              "maleVariant": "",
              "stringId": "1664894783925952512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As we agreed – I got rid of the problem.",
              "maleVariant": "",
              "stringId": "1664921029732900864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I told you to resolve the problem not massacre the European Space Council. Do you understand what the cover-up will cost?",
              "maleVariant": "",
              "stringId": "1664925576408768512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I hate that bitch. I always have.",
              "maleVariant": "",
              "stringId": "1664925927035805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard]",
              "maleVariant": "",
              "stringId": "1664955908491223044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I think we're clear.",
              "maleVariant": "",
              "stringId": "1664965843421667328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need your help.",
              "maleVariant": "",
              "stringId": "1664982889727434752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's what I've collected over the past weeks.",
              "maleVariant": "",
              "stringId": "1665076234868924416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A deep clean. After the shit Frankfurt dumped on us.",
              "maleVariant": "",
              "stringId": "1686759468713308160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I watched the vote. What the hell was that?",
              "maleVariant": "",
              "stringId": "1686760140338819072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have to handle this voting issue before we lose our bases in the Sea of Clouds.",
              "maleVariant": "",
              "stringId": "1686787344930107392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They're about to begin. What's the situation?",
              "maleVariant": "",
              "stringId": "1686799714234593280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Path of least resistance, always. No nuance. Has you written all over it.",
              "maleVariant": "",
              "stringId": "1686827792566513664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're not done talking about this.",
              "maleVariant": "",
              "stringId": "1686827966210699264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A datashard?",
              "maleVariant": "",
              "stringId": "1686870942559432704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proof of my trust.",
              "maleVariant": "",
              "stringId": "1686871674381594624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on, take it.",
              "maleVariant": "",
              "stringId": "1686871967194345472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Use this money only, find the right people for the job – no corporate affiliation.",
              "maleVariant": "",
              "stringId": "1686880382746554368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Biometrics, Trauma Team card, names of her closest associates – driver, chief of security, lover, lover's husband. Everything.",
              "maleVariant": "",
              "stringId": "1691204358805319680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Viper pit's putting it lightly.",
              "maleVariant": "",
              "stringId": "1691217175625265152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need your help, Jack. Got some biz.",
              "maleVariant": "",
              "stringId": "1691217432383778816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"chica\" b=\"Ugh, always biz with you, \" a=\".\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"mano\" b=\"Ugh, always biz with you, \" a=\".\"/>",
              "stringId": "1691225754335899648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You in some trouble?",
              "maleVariant": "",
              "stringId": "1691254167306170368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, V – you alive? How's things in the viper pit?",
              "maleVariant": "",
              "stringId": "1691260805748551680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not yet. Just... need a soul I can trust for now.",
              "maleVariant": "Not yet. Right now I just need a soul I can trust.",
              "stringId": "1691269482522013696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That kinda sounded like a \"yes.\" Meet me at Lizzie's. Be there in an hour.",
              "maleVariant": "",
              "stringId": "1691270077089771520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you sure this is a good idea?",
              "maleVariant": "",
              "stringId": "1713809925118906368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have a few questions.",
              "maleVariant": "",
              "stringId": "1713810439072141312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, regarding the--",
              "maleVariant": "",
              "stringId": "1713810925493964800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, I--",
              "maleVariant": "",
              "stringId": "1713812098707902464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take this.",
              "maleVariant": "",
              "stringId": "1714047338663661572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now she's holding a gun to my head to make sure I don't try anything.",
              "maleVariant": "",
              "stringId": "1714047438488096780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What would you do in my position?",
              "maleVariant": "",
              "stringId": "1714155331623342080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Visitor authorization granted.",
              "maleVariant": "",
              "stringId": "1714256674430144512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Emergency protocols active. System security confirmed.",
              "maleVariant": "",
              "stringId": "1714270546922852352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We would've lost the license, now we gained a week. That's a win.",
              "maleVariant": "",
              "stringId": "1715755077134643200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But she won't pull the trigger because she needs me. I do what she doesn't have the guts to do.",
              "maleVariant": "",
              "stringId": "1715757552277319680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have to defend yourself. Remind Abernathy you're not her doormat.",
              "maleVariant": "",
              "stringId": "1715758698412830720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You need to defend yourself.",
              "maleVariant": "",
              "stringId": "1715758722991452164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's not much you can do. She's got the Night City board on her side. If she wants you out, well... you understand.",
              "maleVariant": "",
              "stringId": "1715758996107751424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seems we see eye to eye on this issue.",
              "maleVariant": "",
              "stringId": "1719876415747190784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not much you can do.",
              "maleVariant": "",
              "stringId": "1719876624556421120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're right.",
              "maleVariant": "",
              "stringId": "1719949528807698432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smack her once... but hard. She'll respect you.",
              "maleVariant": "",
              "stringId": "1719949632524447744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do.",
              "maleVariant": "",
              "stringId": "1720185136184487936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You said it. I need to defend myself.",
              "maleVariant": "",
              "stringId": "1720189214138494976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Said it yourself – she's left me no choice.",
              "maleVariant": "",
              "stringId": "1720189228818558976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is a huge gamble.",
              "maleVariant": "",
              "stringId": "1720192717338636292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'll happen if I refuse?",
              "maleVariant": "",
              "stringId": "1720193534221283328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If anyone finds out we even talked about this... I'll go down with you.",
              "maleVariant": "",
              "stringId": "1720193546418319360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't ask stupid questions.",
              "maleVariant": "",
              "stringId": "1721192329990041600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This isn't a request, V.",
              "maleVariant": "",
              "stringId": "1721226670518497280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And if I refuse?",
              "maleVariant": "",
              "stringId": "1721238124290306048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's true. But everyone knows you only made it to where you are thanks to me.",
              "maleVariant": "",
              "stringId": "1721250526058704896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So it's not like they could ever believe you played no part in it.",
              "maleVariant": "",
              "stringId": "1721252510031605760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We seem to understand each other. Take this, too.",
              "maleVariant": "",
              "stringId": "1721266565932838912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And for the core task... use someone you know inside out. Are we clear?",
              "maleVariant": "",
              "stringId": "1721267335218524160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In that case, I'll await your report. And good luck with it.",
              "maleVariant": "",
              "stringId": "1721268274339966976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1725614529316843520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1725703099008348160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Use it. And make sure Abernathy won't be a problem anymore.",
              "maleVariant": "",
              "stringId": "1766522987259817984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Director Abernathy.",
              "maleVariant": "",
              "stringId": "1767788640067317760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Divide and conquer. Effective strategy.",
              "maleVariant": "",
              "stringId": "1767819329873571840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have my AV waiting outside. It's yours to use.",
              "maleVariant": "",
              "stringId": "1770816201841201152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you online?",
              "maleVariant": "",
              "stringId": "1770818042905776128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Affirmative. Online and in their system. No sign we've been detected.",
              "maleVariant": "",
              "stringId": "1770819268665954304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can you get access to their preliminary declarations?",
              "maleVariant": "",
              "stringId": "1770819317907083264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They usually stick to them. It should give us a good overview.",
              "maleVariant": "",
              "stringId": "1770819594194276352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. On it…",
              "maleVariant": "",
              "stringId": "1770865634715824128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pff, \"effective\"...",
              "maleVariant": "",
              "stringId": "1795370850551001088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have you worked together long?",
              "maleVariant": "",
              "stringId": "1795372041548787716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This isn't the first time Abernathy's set you up.",
              "maleVariant": "This... isn't the first time Abernathy's set you up.",
              "stringId": "1795372053326393344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Always,\" meaning how long?",
              "maleVariant": "",
              "stringId": "1795372057722023936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not the first time she's screwed you.",
              "maleVariant": "",
              "stringId": "1795372294331101184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before Night City we worked on an assignment in Osaka. Two years of subterfuge. She spent them digging a trap for me.",
              "maleVariant": "",
              "stringId": "1795375389979111424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When she became Director of Spec Ops, that's when she finally shoved me in it.",
              "maleVariant": "",
              "stringId": "1795375723963150336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smeared me in front of Japanese execs so she'd be the one promoted to Director of Spec Ops. The cunt.",
              "maleVariant": "",
              "stringId": "1795386598501576704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, not the first...",
              "maleVariant": "",
              "stringId": "1795387514621456384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honestly?",
              "maleVariant": "",
              "stringId": "1795392317099536384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please.",
              "maleVariant": "",
              "stringId": "1795392490810830848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take money]",
              "maleVariant": "",
              "stringId": "1795411612105895940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Credchip transactions are traceable... paper disappears as soon as you spend it.",
              "maleVariant": "Credchip transactions are traceable… heh, paper disappears as soon as you spend it.",
              "stringId": "1795411626467192832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What can I say.",
              "maleVariant": "",
              "stringId": "1795416648223485952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good...",
              "maleVariant": "",
              "stringId": "1795419218778189824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Viper pit. Couldn'ta said it better.",
              "maleVariant": "",
              "stringId": "1795439890791329792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jenkins, Night City counterintel.",
              "maleVariant": "",
              "stringId": "1796765494513954816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need an analysis of the legislative procedures for lunar base licenses.",
              "maleVariant": "",
              "stringId": "1796773184367685632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. Yes, the Sea of Clouds.",
              "maleVariant": "",
              "stringId": "1796776007016837120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Black clinics. Operating outside earthly jurisdiction.",
              "maleVariant": "",
              "stringId": "1796776160679358464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, not yet. We're just exploring options for now.",
              "maleVariant": "",
              "stringId": "1796776562342686720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On the authority of Director Abernathy, yes...",
              "maleVariant": "",
              "stringId": "1796777138539393024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, unofficially.",
              "maleVariant": "",
              "stringId": "1796778699407683584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "1834060759127052292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1834062925954830336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please enter the antechamber.",
              "maleVariant": "",
              "stringId": "1867885420168105984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And don't disappoint me.",
              "maleVariant": "",
              "stringId": "1877842760743972864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh… Yeah, if that's how it's got to be.",
              "maleVariant": "",
              "stringId": "1967909775475757056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's clear… hmm.",
              "maleVariant": "",
              "stringId": "1967910042720030724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Say again?",
              "maleVariant": "",
              "stringId": "1967910184537837572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We need to talk.",
              "maleVariant": "",
              "stringId": "1973468807313772548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1987894907416715292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm... Yeah, just as we thought.",
              "maleVariant": "",
              "stringId": "1995078146484969472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhh...",
              "maleVariant": "",
              "stringId": "1995080489792282624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A datashard?",
              "maleVariant": "",
              "stringId": "2298340129827299328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't go wrong with cash.",
              "maleVariant": "",
              "stringId": "2298343120533823488"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}