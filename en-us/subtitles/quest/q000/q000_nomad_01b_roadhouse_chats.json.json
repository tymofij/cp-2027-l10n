        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to drive rigs for SovOil. Lousy stint, that was.",
              "maleVariant": "",
              "stringId": "1673214572405739520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heheh, bet it padded your pockets, though.",
              "maleVariant": "",
              "stringId": "1673215898896322560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, but imagine haulin' a tank of rocket fuel across two states.",
              "maleVariant": "",
              "stringId": "1673215930873696256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Starts with short-term memory loss. Then come hallucinations. Violent seizures at the end.",
              "maleVariant": "",
              "stringId": "1673218402879033344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They know where it came from?",
              "maleVariant": "",
              "stringId": "1673221018748997632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drugs, I heard. Started somewhere in Pacifica they were saying on the news.",
              "maleVariant": "",
              "stringId": "1673221050357272576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So there we are, and the 6th Street jarheads walk in â€“ five of 'em!",
              "maleVariant": "",
              "stringId": "1673234458389532672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit, they found you?",
              "maleVariant": "",
              "stringId": "1673234684831617024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck knows if they were lookin' or if it was just a coincidence.",
              "maleVariant": "",
              "stringId": "1673234724291629056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck up once, you leave nuttin' behind but a crater.",
              "maleVariant": "",
              "stringId": "1724434841739390976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What happened?",
              "maleVariant": "",
              "stringId": "1724444959373717504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We hauled ass out through the kitchen! Wouldn't be here otherwise.",
              "maleVariant": "",
              "stringId": "1724445279130677248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Talk]",
              "maleVariant": "",
              "stringId": "1866434054048468996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I told you to disappear. Now don't test my patience.",
              "maleVariant": "",
              "stringId": "1866435449325637632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Talk]",
              "maleVariant": "",
              "stringId": "1866436089124769792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't cross the sheriff. Better off doin' what he says.",
              "maleVariant": "",
              "stringId": "1866436089158324228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Light on your toes, there! Forward and back!",
              "maleVariant": "",
              "stringId": "2177791133467156480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"vagabunda\" b=\"Don't serve your kind around here, \" a=\".\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"vagabundo\" b=\"Don't serve your kind around here, \" a=\".\"/>",
              "stringId": "2260354329968779264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Boxin' is doin' the thinkin' with your feet! Hop fro, jump back.",
              "maleVariant": "",
              "stringId": "2260702293034975232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jump in, thrash-bam-bam, and guard up! Feint... dodge... low and balanced!",
              "maleVariant": "",
              "stringId": "2260702509125517312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on. Walk.",
              "maleVariant": "",
              "stringId": "2260705122562785280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Talk]",
              "maleVariant": "",
              "stringId": "2260706115421650948"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}