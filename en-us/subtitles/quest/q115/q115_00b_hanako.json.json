        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様がお待ちです\" t=\"Hanako-sama is waiting.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1651729622285426688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You could use some air. Right now.",
              "maleVariant": "",
              "stringId": "1651764434454978560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do not expect you to understand. I wish you merely to know I do not oppose my brother willingly, that it is difficult.",
              "maleVariant": "",
              "stringId": "1680635610711068672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't try anything!",
              "maleVariant": "",
              "stringId": "1680638817122258944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're getting the fuck outta here.",
              "maleVariant": "",
              "stringId": "1680645781495615488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you well?",
              "maleVariant": "",
              "stringId": "1680645781512392704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's fly the fuck outta here.",
              "maleVariant": "",
              "stringId": "1680647899753353216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's fine. I'm OK.",
              "maleVariant": "",
              "stringId": "1680660288569741316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are bleeding.",
              "maleVariant": "",
              "stringId": "1680661102700924928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, i-it's just, uh...",
              "maleVariant": "Yeah, yeah, it's just, um…",
              "stringId": "1680662543645675520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck...",
              "maleVariant": "",
              "stringId": "1680662972773306368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need to leave.",
              "maleVariant": "",
              "stringId": "1680665149633859584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am fine. She is not.",
              "maleVariant": "I am fine. He is not.",
              "stringId": "1680666391701491712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's fine... I'm... I'm OK...",
              "maleVariant": "It's fine... I'm OK...",
              "stringId": "1680667908647030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I gotta go…",
              "maleVariant": "Ugh, I gotta go...",
              "stringId": "1680668037059842048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can lead you to Mikoshi.",
              "maleVariant": "",
              "stringId": "1680695280238727168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Got my attention. Go on.",
              "maleVariant": "",
              "stringId": "1680698203433734148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] And you'll help me in exchange for... what?",
              "maleVariant": "",
              "stringId": "1680701486130925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure know how to prick up my ears.",
              "maleVariant": "",
              "stringId": "1680703821066407936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you'll help me in exchange for…?",
              "maleVariant": "",
              "stringId": "1680703918122602496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My brother...",
              "maleVariant": "",
              "stringId": "1680791610030166016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He must be made to take responsibility for his deeds.",
              "maleVariant": "",
              "stringId": "1680810055589302272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna avenge your father.",
              "maleVariant": "",
              "stringId": "1680812089524752384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sit. We have little time.",
              "maleVariant": "",
              "stringId": "1681004401416605708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's get to it, then.",
              "maleVariant": "",
              "stringId": "1681005282337886208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... don't know.",
              "maleVariant": "",
              "stringId": "1681006055666880524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Three times now that AV has flown by.",
              "maleVariant": "",
              "stringId": "1681006055683657736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think someone mighta tailed you?",
              "maleVariant": "",
              "stringId": "1681006055683657748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We must not get distracted. Did you bring Soulkiller?",
              "maleVariant": "",
              "stringId": "1681006055683657760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seem on edge. Something got ya spooked?",
              "maleVariant": "",
              "stringId": "1681007519780966400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your biz, not mine.",
              "maleVariant": "",
              "stringId": "1681018084863643648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just say it. Be easier that way.",
              "maleVariant": "",
              "stringId": "1682212773218357248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, sounds like dirty laundry, a family matter. Why should I get involved?",
              "maleVariant": "Sounds like dirty laundry, a family matter. Why should I get involved?",
              "stringId": "1682212880407990272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't like this at all. Sounds like you wanna use me.",
              "maleVariant": "",
              "stringId": "1682218315672326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on. Talk.",
              "maleVariant": "",
              "stringId": "1682226487787130880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Responsibility\"? You wanna kill your baby bro.",
              "maleVariant": "",
              "stringId": "1682234701626871808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want the Arasaka Corporation to know the truth.",
              "maleVariant": "",
              "stringId": "1682235383419375616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want him punished.",
              "maleVariant": "",
              "stringId": "1682235756930535424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still vague. You want him dead.",
              "maleVariant": "",
              "stringId": "1682236259072610304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want the Arasaka Corporation to know the truth.",
              "maleVariant": "",
              "stringId": "1682236572286455808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds like you want to avenge dead ol' dad.",
              "maleVariant": "",
              "stringId": "1682257105266806784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You already are. You willed it so.",
              "maleVariant": "",
              "stringId": "1682286027039707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Offer's intriguing. Need to think about it.",
              "maleVariant": "",
              "stringId": "1682287103667212288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta think about it.",
              "maleVariant": "",
              "stringId": "1682288154709463040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't like what I'm hearin'.",
              "maleVariant": "",
              "stringId": "1682289000096600064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How you plan to do this?",
              "maleVariant": "",
              "stringId": "1682291571288838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Naturally. But think quickly.",
              "maleVariant": "",
              "stringId": "1682292578022461440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are bleeding.",
              "maleVariant": "",
              "stringId": "1682292915093508096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. I simply offer you an opportunity.",
              "maleVariant": "",
              "stringId": "1682295622281859072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Call me crazy, but that sounds like fuckin' suicide.",
              "maleVariant": "",
              "stringId": "1682296441244880896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds risky.",
              "maleVariant": "",
              "stringId": "1682296531623743488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If it works, you take the throne?",
              "maleVariant": "",
              "stringId": "1682298853036789760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's assume it works. Then what? You take the Arasaka throne?",
              "maleVariant": "",
              "stringId": "1682299852438769664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is a fact my brother has never understood.",
              "maleVariant": "",
              "stringId": "1682300643417395200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone must know their place, V.",
              "maleVariant": "",
              "stringId": "1682303389092679680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know my place. And I have no wish to change it.",
              "maleVariant": "",
              "stringId": "1682303881889845248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This I know. And that is why we will take all the necessary precautions.",
              "maleVariant": "",
              "stringId": "1682334337066733568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was to be its heart – to uphold life, ensure continuity, stability, never to oppose it.",
              "maleVariant": "",
              "stringId": "1682336268342398976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not the first to try.",
              "maleVariant": "",
              "stringId": "1682353465123016704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It seems you are running out of time.",
              "maleVariant": "",
              "stringId": "1682369908925403136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nope. Don't have Soulkiller.",
              "maleVariant": "",
              "stringId": "1682387207879266308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wanna hear her out.",
              "maleVariant": "",
              "stringId": "1682387470912458756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna leave?",
              "maleVariant": "",
              "stringId": "1682403125766262784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where would you get that idea?",
              "maleVariant": "",
              "stringId": "1682419939590148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Figured it as a given. Besides, Takemura said you were supposed to step in for Yorinobu here in Night City.",
              "maleVariant": "",
              "stringId": "1682420236815306752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No wish to? Or no permission?",
              "maleVariant": "",
              "stringId": "1682429303558377476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, your biz.",
              "maleVariant": "",
              "stringId": "1682431218644029440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know my place too.",
              "maleVariant": "",
              "stringId": "1682431707632766976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, whatever. Your biz, not mine.",
              "maleVariant": "",
              "stringId": "1682434210625622016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know my place too. Know exactly where I came from.",
              "maleVariant": "",
              "stringId": "1682434402154319872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe, once it's all said and done, I'll find my way back there.",
              "maleVariant": "",
              "stringId": "1682435970371031040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut up.",
              "maleVariant": "",
              "stringId": "1682439290313641984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut up. For one goddamn minute, shut up!",
              "maleVariant": "",
              "stringId": "1682439935347265536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No wish to? Or no permission? You being a woman.",
              "maleVariant": "No wish to? Or no permission? You bein' a woman.",
              "stringId": "1682457408968724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I decide my own fate. As should you.",
              "maleVariant": "",
              "stringId": "1682461018385420288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Perhaps. If you can prove your value.",
              "maleVariant": "",
              "stringId": "1682494063880572928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesus, I'm gonna fuckin' puke…",
              "maleVariant": "",
              "stringId": "1682494155920379904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excellent. You have come.",
              "maleVariant": "",
              "stringId": "1683591408747679744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Something got you spooked?",
              "maleVariant": "",
              "stringId": "1683606571341189120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take me for a fool?",
              "maleVariant": "",
              "stringId": "1683611739210256384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course I didn't bring Soulkiller. 'Fraid we don't know each other that well.",
              "maleVariant": "",
              "stringId": "1683612879104643072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take me for a fool, think I'd fall for that? Got plans to walk outta here alive.",
              "maleVariant": "",
              "stringId": "1683612960172150784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is gonna be a disaster.",
              "maleVariant": "",
              "stringId": "1683648943441711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck...",
              "maleVariant": "",
              "stringId": "1683658588377976832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your coffee, ma'am.",
              "maleVariant": "",
              "stringId": "1683671896803905536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait.",
              "maleVariant": "",
              "stringId": "1683816371115134976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is everything all right, Hanako-sama?",
              "maleVariant": "",
              "stringId": "1683816813463212032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Do you feel all right?",
              "maleVariant": "",
              "stringId": "1683821015618949120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh?",
              "maleVariant": "",
              "stringId": "1683821794551533568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gettin' us outta here!",
              "maleVariant": "",
              "stringId": "1683822307951120384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What gave you that idea?",
              "maleVariant": "",
              "stringId": "1683859895541960704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Quit buttin' in so I can hear her out.",
              "maleVariant": "",
              "stringId": "1683860017176776704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Finger on the trigger. Don't say a word.",
              "maleVariant": "",
              "stringId": "1683951113618513920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We oughta leave. Right now.",
              "maleVariant": "",
              "stringId": "1684900268070539264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You catch a whiff of that? Smells like shit. Careful not to step in it.",
              "maleVariant": "",
              "stringId": "1684904963140882432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for meeting me.",
              "maleVariant": "Thanks for meetin' me.",
              "stringId": "1685002531829899264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesus fuck, how many times you gonna bow?",
              "maleVariant": "",
              "stringId": "1689301500272971784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, V. And I thank you.",
              "maleVariant": "",
              "stringId": "1689302645317963776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let us begin.",
              "maleVariant": "",
              "stringId": "1689302985526349824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's step to it, then.",
              "maleVariant": "",
              "stringId": "1689306773821722624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "God, I feel sick. Don't like this at all.",
              "maleVariant": "",
              "stringId": "1689308648474284032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are here because I know how to save your life.",
              "maleVariant": "",
              "stringId": "1689318612009881600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are bleeding.",
              "maleVariant": "",
              "stringId": "1689422413551243264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck...",
              "maleVariant": "",
              "stringId": "1689422966175961088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu will soon call a meeting of the board. Representatives of all factions are expected to attend.",
              "maleVariant": "",
              "stringId": "1692314910178807808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The perfect moment for them to learn the circumstances of my father's death.",
              "maleVariant": "",
              "stringId": "1692318855005605888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu raised his hand against our father. But he is an Arasaka, he is family.",
              "maleVariant": "",
              "stringId": "1692326256039563264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu is on high alert after the parade. He may harbor suspicions.",
              "maleVariant": "",
              "stringId": "1692403056027504640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So?",
              "maleVariant": "",
              "stringId": "1692436520818839552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But what if I am the last? You cannot take that chance.",
              "maleVariant": "",
              "stringId": "1692441869881839616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She emptied the place? Shit...",
              "maleVariant": "",
              "stringId": "1692705925679996940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This porcelain bitch is the worst kind of news…",
              "maleVariant": "",
              "stringId": "1693954801086947328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are bleeding.",
              "maleVariant": "",
              "stringId": "1695297798067326976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck...",
              "maleVariant": "",
              "stringId": "1695298475313205248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuckin' busboys here must make a killin' in tips.",
              "maleVariant": "",
              "stringId": "1702496054254690304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can you chill for two seconds?",
              "maleVariant": "",
              "stringId": "1702509824507043844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. I'll be careful.",
              "maleVariant": "",
              "stringId": "1702513631324463104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can you chill for two secs? We go in, we talk, we leave. That's it.",
              "maleVariant": "",
              "stringId": "1702514784439296000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. I'll be careful.",
              "maleVariant": "",
              "stringId": "1702514899010904064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take a drink]",
              "maleVariant": "",
              "stringId": "1702534570548322308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm.",
              "maleVariant": "Ahh.",
              "stringId": "1702584677163524096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not exactly one of your strong suits...",
              "maleVariant": "",
              "stringId": "1702784518887182336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, first sign of trouble, we delta.",
              "maleVariant": "",
              "stringId": "1702785258410086400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had I any ill intentions, I would have chosen a site far more discreet than this.",
              "maleVariant": "",
              "stringId": "1702790912700977152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh... I'll think it over...",
              "maleVariant": "",
              "stringId": "1733115674434920448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] We'll see.",
              "maleVariant": "",
              "stringId": "1733116807266099204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't delay, make your decision.",
              "maleVariant": "",
              "stringId": "1743248069187473408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You prefer to keep your guard up. Understandable but unnecessary.",
              "maleVariant": "",
              "stringId": "1754353495610101760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are here because I know how to save your life.",
              "maleVariant": "",
              "stringId": "1754373025631703040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin'? Mikoshi don't exist in realspace.",
              "maleVariant": "",
              "stringId": "1754378767650656256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yet its access points do. And one is very near.",
              "maleVariant": "",
              "stringId": "1754378872072048640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where?",
              "maleVariant": "",
              "stringId": "1754380263339462656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here in Night City. Beneath Arasaka Tower.",
              "maleVariant": "",
              "stringId": "1754380311372632064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will get you into this meeting. And you will testify against my brother.",
              "maleVariant": "",
              "stringId": "1754384127400742912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] I won't.",
              "maleVariant": "",
              "stringId": "1754519707757449216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No worries... 'bout that...",
              "maleVariant": "",
              "stringId": "1754521658662457344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu--",
              "maleVariant": "",
              "stringId": "1754604277777436672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、すぐに邸宅へお向かいください\" t=\"Hanako-sama, you are expected at the residence.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1754604384077877248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"すぐに済むわ\" t=\"We will finish soon.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1754605305079926784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Testify...\" Listen, mind if I'm straight with you?",
              "maleVariant": "",
              "stringId": "1760682294001930240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want me to throw Yorinobu under the bus in front of Arasaka top brass?",
              "maleVariant": "",
              "stringId": "1760684011720413184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like, such as...?",
              "maleVariant": "Like, such as…?",
              "stringId": "1760684595282317312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will see.",
              "maleVariant": "",
              "stringId": "1760684665629184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Help me get rid of Yorinobu. I will help you get rid of the construct.",
              "maleVariant": "",
              "stringId": "1760688551500845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Bow] Thank you for meeting with me.",
              "maleVariant": "",
              "stringId": "1761953646030544896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"女が来ました。武装しているようです\" t=\"She's here. And she looks like she's armed.\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"jpn\" o=\"男が来ました。武装しているようです\" t=\"He's here. And he looks like he's armed.\" b=\"\" a=\"\"/>",
              "stringId": "1802644506266177536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"もちろんです。わかりました\" t=\"Of course. Understood.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1802682666580840448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before we discuss details, I feel I owe you an explanation. I must tell you why I changed my mind and wish to help.",
              "maleVariant": "",
              "stringId": "2060540422630432768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I knew. From the start.",
              "maleVariant": "",
              "stringId": "2060545501429260288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All the board members knew. Not one soul even slightly interested in the matter believed in the poisoning. Details were disparate, inconsistent.",
              "maleVariant": "",
              "stringId": "2060546556397379584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't do a thing about it – why?",
              "maleVariant": "",
              "stringId": "2060549036355117056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're no better than your brother.",
              "maleVariant": "",
              "stringId": "2060549514304446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You knew but didn't do a thing about it? Why? Suddenly sprout a conscience just yesterday?",
              "maleVariant": "You knew but didn't do a thing about it – why? Suddenly sprout a conscience just yesterday?",
              "stringId": "2060564173782802432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Helped your brother cover up. Makes you no better than him, not a smidge.",
              "maleVariant": "",
              "stringId": "2060564275083632640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My father had flaws – I know this, I harbor no illusions about it. When a foe needed removal, my father ordered it without a thought to mercy.",
              "maleVariant": "",
              "stringId": "2060564778936983552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You do not know my family, not in the least.",
              "maleVariant": "",
              "stringId": "2060575063773044736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Power.",
              "maleVariant": "",
              "stringId": "2060586480299433984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ties that bind.",
              "maleVariant": "",
              "stringId": "2060586581533155328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Power.",
              "maleVariant": "",
              "stringId": "2060586686105542660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Family.",
              "maleVariant": "",
              "stringId": "2060586793043517440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weren't sure of anything – if you could trust me, if Yorinobu'd killed your father. Now…?",
              "maleVariant": "",
              "stringId": "2066335169236197376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You... knew?",
              "maleVariant": "",
              "stringId": "2066336550521167872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A way of being Yorinobu could not abide. But to my father, ends ever justified means.",
              "maleVariant": "",
              "stringId": "2066341865509642240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka has always valued just one thing – do you know what it is?",
              "maleVariant": "",
              "stringId": "2066342513462501376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The corp.",
              "maleVariant": "",
              "stringId": "2066348206911414272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea.",
              "maleVariant": "",
              "stringId": "2066348302742872064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The corporation.",
              "maleVariant": "",
              "stringId": "2066348744554078208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea.",
              "maleVariant": "",
              "stringId": "2066348927509618688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Family. Our family.",
              "maleVariant": "",
              "stringId": "2066356138659045376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, family.",
              "maleVariant": "",
              "stringId": "2066357164552249344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh, guess Yorinobu's instructions were a little different.",
              "maleVariant": "",
              "stringId": "2066358214688530432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't say why you changed your mind.",
              "maleVariant": "",
              "stringId": "2066362249122439172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, he's your brother, it's been hard, it'll be hard. Let's say I get that. But what changed your mind?",
              "maleVariant": "",
              "stringId": "2066362401291788288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We were gonna talk biz, specifics.",
              "maleVariant": "",
              "stringId": "2066362485630853120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We received a warning during the parade – security protocols were violated. The first doubts sprouted then.",
              "maleVariant": "",
              "stringId": "2066362596998012928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got biz to discuss.",
              "maleVariant": "",
              "stringId": "2066362721484955648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They grew yet greater in your hideout, when my brother's assault group arrived not to rescue, but to kill.",
              "maleVariant": "",
              "stringId": "2066371935548760064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu was just plain willin' to sacrifice you.",
              "maleVariant": "",
              "stringId": "2066372640745480192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My father was right about my brother. He never cared for us.",
              "maleVariant": "",
              "stringId": "2066373112655011840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was brought up to be the heart of the family. It is time for the Arasakas to listen to their heart.",
              "maleVariant": "",
              "stringId": "2066375012121075712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It will bring justice.",
              "maleVariant": "",
              "stringId": "2066376065713463296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did you bring Soulkiller, as we agreed you would?",
              "maleVariant": "",
              "stringId": "2066386872522268672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let us do so.",
              "maleVariant": "",
              "stringId": "2066387764197744640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please sit, you are my guest.",
              "maleVariant": "",
              "stringId": "2258820060879888384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And? I will need to return to the estate soon. So as not raise any suspicions.",
              "maleVariant": "",
              "stringId": "2258820367298961412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is all in order? You may speak plainly.",
              "maleVariant": "",
              "stringId": "2258821506169937924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for seeing me.",
              "maleVariant": "",
              "stringId": "2456241390180503552"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}