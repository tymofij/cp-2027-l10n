    "DataType": "CR2W"
  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah, it's not what you think. Not yet, anyway.",
              "maleVariant": "",
              "stringId": "1648957578149761024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eeaasy there. Don't move just yet.",
              "maleVariant": "",
              "stringId": "1648958390989099008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vik?",
              "maleVariant": "",
              "stringId": "1648958992435515392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're in pain, I know. Delirious when you arrived so I had to dose you with betahaloperidol.",
              "maleVariant": "",
              "stringId": "1648961674424487936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So how bad is it?",
              "maleVariant": "",
              "stringId": "1682515973600108548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's the diagnosis, Vik? No sugarcoating.",
              "maleVariant": "",
              "stringId": "1683606889656147968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll order a weaker drink next time.",
              "maleVariant": "",
              "stringId": "1683606971277303808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just give it to me straight, Vik.",
              "maleVariant": "",
              "stringId": "1683608974426562560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Order a weaker poison next time.",
              "maleVariant": "",
              "stringId": "1683609018433200128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try sitting up. Carefully.",
              "maleVariant": "",
              "stringId": "1683619156418289664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Try to sit] Think I got some feeling back…",
              "maleVariant": "",
              "stringId": "1683620842713047040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Try to sit] No promises. Here goes.",
              "maleVariant": "",
              "stringId": "1683621626376806400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll try. Think I'm gettin' some feeling back.",
              "maleVariant": "I'll try. I think I'm gettin' some feeling back.",
              "stringId": "1683622106859495424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, but don't expect a miracle…",
              "maleVariant": "",
              "stringId": "1683622179404177408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh...",
              "maleVariant": "",
              "stringId": "1683625477788065792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm…",
              "maleVariant": "",
              "stringId": "1683627065986441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How much longer is this going to go on?",
              "maleVariant": "",
              "stringId": "1683634579192864768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well? I'm sitting.",
              "maleVariant": "",
              "stringId": "1683634695492526084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you got somethin' to say, say it.",
              "maleVariant": "",
              "stringId": "1683636309930467328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit straight] Yeah, I know... I'll try.",
              "maleVariant": "",
              "stringId": "1683637187160760320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit straight] What options do I have?",
              "maleVariant": "",
              "stringId": "1683637187160760336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit straight] The hell you think I've been doing?",
              "maleVariant": "",
              "stringId": "1683639974611312640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know, Vik. I'll try. Soon.",
              "maleVariant": "",
              "stringId": "1683640430129504256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I know. Thoughts on where to start?",
              "maleVariant": "",
              "stringId": "1683640536379613184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Take matters into my own hands...\" The fuck you think I've been doing?!",
              "maleVariant": "\"Take matters into my own hands\"? The fuck you think I been doing?!",
              "stringId": "1683640564498227200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now, see that setup over there?",
              "maleVariant": "",
              "stringId": "1683660001037180928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give it a fuckin' rest, Johnny. It's my decision.",
              "maleVariant": "",
              "stringId": "1683660772218691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enough, Johnny. I just-- I gotta think about it.",
              "maleVariant": "Enough, Johnny. I just… gotta think about it.",
              "stringId": "1683660910530060288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well? Looks like I can sit up all right.",
              "maleVariant": "",
              "stringId": "1683726677300666368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you got somethin' to say, Vik, just say it.",
              "maleVariant": "",
              "stringId": "1683726782695137280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Endotrizine. Rogue. 'Saka Tower.",
              "maleVariant": "",
              "stringId": "1683733734770794496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, that should be a little better.",
              "maleVariant": "",
              "stringId": "1683738725908668416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So how bad we talkin'?",
              "maleVariant": "",
              "stringId": "1689476912391262208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That bad, huh?",
              "maleVariant": "",
              "stringId": "1689476912408039432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "S-Slowly.",
              "maleVariant": "",
              "stringId": "1689494856294862848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh… Another victory... for the history books.",
              "maleVariant": "Argh… Another victory... for the history books.",
              "stringId": "1689497413763338240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This ain't no joke, kid.",
              "maleVariant": "",
              "stringId": "1689517781974650880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lookin' at you, not long at all.",
              "maleVariant": "",
              "stringId": "1689553667516928000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Next attack, you won't be able to crawl back here. You'll flatline in some back alley.",
              "maleVariant": "",
              "stringId": "1689556979591024640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is your last chance to take matters into your own hands. Understand?",
              "maleVariant": "",
              "stringId": "1689561404296531968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You tell me.",
              "maleVariant": "",
              "stringId": "1689565193850195968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well whatever it is, it keeps landing you on my table.",
              "maleVariant": "",
              "stringId": "1689574067739168768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But I can tell you don't have good news.",
              "maleVariant": "",
              "stringId": "1689585438748971008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, you'll do it now.",
              "maleVariant": "",
              "stringId": "1689632916860964864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesus, again... Do I have to spell it out?",
              "maleVariant": "",
              "stringId": "1689636587514499072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's in your hands! Not mine and not those of that... thing in your head!",
              "maleVariant": "",
              "stringId": "1689637429344866304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah... What about it?",
              "maleVariant": "",
              "stringId": "1689680205625298944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll find the last dose of pseudoendotrizine there. A gift from Misty.",
              "maleVariant": "",
              "stringId": "1689680788465782784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna give in to the voices in your head? Go ahead. Take a puff, go silent, get out.",
              "maleVariant": "",
              "stringId": "1689684056415801344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or... find another way. End things on your own terms.",
              "maleVariant": "",
              "stringId": "1689685496739794944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Manage to walk those few yards to the table... the rest'll be up to you, and you alone.",
              "maleVariant": "",
              "stringId": "1689689061612982272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Only way this is gonna work.",
              "maleVariant": "",
              "stringId": "1689713830303502336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Shift and get comfortable] How'd I get here?",
              "maleVariant": "",
              "stringId": "1690664218099847168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How... How'd I get here?",
              "maleVariant": "",
              "stringId": "1690664218166956032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dragged yourself here, beat to hell an' back. Gave Misty a helluva fright.",
              "maleVariant": "",
              "stringId": "1690664218183733252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then you shoved my patient at the time off the table…",
              "maleVariant": "",
              "stringId": "1690664218183733268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And demanded you be treated immediately.",
              "maleVariant": "",
              "stringId": "1690664218183733284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I, uh... wasn't completely myself.",
              "maleVariant": "",
              "stringId": "1690664218250842128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. And that's a bit worrying…",
              "maleVariant": "",
              "stringId": "1690664218250842140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You heard 'im. We've got one last chance. I'm takin' the wheel.",
              "maleVariant": "",
              "stringId": "1690678024892977152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell him it was your guardian angel.",
              "maleVariant": "",
              "stringId": "1690688205827739648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As I see it, there won't be a next time.",
              "maleVariant": "",
              "stringId": "1690702447284981760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You've come to a critical juncture.",
              "maleVariant": "",
              "stringId": "1690703160031449088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a little present from me over there, too. Blockers.",
              "maleVariant": "",
              "stringId": "1690723658685616128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Egh... Think I can do it...",
              "maleVariant": "",
              "stringId": "1693957206083379200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take]",
              "maleVariant": "",
              "stringId": "1693962112580198404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But light oversensitivity's a good sign. Means your optic nerves aren't damaged.",
              "maleVariant": "",
              "stringId": "1703720226720190464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Shift and get comfortable] Head feels like it's gonna explode…",
              "maleVariant": "",
              "stringId": "1703721871692664832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My head... Agh...",
              "maleVariant": "",
              "stringId": "1703722424988471296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Almost done stabilizing the biochip.",
              "maleVariant": "",
              "stringId": "1703722838832058368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ergh… feels like…",
              "maleVariant": "",
              "stringId": "1703723075608907776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Someone's using your head as a knife block?",
              "maleVariant": "",
              "stringId": "1703723390768910336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Am...",
              "maleVariant": "",
              "stringId": "1703840397506830340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] I need to think about it.",
              "maleVariant": "",
              "stringId": "1704345085306134532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gromatsky's assault continues… He is relentless!",
              "maleVariant": "",
              "stringId": "1712547418393468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh! And he lands a huge – and I mean huge – right hook! He's got Walsh on the ropes!",
              "maleVariant": "",
              "stringId": "1712553750467108864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, get up, you hack!",
              "maleVariant": "",
              "stringId": "1712577304319418368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Boom! Gromatsky lands a devastating blow, and would that be that? I mean, what's more, could this spell the end of Walsh's career?!",
              "maleVariant": "",
              "stringId": "1712577965928931328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Does Walsh have any gas left in his tank?! And will he find that spark?!",
              "maleVariant": "",
              "stringId": "1742954123806773248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait a second! The ref holds off, he's not ending the bout!",
              "maleVariant": "",
              "stringId": "1742955377954000896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for everything, Vik.",
              "maleVariant": "",
              "stringId": "1742969025548128260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Say... fight you're watching – who won?",
              "maleVariant": "",
              "stringId": "1742969278682763264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Say... who won the bout?",
              "maleVariant": "",
              "stringId": "1742970467918942208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't thank ya enough for all you've done, Vik.",
              "maleVariant": "",
              "stringId": "1742970612890865664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, so you heard that.",
              "maleVariant": "",
              "stringId": "1742972770591838208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did he get up? Walsh, was it?",
              "maleVariant": "",
              "stringId": "1742973177422548992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nope. Never does.",
              "maleVariant": "",
              "stringId": "1742973383027331072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin'?",
              "maleVariant": "",
              "stringId": "1742973505433899008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's a rerun. One I like to go back to.",
              "maleVariant": "",
              "stringId": "1742973569036324864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why'd you get so mad, then, if you already knew who won?",
              "maleVariant": "",
              "stringId": "1742974199054340096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know, really. Guess I like to think about where Walsh mighta tripped up.",
              "maleVariant": "",
              "stringId": "1742974449252962304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Any theories?",
              "maleVariant": "",
              "stringId": "1742975753815724032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Entering the ring against a stronger opponent. But…",
              "maleVariant": "",
              "stringId": "1742978040600596480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Only wish I coulda done more.",
              "maleVariant": "",
              "stringId": "1742980802231013376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on, now. I wanna close up.",
              "maleVariant": "",
              "stringId": "1742981963197583360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take the meds... and do what you gotta do, V.",
              "maleVariant": "",
              "stringId": "1742982852725575680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, Vik, I will. An' you hang in there, too.",
              "maleVariant": "",
              "stringId": "1742983494504419328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feels a little strange to call that a mistake.",
              "maleVariant": "",
              "stringId": "1743041574793515008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "G'luck, kid.",
              "maleVariant": "",
              "stringId": "1743042172079181824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...I...",
              "maleVariant": "",
              "stringId": "1744825070818877440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...dead?",
              "maleVariant": "",
              "stringId": "1744825127156768768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, Christ, you're talkin' out loud!",
              "maleVariant": "",
              "stringId": "1751739992458006528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uhh... out loud? What?",
              "maleVariant": "",
              "stringId": "1751741005768298496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Shut up, it's my decision!",
              "maleVariant": "",
              "stringId": "1751767548062208000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just go fix this thing!",
              "maleVariant": "",
              "stringId": "1751781623743778816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An' I gotta think about it.",
              "maleVariant": "",
              "stringId": "1751799806622044160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wha…? Am I…?",
              "maleVariant": "",
              "stringId": "1751830949765451776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How'd I get here?",
              "maleVariant": "",
              "stringId": "1754883812151189504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Head feels like it's gonna explode…",
              "maleVariant": "",
              "stringId": "1754888388103299072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh.",
              "maleVariant": "",
              "stringId": "1760509687722020864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll find somethin'  there that'll help you end this on your own terms.",
              "maleVariant": "",
              "stringId": "1760509687738798108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you can manage to cross those few yards... the rest'll be up to you and you alone.",
              "maleVariant": "",
              "stringId": "1760509687738798132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"On your own terms,\" huh?",
              "maleVariant": "",
              "stringId": "1760535757301092352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really subtle there, doc.",
              "maleVariant": "",
              "stringId": "1760535757301092376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to stretch your noodle? Fine. Just not in this basement.",
              "maleVariant": "",
              "stringId": "1760545801736425472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to stretch your noodle? Fine. Just not in this basement.",
              "maleVariant": "",
              "stringId": "1760546718493839360"
            },
