  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for the help.",
              "maleVariant": "",
              "stringId": "1633184049081098240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Activate fog emitters] Let it fog!",
              "maleVariant": "",
              "stringId": "1633243827341893636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Change light cycle to night] Say goodnight.",
              "maleVariant": "",
              "stringId": "1633244053112889344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Activate ambient jungle sounds]",
              "maleVariant": "",
              "stringId": "1633244164127727616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Blast own music on sound emitters]",
              "maleVariant": "",
              "stringId": "1633244623538233344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You were supposed to jump with us.",
              "maleVariant": "",
              "stringId": "1663478517184991232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't like all enemies in one big puddle – decided to ripple 'em out.",
              "maleVariant": "",
              "stringId": "1663478853467508736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Missed all the fun.",
              "maleVariant": "",
              "stringId": "1663480778065842180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seems you jumped the puddle entirely.",
              "maleVariant": "",
              "stringId": "1663481962671509504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remind me a lot of your old man.",
              "maleVariant": "",
              "stringId": "1663482036172492800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What? But you're too young to--",
              "maleVariant": "",
              "stringId": "1663482431208820736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not now!",
              "maleVariant": "",
              "stringId": "1663484082640523264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's get outta here.",
              "maleVariant": "",
              "stringId": "1663489952535416832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is this chick?",
              "maleVariant": "Who is this mon?",
              "stringId": "1663493636426174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not now!",
              "maleVariant": "",
              "stringId": "1672148747281108992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, you're Johnny Silverhand some which way, huh?",
              "maleVariant": "",
              "stringId": "1672150034009690112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's right.",
              "maleVariant": "",
              "stringId": "1672155359366483968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nova... Nice to meet you.",
              "maleVariant": "",
              "stringId": "1672155422515924992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look...",
              "maleVariant": "",
              "stringId": "1672311634989551616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. More of 'em.",
              "maleVariant": "",
              "stringId": "1672312015530364928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh… Always did prefer night ops.",
              "maleVariant": "",
              "stringId": "1672312651856613376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Little cover oughta help...",
              "maleVariant": "",
              "stringId": "1672312811189833728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Always had a thing for birds, didn't ya?",
              "maleVariant": "",
              "stringId": "1672313124403679232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time to liven up this party.",
              "maleVariant": "",
              "stringId": "1672313238958510080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mh. What's this?",
              "maleVariant": "",
              "stringId": "1672319767896002560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "El Salvador.",
              "maleVariant": "",
              "stringId": "1672319828562415616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beautiful. Too bad they're not real.",
              "maleVariant": "",
              "stringId": "1672321830856036352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is that--?",
              "maleVariant": "",
              "stringId": "1672323038345826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We take 'em out quietly, won't have a chance to set off the alarm.",
              "maleVariant": "",
              "stringId": "1672347785293074432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not even to shit their pants.",
              "maleVariant": "",
              "stringId": "1672347940213886976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's move.",
              "maleVariant": "",
              "stringId": "1672348063559979008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One for you, one for me.",
              "maleVariant": "",
              "stringId": "1672348386420723712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahh. Ooh, that felt good.",
              "maleVariant": "",
              "stringId": "1672348871299043328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland...? You read me?",
              "maleVariant": "",
              "stringId": "1693694557121650688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How many pieces you in?",
              "maleVariant": "",
              "stringId": "1693704318105919488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're goin' after Weyland.",
              "maleVariant": "",
              "stringId": "1693705724674154496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, Weyland. You ready?",
              "maleVariant": "",
              "stringId": "1693996028006473728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was that a real question?",
              "maleVariant": "",
              "stringId": "1693996730485284864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice. Remind me of your dad.",
              "maleVariant": "",
              "stringId": "1694008751343771648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Move!",
              "maleVariant": "",
              "stringId": "1697012564665278464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better late than never, I suppose...",
              "maleVariant": "",
              "stringId": "1698512071982817280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In position.",
              "maleVariant": "",
              "stringId": "1700114673346519040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still one, I hope. An' I think the local tribe's taken an interest in me.",
              "maleVariant": "",
              "stringId": "1702699546247565312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll get you outta there. Don't move.",
              "maleVariant": "",
              "stringId": "1702699795087233024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As if I could.",
              "maleVariant": "",
              "stringId": "1702701049972658176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're alive, aren't you? Plus, now we got elevator access.",
              "maleVariant": "",
              "stringId": "1702718791996018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon.",
              "maleVariant": "",
              "stringId": "1702721891033026560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Prolly everything else in the building, too.",
              "maleVariant": "",
              "stringId": "1702725490953818112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's gotta be a way. Need to look around.",
              "maleVariant": "",
              "stringId": "1702726079381114880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Might wanna turn the ambient jungle sounds back on. Maybe see what else is on there, too.",
              "maleVariant": "",
              "stringId": "1703815839989342208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland, let's find him.",
              "maleVariant": "",
              "stringId": "1703817008723763200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta find Weyland. Time's tight.",
              "maleVariant": "",
              "stringId": "1703817841075003392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agreed.",
              "maleVariant": "",
              "stringId": "1703824645595045888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit, great! They jammed the elevators.",
              "maleVariant": "",
              "stringId": "1830157220754096128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You and your fucking ego.",
              "maleVariant": "",
              "stringId": "1830160637769678848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey… you still breathin'?",
              "maleVariant": "",
              "stringId": "1883848372864950272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland never jumped. Crashed by the waterfall.",
              "maleVariant": "",
              "stringId": "1883849389379690496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go get him.",
              "maleVariant": "",
              "stringId": "1883850914395070468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He'll manage on his own.",
              "maleVariant": "",
              "stringId": "1883853045755813888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got it.",
              "maleVariant": "",
              "stringId": "1883853183362539520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't have the time to waste. He'll be fine.",
              "maleVariant": "",
              "stringId": "1883853194804600832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Almost forgot what an asshole you are. Fine, do what you want, but I'm helpin' Weyland.",
              "maleVariant": "",
              "stringId": "1883853660087132160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll try to reestablish contact with him, then find someone who's got access for the elevator going down.",
              "maleVariant": "",
              "stringId": "1883860371392962560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I've reached Weyland.",
              "maleVariant": "",
              "stringId": "1883892735666036736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Finally, let's get to work. Hey, Weyland, you hear me?",
              "maleVariant": "",
              "stringId": "1883895866982408192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Head for the waterfall. Waiting for you there.",
              "maleVariant": "",
              "stringId": "1883895893540741120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Loud an' clear. Door's all mine.",
              "maleVariant": "",
              "stringId": "1883896993622794240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We each get one. Johnny, choice is yours.",
              "maleVariant": "",
              "stringId": "1883897453872160768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's find an ace among all these suits, grab the access shard from his cold, dead hands, and get outta this fucking jungle.",
              "maleVariant": "",
              "stringId": "1883923455016968192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need an access token to get outta here. We'll find one on an officer.",
              "maleVariant": "",
              "stringId": "1883923478421184512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Way ahead of ya.",
              "maleVariant": "",
              "stringId": "1883924048594870272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh goddamn it, knew I forgot somethin'. Well, I know where we can find one, already stiff.",
              "maleVariant": "",
              "stringId": "1883924540637061120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See that one? A high-ranker, holding the access we need.",
              "maleVariant": "",
              "stringId": "1884002335950118912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All yours, Johnny.",
              "maleVariant": "",
              "stringId": "1884003087468732416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good to go. Johnny, do the honors.",
              "maleVariant": "",
              "stringId": "1884005626801037312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As Spider Murphy loved to say… \"Look what the cat dragged in.\"",
              "maleVariant": "",
              "stringId": "1884896731734220800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's grab the access token and make for the elevator.",
              "maleVariant": "",
              "stringId": "1884899799112622080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Move your asses! I need backup!",
              "maleVariant": "",
              "stringId": "1885344966399500288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You piece of shit – Weyland's dead!",
              "maleVariant": "",
              "stringId": "1885344974200905728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Found a way through by the elevator. Come find me.",
              "maleVariant": "",
              "stringId": "1885350755243663360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Found a way through by the elevator. Waitin' on you two.",
              "maleVariant": "",
              "stringId": "1885350763699380224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go. But keep your eyes lookin' three-sixty.",
              "maleVariant": "",
              "stringId": "1885371675677040640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm downstairs. C'mon.",
              "maleVariant": "",
              "stringId": "1885404138432372736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guards.",
              "maleVariant": "",
              "stringId": "1886379122415902720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna take the cover route. Meet up at the AV.",
              "maleVariant": "",
              "stringId": "1886398579590832128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Slippin' by's an option, and so is takin' 'em out all at once. I'll follow your lead.",
              "maleVariant": "",
              "stringId": "1886398803902210048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, to the bird.",
              "maleVariant": "",
              "stringId": "1886424714617823232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just keep scoutin' around, asshole. Me – I'm getting Weyland back.",
              "maleVariant": "",
              "stringId": "1886428196057915392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yo, slick skin.",
              "maleVariant": "",
              "stringId": "1893706234298638336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From a half-reptile, I'll take that as a compliment.",
              "maleVariant": "",
              "stringId": "1893706699597946880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Occupied at the term…",
              "maleVariant": "",
              "stringId": "1902225504179593216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They spotted us!",
              "maleVariant": "",
              "stringId": "1907072280166408192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, was good while it lasted…",
              "maleVariant": "",
              "stringId": "1907073202175090692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We've been made!",
              "maleVariant": "",
              "stringId": "1907073221972205572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Over here.",
              "maleVariant": "",
              "stringId": "1912861464823078912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, boy scout…",
              "maleVariant": "",
              "stringId": "1912861662743896064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See if we can't use this term somehow.",
              "maleVariant": "",
              "stringId": "1914431701460721664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time for a classic.",
              "maleVariant": "",
              "stringId": "1914432136376492032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nighty night.",
              "maleVariant": "",
              "stringId": "1914433962039582724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let there be fog.",
              "maleVariant": "",
              "stringId": "1914433985309581316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got the access token. Now, the elevator.",
              "maleVariant": "",
              "stringId": "1949049036028780544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Initiating \"Hayabusa\" protocol.",
              "maleVariant": "",
              "stringId": "1988081562063740928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The facility is in lockdown.",
              "maleVariant": "",
              "stringId": "1988083559961731072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Security personnel must report to designated zones.",
              "maleVariant": "",
              "stringId": "1988083780498235392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Intruder detected.",
              "maleVariant": "",
              "stringId": "1988192750797856768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh… Fuck me...",
              "maleVariant": "",
              "stringId": "2056082450839789568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah? Nice work. Let's go.",
              "maleVariant": "",
              "stringId": "2056215078725246976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go pay our respects.",
              "maleVariant": "",
              "stringId": "2056220515147210752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meet me in the atrium NOW, or you'll regret it!",
              "maleVariant": "",
              "stringId": "2056236177550761984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "2401210881093066756"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}