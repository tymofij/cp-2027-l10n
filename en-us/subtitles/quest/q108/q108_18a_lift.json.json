        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mainframe. Gotta find the mainframe.",
              "maleVariant": "",
              "stringId": "1200972978767962128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bad luck. Say someone finds a tape later? A tape of us breaking into Arasaka Tower?",
              "maleVariant": "",
              "stringId": "1653783024840806400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Worry about that if we make it out.",
              "maleVariant": "",
              "stringId": "1653783874271252480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Why's it matter?",
              "maleVariant": "",
              "stringId": "1731559286131195908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't like the guy, don't trust 'im. And medias're bad luck.",
              "maleVariant": "",
              "stringId": "1731559286131195928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thompson – know him well?",
              "maleVariant": "",
              "stringId": "1731578913007665152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1731592805515591680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right.",
              "maleVariant": "",
              "stringId": "1731593707274174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You recordin' this whole thing?",
              "maleVariant": "",
              "stringId": "1731599679812075540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. Why? Wanna say a few words to the folks at home?",
              "maleVariant": "",
              "stringId": "1731599679828852740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do what you gotta do. Just stay out of my way.",
              "maleVariant": "",
              "stringId": "1731599679828852756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You recording all this?",
              "maleVariant": "",
              "stringId": "1731601536194248708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Why?",
              "maleVariant": "",
              "stringId": "1733108619160129536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What?",
              "maleVariant": "",
              "stringId": "1733133284570107904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We do not as yet have word on the exact number of people hurt in the riots.",
              "maleVariant": "",
              "stringId": "2237425638401720328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But our reporters are on the scene, at Arasaka Tower…",
              "maleVariant": "",
              "stringId": "2237425638401720344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And they all agree – a psychoactive agent has been dispersed at the site, affecting those in the crowd.",
              "maleVariant": "",
              "stringId": "2237425638401720360"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}