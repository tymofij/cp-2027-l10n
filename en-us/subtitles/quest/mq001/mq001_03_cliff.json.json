  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goddamn, stuck again... C'mere, help me out.",
              "maleVariant": "",
              "stringId": "1811292423955697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Open]",
              "maleVariant": "",
              "stringId": "1811296530565189636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Press there, a little to the left... Yep, that's it.",
              "maleVariant": "",
              "stringId": "1811305132646486016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Holy shit, Mitch... What the hell?!",
              "maleVariant": "",
              "stringId": "1811373862323769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Relax, V. It's what Scorpion wanted.",
              "maleVariant": "",
              "stringId": "1811385601476022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He wanted you to stuff him in the trunk like a spare tire?",
              "maleVariant": "",
              "stringId": "1811385744166244352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If that's what he wanted...",
              "maleVariant": "",
              "stringId": "1811386006964555780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul won't be mad about this?",
              "maleVariant": "",
              "stringId": "1811386633811677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul won't like this. Blasting a good car... Terrible waste.",
              "maleVariant": "Saul won't like this. Blastin' a good car... It's a terrible waste.",
              "stringId": "1811386964826148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll tell him it was an accident.",
              "maleVariant": "",
              "stringId": "1811387447355658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I mean, if that's what Scorpion wanted...",
              "maleVariant": "",
              "stringId": "1811387679434887168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He did. Said it a million times if he said it at all.",
              "maleVariant": "",
              "stringId": "1811388212967133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pick up gas can]",
              "maleVariant": "",
              "stringId": "1811389513553694724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here you go.",
              "maleVariant": "",
              "stringId": "1811398952448970752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great. Go ahead, put it in the passenger seat.",
              "maleVariant": "",
              "stringId": "1811399237124771840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Place can]",
              "maleVariant": "",
              "stringId": "1811399600636710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See the driver's already behind the wheel...",
              "maleVariant": "I see the driver's already behind the wheel...",
              "stringId": "1811414052681568256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No point wasting time. Another hour or two, and he'll puff up like a balloon.",
              "maleVariant": "",
              "stringId": "1811416248164831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's do this thing.",
              "maleVariant": "",
              "stringId": "1811418292787367936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure this is a good idea?",
              "maleVariant": "",
              "stringId": "1811419470111723520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You absolutely sure this is a good idea?",
              "maleVariant": "Are you absolutely sure this is a good idea?",
              "stringId": "1811421128254312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, V. No, I am not.",
              "maleVariant": "",
              "stringId": "1811422682579488768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's do it.",
              "maleVariant": "",
              "stringId": "1811422853220552704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hell yeah. I've already got a rag in the can. You light it, I'll start the car.",
              "maleVariant": "",
              "stringId": "1811423155025891328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Light]",
              "maleVariant": "",
              "stringId": "1811423554004865028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She's burning! Quick, before she blows!",
              "maleVariant": "",
              "stringId": "1811423894330691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Out with a bang. Just like he wanted.",
              "maleVariant": "",
              "stringId": "1811424662542635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Soot and ash. That much remains.",
              "maleVariant": "",
              "stringId": "1811425287259049984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure. I'll drink to a send-off like that.",
              "maleVariant": "",
              "stringId": "1811425434059689984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure thing.",
              "maleVariant": "",
              "stringId": "1811425723818987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So... How about a beer?",
              "maleVariant": "",
              "stringId": "1811722638246825984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I gotta go.",
              "maleVariant": "",
              "stringId": "1811735446527168512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry... Wakes aren't really my thing.",
              "maleVariant": "",
              "stringId": "1811735716288024576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, come on.",
              "maleVariant": "",
              "stringId": "1811741647050657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaaah... Nothing like a good cold one.",
              "maleVariant": "",
              "stringId": "1811744593180119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agreed.",
              "maleVariant": "",
              "stringId": "1811747013125754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take]",
              "maleVariant": "",
              "stringId": "1845779842239057924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is it?",
              "maleVariant": "",
              "stringId": "1845780327872352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not a clue... Scorpion always had it on him. No idea why.",
              "maleVariant": "",
              "stringId": "1845792706219757568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen... I'm going to sit here for a while, all right? Brood in peace.",
              "maleVariant": "",
              "stringId": "1845792948096880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. He wanted me to take him here... and send him off with a bang.",
              "maleVariant": "",
              "stringId": "1863237586613751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We set him up behind the wheel, douse the car... then light it, let him fly off the edge.",
              "maleVariant": "",
              "stringId": "1863238860105109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're out of your goddamn mind!",
              "maleVariant": "",
              "stringId": "1863240077191467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mitch. You've fucking gonked.",
              "maleVariant": "",
              "stringId": "1863241269229441024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, yeah, quit complaining.",
              "maleVariant": "",
              "stringId": "1863243586733395968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Grab me that can, will you?",
              "maleVariant": "",
              "stringId": "1863245705880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right. Shall we get this show on the road?",
              "maleVariant": "",
              "stringId": "1863249863308005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But it was my best friend's dying wish. So I'll do whatever the fuck I can to make it happen. With or without you.",
              "maleVariant": "",
              "stringId": "1863252538233769984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welp, that's your loss.",
              "maleVariant": "",
              "stringId": "1863258104175841280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, V! Thanks. I appreciate it.",
              "maleVariant": "",
              "stringId": "1863258316642504704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mitch – why me? Why'd you ask me to help?",
              "maleVariant": "",
              "stringId": "1863262761832992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why'd you ask me to help?",
              "maleVariant": "",
              "stringId": "1863262994465869828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd \"Scorpion\" come from?",
              "maleVariant": "",
              "stringId": "1863263606398046208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, speakin' of... Where'd that nickname come from? \"Scorpion\"?",
              "maleVariant": "",
              "stringId": "1863264231651332096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Funny... You know, I haven't the slightest idea. Never asked him.",
              "maleVariant": "",
              "stringId": "1863264970436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He a Scorpio or somethin'?",
              "maleVariant": "",
              "stringId": "1863265301551173632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh – maybe he had a big stinger, hehehe!",
              "maleVariant": "",
              "stringId": "1863266236545421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, Scorpion, you old bastard... I'm 'onna miss you, man.",
              "maleVariant": "",
              "stringId": "1863266849886883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, you seem like a good egg.",
              "maleVariant": "",
              "stringId": "1863267756393418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm serious.",
              "maleVariant": "",
              "stringId": "1863268095561617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know, I uh... I guess the fact that I'm still here to send him off right – that's thanks to you.",
              "maleVariant": "",
              "stringId": "1863268252848017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your paths crossed. Just for a moment... But damn, was it the right one.",
              "maleVariant": "",
              "stringId": "1863269066895314944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, there's a... a few things of his you oughta have. Like this little good luck charm. Go on, it's yours.",
              "maleVariant": "",
              "stringId": "1863270157296914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Understood. I'll be off, then.",
              "maleVariant": "Understood. I'll, uh... I'll be off, then.",
              "stringId": "1863272334694338560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, and, uh... thanks, V. For today... And for getting me out of that shit back then, too.",
              "maleVariant": "",
              "stringId": "1863272509596815360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're all right. You're really all right.",
              "maleVariant": "",
              "stringId": "1863273061936320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothing for me, thanks.",
              "maleVariant": "",
              "stringId": "1873522370078363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, thanks. Rather keep things sober.",
              "maleVariant": "",
              "stringId": "1873523224223207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, my city choom... everyone grieves their own way.",
              "maleVariant": "",
              "stringId": "1873524512897945600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you say so.",
              "maleVariant": "",
              "stringId": "1873528054668906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure. Listen, if you want, you can take my car back to camp. Whatever works for you.",
              "maleVariant": "",
              "stringId": "1908015766788427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "May the road be kind to ya, friend.",
              "maleVariant": "",
              "stringId": "1934467678261235712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You lost your damn mind?!",
              "maleVariant": "",
              "stringId": "2121969965643616260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V... the can. What are you waiting for?",
              "maleVariant": "",
              "stringId": "2238669820146728960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's do this, V. For Scorpion.",
              "maleVariant": "",
              "stringId": "2238714786642378752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go ahead, light it!",
              "maleVariant": "",
              "stringId": "2238716481912946688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon. It's what he wanted.",
              "maleVariant": "",
              "stringId": "2238716544173195264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cat got your tongue?",
              "maleVariant": "",
              "stringId": "2238721843760185344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, no need for a moment of silence just yet.",
              "maleVariant": "",
              "stringId": "2238722204570992644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You still with us, V?",
              "maleVariant": "",
              "stringId": "2238722248493744132"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}