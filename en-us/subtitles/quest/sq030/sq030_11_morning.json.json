  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will ya sit with me a while?",
              "maleVariant": "",
              "stringId": "1751653949060923392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Mornin'.",
              "maleVariant": "",
              "stringId": "1751654786579546116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So... yesterday... What was that, exactly?",
              "maleVariant": "",
              "stringId": "1751657975072669696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The beginning of something amazing.",
              "maleVariant": "",
              "stringId": "1751658319592800260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A pleasant distraction.",
              "maleVariant": "",
              "stringId": "1751658742043099136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think it was the same for you and me both...",
              "maleVariant": "I think it was the same for you and me both...",
              "stringId": "1751659209204678656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think it... I mean... I hope it was the beginning of something nice.",
              "maleVariant": "I think it... I mean, I hope it was the beginning of somethin' nice.",
              "stringId": "1751659374745468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin'?",
              "maleVariant": "",
              "stringId": "1751663244695552000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Releasing tension. Right. 'Course.",
              "maleVariant": "",
              "stringId": "1751663978883301376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Unless... you see things differently?",
              "maleVariant": "Unless you see things, uh... differently?",
              "stringId": "1751669740279939072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know... yesterday, I think I'd made up my mind...",
              "maleVariant": "",
              "stringId": "1751670915859464192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Actually called you 'cause I wanted to say goodbye.",
              "maleVariant": "",
              "stringId": "1751671777755381760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What're you trying to say?!",
              "maleVariant": "",
              "stringId": "1751673126509330436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City's not for everyone.",
              "maleVariant": "",
              "stringId": "1751675690588028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Since I've known you, Judy, all you've been is chaotic, unstable and hotheaded.",
              "maleVariant": "",
              "stringId": "1751676894034509824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gimme your hand, V.",
              "maleVariant": "",
              "stringId": "1751688305896833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Give hand]",
              "maleVariant": "",
              "stringId": "1763534162501812228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What for?",
              "maleVariant": "",
              "stringId": "1763534572738297856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? What for?",
              "maleVariant": "",
              "stringId": "1763535795797344256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know what to say...",
              "maleVariant": "",
              "stringId": "1763536185364299780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure?",
              "maleVariant": "",
              "stringId": "1763536510573854720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's like that joke…",
              "maleVariant": "",
              "stringId": "1763536567079518208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dunno what to say... Nobody's ever done anything like this for me. Really shows trust.",
              "maleVariant": "",
              "stringId": "1763537070899314688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't say anything. Just gimme your hand.",
              "maleVariant": "",
              "stringId": "1763538702735228928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All set, congrats. Just gave you unlimited access to my pad.",
              "maleVariant": "",
              "stringId": "1763539730272600064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I-if it's what you want. Or just drop by when the urge grabs you.",
              "maleVariant": "",
              "stringId": "1763540350408835072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure? Isn't exactly a small thing, this...",
              "maleVariant": "You sure about this? Haven't known each other that long.",
              "stringId": "1763541263978577920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's like that joke... Know the one?",
              "maleVariant": "It's like that joke, uh... Y'know the one?",
              "stringId": "1763541469163929600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna go there now?",
              "maleVariant": "",
              "stringId": "1763541804339150848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, Night City's not for everyone. Grew up here, thought nothin' could surprise me...",
              "maleVariant": "Look, Night City's not for everyone. I grew up here, thought nothin' could surprise me...",
              "stringId": "1765038645287305216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It'll be the same anywhere else.",
              "maleVariant": "",
              "stringId": "1765039078659571712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People shouldn't live so cramped together, on top of each other.",
              "maleVariant": "",
              "stringId": "1765039389658824704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know... think if you lined up all cities next to each other, they'd look the same.",
              "maleVariant": "",
              "stringId": "1765040187331559424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know why I came to Night City? 'Cause I thought it offered opportunities you couldn't come by anywhere else.",
              "maleVariant": "",
              "stringId": "1765040403556319232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now?",
              "maleVariant": "",
              "stringId": "1765041234733154304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe so... Maybe all cities got allurin' faces... But stay long in one and you see what's behind it.",
              "maleVariant": "",
              "stringId": "1765043703282384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I feel you...",
              "maleVariant": "",
              "stringId": "1765043852817711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't remember, or you want me to remind you?",
              "maleVariant": "You don't remember? Or do you want me to remind you?",
              "stringId": "1799689038386126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know exactly what I'm tryna say. What did it mean, like, to you?",
              "maleVariant": "",
              "stringId": "1799690050371018752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pleasant distraction. A chance to let loose, nice way to spend the evening...",
              "maleVariant": "",
              "stringId": "1799698894799200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd better get goin'.",
              "maleVariant": "",
              "stringId": "1799699640026357760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... Can be such a gonk sometimes.",
              "maleVariant": "",
              "stringId": "1799719924435476480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "City's chewed me up, an' it's spittin' me out.",
              "maleVariant": "",
              "stringId": "1799725237377130496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "M'gonna miss you too, V...",
              "maleVariant": "",
              "stringId": "1799747156356849664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd pack up my shit and bail too, if I could…",
              "maleVariant": "",
              "stringId": "1799755172879753216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But... it's just not the right time.",
              "maleVariant": "",
              "stringId": "1799755534428758016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Corps're makin' sure of that. Makin' life uniform, everywhere.",
              "maleVariant": "Corps're makin' sure of that, makin' life uniform. Everywhere.",
              "stringId": "1799760435238100992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I can't leave Night City yet.",
              "maleVariant": "",
              "stringId": "1799762395437686784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now it feels like I'm standin' in quicksand. That's no way to live... for anyone.",
              "maleVariant": "Now, it feels like I'm standin' in quicksand. That's no way to live... for anyone.",
              "stringId": "1799765144753627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I can't leave Night City, not yet. But someday – who knows.",
              "maleVariant": "",
              "stringId": "1799767162566172672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can if you want. Gonna stick around for a bit before I head back.",
              "maleVariant": "",
              "stringId": "1799810038117392384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I like you. Wanna see you lots. Way I see it, there isn't anything else to consider.",
              "maleVariant": "",
              "stringId": "1799811422556479488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ummm, no...?",
              "maleVariant": "",
              "stringId": "1799813255802871808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"What do lesbians bring on their second date?\" Moving vans. Both.",
              "maleVariant": "",
              "stringId": "1799813395892625408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah-- Oh, that one? Vintage as groaners go.",
              "maleVariant": "",
              "stringId": "1799814666280198144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait.",
              "maleVariant": "",
              "stringId": "1837495541293604868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "H-hang on... that-- that didn't come out quite right.",
              "maleVariant": "Hang on... that didn't come out quite right.",
              "stringId": "1837496417886363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya.",
              "maleVariant": "",
              "stringId": "1837496711454089216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, Judy.",
              "maleVariant": "I'll see ya, Judy.",
              "stringId": "1837497600344547328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, bye, V. Have a nice life.",
              "maleVariant": "",
              "stringId": "1837498515004809216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meant what you said, V. Don't pretend like you can take it back.",
              "maleVariant": "",
              "stringId": "1837502173259980800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's not make this longer than it needs to be. See ya, V.",
              "maleVariant": "",
              "stringId": "1837504886521683968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Judy... s-sorry. Thought you felt the same way.",
              "maleVariant": "Judy... sorry, I... I thought you felt the same way.",
              "stringId": "1837507113915211776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You thought one thing, I thought another. Crossed wires, it happens.",
              "maleVariant": "",
              "stringId": "1837509961579192320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa, hah... Askin' me to move in?",
              "maleVariant": "Whoa... Askin' me to move in?",
              "stringId": "1846011584026730496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mornin'.",
              "maleVariant": "",
              "stringId": "1856223289511993344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure about this? Haven't known each other that long...",
              "maleVariant": "",
              "stringId": "1869373333740150784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I like you, consider you a friend. Way I see it, there's nothing else to consider.",
              "maleVariant": "",
              "stringId": "1869377518497914880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take care, V.",
              "maleVariant": "",
              "stringId": "1869386072193720324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This really it? Last time we see each other? Didn't seem so set on leaving just a sec ago.",
              "maleVariant": "",
              "stringId": "1869386075045847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See no reason to delay. I'll stay here a couple days... then head off.",
              "maleVariant": "",
              "stringId": "1869390033227309056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's been nice knowin' ya, V.",
              "maleVariant": "",
              "stringId": "1869391729823277056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya... And hit me up on the holo sometimes.",
              "maleVariant": "",
              "stringId": "1869395868645023744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1894067942620987392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Later.",
              "maleVariant": "",
              "stringId": "1895239887110602752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, Judy.",
              "maleVariant": "I'll see ya, Judy.",
              "stringId": "1895239889526521856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here's your coffee. Finally.",
              "maleVariant": "",
              "stringId": "1915739148995223552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're crazy. But I'll miss ya.",
              "maleVariant": "",
              "stringId": "1917219553321639936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna miss ya.",
              "maleVariant": "I'm gonna miss ya.",
              "stringId": "1917262112001581056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ruined my plans, you know that?",
              "maleVariant": "",
              "stringId": "1918361810695315456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was gonna leave Night City in the dust, for good. Even started packing.",
              "maleVariant": "",
              "stringId": "1918376736511975424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why did you wanna leave?",
              "maleVariant": "",
              "stringId": "1918384194991120388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why'd you wanna leave?",
              "maleVariant": "",
              "stringId": "1918384482703597568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, well, guess I'll have to make it up to you, won't I?",
              "maleVariant": "Oh, well, uh... I guess I'll have to make it up to ya, won't I?",
              "stringId": "1918384511778512896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Positively burst into my life, you did, so I should've expected an ending just as abrupt, like this...",
              "maleVariant": "Positively burst into my life, you did... so I should've expected an ending just as abrupt, like this...",
              "stringId": "1918389346535956480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna make it up to you, somehow.",
              "maleVariant": "",
              "stringId": "1918396618234355712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But now I think... Think I gotta stay.",
              "maleVariant": "",
              "stringId": "1918404726344081408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know, everything that happened... Guess it just got to me.",
              "maleVariant": "",
              "stringId": "1918406390945898496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But now, from where I stand... Night City's started to feel like the place to be.",
              "maleVariant": "",
              "stringId": "1918410556913180672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm... Yeah, that would be appropriate.",
              "maleVariant": "",
              "stringId": "1918415779878625280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck, Judy? Sayin' you wanna leave? Now?",
              "maleVariant": "The fuck, Judy? You sayin' you wanna leave? Now?",
              "stringId": "1924289750914260992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know, had this bizarre convo with a doll... Told me I had to fight. And I thought – fight for what?",
              "maleVariant": "Y'know, I had this bizarre convo with a doll... Told me I had to fight. And I thought – fight for what?",
              "stringId": "1924290877487538176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not like we won't keep in touch...",
              "maleVariant": "",
              "stringId": "1924291545237512192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now, just when I thought I knew what it was all for... Friendship, trust... You're tellin' me you're leavin'?",
              "maleVariant": "And now, just when I thought I knew what it was all for... Friendship, trust... You're tellin' me you're leavin'.",
              "stringId": "1924291792433012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pick up clothes]",
              "maleVariant": "",
              "stringId": "2240452560525975556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pick up clothes]",
              "maleVariant": "",
              "stringId": "2240460289269293056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Drink]",
              "maleVariant": "",
              "stringId": "2260589178882424836"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}