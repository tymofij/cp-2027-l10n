  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmmm-hmmm, hmmm-hmmm…",
              "maleVariant": "",
              "stringId": "1395348872451047424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great.",
              "maleVariant": "",
              "stringId": "1395421141030387712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great, signal's clear. Just need your help with a few basic checks.",
              "maleVariant": "",
              "stringId": "1395429570155364352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Next up – sound check.",
              "maleVariant": "",
              "stringId": "1395439771843543040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know that song.",
              "maleVariant": "",
              "stringId": "1395441654498836480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't know the title. Can hum it back, though.",
              "maleVariant": "",
              "stringId": "1395444193243938816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, I know that! Etta Sorrentino – \"Only You.\"",
              "maleVariant": "",
              "stringId": "1395448308678246400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea what the song is, but I heard it. Uh, goes something like…",
              "maleVariant": "No idea what the song is... but I heard it. Goes something like…",
              "stringId": "1395448447006392320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First up – motion sensors and scrollin'.",
              "maleVariant": "",
              "stringId": "1395452313131270144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great, motion calibrated. Now swim hither?",
              "maleVariant": "Great, motion calibrated. Back my way now, V.",
              "stringId": "1395476029554802688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never did go in for sentimental stuff... Got stuck in my head lately, though, that one.",
              "maleVariant": "Dead on.",
              "stringId": "1395489315063054336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, we're all tuned and synced.",
              "maleVariant": "",
              "stringId": "1395490370769371136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Incredible.",
              "maleVariant": "",
              "stringId": "1396911122395676672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had no idea there were buildings down here.",
              "maleVariant": "",
              "stringId": "1396911768805031936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wanna explore that?",
              "maleVariant": "",
              "stringId": "1396913168863707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's-- That's incredible... What is it?",
              "maleVariant": "",
              "stringId": "1396915296667680784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had no idea... If you'd said we'd see a sunken town…",
              "maleVariant": "",
              "stringId": "1396915609092997120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Impressive, huh?",
              "maleVariant": "",
              "stringId": "1396917884184150016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like it?",
              "maleVariant": "",
              "stringId": "1396918688366444544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enjoying the view?",
              "maleVariant": "Like the view, asshole?",
              "stringId": "1397900115623002112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Swim over here an' look straight at me, will ya?",
              "maleVariant": "",
              "stringId": "1404236571471450112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmmm-hmmm, hmmm-hmmm…",
              "maleVariant": "",
              "stringId": "1404259944045940736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our very own Atlantis.",
              "maleVariant": "",
              "stringId": "1405226566315347968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dunno. Sure as hell never seen anything like it, can tell you that.",
              "maleVariant": "",
              "stringId": "1405230930891313152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, that oughta do it.",
              "maleVariant": "",
              "stringId": "1741558539680202752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't fuck up a thing.",
              "maleVariant": "",
              "stringId": "1764996410860617728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to calibrate our gear.",
              "maleVariant": "",
              "stringId": "1782099299114115072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Circle around me, swim.",
              "maleVariant": "",
              "stringId": "1782103737459499008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But... I hear you just fine.",
              "maleVariant": "",
              "stringId": "1782108887678935040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need a wider range. Levels can't be short of perfect.",
              "maleVariant": "",
              "stringId": "1782109106067955712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yep, view's preem.",
              "maleVariant": "",
              "stringId": "1782299025361035268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Um, sorry.",
              "maleVariant": "",
              "stringId": "1782300205017423872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang on, you can see what I see?",
              "maleVariant": "",
              "stringId": "1782300311837958144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Visual sensors're gettin' stimmed right now, can't be denied.",
              "maleVariant": "",
              "stringId": "1782302290811248640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, uff... sorry.",
              "maleVariant": "Oh, um... sorry.",
              "stringId": "1782308983997685760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa. Y-You see what I see, like, in real-time?",
              "maleVariant": "Whoa. You see what I see? In real-time? Like, what I'm lookin' at?",
              "stringId": "1782309491240034304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hahaha... Don't get excited. Oxygen supply's limited.",
              "maleVariant": "I'm gonna pretend I didn't hear that.",
              "stringId": "1782318042503143424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaaat? Don't be.",
              "maleVariant": "Don't lemme catch you at it again.",
              "stringId": "1782334331686707200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, yeah. I see everything.",
              "maleVariant": "Mhm. And I gotta say it's pretty fucking weird.",
              "stringId": "1782338317668016128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To think I never realized how fit you were.",
              "maleVariant": "",
              "stringId": "1795274115472121856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right. We were gonna take in our surroundings.",
              "maleVariant": "Right. We were, uh, gonna take in our surroundings.",
              "stringId": "1835925363543990272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could prove more difficult than we thought...",
              "maleVariant": "Exactly. Landscape, dammit.",
              "stringId": "1835926310668165120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That proves it – I officially suck. Probably fucked up the settings…",
              "maleVariant": "",
              "stringId": "1855014318310645760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whatever you wanna do, Judy. Guess I'm with you.",
              "maleVariant": "",
              "stringId": "1857415626962448384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahh! Too loud!",
              "maleVariant": "Ouch! Too loud!",
              "stringId": "1857833897181274112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang on, that's prolly too loud. Should be OK... now.",
              "maleVariant": "",
              "stringId": "1857834701095133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now? Better?",
              "maleVariant": "",
              "stringId": "1862100099638452224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lots, thanks.",
              "maleVariant": "",
              "stringId": "1862101175477428224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Read me?",
              "maleVariant": "",
              "stringId": "1862129844048584704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't fuck anything up.",
              "maleVariant": "",
              "stringId": "1863285596914147328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa! OK... Works better than I thought. Gonna dial it down a bit.",
              "maleVariant": "",
              "stringId": "1863285603759251456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa! Oww!",
              "maleVariant": "",
              "stringId": "1866398991413620736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm 'onna hum a song, you gimme the title.",
              "maleVariant": "",
              "stringId": "1867766968559034368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got some connection to this place?",
              "maleVariant": "",
              "stringId": "1900935126233104388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wanna explore all that?",
              "maleVariant": "",
              "stringId": "1900939029653733376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to be called Laguna Bend. Just fifteen years ago, people still lived here.",
              "maleVariant": "",
              "stringId": "1900939610464174080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got some connection to this place, right?",
              "maleVariant": "",
              "stringId": "1900940395621105664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What happened here? Why'd it get flooded?",
              "maleVariant": "",
              "stringId": "1900940529218076672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Let go] Let's get moving.",
              "maleVariant": "",
              "stringId": "1900940777118220288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds nova. Let's do this.",
              "maleVariant": "",
              "stringId": "1900943117103644672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah... I remember.",
              "maleVariant": "",
              "stringId": "1900943312558211072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What happened here?",
              "maleVariant": "",
              "stringId": "1900986615257350144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why are you showing me this?",
              "maleVariant": "",
              "stringId": "1900990045862477824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah... I remember.",
              "maleVariant": "",
              "stringId": "1900999202363654144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why are you showing me this?",
              "maleVariant": "Why're you showing me this?",
              "stringId": "1901002420267237376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This'll be perfect? For what, exactly?",
              "maleVariant": "",
              "stringId": "1901002630888407040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why's this stand to be perfect?",
              "maleVariant": "",
              "stringId": "1901002699289116672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Cause someone stood to profit.",
              "maleVariant": "",
              "stringId": "1901008045550321664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NC Dam Ltd. made plans to build a dam. People protested, and... the rest is history.",
              "maleVariant": "NC Dam Ltd. made plans to build a dam. People protested, and the rest is history.",
              "stringId": "1901008846863712256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You got it. Grew up here.",
              "maleVariant": "",
              "stringId": "1901009424335486976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Haven't been back since Laguna Bend was wiped off the map.",
              "maleVariant": "",
              "stringId": "1901010057054633984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ready to wrestle with the demons of my past?",
              "maleVariant": "",
              "stringId": "1901014844953313280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's important to me.",
              "maleVariant": "Wanna get the right emotional reactions.",
              "stringId": "1901028030385471488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a feeling this'll be perfect for my experiment.",
              "maleVariant": "",
              "stringId": "1901028314742505472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hopin' you'll be able to pick up all the unfiltered mess of emotions this place brings out in me.",
              "maleVariant": "",
              "stringId": "1901029829892882432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Grab] Down we go!",
              "maleVariant": "",
              "stringId": "1901034579589840900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Down we go!",
              "maleVariant": "",
              "stringId": "1901035122768986112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not me. I want you to explore it.",
              "maleVariant": "",
              "stringId": "1902600120401879040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Houses, playgrounds – all flooded.",
              "maleVariant": "",
              "stringId": "1902746591470247936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now grab this and hold on tight.",
              "maleVariant": "",
              "stringId": "1902819286023102464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like it or not, you'll react to those feelings. That's what I wanna capture.",
              "maleVariant": "",
              "stringId": "1902831822143447040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, that oughta do it. We are now…",
              "maleVariant": "",
              "stringId": "1966402443716288516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Synced.",
              "maleVariant": "",
              "stringId": "1966402443733065748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What now?",
              "maleVariant": "",
              "stringId": "1966404144221347872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Prepare to have your mind blown. Wait for it...",
              "maleVariant": "",
              "stringId": "1966404144221347888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you've got talent, girl.",
              "maleVariant": "",
              "stringId": "1966409164769681408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't dive down alone. Wetsuit won't stand the pressure.",
              "maleVariant": "",
              "stringId": "2010042641268039680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, this isn't a joke! You go down there alone, the pressure'll kill you!",
              "maleVariant": "",
              "stringId": "2010042680577056768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hear you just fine.",
              "maleVariant": "",
              "stringId": "2066599565160296448"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}