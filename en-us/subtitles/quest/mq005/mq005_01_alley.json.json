    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck you lookin' at? Nothin' to see here – move along!",
              "maleVariant": "",
              "stringId": "1827261479582191616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's going on?",
              "maleVariant": "",
              "stringId": "1827262982250319872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stop it or else you'll kill him.",
              "maleVariant": "",
              "stringId": "1827263983934001152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's goin' on here?",
              "maleVariant": "",
              "stringId": "1827264974796378112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take it easy, or you'll kill 'im.",
              "maleVariant": "",
              "stringId": "1827265192178765828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is this how you protect and serve?",
              "maleVariant": "",
              "stringId": "1827265657964613632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This your first day in Night City? What's it fuckin' look like?",
              "maleVariant": "",
              "stringId": "1827268203001171968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get outta here, piss-ant!",
              "maleVariant": "",
              "stringId": "1827268956096843776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, and? He your brother or somethin'? The fuck you care?",
              "maleVariant": "",
              "stringId": "1827271164548894720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, is this how Night City's finest \"protect and serve\"?",
              "maleVariant": "Is this how Night City's finest \"protect and serve\"?",
              "stringId": "1827271838573547520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't care – don't know 'im.",
              "maleVariant": "I don't care – don't know 'im.",
              "stringId": "1827273266364960768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, why the fuck we talkin'?",
              "maleVariant": "",
              "stringId": "1827273370081710080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He put a girl into a coma. Daughter of a friend.",
              "maleVariant": "",
              "stringId": "1827274107742343168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like to know what's goin' on around me. Not every day you see a man in a thousand-eddie suit gettin' beaten within an inch of his life. Wouldn't you agree?",
              "maleVariant": "I like to know what's goin' on around me. It's not every day you see a man in a thousand-eddie suit gettin' beaten within an inch of his life. Wouldn't you agree?",
              "stringId": "1827274573695963136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave him alone.",
              "maleVariant": "",
              "stringId": "1827275835074170880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll deal with him.",
              "maleVariant": "",
              "stringId": "1827276407428894720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was just about to leave.",
              "maleVariant": "",
              "stringId": "1827277248974049280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was just leaving, officer. Enjoy your evening.",
              "maleVariant": "",
              "stringId": "1827278846433447936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave him alone.",
              "maleVariant": "Leave 'im alone.",
              "stringId": "1827279028080365572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From what I can tell, your guy's a mid-level manager.",
              "maleVariant": "",
              "stringId": "1827279494654742528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See this badge? That means I can do whatever the fuck I want!",
              "maleVariant": "",
              "stringId": "1827280837234675712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And?",
              "maleVariant": "",
              "stringId": "1827282091348348928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Corp won't let this slide. They'll pull the data from this asshole's biomon and find you. And then, they'll shoot you.",
              "maleVariant": "The corp won't let this slide. They'll pull the data from this asshole's biomon and find you. And then, they'll shoot you.",
              "stringId": "1827282222948831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rise and shine, corpo-swine...",
              "maleVariant": "",
              "stringId": "1827295322112544768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Wake]",
              "maleVariant": "",
              "stringId": "1827345226008645636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Akh... oh fuck... khe khe...",
              "maleVariant": "",
              "stringId": "1827347155774361600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heard you've been showing spiked braindances.",
              "maleVariant": "",
              "stringId": "1827348805343793152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The truth. What did they want from you?",
              "maleVariant": "",
              "stringId": "1827349428986466304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eddies.",
              "maleVariant": "",
              "stringId": "1827350291368923136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heard you've been showin' girls XBDs. You know – kind where your nose bleeds, your mouth froths, brain fries a little...",
              "maleVariant": "",
              "stringId": "1827351269296066560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, no, no... Wait, I didn't know... Pusher must've sold me a bad batch...",
              "maleVariant": "",
              "stringId": "1827353522643947520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bullshit. Corpo-scum like you don't get their merch on random street corners. You know what you're watchin', and what you're showin' to others.",
              "maleVariant": "",
              "stringId": "1827353934843367424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, all right, it's like... You know how it is when... when you want to get a girl in a club, right?",
              "maleVariant": "",
              "stringId": "1827354832206319616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The truth. What did they want from you?",
              "maleVariant": "The truth. What do they want from you?",
              "stringId": "1827355333928964096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I-I-I have no idea. They said somethin' about some slut a-and a virtu...",
              "maleVariant": "",
              "stringId": "1827356336787050496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is it really that hard to figure out? I'll give you a hint. It starts with \"euro\" and ends with \"dollars.\"",
              "maleVariant": "",
              "stringId": "1827356739322793984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah... yeah, no, of course... you need a reward... I-is this enough?",
              "maleVariant": "",
              "stringId": "1827357086527279104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on, get outta here.",
              "maleVariant": "",
              "stringId": "1827358336866078724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hand over your eddies.",
              "maleVariant": "",
              "stringId": "1827359292932509696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Punch]",
              "maleVariant": "",
              "stringId": "1827359971017248768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Shoot]",
              "maleVariant": "",
              "stringId": "1827360292183494656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothing. It's your lucky day.",
              "maleVariant": "",
              "stringId": "1827362979608285184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice chattin' with ya. Now hand over the eddies.",
              "maleVariant": "",
              "stringId": "1827365260957339652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1827365397221888004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know what I want? For suit 'n' tie slimebags like you to disappear from Night City. For the NCPD to lock you up for good.",
              "maleVariant": "",
              "stringId": "1827369628435763200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I could've killed you. As a matter of fact, I still want to pretty badly.",
              "maleVariant": "",
              "stringId": "1827373095413895168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take this, you corpo bastard!",
              "maleVariant": "",
              "stringId": "1863401298620076032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep walkin', before you end up like him.",
              "maleVariant": "",
              "stringId": "1863473810435788800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? You wanna be some kinda hero?",
              "maleVariant": "",
              "stringId": "1863477652133040128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rotten skiff or an XBD?",
              "maleVariant": "",
              "stringId": "1863480843444736000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Second one.",
              "maleVariant": "",
              "stringId": "1863481154292994048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, yeah.",
              "maleVariant": "",
              "stringId": "1863483796771622912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You think I don't know that? Fuckin' corpos walkin' around like holy cows in a two piece. But I ain't lettin' 'im go. No fuckin' chance.",
              "maleVariant": "",
              "stringId": "1863487317436846080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah? And what's in it for you?",
              "maleVariant": "",
              "stringId": "1863488880955936768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin'. It's just how I unwind.",
              "maleVariant": "",
              "stringId": "1863489446633328640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, let's go, boys.",
              "maleVariant": "",
              "stringId": "1863489872002863104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take care of yourself... Officer.",
              "maleVariant": "",
              "stringId": "1863490297556946944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit... khe khe... If it wasn't for you, I'd be... well, thank you. Were you sent by the company?",
              "maleVariant": "",
              "stringId": "1863491611061964800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wrong answer.",
              "maleVariant": "",
              "stringId": "1863492293928210432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then... hold on... Whaddaya want from me?",
              "maleVariant": "",
              "stringId": "1863492519145558016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin'.",
              "maleVariant": "",
              "stringId": "1863494916358066176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I don't... I don't believe you. You saved me just like that, because you... felt like it?",
              "maleVariant": "",
              "stringId": "1863495466248097792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't measure others by your own yardstick. Prick.",
              "maleVariant": "",
              "stringId": "1863499430687461376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know it's not if you're askin'. Don't make me finish what the badges started.",
              "maleVariant": "",
              "stringId": "1863501473850380288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK... Here! Here, here... Tha-that's everything I have on me...",
              "maleVariant": "",
              "stringId": "1863501842663919616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have a pleasant evening.",
              "maleVariant": "",
              "stringId": "1863502495398285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But you just need a little... a-a nudge in the right direction.",
              "maleVariant": "",
              "stringId": "1863506211534852096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But when I asked what I had to do with it, they started beating on me so hard I thought I'd die.",
              "maleVariant": "",
              "stringId": "1863507186089775104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, this city's really going down the drain. For the police to do this, in plain sight, for no reason...",
              "maleVariant": "",
              "stringId": "1863507643503792128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on, get outta here.",
              "maleVariant": "",
              "stringId": "1863517885054636032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But... I don't understand... Just like that? For nothing in return?",
              "maleVariant": "",
              "stringId": "1863518633284915200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But that wouldn't change a damn thing. Waste of lead, in fact. City's teemin' with others just waitin' to replace you.",
              "maleVariant": "But that wouldn't change a damn thing. A waste of lead, in fact. City's teemin' with others just waitin' to replace you.",
              "stringId": "1863524383038164992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now get the fuck outta my sight. Make me sick just lookin' at you.",
              "maleVariant": "Now get the fuck outta my sight. You make me sick just lookin' at ya.",
              "stringId": "1863525630742941696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh! Gah, what the hell was that for?",
              "maleVariant": "",
              "stringId": "1863538853990944768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I see you again, or hear that you're jackin' girls into virtus, infected or otherwise...",
              "maleVariant": "",
              "stringId": "1863539628393680896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To be honest with you... I have no idea. But I'm guessin' you do.",
              "maleVariant": "To be honest with ya... I have no idea. But I'm guessin' you do.",
              "stringId": "1863543075104935936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somethin' in this whole story of yours just... doesn't sit right with me.",
              "maleVariant": "Somethin' in this whole story of yours... it just doesn't sit right with me.",
              "stringId": "1863543425715195904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not really in the mood for an interrogation, but if I was... I've got a feeling you'd have more than a black eye to worry about.",
              "maleVariant": "",
              "stringId": "1863544914173022208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll put you in a coma you'll never wake up from. Do you understand?",
              "maleVariant": "",
              "stringId": "1863546083595309056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I-I understand!",
              "maleVariant": "",
              "stringId": "1863546706784997376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now get the fuck outta my sight. Make me sick...",
              "maleVariant": "Now get the fuck outta my sight. You make me sick...",
              "stringId": "1863547010519715840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh! Psychos... city of fucking psychos!",
              "maleVariant": "",
              "stringId": "1863551046832672768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get outta my sight. Chop-chop, on the double.",
              "maleVariant": "",
              "stringId": "1863551444654018560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That'll do.",
              "maleVariant": "",
              "stringId": "1903919614585229316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give me everything you've got.",
              "maleVariant": "",
              "stringId": "1903920296931381248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That'll do. Now run off to your little castle made of glass before you run into someone who won't be so understanding.",
              "maleVariant": "That'll do. Now, run off to your little castle made of glass before you run into someone who won't be so understanding.",
              "stringId": "1903923283443601408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you! You have my gratitude.",
              "maleVariant": "",
              "stringId": "1903923759916535808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cocksuckin' suits.",
              "maleVariant": "",
              "stringId": "1923983258006376448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not asking you to. What I want is for you to let me finish the job.",
              "maleVariant": "",
              "stringId": "1923987034440257536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time to wake up.",
              "maleVariant": "",
              "stringId": "2370851565339693056"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}