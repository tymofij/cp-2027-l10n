        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Randy's been taken.",
              "maleVariant": "",
              "stringId": "1538453425783750656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss, I... I-I don't know how to say this, but... Randy…",
              "maleVariant": "",
              "stringId": "1538453519669051392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Randy's been taken.",
              "maleVariant": "",
              "stringId": "1538454087997243396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wh-why didn't you call?!",
              "maleVariant": "",
              "stringId": "1538497473928151040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss, I-I...",
              "maleVariant": "",
              "stringId": "1538501431404969984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was abducted.",
              "maleVariant": "",
              "stringId": "1538503428464099328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Abducted? But he's alive, right?",
              "maleVariant": "",
              "stringId": "1538503989963964416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Randy doesn't like it when anyone pokes through his stuff…",
              "maleVariant": "",
              "stringId": "1538515168857866240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's the trailer. Eyes peeled for details, eh?",
              "maleVariant": "",
              "stringId": "1567332748217204736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uncle River!",
              "maleVariant": "",
              "stringId": "1567332748217204764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lovely neighborhood. Mwah.",
              "maleVariant": "",
              "stringId": "1619595370849026048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Threw a blanket over the truth! Like always!",
              "maleVariant": "",
              "stringId": "1774857522462724096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River! Tell me, dammit!",
              "maleVariant": "",
              "stringId": "1774862711353643008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Monique, Dorian. Hey, guys.",
              "maleVariant": "",
              "stringId": "1774871715819687936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is this your girlfriend?",
              "maleVariant": "Is this your partner?",
              "stringId": "1774872828081999872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just a friend.",
              "maleVariant": "",
              "stringId": "1774873658218647552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Police caught the guy. But he caught a bullet, and he's in a coma, bein' watched.",
              "maleVariant": "",
              "stringId": "1816642810911989760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guy who did it got caught.",
              "maleVariant": "",
              "stringId": "1816642940532760576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to prepare for the worst.",
              "maleVariant": "",
              "stringId": "1816645322746437632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We hope Randy's OK, but... you oughta prepare for the worst.",
              "maleVariant": "We hope Randy's OK, but, uh... you oughta prepare for the worst.",
              "stringId": "1816645931071512576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're lookin', Joss...",
              "maleVariant": "",
              "stringId": "1816651264313597952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But where's Randy?",
              "maleVariant": "",
              "stringId": "1816654703374442496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heeere?!",
              "maleVariant": "",
              "stringId": "1816655571880583168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, please...",
              "maleVariant": "",
              "stringId": "1816658079604613120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where's my Randy?!",
              "maleVariant": "",
              "stringId": "1816658516516872192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We don't know that. But we're tryin' to find out.",
              "maleVariant": "We don't know that, but we're tryin' to find out.",
              "stringId": "1816659437653139456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arguing's not gonna help anyone.",
              "maleVariant": "",
              "stringId": "1816664082123399168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's my child! You got no place tellin' me how to behave!",
              "maleVariant": "",
              "stringId": "1816665555951493120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss...",
              "maleVariant": "",
              "stringId": "1816668743890968576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go, V.",
              "maleVariant": "",
              "stringId": "1835800900226125824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then why're you here instead of searchin' for him?!",
              "maleVariant": "",
              "stringId": "1835884804022865920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to look through Randy's stuff.",
              "maleVariant": "",
              "stringId": "1835885395805605892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arguing won't help.",
              "maleVariant": "",
              "stringId": "1835885520594538496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, we need to look through Randy's things.",
              "maleVariant": "",
              "stringId": "1835889417673379840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why?",
              "maleVariant": "",
              "stringId": "1835889878258290688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To find any lead we can. Trust me, Joss.",
              "maleVariant": "",
              "stringId": "1835889970868523008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just don't destroy anything.",
              "maleVariant": "",
              "stringId": "1835892085183614976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The kids're playing outside . Tell 'em it's time to eat.",
              "maleVariant": "",
              "stringId": "1862033175578906624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, V – is this about my Randy?",
              "maleVariant": "",
              "stringId": "1871940824978501632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss – meet V.",
              "maleVariant": "",
              "stringId": "1871942688524546048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You gonna stay over?",
              "maleVariant": "",
              "stringId": "1872261159225470976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll see.",
              "maleVariant": "",
              "stringId": "1872261159275802624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your mom's waiting for you with dinner.",
              "maleVariant": "",
              "stringId": "1872261159326134272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure been a while. Used to come over every weekend... long time ago.",
              "maleVariant": "",
              "stringId": "1918469973729054720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And changed at all since?",
              "maleVariant": "And? Changed at all since?",
              "stringId": "1918477925441572864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Places like this never change.",
              "maleVariant": "",
              "stringId": "1918478127791575040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's on your mind?",
              "maleVariant": "",
              "stringId": "1946218694827126784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think all the women in your life have... character.",
              "maleVariant": "Think it's good she learned the truth .",
              "stringId": "1946218694860681216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River...",
              "maleVariant": "",
              "stringId": "1993759722924118020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You either got business with me or news 'bout my Randy. Wouldn't drag your butt out here if ya didn't.",
              "maleVariant": "",
              "stringId": "1993759722924118032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out] Change much since then?",
              "maleVariant": "",
              "stringId": "2257734434446598148"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}