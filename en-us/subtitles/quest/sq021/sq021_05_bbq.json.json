        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "1350107290104152064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Go to sleep]",
              "maleVariant": "",
              "stringId": "1350115683376218112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss, we're finished.",
              "maleVariant": "",
              "stringId": "1600702994235703296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course.",
              "maleVariant": "",
              "stringId": "1600710286301642756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't, unfortunately.",
              "maleVariant": "",
              "stringId": "1600711198948638720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course I'll stay.",
              "maleVariant": "",
              "stringId": "1600715031753965568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can't, River, I'm sorry.",
              "maleVariant": "",
              "stringId": "1600715129347031040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Find your way all right?",
              "maleVariant": "",
              "stringId": "1600716115998326784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Sure.",
              "maleVariant": "",
              "stringId": "1740053952275124224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All done? Off to bed, then.",
              "maleVariant": "",
              "stringId": "1775169240199991296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River's a great investigator.",
              "maleVariant": "",
              "stringId": "1781950784164827136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River's a helluvan investigator. Randy's good as found.",
              "maleVariant": "River's a helluvan investigator. Randy's as good as found.",
              "stringId": "1781952809258684416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I still need your help. You leave now, I'll be all alone in this.",
              "maleVariant": "",
              "stringId": "1783702979909652480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't promise you anything, Joss... except that we'll do our best.",
              "maleVariant": "I can't promise ya anything, Joss, except... that we'll do our best.",
              "stringId": "1783871392925749248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll do your best...",
              "maleVariant": "",
              "stringId": "1783875090288611328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stayin', right? Ya gotta help me see this through, to the end.",
              "maleVariant": "",
              "stringId": "1783943455564763136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yup. Gonna go hit the sack soon.",
              "maleVariant": "Yup. Gonna hit the sack soon.",
              "stringId": "1816759538442641408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't promise anything.",
              "maleVariant": "",
              "stringId": "1816820710739091456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Joss? Ya mind if V takes Randy's trailer for the night?",
              "maleVariant": "",
              "stringId": "1816829845564104704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We left some for you, Uncle River.",
              "maleVariant": "",
              "stringId": "1837251816229834752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, Monique.",
              "maleVariant": "",
              "stringId": "1837251981317640192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell me you found something.",
              "maleVariant": "",
              "stringId": "1837253074957242368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now you, no bullshit, don't look at my brother, just tell me…",
              "maleVariant": "",
              "stringId": "1837258843349979140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will you find my boy?",
              "maleVariant": "",
              "stringId": "1837258843349979152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River... Tell me, and be honest... Will you find him?",
              "maleVariant": "",
              "stringId": "1837258843400310788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will. I promise.",
              "maleVariant": "",
              "stringId": "1837258843400310800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Found a strong lead.",
              "maleVariant": "",
              "stringId": "1837261049906204676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know more tomorrow.",
              "maleVariant": "",
              "stringId": "1837261341141897216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like Randy knew his kidnapper. They were in touch.",
              "maleVariant": "",
              "stringId": "1837262193172811776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll know more tomorrow.",
              "maleVariant": "",
              "stringId": "1837262232347611136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why would he do something like that?",
              "maleVariant": "",
              "stringId": "1838602908469432320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know him, kids... Look for help in the wrong places.",
              "maleVariant": "",
              "stringId": "1838603052904484864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I sent some evidence to the lab. There's a chance we could know where to find Randy by morning.",
              "maleVariant": "",
              "stringId": "1838606029971132416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't give up until I do, Joss.",
              "maleVariant": "",
              "stringId": "1838619807974559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, I'll stay.",
              "maleVariant": "",
              "stringId": "1838624776010207236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, I'm with you.",
              "maleVariant": "",
              "stringId": "1838624968092553216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really can't. You'll do just fine without me. Don't doubt it.",
              "maleVariant": "I really can't. You'll do just fine without me, don't doubt it.",
              "stringId": "1838624992637620224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really can't. But you'll be fine.",
              "maleVariant": "",
              "stringId": "1838625098250194944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang tough, Joss. River'll find 'im, safe.",
              "maleVariant": "Hang tough, Joss. River will find him, safe.",
              "stringId": "1838629406790258688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll... I'll see ya 'round.",
              "maleVariant": "I'll, uh... I'll see ya around.",
              "stringId": "1838629636956884992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks. Head over to Randy's. Got a long day ahead of us tomorrow…",
              "maleVariant": "",
              "stringId": "1838632430682116096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "1863135459624849408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why aren't you sleeping?",
              "maleVariant": "",
              "stringId": "1863237598175006720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not going to sleep?",
              "maleVariant": "",
              "stringId": "1863248361195278336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to mull over some things. G'night, V.",
              "maleVariant": "",
              "stringId": "1863249962261778432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oughta get some sleep, Joss.",
              "maleVariant": "",
              "stringId": "1863488217301356548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1863488217301356556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should get some sleep.",
              "maleVariant": "",
              "stringId": "1863488217334910976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When's the last time you ate?",
              "maleVariant": "",
              "stringId": "1885172483379298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yesterday.",
              "maleVariant": "",
              "stringId": "1885173104840933376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have mine. I'm not hungry.",
              "maleVariant": "",
              "stringId": "1885173308851879936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, maybe later.",
              "maleVariant": "",
              "stringId": "1885174294563651584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dorian, please.",
              "maleVariant": "",
              "stringId": "1921320423465652224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But it really tastes like boogers!",
              "maleVariant": "",
              "stringId": "1921321103513325568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh! Boogers, ewww!",
              "maleVariant": "",
              "stringId": "1921321389162205184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, let's not worry Joss more than we need to, huh?",
              "maleVariant": "",
              "stringId": "1926022453181681664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're hot on the trail, we'll know more tomorrow. That's it.",
              "maleVariant": "",
              "stringId": "1926025512322469888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Go to sleep]",
              "maleVariant": "",
              "stringId": "1953007455896047620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "River's a great investigator.",
              "maleVariant": "",
              "stringId": "2077865719711358976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't promise anything.",
              "maleVariant": "",
              "stringId": "2077865719728136200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] See you.",
              "maleVariant": "",
              "stringId": "2324428046378688516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Go to sleep]",
              "maleVariant": "",
              "stringId": "2328690027352776704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Go to sleep]",
              "maleVariant": "",
              "stringId": "2328691793288978432"
            }
          ]
        }
      }
    },
