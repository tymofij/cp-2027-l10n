    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "User! I have an important message for you!",
              "maleVariant": "",
              "stringId": "1853175403468836864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. Hit me.",
              "maleVariant": "",
              "stringId": "1853176057327276036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anything.",
              "maleVariant": "",
              "stringId": "1853176636644544512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hit me with it.",
              "maleVariant": "",
              "stringId": "1853178517739556864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anything.",
              "maleVariant": "",
              "stringId": "1853178574496878596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "While running a routine diagnostic check, I discovered a file with my owner's metadata.",
              "maleVariant": "",
              "stringId": "1853181683315003392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Name. Regina Jones.",
              "maleVariant": "",
              "stringId": "1853182565746233344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. I'll take you to Regina.",
              "maleVariant": "",
              "stringId": "1853182829349851140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. I'm keeping you.",
              "maleVariant": "",
              "stringId": "1853185656931504128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. I'll take you to Regina.",
              "maleVariant": "",
              "stringId": "1853268552753106944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't think so... You're mine now. Finders keepers.",
              "maleVariant": "",
              "stringId": "1853268718008684548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are the best user I know!",
              "maleVariant": "",
              "stringId": "1853271584932909056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Increasing volume by 300 percent. TAKE ME BACK TO MY OWNER!!!",
              "maleVariant": "",
              "stringId": "1853272962107461632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The name \"Skippy\" mean anything to you?",
              "maleVariant": "",
              "stringId": "1853278246964056064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, the name \"Skippy\" ring any bells? Fits in a holster, more than a little psycho...",
              "maleVariant": "",
              "stringId": "1853279207426117632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have Skip?! Little bastard got swiped a while back... Let me see him!",
              "maleVariant": "",
              "stringId": "1853280233268989952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's see how you're doin'. Run diagnostics.",
              "maleVariant": "",
              "stringId": "1853284403782897692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Hand over Skippy]",
              "maleVariant": "",
              "stringId": "1853284793735729156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There a reward?",
              "maleVariant": "",
              "stringId": "1853287487149985796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Performance – 82 percent. Memory – damaged. Mode – Puppy-Loving Pacifist. Permanent lock.",
              "maleVariant": "",
              "stringId": "1853289713520103444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Permanent lock? Hm. Something's gonked... Return to factory settings.",
              "maleVariant": "",
              "stringId": "1853289713536880644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please set pistol name.",
              "maleVariant": "",
              "stringId": "1853289713536880652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Performance – 82 percent. Memory – damaged. Mode – Stone Cold Killer.",
              "maleVariant": "",
              "stringId": "1853290501277503492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm... I'd better do a full scan. At least it still works.",
              "maleVariant": "",
              "stringId": "1853290501277503500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You killed him...",
              "maleVariant": "",
              "stringId": "1853292659817336832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd you get him from?",
              "maleVariant": "",
              "stringId": "1853293038999195648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait... Where'd you get this iron anyway?",
              "maleVariant": "",
              "stringId": "1853296821707755520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh... Knew a guy, everyone called him Danish... Great techie. I mean, before he started hitting the bottle.",
              "maleVariant": "",
              "stringId": "1853297455450312704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyway, poker game... Danish cleaned out his credchip, then bet his gun. I think you can put together the rest.",
              "maleVariant": "",
              "stringId": "1853299312738455552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The combat features are great... But why the voice interface, you know? Weird as hell.",
              "maleVariant": "",
              "stringId": "1853300544085446656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So – somethin' for my trouble?",
              "maleVariant": "",
              "stringId": "1853301901546770432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You... You killed him! You killed Skippy.",
              "maleVariant": "",
              "stringId": "1853301963102375940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure... Here you go.",
              "maleVariant": "",
              "stringId": "1853309454112542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come on. He's just an algorithm. Not a shred of consciousness.",
              "maleVariant": "",
              "stringId": "1853310723946467328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not so sure. He... he had a personality. Quirks, mannerisms.",
              "maleVariant": "",
              "stringId": "1853311961702354944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Protocol errors. A good, hard reset always helps.",
              "maleVariant": "",
              "stringId": "1853312884868665344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Say anything to continue.",
              "maleVariant": "",
              "stringId": "1864915126801715200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Automated response. Ha ha ha! The joke gets funnier the more you say it!",
              "maleVariant": "",
              "stringId": "1864916787980980224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It looks like you're trying to be funny. Automated response. Ha ha ha! I have never heard that one before!",
              "maleVariant": "",
              "stringId": "1864917057557286912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "User. Please return me to my real owner. Thank you in advance.",
              "maleVariant": "",
              "stringId": "1864920224491687936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yell all you want.",
              "maleVariant": "",
              "stringId": "1864923361982074880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bad user! Bad user!",
              "maleVariant": "",
              "stringId": "1864923598490488832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Choom, I'm the only user you know. Your memory got wiped.",
              "maleVariant": "",
              "stringId": "1864926315040526336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fun fact. There is a 92 percent probability Regina Jones will reward you!",
              "maleVariant": "",
              "stringId": "1864926761750679552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feels kinda weird now. It's too... quiet.",
              "maleVariant": "",
              "stringId": "1864951907660124160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Maybe you should get a dog.",
              "maleVariant": "",
              "stringId": "1864952743735906304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe. Someday. See ya, Reggie.",
              "maleVariant": "",
              "stringId": "1864956317266137088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmmm... Daisy.",
              "maleVariant": "",
              "stringId": "1864958509712723968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My name is Daisy.",
              "maleVariant": "",
              "stringId": "1864959131275022336"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}