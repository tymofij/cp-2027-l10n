  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dis way.",
              "maleVariant": "",
              "stringId": "1508436663636873216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know Pacifica well?",
              "maleVariant": "",
              "stringId": "1508439916084744192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah. You guys aren't exactly great at rollin' out the welcome mat for outsiders.",
              "maleVariant": "",
              "stringId": "1508441961277706240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Grann Ertha.",
              "maleVariant": "",
              "stringId": "1508444466082500608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Placide, pitit mwen.\" t=\"Placide, my child.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1508446044684308480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Pou ou.\" t=\"For you.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1508455182663438336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Ou pa t 'bezwen…\" t=\"You didn't have to…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1508457259179462656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Petèt, men sa se pou ou. P’ap pran “non” pou yon repons.\" t=\"Maybe. But this is yours. Won't take \\\"no\\\" for an answer.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1508457755449511936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"creo\" m=\"Vini nan.\" b=\"\" a=\" We talk here.\"/>",
              "maleVariant": "",
              "stringId": "1508471457569591296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Gen lòt ki genyen li pi mal…\" t=\"Others have it worse…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1533136402835116032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Mèsi.\" t=\"Thank you.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1533138365215739904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go. Not far now.",
              "maleVariant": "",
              "stringId": "1533138839725740032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Aye, Placide.\" t=\"Hey, Placide.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551666024883965952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Mwen pral deplase yo ankò.\" t=\"Gonna move 'em all over again.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551668439360229376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Ki etaj?\" t=\"Which floor?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551669307061063680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Dezyèm.\" t=\"Second.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551669342511321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Placide! Mwen gen sa ou te bay lòd.\" t=\"Placide! Got your order ready.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551679075746074624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Li pa konekte ak Agwè depi Josue ... ou konnen kisa pou w fè.\" t=\"She hasn't jacked into Agwe since Josue... So you know what to do.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551683851112173568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Li si sansib ke sa?\" t=\"She's that sensitive?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551685002213740544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Li pa ta pran yon souf san li pa di-sa.\" t=\"She wouldn't take a breath without his say-so.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551685035482959872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Oke mwen pral vini pita.\" t=\"OK! I'll come later.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1551707325071380480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You deal wid Animal before?",
              "maleVariant": "",
              "stringId": "1551746421957349376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Sibil pa ap renmen sa.\" t=\"Sibil won't like that.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1557400095127048192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Ki diferans ki genyen?\" t=\"What's the difference?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1557400628072091648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Li pa leve pou yon anè, nenpòt jan.\" t=\"She hasn't got up for a year now, anyway.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1557401238695645184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah. A few times.",
              "maleVariant": "",
              "stringId": "1557481057944932352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Lovelie? Oh tanpri. Josue te toujou genyen li nan yon kòd.\" t=\"Lovelie? Oh please. Josue always had her on a leash.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1590989069116313600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pacifica's biggest, ugliest temple to greed... but never finished.",
              "maleVariant": "",
              "stringId": "1662078064263274496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Until last week it was deserted, empty.",
              "maleVariant": "",
              "stringId": "1662079130774118400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But now?",
              "maleVariant": "",
              "stringId": "1662079919991136256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De Animals crawled in, made a nest.",
              "maleVariant": "",
              "stringId": "1662080467515580416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I mean, it makes no sense, them being here.",
              "maleVariant": "",
              "stringId": "1695268657450782720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You want the Animals out of the GIM? Extinct?",
              "maleVariant": "",
              "stringId": "1695269540066562048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They tryna provoke you? Make you do somethin' stupid?",
              "maleVariant": "",
              "stringId": "1695270130960109568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That why you hired me?",
              "maleVariant": "",
              "stringId": "1695276575457873920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You learn everyt'ing soon.",
              "maleVariant": "",
              "stringId": "1695277157425942528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Middle of Pacifica, no backup, cut off…",
              "maleVariant": "",
              "stringId": "1695280975316324352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's your turf. Hell, even badges don't patrol around here.",
              "maleVariant": "",
              "stringId": "1695282075633258496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dey do not leave GIM.",
              "maleVariant": "",
              "stringId": "1695283186318512128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How many we talkin' about in there?",
              "maleVariant": "",
              "stringId": "1695283651919810560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thirty. Maybe more.",
              "maleVariant": "",
              "stringId": "1695284081986965504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Oke, jijman ta dwe pase pa kounye a.\" t=\"Well, judgment should be passed by now.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1699656813256921088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Menm si li mache alè, li p'ap janm mete konfyans lan neg lan.\" t=\"Even if he walks away from this, she'll never trust him again.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1699657598044753920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Hm. E li te tèlman enpòtan.\" t=\"Hm. And he was so important.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1699657640088457216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Placide! Ou gen yon minit?\" t=\"Placide! Do you have a minute?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714450419366862848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Pa jodi a.\" t=\"Not today.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714450419366862868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Men, ou te di…\" t=\"But you said…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714450419366862888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Pita, Emilie, mwen pap bliye.\" t=\"Later, Emilie – I haven't forgotten.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714450419366862908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our interest is now in de GIM – de Grand Imperial Mall.",
              "maleVariant": "",
              "stringId": "1760464909770006528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was to be its own city, where suits would burn their eddies.",
              "maleVariant": "",
              "stringId": "1760483650608144424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A closed circle. De corps feed their sheep, they spit de cash back out. All de toys here are grown from corpo cash.",
              "maleVariant": "",
              "stringId": "1760483650608144440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1760644622408851456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I must wait long?",
              "maleVariant": "",
              "stringId": "1760644677471674368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are coming?",
              "maleVariant": "",
              "stringId": "1760644752985923584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welcome to Pacifica.",
              "maleVariant": "",
              "stringId": "1773591285874053120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Tonnè, fout nou deyò, banfout sèpan! Tounen nan vil la! Se Pasifika!\" t=\"Get the fuck out, snakes! Back to the city! This is Pacifica!\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1773592293530423296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Has Holt's campaign promise, and I quote…",
              "maleVariant": "",
              "stringId": "1780462637419614208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pacifica – a hotbed of controversy, a city within the city.",
              "maleVariant": "",
              "stringId": "1780464028821245952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All that and more...",
              "maleVariant": "",
              "stringId": "1780464106617196544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"I will clean up Pacifica, set it straight.\"",
              "maleVariant": "",
              "stringId": "1795071582414376960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Already proved to be false?",
              "maleVariant": "",
              "stringId": "1795072093632925696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We decided to take a closer look.",
              "maleVariant": "",
              "stringId": "1795072908418420736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A dirty stain on Night City's map…",
              "maleVariant": "",
              "stringId": "1795077384848084992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or a chance at urban renewal?",
              "maleVariant": "",
              "stringId": "1795077829091987456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh.",
              "maleVariant": "",
              "stringId": "1973686555258933248"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}