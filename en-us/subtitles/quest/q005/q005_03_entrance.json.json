  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The name on the reservation is…?",
              "maleVariant": "",
              "stringId": "1611407206705410048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're out front, Bug.",
              "maleVariant": "",
              "stringId": "1618360178349846528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Member, reservation's in your name… Ramón.",
              "maleVariant": "",
              "stringId": "1618361853001216000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welcome to Konpeki Plaza. Please come through single file.",
              "maleVariant": "",
              "stringId": "1618383257205616640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You got it, holm-- uh, ahem, sir.",
              "maleVariant": "",
              "stringId": "1618384330528010240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'd like to check in.",
              "maleVariant": "",
              "stringId": "1618389928464232452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gimme a sec, need to check somethin'.",
              "maleVariant": "",
              "stringId": "1622943069616615424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, I'll be over by security.",
              "maleVariant": "",
              "stringId": "1622943980921434112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Victorino.",
              "maleVariant": "",
              "stringId": "1624367202455924736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Double room, two adults, one night. Correct?",
              "maleVariant": "",
              "stringId": "1624368794278178816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's the one.",
              "maleVariant": "",
              "stringId": "1624369099908722688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Perfect... I will go ahead and notify Taki-san of your arrival.",
              "maleVariant": "",
              "stringId": "1624371770573074432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit, no good, not part of the plan. Talk her up, V, stall!",
              "maleVariant": "",
              "stringId": "1624375881779523584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That won't be necessary.",
              "maleVariant": "",
              "stringId": "1624380179984822276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That won't be necessary.",
              "maleVariant": "",
              "stringId": "1624391902376636416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will be in the Lapis Lazuli Suite on level forty-two.",
              "maleVariant": "",
              "stringId": "1624394466388226048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, one more little formality... Please validate your SID chip.",
              "maleVariant": "",
              "stringId": "1624396421084884992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait for you by the elevators. Ugh, this little bastard's heavier than I thought.",
              "maleVariant": "",
              "stringId": "1624421744111398912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This should only take a moment, ma'am.",
              "maleVariant": "This should only take a moment, sir.",
              "stringId": "1631482970540658688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahem. Hold on, got something.",
              "maleVariant": "",
              "stringId": "1631483996450639872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Care to explain why you’re bringing military equipment onto the premises, into Konpeki Plaza?",
              "maleVariant": "",
              "stringId": "1631489298352885760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go ahead.",
              "maleVariant": "",
              "stringId": "1631490872911384576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'd like to check in.",
              "maleVariant": "",
              "stringId": "1631546040910663680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, just a moment, please.",
              "maleVariant": "",
              "stringId": "1631557606032855040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Señorita\" b=\"\" a=\", do you know how long we been traveling? Eighteen hours from New Barcelona.\"/>",
              "maleVariant": "",
              "stringId": "1631562400743084032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "With a delay on MetaKey 'cause some cyberpsycho blew himself into bits inside the terminal…",
              "maleVariant": "",
              "stringId": "1631562523367755776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, a nightmare…",
              "maleVariant": "",
              "stringId": "1631562979775139840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, I understand.",
              "maleVariant": "",
              "stringId": "1631563135031496704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the hell, V?! I said stall!",
              "maleVariant": "",
              "stringId": "1631565204819857408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, this is the reception desk. Apologies for any disturbance.",
              "maleVariant": "",
              "stringId": "1631565693959589888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stevie Shaviro – assistant to Taki-san. How can I help you?",
              "maleVariant": "",
              "stringId": "1631565865942831104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm in the process of checking in a Mr. …",
              "maleVariant": "",
              "stringId": "1631566215680675840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Victorino. Good, he's arrived. Please send him upstairs immediately.",
              "maleVariant": "",
              "stringId": "1631566362800082944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, right away.",
              "maleVariant": "",
              "stringId": "1631566514919100416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Taki-san is expecting you, he awaits.",
              "maleVariant": "",
              "stringId": "1631567084656578560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the hell, V?! I said stall! Do I gotta do everything?!",
              "maleVariant": "",
              "stringId": "1631569380786360320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry. I just... blanked.",
              "maleVariant": "",
              "stringId": "1631570805473988608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're there to meet Hajime Taki – military tech department rep. Papers are for the Flathead.",
              "maleVariant": "",
              "stringId": "1661751726453870592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "New Barcelona? Really?",
              "maleVariant": "",
              "stringId": "1661776630234533888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's called improvisin' – you should try it.",
              "maleVariant": "",
              "stringId": "1661780865743376384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not bad.",
              "maleVariant": "",
              "stringId": "1673436984802856964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We nail the part or what? Heheheh. How do I look?",
              "maleVariant": "",
              "stringId": "1673437062632361984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't believe I'm sayin' this, but... not bad at all.",
              "maleVariant": "",
              "stringId": "1673437169369010176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like somebody put clothes on a dog.",
              "maleVariant": "",
              "stringId": "1673437230454853632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like a solo cosplaying a suit.",
              "maleVariant": "",
              "stringId": "1673437334641364992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I look good in everything.",
              "maleVariant": "",
              "stringId": "1673441381087760384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, yeah, 'cause you look so much better.",
              "maleVariant": "",
              "stringId": "1673441825230999552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just feel weird…",
              "maleVariant": "",
              "stringId": "1677620275231641600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can kick the rat outta the corp, but you'll never kick the corp outta the rat.",
              "maleVariant": "",
              "stringId": "1677623386734120960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Other gate, please.",
              "maleVariant": "",
              "stringId": "1689436367934451712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have I asked you to do that?",
              "maleVariant": "",
              "stringId": "1711240145806438400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have I asked you to do that?",
              "maleVariant": "",
              "stringId": "1711240912726536192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Um, no... I just thought that since...",
              "maleVariant": "",
              "stringId": "1711244395945013248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who asked you to think?",
              "maleVariant": "",
              "stringId": "1711244993465561088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excuse me?",
              "maleVariant": "",
              "stringId": "1711245840547196928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welcome to Konpeki Plaza.",
              "maleVariant": "",
              "stringId": "1712779485304991744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welcome to Konpeki Plaza.",
              "maleVariant": "",
              "stringId": "1714207462123253760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or a couple of stools at the bar, if you prefer?",
              "maleVariant": "",
              "stringId": "1714210353139560448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't worry, got this.",
              "maleVariant": "",
              "stringId": "1725480866495123456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No sweat.",
              "maleVariant": "",
              "stringId": "1725480943770980352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know damn well it wasn't me. Who was it?",
              "maleVariant": "",
              "stringId": "1725540639689199616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A word of advice, if I may. Do only what you're told to do.",
              "maleVariant": "",
              "stringId": "1725541959888326656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now, what was that room number?",
              "maleVariant": "",
              "stringId": "1725544998997118976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We don't do reservations weekdays, so feel free to grab any available table.",
              "maleVariant": "",
              "stringId": "1756320354372734976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah! You are here to see Taki-san, am I right?",
              "maleVariant": "",
              "stringId": "1771994452703571968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"Youkoso.\" b=\"\" a=\" Greetings and welcome to Konpeki Plaza.\"/>",
              "maleVariant": "",
              "stringId": "1772053325800009728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please accept my apologies for the confusion.",
              "maleVariant": "",
              "stringId": "1785054331002474496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't come on too strong?",
              "maleVariant": "",
              "stringId": "1785060877992251392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaddaya think, \"Hannah\"?",
              "maleVariant": "Whaddaya think, \"Harry\"?",
              "stringId": "1785202272895905792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't need that, Jack. Enough.",
              "maleVariant": "",
              "stringId": "1785204494501294080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bar don't look too shabby.",
              "maleVariant": "",
              "stringId": "1785207824325685248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sheeit, look like some fuckin' travelin' salesman with this case.",
              "maleVariant": "",
              "stringId": "1785211297393946624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Already? That was fast. Not your style?",
              "maleVariant": "",
              "stringId": "1785216124198989824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're arms dealers.",
              "maleVariant": "",
              "stringId": "1789937192904945668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're arms dealers.",
              "maleVariant": "",
              "stringId": "1789939079737765888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excuse me?",
              "maleVariant": "",
              "stringId": "1789939668316057600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't intend to explain myself.",
              "maleVariant": "",
              "stringId": "1790835882881503232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't intend to explain myself, to you or anyone else.",
              "maleVariant": "",
              "stringId": "1790836806635016192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Actually, we have rules, see, and they say you have to.",
              "maleVariant": "",
              "stringId": "1790837470660448256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could bring Misty here one day. When we, uh... close this deal.",
              "maleVariant": "",
              "stringId": "1790882350032015360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goin' upstairs.",
              "maleVariant": "",
              "stringId": "1790917708467720192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Place hand]",
              "maleVariant": "",
              "stringId": "1867942095602053124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everything seems to be in order. We wish you a pleasant stay.",
              "maleVariant": "",
              "stringId": "1867943081817141248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you.",
              "maleVariant": "",
              "stringId": "1902663343964737536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honor's all yours, Hannah.",
              "maleVariant": "Honor's all yours, Harry.",
              "stringId": "1902663716486041600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Apologies, but only hotel staff are authorized to be here.",
              "maleVariant": "",
              "stringId": "1934254376646365184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "About time.",
              "maleVariant": "",
              "stringId": "1934361222363238400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Welp, here we are. Lapis Lazuli.",
              "maleVariant": "",
              "stringId": "1934424656631058432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better get goin'.",
              "maleVariant": "",
              "stringId": "1967553783536623616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like to stop by our room first.",
              "maleVariant": "",
              "stringId": "1983646104539701248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm?",
              "maleVariant": "",
              "stringId": "1983681542298083328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Quaint, cozy. Not like the hotel we had in Zurich for that convention.",
              "maleVariant": "",
              "stringId": "1983681780903649280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Suited up again? Right.",
              "maleVariant": "",
              "stringId": "1983703276896198656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll go freshen up first, notify him ourselves.",
              "maleVariant": "",
              "stringId": "1987927328410116096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But Taki-san is expecting you, no…?",
              "maleVariant": "",
              "stringId": "1987936720899612672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trust me – places like this, you're either strong or you're nobody.",
              "maleVariant": "",
              "stringId": "1987952445127204864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What? I'm takin' this seriously!",
              "maleVariant": "",
              "stringId": "1989691410058387456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna have a look around.",
              "maleVariant": "",
              "stringId": "1995491546781069316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wouldn't hurt to take a peek inside…",
              "maleVariant": "",
              "stringId": "1995491857813876736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Room – let's go.",
              "maleVariant": "",
              "stringId": "1995492648758947840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will, I'm sure. Now let's go check out our digs.",
              "maleVariant": "",
              "stringId": "1995493594289287168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pff...",
              "maleVariant": "",
              "stringId": "2433612114325393408"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}