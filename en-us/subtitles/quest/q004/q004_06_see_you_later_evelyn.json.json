  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get everything you need?",
              "maleVariant": "",
              "stringId": "1153430861407727616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yep, that'll do. Thanks, Judy.",
              "maleVariant": "",
              "stringId": "1153437726644514816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, what now?",
              "maleVariant": "",
              "stringId": "1187911673528070144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Speakin' of harm's way. Know what I see lookin' at you?",
              "maleVariant": "",
              "stringId": "1215248765245059072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, what now?",
              "maleVariant": "",
              "stringId": "1215253551499571200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Walking, talking corpses.",
              "maleVariant": "",
              "stringId": "1215277220846235648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We really needed that recording, Judy.",
              "maleVariant": "",
              "stringId": "1215278023115927552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Relax – everything's under control.",
              "maleVariant": "",
              "stringId": "1215278850266230784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We needed that recording, Judy. BD practically does our job for us.",
              "maleVariant": "We needed that recording, Judy. The BD practically does our job for us.",
              "stringId": "1215279355008774144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Relax, I got it all under control.",
              "maleVariant": "",
              "stringId": "1215288322699161600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If Arasaka finds out you have it, you're dead. I'M dead.",
              "maleVariant": "",
              "stringId": "1215289496768737280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you fuck up and they come knockin' on my door...",
              "maleVariant": "",
              "stringId": "1215357349282836480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Evelyn, please... no shortcuts.",
              "maleVariant": "",
              "stringId": "1215358773886578688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Connection to Yorinobu – what's its nature, exactly?",
              "maleVariant": "",
              "stringId": "1215374043770384384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think I see how you know so much about Yorinobu.",
              "maleVariant": "",
              "stringId": "1215389140211331072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V...",
              "maleVariant": "",
              "stringId": "1215394681994211328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We got it. Later, T-Bug.",
              "maleVariant": "",
              "stringId": "1451730960049106944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice work today. See ya soon.",
              "maleVariant": "",
              "stringId": "1451731308562214912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Biz. And biz only.",
              "maleVariant": "",
              "stringId": "1451750420025831424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for the help.",
              "maleVariant": "",
              "stringId": "1461723383613485056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Till next time. I hope.",
              "maleVariant": "",
              "stringId": "1461723763734867968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, Judy. Really.",
              "maleVariant": "",
              "stringId": "1461726047147520000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And don't worry – I know what I'm doin'.",
              "maleVariant": "",
              "stringId": "1461726359220514816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, don't worry. Won't do anything to get you in trouble. But, uh... I hope to see you again.",
              "maleVariant": "",
              "stringId": "1461737359537729536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Depends.",
              "maleVariant": "",
              "stringId": "1461737956588515328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On what?",
              "maleVariant": "",
              "stringId": "1461738418263945216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whether you come alone or with a tag-along. Like a netrunner.",
              "maleVariant": "",
              "stringId": "1461738536056778752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No 'runners. I promise.",
              "maleVariant": "",
              "stringId": "1461742173726306304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya.",
              "maleVariant": "",
              "stringId": "1461742855669805056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep it. I'll put it on Ev's tab.",
              "maleVariant": "",
              "stringId": "1521260301573025792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Portable device for handling BDs. I already uploaded your calibration settings.",
              "maleVariant": "",
              "stringId": "1521261569628565504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And it keeps you outta harm's way. Clever.",
              "maleVariant": "",
              "stringId": "1521268990895845376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course I will be.",
              "maleVariant": "",
              "stringId": "1521282891473612800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1528438486239731716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not as sophisticated as what we got here, but should do the trick.",
              "maleVariant": "",
              "stringId": "1531603878123532288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Judy... relax. That's not gonna happen.",
              "maleVariant": "",
              "stringId": "1531611367338536960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Judy... relax. Nobody will ever know.",
              "maleVariant": "",
              "stringId": "1531612400966037504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's simple – men like pillow talk a bit too much.",
              "maleVariant": "",
              "stringId": "1531616201391337472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So it's just about the sex?",
              "maleVariant": "",
              "stringId": "1531616728195919872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll wipe the cache and your data. You were never here.",
              "maleVariant": "",
              "stringId": "1540084100795854848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do this job for me. I mean me alone. No splitting the payout with anyone else. No middlemen.",
              "maleVariant": "",
              "stringId": "1624111049163272192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No Dex.",
              "maleVariant": "",
              "stringId": "1624112012007698432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll think about it.",
              "maleVariant": "",
              "stringId": "1624204244618440708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No way. Dex trusts me.",
              "maleVariant": "",
              "stringId": "1624204831300907008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wanna haze our fixer?",
              "maleVariant": "",
              "stringId": "1624213629927288832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I agree, there'll be hell to pay. For sure.",
              "maleVariant": "If I agree, there'll be hell to pay, for sure.",
              "stringId": "1624223442065289216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No fuckin' way. Dex took a leap with me. He trusts me.",
              "maleVariant": "",
              "stringId": "1624223538668498944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wanna fuck over our fixer. I knew somethin' was up.",
              "maleVariant": "",
              "stringId": "1624223636496445440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. Whatever you decide, it stays between us.",
              "maleVariant": "",
              "stringId": "1624237835289227264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can offer fifty percent. Eddies enough to do whatever the hell you like.",
              "maleVariant": "",
              "stringId": "1624238629656850432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll be finished. Dex won't forgive a dirt move like this.",
              "maleVariant": "",
              "stringId": "1624291085417836544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dex isn't the only fixer in town. But my offer's the only one you'll ever get.",
              "maleVariant": "",
              "stringId": "1624291795362508800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh, lemme think about it.",
              "maleVariant": "",
              "stringId": "1624296397621293056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now I'm doing the same. Fifty percent. With cred like that in your account, you won't need Dex for anything.",
              "maleVariant": "",
              "stringId": "1624310363311435776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not about to fuck Dex over.",
              "maleVariant": "",
              "stringId": "1624312528612487172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll think about it.",
              "maleVariant": "",
              "stringId": "1624313197654306816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, Evelyn, it's not just about the money.",
              "maleVariant": "No, Evelyn. It's not just about the money.",
              "stringId": "1624317066882969600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I thought you were a merc. I thought it was always about the money.",
              "maleVariant": "",
              "stringId": "1624317609323917312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not behind the back of someone who trusts me. Sorry.",
              "maleVariant": "",
              "stringId": "1624317930355945472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fine. I never asked. But V?",
              "maleVariant": "",
              "stringId": "1624322597743882240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm?",
              "maleVariant": "",
              "stringId": "1624323098107572224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just hope you're as good as you are naive.",
              "maleVariant": "",
              "stringId": "1624323200415035392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll think about it.",
              "maleVariant": "",
              "stringId": "1624327651813093376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dex is a middleman. And a useless one, at this point.",
              "maleVariant": "",
              "stringId": "1624344770697097216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your choice.",
              "maleVariant": "",
              "stringId": "1624353950585712640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure you wanna do this?",
              "maleVariant": "",
              "stringId": "1624356027571499008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But if we're smart...",
              "maleVariant": "",
              "stringId": "1624368769514729472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you need me, call. I'll send you my number.",
              "maleVariant": "",
              "stringId": "1624371996259905536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1624373632977989632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good luck.",
              "maleVariant": "",
              "stringId": "1624373768386899968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mmhm.",
              "maleVariant": "",
              "stringId": "1624374722725277696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Evelyn, you don't mess with fixers. That's the one rule every merc in this city knows – and actually follows.",
              "maleVariant": "",
              "stringId": "1631836209907625984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doesn't matter. Mistake like that'll cost you your rep. And without a rep, you're nobody.",
              "maleVariant": "",
              "stringId": "1631838636060831744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure everything's under control?",
              "maleVariant": "",
              "stringId": "1634461398558437376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Intel on Yorinobu, the chip, the BD from Konpeki... Winds are right, plan's perfect. A job like this comes one in a million.",
              "maleVariant": "Intel on Yorinobu, the chip, the BD from Konpeki... Winds are right, plan's perfect. Job like this comes one in a million.",
              "stringId": "1634464108565360640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mm. Thanks. Now the punch line, please.",
              "maleVariant": "",
              "stringId": "1634504061575884800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just sounds too good to be true. What's the catch?",
              "maleVariant": "",
              "stringId": "1634504834300899328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wanna know if you're really in control here. That you're not sending me into the sorta shit I can't wade out of.",
              "maleVariant": "",
              "stringId": "1634508587800326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cold feet? Are you looking to get out of it?",
              "maleVariant": "",
              "stringId": "1634511942186561536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V. I have zero reasons to haze you. We're in this together. Really. Trust me that I got this.",
              "maleVariant": "",
              "stringId": "1634528072590553088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're sure Yorinobu doesn't expect this kinda move from you?",
              "maleVariant": "",
              "stringId": "1679164748591656960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's got no idea, trust me. He's too busy with NetWatch to give me that kind of attention.",
              "maleVariant": "",
              "stringId": "1679164748608434184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Besides... even IF some suspicion takes root in his mind, it shouldn't matter, he won't react in time. And that's something you'll handle, right?",
              "maleVariant": "",
              "stringId": "1679164748608434196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're not worried? Arasaka will flag and screen anyone who's had dealings with Yorinobu.",
              "maleVariant": "",
              "stringId": "1679164748692320256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, they'll have a long list, then. Packed full of big names. Much bigger than mine.",
              "maleVariant": "",
              "stringId": "1679164748692320268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You tell me who they check first. Corpo hotshots and cutthroats? Or a little bedroom plaything like me?",
              "maleVariant": "",
              "stringId": "1679164748692320280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not afraid they'll tie you to the heist?",
              "maleVariant": "",
              "stringId": "1679167788455424000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure Yorinobu doesn't suspect a thing?",
              "maleVariant": "",
              "stringId": "1679176160789524480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now go. I need a few words with Judy.",
              "maleVariant": "",
              "stringId": "1733178507373780992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, V.",
              "maleVariant": "",
              "stringId": "1733178555255955456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time's a-wastin', V.",
              "maleVariant": "",
              "stringId": "1733178603373010944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Besides, we'll talk in a bit.",
              "maleVariant": "",
              "stringId": "1740254549625139200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, one sec.",
              "maleVariant": "",
              "stringId": "1740297433967620096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's walk.",
              "maleVariant": "",
              "stringId": "1744387915588956160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The BD will be the death of us. Seriously.",
              "maleVariant": "",
              "stringId": "1753402318679203840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You go that route, city'll always win. So be careful.",
              "maleVariant": "",
              "stringId": "1753410787213860864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better ask yourself that question. Do you want to spend the rest of your days blasting scavs? Or become a legend overnight?",
              "maleVariant": "",
              "stringId": "1753433381828845568"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}