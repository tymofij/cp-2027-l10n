    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey! Where are you going? Get back here!",
              "maleVariant": "",
              "stringId": "1810969713117728768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's here. Come with me.",
              "maleVariant": "",
              "stringId": "1812404591730331648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An abandoned construction site?",
              "maleVariant": "",
              "stringId": "1812412874356658176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goddamn city... Gave it all, then took it all right back...",
              "maleVariant": "",
              "stringId": "1812490838146519040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Apparently, they lured her here. She didn't realize her life was in danger until was too late.",
              "maleVariant": "",
              "stringId": "1812492197000351744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As good a place as any other. More importantly, away from others.",
              "maleVariant": "",
              "stringId": "1812493955453915136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where we headed?",
              "maleVariant": "",
              "stringId": "1821349398313930756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do it.",
              "maleVariant": "",
              "stringId": "1821352295688802304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To the place they ended her life.",
              "maleVariant": "",
              "stringId": "1821354029312086016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want to see what she saw before I die.",
              "maleVariant": "",
              "stringId": "1821357777728016384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goodbye, Ken. You deserve this.",
              "maleVariant": "",
              "stringId": "1821456815966097408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goodbye, Ken.",
              "maleVariant": "",
              "stringId": "1821457164395319296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want to leave me no way out, is that it?",
              "maleVariant": "",
              "stringId": "1821660971079274500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure?",
              "maleVariant": "",
              "stringId": "1821734878759063552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Poetic.",
              "maleVariant": "",
              "stringId": "1847635037289955328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not exactly, it's just... it's how it ought to be.",
              "maleVariant": "",
              "stringId": "1847635125840101376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "At least she wasn't scared.",
              "maleVariant": "",
              "stringId": "1847661245146841088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. You're right...",
              "maleVariant": "",
              "stringId": "1847661279775014912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ken, listen... My company spat me out too. Was handed a dirty job by a higher-up. Thought it was game over for me.",
              "maleVariant": "",
              "stringId": "1851553052737912832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You deserve this.",
              "maleVariant": "",
              "stringId": "1851553421266239488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure about this?",
              "maleVariant": "",
              "stringId": "1851553507450798080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know.",
              "maleVariant": "",
              "stringId": "1851586476559286272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just do it.",
              "maleVariant": "",
              "stringId": "1851588295058505728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goodbye, Ken.",
              "maleVariant": "",
              "stringId": "1851588396946538496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But I got help. He showed me it's possible to find meaning another way. Then I lost him too, just like you lost Enid.",
              "maleVariant": "",
              "stringId": "1851617126905528320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's the problem. I don't want it.",
              "maleVariant": "",
              "stringId": "1851617306606288896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck you! You hear me?!",
              "maleVariant": "",
              "stringId": "1851632997111676928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I...I can do this... Just once...",
              "maleVariant": "",
              "stringId": "1851633087708643328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But look at me! I got my life back! Stupid as it might sound, all you gotta do is want it back.",
              "maleVariant": "",
              "stringId": "1871949538944335872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We had a deal, you hear me!? You gave your word!",
              "maleVariant": "",
              "stringId": "1871975058650152960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And the platypuses?",
              "maleVariant": "",
              "stringId": "1882367499026808832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-what...? What platypuses?",
              "maleVariant": "",
              "stringId": "1882382710072918016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The ones your daughter wanted to give life to. Her unfulfilled dream.",
              "maleVariant": "",
              "stringId": "1882382808471289856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why mention that? Do you want to see me drown in my grief even more?",
              "maleVariant": "",
              "stringId": "1882382904420188160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enid would consider you a coward.",
              "maleVariant": "",
              "stringId": "1882383266388623364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can fulfill her dream.",
              "maleVariant": "",
              "stringId": "1882383352304746496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're a coward, the pathetic way you just roll over.",
              "maleVariant": "",
              "stringId": "1882383690231431168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you die, the last memory of her dies with you, d'you understand? Like she never existed. You want that?",
              "maleVariant": "",
              "stringId": "1882383734053519360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I lost everything. Where would even begin to get the resources for...for a biotech breakthrough?",
              "maleVariant": "",
              "stringId": "1882397653086224384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1882397856694517760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's true. Do you want my apology for being weak? Well, it's my right.",
              "maleVariant": "",
              "stringId": "1882406092042649600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do you think Enid would want this? That because--",
              "maleVariant": "",
              "stringId": "1882406224649764864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enid doesn't want anything! She can't because of me! You get that yet?! Just let me die!",
              "maleVariant": "",
              "stringId": "1882406306942009344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What about the platypuses?",
              "maleVariant": "",
              "stringId": "1883435367717912576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And if that person doesn't have the strength to live in a world without you?",
              "maleVariant": "",
              "stringId": "1883448482887090176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know.",
              "maleVariant": "",
              "stringId": "1904054305497530372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not about the platypuses.",
              "maleVariant": "",
              "stringId": "1904054424431214592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No clue – think of something. Make an effort. You're a hundred times smarter than me.",
              "maleVariant": "",
              "stringId": "1904054487345774592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe the platypuses are stupid idea – I dunno, you're the scientist. That's not what this is about.",
              "maleVariant": "",
              "stringId": "1904054537845194752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do know one thing. Life's a fucking roller coaster. I'm 24 and I... well, I could die really damn soon.",
              "maleVariant": "",
              "stringId": "1904054645487812608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "W-what? What're you talking about?",
              "maleVariant": "",
              "stringId": "1904063983568797696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And y'know what? I don't want the world to forget about me. That's why I crawled out from the bottom through all the shit. Understand?",
              "maleVariant": "",
              "stringId": "1904064078611726336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If there's anyone anywhere who cares about me, they better buckle their ass in and spend whatever time they can with me.",
              "maleVariant": "",
              "stringId": "1904064179644121088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then, when I'm dead, stand over my grave and rock their grandkids' minds with stories about how V lived for somethin'!",
              "maleVariant": "",
              "stringId": "1904064269184122880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then they better find it. For my sake.",
              "maleVariant": "",
              "stringId": "1904066912602275840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good luck, Ken.",
              "maleVariant": "",
              "stringId": "1904072827711766528"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}