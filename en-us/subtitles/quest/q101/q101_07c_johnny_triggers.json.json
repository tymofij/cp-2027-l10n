      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need a smoke. Where'd you stash yours?",
              "maleVariant": "",
              "stringId": "1531611843562201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta get out of here, understand?",
              "maleVariant": "",
              "stringId": "1531745063764807680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I'll kill anyone who gets in my way. You included.",
              "maleVariant": "",
              "stringId": "1531745948058308608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How did you get here?",
              "maleVariant": "",
              "stringId": "1531771635402067972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't smoke.",
              "maleVariant": "",
              "stringId": "1531772520551206912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then go out and get some. Just need one last one!",
              "maleVariant": "",
              "stringId": "1531775927802449920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesus fucking Christ, man!",
              "maleVariant": "Jesus fuckin' Christ, man!",
              "stringId": "1531776637948448768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck kinda joytoy are you supposed to be?",
              "maleVariant": "",
              "stringId": "1531784168183746560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who you work for? Start talkin'!",
              "maleVariant": "",
              "stringId": "1531798393618747392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck!",
              "maleVariant": "",
              "stringId": "1531799943867060224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck!",
              "maleVariant": "",
              "stringId": "1531800673709510656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fucking chip...",
              "maleVariant": "",
              "stringId": "1531800921139892224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See you never, asshole.",
              "maleVariant": "",
              "stringId": "1531827162819645440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take meds]",
              "maleVariant": "",
              "stringId": "1531833199161298944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't smoke.",
              "maleVariant": "",
              "stringId": "1532744625648357376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where did you even come from?",
              "maleVariant": "Where'd you even come from?",
              "stringId": "1553261098178404352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not somethin' to focus on, V. Let it go. Here, got some meds for ya.",
              "maleVariant": "",
              "stringId": "1640255507798315008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here, got one more thing for ya.",
              "maleVariant": "",
              "stringId": "1640255507798315016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vik pulled this outta your skull â€“ a lucky charm?",
              "maleVariant": "",
              "stringId": "1640255507815092224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He was my best friend.",
              "maleVariant": "",
              "stringId": "1653125477661462528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jack... was the best friend I ever had. I'm 'onna miss him somethin' awful.",
              "maleVariant": "Jack... was the best friend I ever had. I'm gonna miss him somethin' awful.",
              "stringId": "1653126780949155840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If he was here, he'd tell you to get your ass out of bed in the morning and do what you need to do.",
              "maleVariant": "",
              "stringId": "1653127023094714368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He talked about you, lots.",
              "maleVariant": "",
              "stringId": "1653130307939389440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He talked about you, like, lots.",
              "maleVariant": "",
              "stringId": "1653133702926487552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We got into a fight... right before he went off to do this job.",
              "maleVariant": "",
              "stringId": "1653140217888559104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He wa'n't mad at you. I hope you know that.",
              "maleVariant": "He wasn't mad at you. I hope you know that.",
              "stringId": "1653141035140304896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. I just wish our last moments together could've been... different.",
              "maleVariant": "",
              "stringId": "1653141880728768512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Lie down] I'm exhausted.",
              "maleVariant": "",
              "stringId": "1653148212466864132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Givin' me pills' that'll kill me?",
              "maleVariant": "",
              "stringId": "1653148644480176128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're lovely, Misty. Thanks.",
              "maleVariant": "",
              "stringId": "1653148806229315584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Misty, wait. Umm... about Jackie...",
              "maleVariant": "",
              "stringId": "1653148869361979392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "G'night, V. Sweet, peaceful dreams.",
              "maleVariant": "",
              "stringId": "1653150122200584192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah?",
              "maleVariant": "",
              "stringId": "1653150838956806144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And then he died... and I thought I was gonna die... with him, in my sleep.",
              "maleVariant": "",
              "stringId": "1654744294154096640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't actually tell if I'm awake now. I mean, I could be dead already, right?",
              "maleVariant": "",
              "stringId": "1654776913927393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're lovely.",
              "maleVariant": "",
              "stringId": "1680714883322662912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "About Jackie...",
              "maleVariant": "",
              "stringId": "1680714883322662928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't you hear? My condition's terminal. What good can this do?",
              "maleVariant": "",
              "stringId": "1680723670523760640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Givin' me a pill to prod that sonofabitch? So he can kill me faster?",
              "maleVariant": "",
              "stringId": "1680747371126378496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your psyche's gonna die, V. You'll feel your old self slipping away. At some point, you won't recognize yourself.",
              "maleVariant": "",
              "stringId": "1680749834860548096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It'll be terrifying. It'll be painful. But it doesn't have to be.",
              "maleVariant": "",
              "stringId": "1680750977925828608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But right now you need rest. So sleep, please sleep.",
              "maleVariant": "",
              "stringId": "1680754164607012864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How the fuck should I know?",
              "maleVariant": "",
              "stringId": "1680853239671083008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How're we even talking?",
              "maleVariant": "",
              "stringId": "1680856361256669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can feel it... our minds... touching...",
              "maleVariant": "",
              "stringId": "1693928484547289100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't be silly. As long as you're alive, there's hope. And don't let anyone tell you otherwise.",
              "maleVariant": "",
              "stringId": "1725750154300542976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really think I can make it, survive this?",
              "maleVariant": "You really think I can make it, survive this?",
              "stringId": "1725751755551592448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course you can. I mean, you did already die and come back once, didn't ya?",
              "maleVariant": "",
              "stringId": "1725752858032787456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Helloooooo Night City!",
              "maleVariant": "",
              "stringId": "1735537728094892032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smack-dab between two Californ-I-As, it's prime real estate, baby, and boy are we open for business!",
              "maleVariant": "",
              "stringId": "1735538628830064640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your man Stan here! Smog reports comin' in ten times the usual. But my, my, what a gorgeous day this is!",
              "maleVariant": "",
              "stringId": "1741406203971530752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take meds]",
              "maleVariant": "",
              "stringId": "1754851376491094016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh. Fuck me...",
              "maleVariant": "",
              "stringId": "1754851939937116160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rip the thing out myself!",
              "maleVariant": "",
              "stringId": "1755975805704888320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, wait!",
              "maleVariant": "",
              "stringId": "1755977876432773120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh...",
              "maleVariant": "",
              "stringId": "1755979911341301760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd puke if I fuckin' could!",
              "maleVariant": "",
              "stringId": "1756064483307446272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm like mold on fruit... creepin' into youâ€¦ nothin' I can do about it.",
              "maleVariant": "",
              "stringId": "1756065191691837440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear me?!",
              "maleVariant": "",
              "stringId": "1756066248757108736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm gonna kill you.",
              "maleVariant": "",
              "stringId": "1756069344270901252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out of my head.",
              "maleVariant": "",
              "stringId": "1756070011198791680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm... gonna kill you!",
              "maleVariant": "",
              "stringId": "1756070917755658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave me alone!",
              "maleVariant": "",
              "stringId": "1756071035716263936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do it. What're you waiting for?!",
              "maleVariant": "",
              "stringId": "1756095520016195584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out! Just get the fuck out!",
              "maleVariant": "",
              "stringId": "1756099862261686272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not like that!",
              "maleVariant": "",
              "stringId": "1756172889003417600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stick some iron in your mouth and pull the trigger.",
              "maleVariant": "",
              "stringId": "1756173216477896704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If Watson's the place you haunt, you borged up ghouls, I got good news â€“ cop cordon 'round your 'hood's packed up an' left.",
              "maleVariant": "",
              "stringId": "1756194282554286080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So you peeps open up and let it all hang out today. Could catch a break, start in on a new chapter, a new book, a new life.",
              "maleVariant": "",
              "stringId": "1756197792817635328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now let's hear what the Rubicones have to say about it! Hit it!",
              "maleVariant": "",
              "stringId": "1756198964001529856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So leave last night's blue dreams and screams in bed, take that first sip of joe with a hot splash of sunshine and seize the day in Night City!",
              "maleVariant": "",
              "stringId": "1756204778397392896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, fuck, fuck, fuckin' fuck...",
              "maleVariant": "",
              "stringId": "1756420060462211072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's just a copy of the engram â€“ I'm out there somewhere, gotta be...",
              "maleVariant": "",
              "stringId": "1756420575841509376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lead to the head, only thing that'll fix this.",
              "maleVariant": "",
              "stringId": "1756420811678834688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hear me, bitch?! A bullet to the fucking brain!",
              "maleVariant": "Hear me, asshole?! A bullet to the fucking brain!",
              "stringId": "1756424308100980736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, you're likely to be fine for a while. But some time down the road, it could turn into pure agony. I'm givin' you options, honey.",
              "maleVariant": "",
              "stringId": "1774717953086349312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The brain can be tricked. Plain old painkillers do that.",
              "maleVariant": "",
              "stringId": "1774718837564399616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck I need pills for? Oughta just stick a gun in my mouth, pull the trigger.",
              "maleVariant": "The fuck I need pills for? Oughta just stick a gun in my mouth and pull the trigger.",
              "stringId": "1774726692237500416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, that way you'd kill two souls. Is that what you want?",
              "maleVariant": "",
              "stringId": "1774726997968707584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm so fucking sorry he's gone.",
              "maleVariant": "",
              "stringId": "1774732426488602624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm so fucking sorry Jack's not here anymore.",
              "maleVariant": "",
              "stringId": "1774759857421312000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jackie was special. Really spiritually rich. He touched so many people with his love. Don't worry, he'll be around.",
              "maleVariant": "",
              "stringId": "1774768516259930112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure you'll be all right?",
              "maleVariant": "",
              "stringId": "1774769616828522496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll be fine. Life is so beautifully powerful, so much more powerful than death.",
              "maleVariant": "",
              "stringId": "1774769918566752256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get up]",
              "maleVariant": "",
              "stringId": "1782045434573447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Omega blockers â€“ taken regularly, they'll keep things from progressing too quickly.",
              "maleVariant": "",
              "stringId": "1801308235405942784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Also, they should keep that guest of yours calm, and quiet.",
              "maleVariant": "",
              "stringId": "1801308235892482048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pseudoendotrizine's from me. Effect'll be opposite. It'll speed things up, free the demon, so to speak.",
              "maleVariant": "",
              "stringId": "1801308236899115008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta lie down...",
              "maleVariant": "I gotta lie down...",
              "stringId": "1801319726121181184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Promise you'll try to get some sleep?",
              "maleVariant": "",
              "stringId": "1801327129218736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a terminal case of bad luck.",
              "maleVariant": "",
              "stringId": "1801330165878054912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sleep's a... small hint of death, the inevitable.",
              "maleVariant": "",
              "stringId": "1802321539337052160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sleep]",
              "maleVariant": "",
              "stringId": "1879531836931792900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1879681356235759620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuckin' ghost off!",
              "maleVariant": "",
              "stringId": "2010204369420050432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll take control.",
              "maleVariant": "",
              "stringId": "2463536371112120320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll find a way!",
              "maleVariant": "",
              "stringId": "2463538192413134848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear me?!",
              "maleVariant": "",
              "stringId": "2463538600804126720"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}