    "DataType": "CR2W"
  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's get her inside!",
              "maleVariant": "Let's get him inside!",
              "stringId": "1419533616736813056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I cannot hold it!",
              "maleVariant": "",
              "stringId": "1419543039408521216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Easy now.",
              "maleVariant": "",
              "stringId": "1419543700212727808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta cut my way through the occipital bone. No other way.",
              "maleVariant": "",
              "stringId": "1424364956879601664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's neurogenic shock. She's dying.",
              "maleVariant": "It's neurogenic shock. He's dying.",
              "stringId": "1642906261335363584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pressure's tanking!",
              "maleVariant": "",
              "stringId": "1652051273440612352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need... to rest...",
              "maleVariant": "",
              "stringId": "1652064332020240384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That your blood?",
              "maleVariant": "",
              "stringId": "1653054588420935680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Misty!",
              "maleVariant": "",
              "stringId": "1653061175676030976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There is risk of--",
              "maleVariant": "",
              "stringId": "1654541687611510784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know what I'm doing.",
              "maleVariant": "",
              "stringId": "1654542046492938240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How is she?",
              "maleVariant": "How is he?",
              "stringId": "1677773336796160000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Slower on the mend than you, but lookin' better every day.",
              "maleVariant": "",
              "stringId": "1677775299378110464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Misty!",
              "maleVariant": "",
              "stringId": "1714273080247373840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, V. Let's get you home.",
              "maleVariant": "",
              "stringId": "1714273080683581440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You don't just die and then rise from the grave.",
              "maleVariant": "",
              "stringId": "1714273081119789056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So I'm Frankenstein. Great.",
              "maleVariant": "",
              "stringId": "1714273081119789076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're Doctor Viktor and I'm your monster. Perfect.",
              "maleVariant": "",
              "stringId": "1714273081304338432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People don't just die and then get up like nothin' happened.",
              "maleVariant": "",
              "stringId": "1714273081455333376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to be true. Now, it's only if they don't have a classified piece of corpo tech primed to resurrect them.",
              "maleVariant": "",
              "stringId": "1714273081455333388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What about me, my psyche? I make it back from the other side, and what – I'm supposed to pretend change is normal?",
              "maleVariant": "",
              "stringId": "1714273081455333400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ask the Arasaka engineers who built the thing. All I know is your mind's gonna go... and it won't be pretty.",
              "maleVariant": "",
              "stringId": "1714273081455333412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From the biochip's perspective, your brain cells are a tumor that needs to be scooped out, while your body's an empty shell to hold the construct.",
              "maleVariant": "",
              "stringId": "1714273081505665024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So this fucking terrorist wants to wipe me and take my place, take my body.",
              "maleVariant": "",
              "stringId": "1714273081505665060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The biochip... it's basically a bomb, fuse lit already. You don't have much time left, much... life. A few weeks tops.",
              "maleVariant": "",
              "stringId": "1714273081589551104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Silverhand's construct is overwriting your consciousness – gradually taking over your body until one day you'll just be... gone.",
              "maleVariant": "",
              "stringId": "1714273081589551116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, it's important you get all this.",
              "maleVariant": "",
              "stringId": "1714273081589551128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll fix me up, right?",
              "maleVariant": "",
              "stringId": "1714273081757323264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're talkin' bullshit.",
              "maleVariant": "",
              "stringId": "1714273081757323280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll fix me up, right? Vik?",
              "maleVariant": "",
              "stringId": "1714273081975427072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I could, I would, V,  believe me. But this is... It's way beyond what I know how to do.",
              "maleVariant": "",
              "stringId": "1714273081992204296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't believe an ounce of the horseshit comin' from your mouth. Know why?",
              "maleVariant": "",
              "stringId": "1714273082059313152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Because it doesn't make any goddamn sense!",
              "maleVariant": "",
              "stringId": "1714273082076090376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sayin' I experienced another psyche's memories? How's that even possible?",
              "maleVariant": "Sayin' I experienced another psyche's memories? How's that possible?",
              "stringId": "1714273082227085312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is, then?",
              "maleVariant": "",
              "stringId": "1714284557725388800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First time ever I've seen you scared.",
              "maleVariant": "",
              "stringId": "1714285082818695168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doubt you can top the terrorist in my brain…",
              "maleVariant": "",
              "stringId": "1714285317313843200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is?",
              "maleVariant": "",
              "stringId": "1714286572618698752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never given me that look before, Vik. What is it?",
              "maleVariant": "",
              "stringId": "1714286663685427200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think you can top the terrorist in my brain?",
              "maleVariant": "",
              "stringId": "1714286764717821952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You, uh, don't got a lot of time left, kid.",
              "maleVariant": "",
              "stringId": "1714296322630668288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Say what now?",
              "maleVariant": "",
              "stringId": "1714296467837472768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why do I have to die?",
              "maleVariant": "",
              "stringId": "1714303736650190852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want to know the detes, all of 'em.",
              "maleVariant": "",
              "stringId": "1714304238976815104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're the best of the best, Vik. Why can't you help me?",
              "maleVariant": "",
              "stringId": "1714306590706290688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You want the long story or the short?",
              "maleVariant": "",
              "stringId": "1714307138935377920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have to die, I am going to die – that what you're saying? Prove it.",
              "maleVariant": "",
              "stringId": "1714307766772355072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't omit a thing, not one dete.",
              "maleVariant": "",
              "stringId": "1714307874515636224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You jacked it in your chipslot. Nothing happened, right? Until you died.",
              "maleVariant": "",
              "stringId": "1714308711312838668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bullet in the head from Dexter DeShawn. But, how do you come back from somethin' like that?",
              "maleVariant": "",
              "stringId": "1714308711329615880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Low caliber – you lucked out. Not least thanks to another poor decision by Mr. DeShawn.",
              "maleVariant": "",
              "stringId": "1714308711329615892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Dex shot you, right? The bullet damaged your chipslot AND your brain.",
              "maleVariant": "",
              "stringId": "1714316245490782208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The biochip revived and... short-circuited you. Started uploading data INTO your head.",
              "maleVariant": "",
              "stringId": "1714321524408242176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. There was-- is a construct, a psyche on the chip. That of Johnny Silverhand.",
              "maleVariant": "",
              "stringId": "1714325676785979392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So what am I supposed to do?",
              "maleVariant": "",
              "stringId": "1714329873724002308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't we just remove the chip?",
              "maleVariant": "",
              "stringId": "1714330172861763584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vik, you've always come through for me.",
              "maleVariant": "",
              "stringId": "1714331494168186880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't we just take the chip out? Or switch it off?",
              "maleVariant": "",
              "stringId": "1714331602062462976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Either way's out of the question. You'd die immediately.",
              "maleVariant": "",
              "stringId": "1714367287603548160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wish I knew, kid.",
              "maleVariant": "",
              "stringId": "1714369806635421696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? You in there?",
              "maleVariant": "",
              "stringId": "1715423139194261504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... my head.",
              "maleVariant": "",
              "stringId": "1715423139194261516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How ya feelin'?",
              "maleVariant": "",
              "stringId": "1715423139194261528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang on, mean to say there's an actual terrorist in my head – right now?",
              "maleVariant": "",
              "stringId": "1715423139311702016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's right. Johnny Silverhand.",
              "maleVariant": "",
              "stringId": "1715423139311702028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had his crowning moment about fifty years ago.",
              "maleVariant": "",
              "stringId": "1715423139328479252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AHQ Bombing – heard of it? His doing. People say he was buried under the rubble...",
              "maleVariant": "",
              "stringId": "1715423139328479264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I dunno, Vik... ears're ringin'. And I'm seein' shit...",
              "maleVariant": "",
              "stringId": "1715423139395588096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These... hallucinations – describe 'em to me.",
              "maleVariant": "",
              "stringId": "1715423139412365332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Blinding lights. It's loud. I'm on stage and almost can't breathe. I'm so damn full of... hatred.",
              "maleVariant": "Blinding lights. It's loud. I'm on stage and I almost can't breathe. I'm so damn full of... hatred.",
              "stringId": "1715423139412365344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then I let it all out into a mic. And I realize it didn't help, I don't feel any better.",
              "maleVariant": "",
              "stringId": "1715423139412365360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And then, promise not to laugh, I plant a bomb in Arasaka Tower.",
              "maleVariant": "And then, heh, promise not to laugh... I plant a bomb in Arasaka Tower.",
              "stringId": "1715423139429142536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin' there to laugh about...",
              "maleVariant": "",
              "stringId": "1715423139429142552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They killed me.",
              "maleVariant": "",
              "stringId": "1715423139479474176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City looked... different.",
              "maleVariant": "",
              "stringId": "1715423139496251400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They killed me, Vik.",
              "maleVariant": "",
              "stringId": "1715423139764686860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never been so afraid.",
              "maleVariant": "",
              "stringId": "1715423139764686876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City looked different... strange.",
              "maleVariant": "",
              "stringId": "1715423139798241280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I... I hated it. Hated everybody, every inch.",
              "maleVariant": "And I hated it. Hated everybody, every inch.",
              "stringId": "1715423139815018504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] I feel... like something ain't right.",
              "maleVariant": "",
              "stringId": "1715423139865350144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was too real, even for a lucid dream.",
              "maleVariant": "",
              "stringId": "1715423140066676736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You weren't dreaming, V. Those were memories.",
              "maleVariant": "",
              "stringId": "1715423140066676748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's a personality construct on that shard. Dreams you had were from his past.",
              "maleVariant": "",
              "stringId": "1715423140083453976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How's that even possible?",
              "maleVariant": "",
              "stringId": "1715423140133785600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mean to say there's a terrorist in my head?",
              "maleVariant": "",
              "stringId": "1715423140133785620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If there's nothing you can do to help me, then... well, what the hell do I do? Tell me, please.",
              "maleVariant": "",
              "stringId": "1715455847383486464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vik?",
              "maleVariant": "",
              "stringId": "1715457323442950144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's that shard from those TV ads!",
              "maleVariant": "",
              "stringId": "1725569191054462976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, I remember this thing, saw it on TV... Oh, what was that stupid slogan? \"Soul\" something?",
              "maleVariant": "Fuck, I remember this thing, saw it on TV... What was that stupid slogan? \"Soul\" somethin'?",
              "stringId": "1725570104976527360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's it! It's just supposed to let you communicate with constructs.",
              "maleVariant": "",
              "stringId": "1725573453104861184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe. But this is a prototype, by the look of things. Somehow made it out of Arasaka labs.",
              "maleVariant": "",
              "stringId": "1725579290904940544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You two are connected in a way I can't make head or tail of.",
              "maleVariant": "",
              "stringId": "1772317707660189696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two? Me and who, Vik? Who's the other?",
              "maleVariant": "",
              "stringId": "1772320213253513216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny Silverhand. A terrorist – real talk o' the town back in my day.",
              "maleVariant": "",
              "stringId": "1772320290009276416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyway, that's not what's important right now…",
              "maleVariant": "",
              "stringId": "1772321542361341952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As far as it was concerned, your brain was an empty vessel that needed to be filled by the engram it was carrying.",
              "maleVariant": "",
              "stringId": "1772339636152590336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Empty vessel? But I'm right here – all of this is me!",
              "maleVariant": "",
              "stringId": "1772341580195717120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The shard doesn't read, it writes. Headache of yours? It's the biochip rewiring your neural pathways, building new neural structures, and doing away with the old.",
              "maleVariant": "",
              "stringId": "1772341789827031040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From where the shard's sitting, YOU, what's left – it's all a tumor that needs to be excised.",
              "maleVariant": "",
              "stringId": "1772344391520247808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not willful on his part, it's automatic, inevitable. And neither of you can stop it.",
              "maleVariant": "",
              "stringId": "1772406218664992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The nanites off the chip started fixing the damage. Then they took your hand and coaxed you off the path towards the light.",
              "maleVariant": "",
              "stringId": "1772412155165433856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your body's only a body. You can modify it, replace it with chrome – hell, get a new one altogether.",
              "maleVariant": "",
              "stringId": "1772440314195763200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Secure Your Soul.\"",
              "maleVariant": "",
              "stringId": "1772453879581532160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your mind, though? Well, that's a whole 'nother story.",
              "maleVariant": "",
              "stringId": "1772461340761808896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're askin' too much from an old-timer like Vik.",
              "maleVariant": "",
              "stringId": "1772467106117869568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whatever's happenin' to me, there's a logical explanation. Gotta be.",
              "maleVariant": "Whatever's happenin' to me, there's a logical explanation, gotta be.",
              "stringId": "1772478113431183360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck…",
              "maleVariant": "",
              "stringId": "1801434087709761536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck…",
              "maleVariant": "",
              "stringId": "1801434405470232576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1895427437896622080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Talk to me.",
              "maleVariant": "",
              "stringId": "1895431926825312260"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}