  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] What d'you recommend?",
              "maleVariant": "",
              "stringId": "1338819857663135744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When Samurai started out, we spent every enny we had on guitars, strings, y'know...",
              "maleVariant": "",
              "stringId": "1702413766406529040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "G'morning.",
              "maleVariant": "",
              "stringId": "1702460207820791808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The usual, Mr. Eurodyne?",
              "maleVariant": "",
              "stringId": "1702506655509733376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Double espresso, Jamaican blend. Nothin' else, no sugar.",
              "maleVariant": "",
              "stringId": "1702506655509733392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You got it. And for you?",
              "maleVariant": "",
              "stringId": "1702506655509733408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Same for me.",
              "maleVariant": "",
              "stringId": "1702508118751404032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just a regular coffee.",
              "maleVariant": "",
              "stringId": "1702508118751404048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What he's havin'.",
              "maleVariant": "",
              "stringId": "1702508532376887296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just a regular coffee.",
              "maleVariant": "",
              "stringId": "1702508532410441728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great. Be right back with it.",
              "maleVariant": "",
              "stringId": "1702510935662747648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1714143067984179204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ehh, had almost nothin' left over for food, but we still went across the street every day, right to Caliente.",
              "maleVariant": "",
              "stringId": "1806729269018750976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny said we were so hungry and high we woulda eaten the cardboard boxes out from under the bums outside.",
              "maleVariant": "",
              "stringId": "1806731393802825728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course he was wrong.",
              "maleVariant": "",
              "stringId": "1806733940349333504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothing for now, thanks.",
              "maleVariant": "",
              "stringId": "1806746070477828096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin' for me, thanks.",
              "maleVariant": "",
              "stringId": "1806748806321664000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, no. She'll have what I'm having.",
              "maleVariant": "No, no, no. He'll have what I'm having.",
              "stringId": "1806749168910856192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They really got to you, didn't they?",
              "maleVariant": "",
              "stringId": "1806762033512312832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They're nice girls. What're you afraid of?",
              "maleVariant": "",
              "stringId": "1806764096673673220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dunno what this is about – my money's on money.",
              "maleVariant": "",
              "stringId": "1806765904787795968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They don't back out now, let me know. I'll help.",
              "maleVariant": "",
              "stringId": "1806768097704144896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This all stinks of a media set-up.",
              "maleVariant": "",
              "stringId": "1806770113318875136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not exaggerating, Ker? The girls just wanna have fun, sing your song to millions of adoring fans. You stand to gain here too.",
              "maleVariant": "Not exaggerating, Ker? The girls just wanna have fun, sing your song. To millions of adoring fans. You stand to gain here too.",
              "stringId": "1806773281696116736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't believe for a millisec you torched that truck for art's sake. This is about money, isn't it? They screw you on the royalties?",
              "maleVariant": "I don't believe for a millisec you torched that truck for art's sake. This is about money, isn't it? What, they screw ya on the royalties?",
              "stringId": "1806773389573615616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell you this much – if they don't back out now, gimme a call. We'll convince 'em another way.",
              "maleVariant": "I'll tell you this much – if they don't back out now, gimme a call. We'll convince 'em another way.",
              "stringId": "1806773494179557376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honestly, to me this all stinks of one big media play.",
              "maleVariant": "Honestly? To me this all stinks of one big media play.",
              "stringId": "1806773615478829056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What if I'm a product just like those dolls? Drownin' in dirty money, 'stead of swimmin' in it.",
              "maleVariant": "",
              "stringId": "1806773699784339456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. You got a rocker's soul. I can feel it.",
              "maleVariant": "",
              "stringId": "1806773942550654980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm with Johnny here.",
              "maleVariant": "",
              "stringId": "1806774544886263808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny's dead. Better think about what's important to you.",
              "maleVariant": "",
              "stringId": "1806776769041788928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, not like we know each other that well, but trust me – you got a rocker's soul... and a rocker's balls.",
              "maleVariant": "",
              "stringId": "1806781239817531392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kerry, this shit's straight binary. Wanna be free? Keep the right to rebel?",
              "maleVariant": "",
              "stringId": "1806781307161276416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny's dead.",
              "maleVariant": "",
              "stringId": "1806781428561211392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better be goin'.",
              "maleVariant": "",
              "stringId": "1806781522916274176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look at this. Unlistenable. That trash is their biggest hit.",
              "maleVariant": "",
              "stringId": "1810962761088888832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, anyone could churn out hits like that. Even you.",
              "maleVariant": "",
              "stringId": "1810966752371798016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, no, no, no. You know how Us Cracks got started? Bunch of MSM suits decided the world needed this dookie and so served it up sprinkled with millions of eddies.",
              "maleVariant": "",
              "stringId": "1810975524473401344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "World ate it up like it always does. And I gotta be part of that?",
              "maleVariant": "",
              "stringId": "1810987186299465728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somethin' tells me this isn't about Art-with-a-capital-A. Not about eds either. You're afraid, Kerry.",
              "maleVariant": "Somethin' tells me this isn't about Art-with-a-capital-A. It's not about eds either. You're afraid, Kerry.",
              "stringId": "1810988381558665216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No clue what you're talkin' about. Got nothin' to do with money.",
              "maleVariant": "",
              "stringId": "1810992917899767808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks. Hope they're off my sonar now, 'cause if not... shit. Last thing I want is Johnny to've been right all along.",
              "maleVariant": "",
              "stringId": "1810996332700758016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No! Not it at all!",
              "maleVariant": "",
              "stringId": "1811002801944915968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah fuck... Sure hope you're wrong. 'Cause otherwise, Johnny woulda been right. From the get-go.",
              "maleVariant": "",
              "stringId": "1811003436375339008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, come on. Somehow you know where to find the gear truck of some Asian dolls doin' a cover of your song? Juicy little scandal for the media...",
              "maleVariant": "Come on... Somehow you know where to find the gear truck of some Asian dolls doing a cover of your song? Juicy little scandal for the media...",
              "stringId": "1811008502591254528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well... Johnny accused me of leaving Samurai for money. And fame.",
              "maleVariant": "",
              "stringId": "1811017537256288256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And that's not something you'll admit to.",
              "maleVariant": "",
              "stringId": "1811024007658074112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But that's not true.",
              "maleVariant": "",
              "stringId": "1811037898437947392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, it's true, won't lie. But he also said I'd be putting myself on a corp leash.",
              "maleVariant": "",
              "stringId": "1811038301661556736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, thanks...",
              "maleVariant": "",
              "stringId": "1811054643156480000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This city doesn't forgive, it eats people alive. But you – well, you survived.",
              "maleVariant": "",
              "stringId": "1811054744356646912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never thought about it that way. You may be right, doll.",
              "maleVariant": "Never thought about it that way. You may be right, man.",
              "stringId": "1811056605956534272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shouldn'ta signed a multimillion-eddie deal with the biggest media conglomerate in the country.",
              "maleVariant": "Then ya shouldn'ta signed a multimillion-eddie deal with the biggest media conglomerate in the country.",
              "stringId": "1811065932494794752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, seriously think that? They got the rights to my songs, but fuck, not to me!",
              "maleVariant": "",
              "stringId": "1811066830948265984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's not how this city works. There's a contract. There's no \"you.\"",
              "maleVariant": "",
              "stringId": "1811068059174694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're wrong. And I'll prove it to ya.",
              "maleVariant": "",
              "stringId": "1811069639655550976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure hope so, Ker.",
              "maleVariant": "",
              "stringId": "1811069789845188608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So they say.",
              "maleVariant": "",
              "stringId": "1811086428716265472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're alive. Decide whose truth matters to you more – yours or someone else's.",
              "maleVariant": "And you're alive. Decide whose truth matters more to you – yours or someone else's.",
              "stringId": "1811087041420193792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That a trick question?",
              "maleVariant": "",
              "stringId": "1811092629508190208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Only if you don't have the 'nads to answer it honestly.",
              "maleVariant": "",
              "stringId": "1811093821378392064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for, uh... you know.",
              "maleVariant": "",
              "stringId": "1811096721689276416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't mention it.",
              "maleVariant": "",
              "stringId": "1811097155346755584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, what's good?",
              "maleVariant": "",
              "stringId": "1814080385762799616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Rachel!",
              "maleVariant": "",
              "stringId": "1814138246220034048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stuffed our faces, downed joe by the gallon.",
              "maleVariant": "",
              "stringId": "1814192906658197504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sit, sit.",
              "maleVariant": "",
              "stringId": "1814307940629557248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Make up your mind, woman.",
              "maleVariant": "Make up your mind, man.",
              "stringId": "1814311938556243972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Drink]",
              "maleVariant": "",
              "stringId": "1815455889254330372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So even after all the years you're still a regular. 'Bout time they hung your picture on the wall.",
              "maleVariant": "So, even after all the years you're still a regular. S'about time they hung your picture on the wall.",
              "stringId": "1822824507046703108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck... They really should.",
              "maleVariant": "",
              "stringId": "1832908666545238016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know this dump. Know that smell. But how...?",
              "maleVariant": "",
              "stringId": "1836989013906026496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whew! Shit... just comin' down now. Whew... fuckin' awesome.",
              "maleVariant": "",
              "stringId": "1927200328867508224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Last time I felt like this was back in Memphis. Last tour, last concert, vibes from the crowd, me on stage...",
              "maleVariant": "",
              "stringId": "1927219448115752960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, fuck... Just thought about those Us Cracks cunts again. You think we got 'em?",
              "maleVariant": "",
              "stringId": "1927236179479621632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They really getting to you that bad?",
              "maleVariant": "",
              "stringId": "1927237777056460804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, if they don't back out now, let me know.",
              "maleVariant": "",
              "stringId": "1927238432022196224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's Johnny got to do with anything?",
              "maleVariant": "",
              "stringId": "1937342789732397060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny's dead. You're not.",
              "maleVariant": "",
              "stringId": "1937342914789765120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Best case scenario, you know the game you're playin'. Worst – they're playin' you.",
              "maleVariant": "Best case scenario, you know the game they're playin'. Worst – they're playin' you.",
              "stringId": "1937343943451209728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ehh, if I'm afraid of anything, it's that Johnny might've been right.",
              "maleVariant": "",
              "stringId": "1937347082585763840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just can't stand the thought that... ugh, Johnny might've been right.",
              "maleVariant": "",
              "stringId": "1937347587546411008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's Johnny got to do with any of this?",
              "maleVariant": "",
              "stringId": "1937351308447707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Us Cracks thing...",
              "maleVariant": "",
              "stringId": "1937356033968697344"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}