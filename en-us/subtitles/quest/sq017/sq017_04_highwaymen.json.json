  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here we are.",
              "maleVariant": "",
              "stringId": "1334369286923227152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'd I say? Drives like a fuckin' cyberpsycho.",
              "maleVariant": "",
              "stringId": "1334374478800490496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now we wait.",
              "maleVariant": "",
              "stringId": "1334406835389599744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There she is. Right on the dot.",
              "maleVariant": "",
              "stringId": "1334407236935487488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What we ordered – musical gear.",
              "maleVariant": "",
              "stringId": "1334433943377678336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's inside?",
              "maleVariant": "",
              "stringId": "1334435073323175936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your lucky day, Ker. Crates 'n' crates of nothin' but gear.",
              "maleVariant": "",
              "stringId": "1334435406686457856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hell yeaaah, we got a hit!",
              "maleVariant": "",
              "stringId": "1334435757296717824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now for the grand finale. Want this set to be a smoldering puddle of chrome and plastic when we're done.",
              "maleVariant": "",
              "stringId": "1337381437262794752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Set up stinger]",
              "maleVariant": "",
              "stringId": "1508094565279199236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, V, honestly?",
              "maleVariant": "",
              "stringId": "1508108005624377344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea. Hope whatever happens, you'll be able to handle it.",
              "maleVariant": "",
              "stringId": "1508108718706388992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're the boss.",
              "maleVariant": "",
              "stringId": "1508198181264551940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This your new signature move?",
              "maleVariant": "",
              "stringId": "1508198528385150976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're the boss.",
              "maleVariant": "You're the boss!",
              "stringId": "1508199279920541696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah – new signature move?",
              "maleVariant": "Ah – a new signature move?",
              "stringId": "1508201172289843200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm, you got it. So don't miss.",
              "maleVariant": "",
              "stringId": "1508201865641848832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got your mixers, speakers, drums, cables by the mile.",
              "maleVariant": "",
              "stringId": "1508216598604410880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guys we're waitin' on – they armed?",
              "maleVariant": "",
              "stringId": "1675125934088417284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You two whacked?! The hell was that?!",
              "maleVariant": "",
              "stringId": "1675139298432806912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You got it.",
              "maleVariant": "",
              "stringId": "1675213599454425088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice cache.",
              "maleVariant": "",
              "stringId": "1802312133223510016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Those are police stingers and grenades. Where'd you get 'em?",
              "maleVariant": "",
              "stringId": "1802318628069699584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, well... quite the arsenal.",
              "maleVariant": "",
              "stringId": "1802361618309967876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're fighting for art. Art with a capital A. C'mon, get to work.",
              "maleVariant": "",
              "stringId": "1802361618309967900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, I know the guy – this ain't about art.",
              "maleVariant": "",
              "stringId": "1802361618326745092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, fess up. This a publicity stunt?",
              "maleVariant": "",
              "stringId": "1803760487245672448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, just spit it out. This a set-up? Flock of medias about to fly in?",
              "maleVariant": "All right, just spit it out. Is this a set-up? Is a flock of medias about to fly in?",
              "stringId": "1803761124830212096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoever's driving that truck – they armed, maybe? Got any huscle?",
              "maleVariant": "",
              "stringId": "1803761684434251776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stuff worth a couple hundred thou. Could be ours.",
              "maleVariant": "",
              "stringId": "1803817188682493952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stuff's worth at least a couple hundred thou'. Really wanna trash it, not klep it?",
              "maleVariant": "Stuff's worth at least a couple hundred thou'. You really wanna trash it, not klep it?",
              "stringId": "1803822169569488896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please... Never stolen a thing in my life.",
              "maleVariant": "",
              "stringId": "1803849159043313664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck me, he really believes that? I remember all my strings getting swiped back in the day. Never had his own.",
              "maleVariant": "",
              "stringId": "1803851578066202624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Light 'er up.",
              "maleVariant": "",
              "stringId": "1803899576271892480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck me, Kerry.",
              "maleVariant": "",
              "stringId": "1803900684859994112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin', it's just a truck of toys for some plastic Japanese dolls. Might as well blow up a cotton candy stand, but...",
              "maleVariant": "",
              "stringId": "1803901155511234560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still a big step forward. I hardly recognize the bastard.",
              "maleVariant": "",
              "stringId": "1803902921816535040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take card] Quit squirming.",
              "maleVariant": "",
              "stringId": "1805378097040928772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Quit squirmin'. And no gonk moves.",
              "maleVariant": "",
              "stringId": "1805379210947407872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lay it out here.",
              "maleVariant": "",
              "stringId": "1805465746401472512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Hide] Now what?",
              "maleVariant": "",
              "stringId": "1805493101853487108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take stinger and grenades] You got it.",
              "maleVariant": "",
              "stringId": "1806644304519966724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What now?",
              "maleVariant": "",
              "stringId": "1806763155089862656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "H-hey, man, lose the iron. Let's talk it out.",
              "maleVariant": "",
              "stringId": "1806794827084673024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not a word. It'll all be over soon.",
              "maleVariant": "",
              "stringId": "1806794829802582016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, fuckin' hell...",
              "maleVariant": "",
              "stringId": "1806794832503713792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Access card, grab it!",
              "maleVariant": "",
              "stringId": "1806794835221622784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know they killed IV Coma right here, in this very spot? The DJ?",
              "maleVariant": "",
              "stringId": "1807063785805262848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Draw gun] Get out! On the ground!",
              "maleVariant": "",
              "stringId": "1811002884727582720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Move, move! 'Less you want somethin' to happen to our friend here!",
              "maleVariant": "",
              "stringId": "1811037494681350148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, move! Don't have time.",
              "maleVariant": "",
              "stringId": "1811302890021924868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right, gents. Now run along and don't look back.",
              "maleVariant": "",
              "stringId": "1811362027594395648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You drive. Get 'em off our ass.",
              "maleVariant": "",
              "stringId": "1812602101136961536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trunk. Grab what's in there.",
              "maleVariant": "",
              "stringId": "1814423292914393088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come on, set those spikes – today, maybe?",
              "maleVariant": "",
              "stringId": "1814498268916965380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Showtime. I take the driver, you get his sidechoom. We want 'em outta the truck!",
              "maleVariant": "",
              "stringId": "1815303914956480512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure hope you know what you're doin'.",
              "maleVariant": "",
              "stringId": "1815305051797413888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Outta the truck, on the ground, hands where I can see 'em.",
              "maleVariant": "",
              "stringId": "1815418889670713344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out! Ground, now!",
              "maleVariant": "",
              "stringId": "1815478078061371392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pop the back.",
              "maleVariant": "",
              "stringId": "1815594361549447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Open]",
              "maleVariant": "",
              "stringId": "1815674506427719684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hahaha...! Fuck yeah!",
              "maleVariant": "",
              "stringId": "1815756838232875012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good workin' with ya, V!",
              "maleVariant": "",
              "stringId": "1815756838232875040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, wouldn't it've been better to have a lawyer fight this fight for you?",
              "maleVariant": "Wouldn't it've been better to have a lawyer fight this fight for ya?",
              "stringId": "1815756838450978820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This here? I'm sendin' a message. This is how you do biz in Night City.",
              "maleVariant": "",
              "stringId": "1815756838450978832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sick burn. But for what?",
              "maleVariant": "",
              "stringId": "1815789471008514052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Considering a new career in terrorism?",
              "maleVariant": "",
              "stringId": "1815791159970848768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Burnin' real nice, I'll grant you that. Thing is, I mean, what's it actually get you?",
              "maleVariant": "It's burnin' real nice, I'll grant ya that. Thing is, I mean, what's it actually get ya?",
              "stringId": "1815792229988130840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Couldn't a lawyer do this better?",
              "maleVariant": "",
              "stringId": "1815803989323374596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Woulda done the same. Props.",
              "maleVariant": "",
              "stringId": "1815805633876418560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cops. Let's delta.",
              "maleVariant": "",
              "stringId": "1815813192314216448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out! On the ground!",
              "maleVariant": "",
              "stringId": "1823373038441947136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, scared two chooms shitless, flamed a truck... Tryin' to be the next Johnny Silverhand, Kerry? Play terrorist?",
              "maleVariant": "Scared two chooms shitless, flamed a truck... Tryin' to be the next Johnny Silverhand, Kerry? Play terrorist?",
              "stringId": "1823380448015507456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You always oversimplify shit? Nuance, V. You say vandalism, I say artistic protest.",
              "maleVariant": "",
              "stringId": "1823380448502046720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is how you do biz in Night City. Respect, Ker.",
              "maleVariant": "Heh, that's how you do biz in Night City. Respect, Ker.",
              "stringId": "1823383719304192004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ya get me, finally. Thanks.",
              "maleVariant": "",
              "stringId": "1823383719790731264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Turned out later she wasn't the target. Some Biotechnica suit, busy eatin' 'er out when the bullets started flyin'. Found the bodies... just like that.",
              "maleVariant": "",
              "stringId": "1832688827151020032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right. Hide. Quick.",
              "maleVariant": "",
              "stringId": "1832695331174346752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out. Do what I say, everything'll be fine.",
              "maleVariant": "",
              "stringId": "1832713424109957120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out. Nice 'n' easy, no weirdness, nothing'll happen.",
              "maleVariant": "Get out. Nice and easy, no weirdness, nothing'll happen.",
              "stringId": "1832719338179260416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, man, listen...",
              "maleVariant": "",
              "stringId": "1832720064733376512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On the ground, I said!",
              "maleVariant": "",
              "stringId": "1832720474902753280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I said, delta the fuck off!",
              "maleVariant": "",
              "stringId": "1832736285432344576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Playin' by ear, huh? Ah, wouldn't be my first time.",
              "maleVariant": "Playin' by ear, huh? Wouldn't be my first time.",
              "stringId": "1834122335081852928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got the wrong truck! Fuck, we're not--",
              "maleVariant": "",
              "stringId": "1834150985047076864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I said – easy. Now get down on the ground. Won't say it a third time.",
              "maleVariant": "",
              "stringId": "1834189849316945920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Next me? Hah, please. Kerry's got a short fuse, sure, but ain't no dynamite in that soul.",
              "maleVariant": "",
              "stringId": "1834206657839230976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mmh. And the real reason?",
              "maleVariant": "",
              "stringId": "1834209712232067072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If those little bitches think they can make it big on MY song, they got another thing comin'.",
              "maleVariant": "",
              "stringId": "1837374321998819348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You even know what I had to sacrifice to get where I am? The price I paid for success?!",
              "maleVariant": "",
              "stringId": "1837374322015596552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And those ten-enny whores wanna klep my tune and expect me to just let 'em?! Fucking pig-ignorant hacks.",
              "maleVariant": "",
              "stringId": "1837374322015596564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, \"success.\" Always a sore spot of Ker's. See nothing's changed.",
              "maleVariant": "",
              "stringId": "1837374322032373764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Automated modules, hollow spikes for better control of the vehicle – standard-issue police tech. 'Nades, too. Where'd you get 'em?",
              "maleVariant": "",
              "stringId": "1922735219663302656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Same place you get designer drugs, the latest, still-patent-pending AudioVox and fresh Yubaris.",
              "maleVariant": "",
              "stringId": "1922741441175089152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Black market?",
              "maleVariant": "",
              "stringId": "1922743994868690944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah – fans.",
              "maleVariant": "",
              "stringId": "1922744103836708864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, you're one to talk. I've flashed my teeth and signed papers enough for one lifetime.",
              "maleVariant": "",
              "stringId": "1922767440205967360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cloaked AV followed her limo. Almost nothin' left of 'er afterwards... or the limo.",
              "maleVariant": "",
              "stringId": "1928413355222626304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wouldn't mind that, actually.",
              "maleVariant": "",
              "stringId": "1928416297812094976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honestly, no idea what's about to go down. Just... improvise.",
              "maleVariant": "",
              "stringId": "1928417011833630720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out]",
              "maleVariant": "",
              "stringId": "2099844757670146052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, stay nice an' still there, boys.",
              "maleVariant": "",
              "stringId": "2231409120504901632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nuh-uh, calm and quiet. It'll be over soon.",
              "maleVariant": "",
              "stringId": "2231409891669635072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go, V! Pop 'er open!",
              "maleVariant": "",
              "stringId": "2231411298506305536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, can't keep me on edge like this, V! Whadda we got there?",
              "maleVariant": "",
              "stringId": "2231414076075077632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get goin'!",
              "maleVariant": "",
              "stringId": "2237193681243402240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give those little bitches a taste of American hospitality. C'mon, V!",
              "maleVariant": "",
              "stringId": "2237198347876364288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Waitin' for a special invite?",
              "maleVariant": "",
              "stringId": "2237199814070824960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the fuuuck?",
              "maleVariant": "",
              "stringId": "2237200606861389824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep your traps shut, gents. No chatter.",
              "maleVariant": "",
              "stringId": "2237202292837380096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in]",
              "maleVariant": "",
              "stringId": "2413983319915163652"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}