  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Ey, 'ey, you there! 'Ey, you dig braindances? 'Ey, feast your optics right here, yo! Got the meanest, preemest shit ever scrolled!",
              "maleVariant": "",
              "stringId": "1811307858842497024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Freshest virtus on the street – prices you can't beat!",
              "maleVariant": "",
              "stringId": "1811313706272698368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Ey, yo! Limited-time offer! Just for you, girl!",
              "maleVariant": "'Ey, yo! Limited-time offer! Just for you, big guy!",
              "stringId": "1811315154817527808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Ey, 'ey, this virtu I got right here? You gotta dive into this mutherfucker.",
              "maleVariant": "",
              "stringId": "1811315449442217984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What've you got?",
              "maleVariant": "",
              "stringId": "1811322854150385668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What've you got?",
              "maleVariant": "",
              "stringId": "1811324676122488832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For you, girl? Best that Night City has to offer!",
              "maleVariant": "For you, bro? Best that Night City has to offer!",
              "stringId": "1811325765299351552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And that would be...?",
              "maleVariant": "",
              "stringId": "1811327230252298240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The most nova-specced, geeked out, turnt up virtu you eeever seen! I'm talkin', like, transcendental!",
              "maleVariant": "",
              "stringId": "1811327333834829824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pay] Fine, I'll buy.",
              "maleVariant": "",
              "stringId": "1811328747634044932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not interested.",
              "maleVariant": "",
              "stringId": "1811329243350446080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And where's it from? You've got a license for exclusive distribution in Night City?",
              "maleVariant": "",
              "stringId": "1811329346681319424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, man, ain't nobody bother with that shit! Know why? 'Cause the stuff I got's way too legit to be legit, feel me?",
              "maleVariant": "",
              "stringId": "1811329435533455360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, you've got my attention.",
              "maleVariant": "",
              "stringId": "1811330519962365952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Haha! Knew you couldn't resist! Virtu's all yours – couples more eddies and you can use my player.",
              "maleVariant": "",
              "stringId": "1811331013715832832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not interested.",
              "maleVariant": "",
              "stringId": "1811334092100087808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You say that now, but I know you'll come crawlin' back 'ventually!",
              "maleVariant": "",
              "stringId": "1811335613944573952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Play braindance]",
              "maleVariant": "",
              "stringId": "1811364514880172036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No need. I've got my own wreath.",
              "maleVariant": "",
              "stringId": "1811369600557658112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course you do. It's prolly top shelf, latest firmware too, right? You see, this virtu's approachin' vintage grade – I doubt you'll be able to run it.",
              "maleVariant": "",
              "stringId": "1811370358048960512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Unknown File Format.\" Hmph, great.",
              "maleVariant": "\"Unknown File Format.\" Great.",
              "stringId": "1811371932204806144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Braindance doesn't play on my tech.",
              "maleVariant": "",
              "stringId": "1811390137665204228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You were right. My tech doesn't read it for some reason.",
              "maleVariant": "",
              "stringId": "1811390966476451840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Told you so! Backwards compatibility's a bitch, ain't it?",
              "maleVariant": "",
              "stringId": "1811391548645847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, we tryin' out my rig? I promise you it'll fire on every damn cylinder. 'Course, it'll cost ya a little extra...",
              "maleVariant": "",
              "stringId": "1811393221283950592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, let's try yours.",
              "maleVariant": "",
              "stringId": "1811393622527848448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want my eddies back.",
              "maleVariant": "",
              "stringId": "1811394130273513472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This virtu better be worth all this trouble.",
              "maleVariant": "",
              "stringId": "1811394530108125184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chill, girl, chill. Satisfaction or your money back guaranteed, all right? Now c'mon.",
              "maleVariant": "Chill, homie, chill. Satisfaction or your money back guaranteed, a'ight? Now c'mon.",
              "stringId": "1811395325532712960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No more bullshit. I want my eddies back.",
              "maleVariant": "",
              "stringId": "1811396983373312000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Ey, 'ey, now, let's not be jumpin' no guns, here. I'm tellin' ya, we can run it on my rig, no problem...",
              "maleVariant": "",
              "stringId": "1811399193838628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go where exactly?",
              "maleVariant": "",
              "stringId": "1811406029211971584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To my rig, yo, where else? Not like I carry that shit on me.",
              "maleVariant": "",
              "stringId": "1811406095700078592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Ey, this here's my rig. Take all the time you need.",
              "maleVariant": "",
              "stringId": "1811418181469614080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Play braindance]",
              "maleVariant": "",
              "stringId": "1811460089713668100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd like a refund.",
              "maleVariant": "",
              "stringId": "1812826847192166400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, holy shit, girl...? You good? A-after you booted up that virtu, y-you started glitchin' out and...",
              "maleVariant": "Heeey, holy shit, choom... You good? After you booted up that virtu, you started glitchin' out and...",
              "stringId": "1812827560945266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Save your breath. You conned me. You knew what you were doing.",
              "maleVariant": "",
              "stringId": "1812828261360480256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now hold up, yo – let's all be chilled for a second!",
              "maleVariant": "",
              "stringId": "1812829326730473472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll let you go this time.",
              "maleVariant": "",
              "stringId": "1812836649700044804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave Night City and don't come back.",
              "maleVariant": "",
              "stringId": "1812837474384097280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm shutting this down.",
              "maleVariant": "",
              "stringId": "1812837714281508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll let you go just this once.",
              "maleVariant": "",
              "stringId": "1812838653033852928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaaat? For real? Oh, God... thank you! I knew you'd have mercy on me!",
              "maleVariant": "",
              "stringId": "1812839200491188224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mercy?",
              "maleVariant": "",
              "stringId": "1812839534861103104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You've got an hour to get out of Night City and never look back.",
              "maleVariant": "",
              "stringId": "1812839969793650688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, my sweet fucking Jesus, thank you! I knew you'd com--",
              "maleVariant": "",
              "stringId": "1812840515288051712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut the fuck up. I wasn't finished.",
              "maleVariant": "",
              "stringId": "1812840787481604096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I ever see your face again, I'll gut you and string you up by your insides. Understand?",
              "maleVariant": "",
              "stringId": "1812841079908478976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time to shutter-up this rinky-dink operation.",
              "maleVariant": "",
              "stringId": "1812842009030701056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's on the recording?",
              "maleVariant": "",
              "stringId": "1851624541093416960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's on the virtu?",
              "maleVariant": "",
              "stringId": "1851626022639030272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Reelfeelin', neuron-shreddin', high-octane, trans-sensational, adrenaline-pumpin' shit, yo!",
              "maleVariant": "",
              "stringId": "1851627111195136000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your brain'll blow its load, girl. Trust me on this one.",
              "maleVariant": "Your mind will blow it's load, bruh. You gotta trust me on this.",
              "stringId": "1851627998894411776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh! Knew you'd come back! So what's good? You ready to buy?",
              "maleVariant": "",
              "stringId": "1851632325167992832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pay] OK, let's try it on yours.",
              "maleVariant": "",
              "stringId": "1851642319808548868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll try it on mine, first.",
              "maleVariant": "",
              "stringId": "1851642646947483648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, why not. Let's run it on yours.",
              "maleVariant": "Why not. Let's run it on yours.",
              "stringId": "1851643061311164416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't wait, huh? With the shit you're about to experience, I'd be bouncin' off the muthafuckin' walls!",
              "maleVariant": "",
              "stringId": "1851644888232546304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll try running it on my own, first. If that doesn't work, I'll be back.",
              "maleVariant": "",
              "stringId": "1851646243177287680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ey, 'ey, no problem! You know where to find me!",
              "maleVariant": "",
              "stringId": "1851649018380472320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want my eddies back.",
              "maleVariant": "",
              "stringId": "1851661231371636736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should've told me that before you took my eddies.",
              "maleVariant": "",
              "stringId": "1851661543025201152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're right, you're right! I-I forgot. But listen, you can use my setup, hm? Ain't no thang, trust me.",
              "maleVariant": "",
              "stringId": "1851662216395542528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, yeah? What's next? Using your toothbrush? Borrowing your underwear? Give me my eddies back, now.",
              "maleVariant": "",
              "stringId": "1851662737160327168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Damn, man, girl's got anger management issues. Here...",
              "maleVariant": "Damn, man! Homeboy's got anger management issues. Here...",
              "stringId": "1851664562252992512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try rippin' me off again and I'll rip out your fucking optics.",
              "maleVariant": "",
              "stringId": "1851665380964356096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come on. I've got my setup just 'round the corner.",
              "maleVariant": "",
              "stringId": "1851670067444764672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wanna know what I do, though? Hm?",
              "maleVariant": "",
              "stringId": "1851684695348400128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, no, no, please, Lord! Have mercy 'pon me, please!",
              "maleVariant": "",
              "stringId": "1851692184479518720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now, start walking.",
              "maleVariant": "",
              "stringId": "1851753526360068096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just didn't wanna get your blood on my threads.",
              "maleVariant": "",
              "stringId": "1851755175241633792"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}