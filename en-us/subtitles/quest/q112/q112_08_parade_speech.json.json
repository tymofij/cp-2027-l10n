    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Silence is not golden.",
              "maleVariant": "",
              "stringId": "1669135755361927168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is the sound of a flame extinguished.",
              "maleVariant": "",
              "stringId": "1669136419068592128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka on the third of March, 1942, in a prisoner-of-war camp, one month after his fighter jet was shot down over Guadalcanal Island.",
              "maleVariant": "",
              "stringId": "1669136734530584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka, pilot in the third squadron in the rank of master sergeant.",
              "maleVariant": "",
              "stringId": "1669136891548549120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck!",
              "maleVariant": "",
              "stringId": "1669217419584880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, what is your status?",
              "maleVariant": "",
              "stringId": "1669218386388729856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's your lucky day today…",
              "maleVariant": "",
              "stringId": "1669223235054485504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You and your fuckin' sick sense of honor…",
              "maleVariant": "",
              "stringId": "1669223459097427968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Today's your lucky day, though.",
              "maleVariant": "Today's... your lucky day, though.",
              "stringId": "1669223849721348096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, preem! Online and in the system.",
              "maleVariant": "",
              "stringId": "1669230128560881664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can thank your old friend, Goro.",
              "maleVariant": "",
              "stringId": "1698486545029283840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Course you're here!",
              "maleVariant": "",
              "stringId": "1713082986469015552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I managed to...",
              "maleVariant": "",
              "stringId": "1713085187102883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda... Is he dead?",
              "maleVariant": "",
              "stringId": "1713085246712332288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's breathin'…",
              "maleVariant": "",
              "stringId": "1713085328887136256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please, V. Show him mercy.",
              "maleVariant": "",
              "stringId": "1724162296454713344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No!",
              "maleVariant": "",
              "stringId": "1724187621863591936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No... You didn't have to!",
              "maleVariant": "",
              "stringId": "1724188104258883584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He'd have done the same to me.",
              "maleVariant": "",
              "stringId": "1724189454287900676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had to. Can't leave any loose ends.",
              "maleVariant": "",
              "stringId": "1724192698280054784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He'd've done the same to me.",
              "maleVariant": "",
              "stringId": "1724194637659115520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had to. Can't just go around leavin' loose ends.",
              "maleVariant": "",
              "stringId": "1724194708593184768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You saw it!",
              "maleVariant": "",
              "stringId": "1724195301483220992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will remember this.",
              "maleVariant": "",
              "stringId": "1724196313199677440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They come back, sting you in the ass, string you up by the neck.",
              "maleVariant": "",
              "stringId": "1724197607595446272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I understand. And I shall remember.",
              "maleVariant": "",
              "stringId": "1724198471135531008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hrr...",
              "maleVariant": "",
              "stringId": "1724200725154500608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is I who thank you, V.",
              "maleVariant": "",
              "stringId": "1724201522659463168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For now...",
              "maleVariant": "",
              "stringId": "1724202997997506560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda!",
              "maleVariant": "",
              "stringId": "1725692919384248320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I see Hanako-sama. She will speak soon.",
              "maleVariant": "",
              "stringId": "1740315746735321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Founder and CEO of the largest and greatest defense contractor in history.",
              "maleVariant": "",
              "stringId": "1773226942591275008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Soldier, industrialist, visionary.",
              "maleVariant": "",
              "stringId": "1773227138012286976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My father.",
              "maleVariant": "",
              "stringId": "1773227278957678592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These words are not for his friends, members of his family or for his business associates.",
              "maleVariant": "",
              "stringId": "1773228423298027520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They are for his enemies.",
              "maleVariant": "",
              "stringId": "1773228678043275264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And it is no coincidence that I utter them here in this city.",
              "maleVariant": "",
              "stringId": "1773228825934434304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was fifty years ago in Night City that our enemies showed their true colors.",
              "maleVariant": "",
              "stringId": "1773229002313306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A cowardly act of terrorism that consumed four thousand lives. The lives of Arasaka Corporation employees, the lives of Night Citizens.",
              "maleVariant": "",
              "stringId": "1773229207901310976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka Tower crumbled.",
              "maleVariant": "",
              "stringId": "1773229364399181824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And yet...",
              "maleVariant": "",
              "stringId": "1773229528446799872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And yet its spire still towers over the rooftops and skyscrapers.",
              "maleVariant": "",
              "stringId": "1773229586697293824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It towers more powerful and enduring than ever before.",
              "maleVariant": "",
              "stringId": "1773229822366846976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Recently, my father fell victim to an equally cowardly act of violence.",
              "maleVariant": "",
              "stringId": "1773229988796829696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was an act of violence aimed at the world of values in which we all share.",
              "maleVariant": "",
              "stringId": "1773230168111714304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As Night City is its witness, the Arasaka Corporation shall rise from this ordeal stronger than ever before, as it did fifty years past.",
              "maleVariant": "",
              "stringId": "1773230362727419904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is happening here and now.",
              "maleVariant": "",
              "stringId": "1773230601903411200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, for we will not stay silent.",
              "maleVariant": "",
              "stringId": "1773231479687352320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Silence is not golden.",
              "maleVariant": "",
              "stringId": "1773231823318290432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is the sound of a flame extinguished.",
              "maleVariant": "",
              "stringId": "1773232269156028416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There should be nothing standing in your way now. We press forward!",
              "maleVariant": "",
              "stringId": "1773457869241335808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Jack out] Break time.",
              "maleVariant": "",
              "stringId": "1794073849835307012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been on a while. Time for a break.",
              "maleVariant": "",
              "stringId": "1794076688959684608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now, V! I have her attention!",
              "maleVariant": "",
              "stringId": "1794077043277709312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pull out her link!",
              "maleVariant": "",
              "stringId": "1794077698058895360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm sorry, Oda.",
              "maleVariant": "",
              "stringId": "1834462117765189632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Brought this on yourself, too…",
              "maleVariant": "",
              "stringId": "1834463290291261440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ough...!",
              "maleVariant": "",
              "stringId": "1857629018768875520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or a nap.",
              "maleVariant": "",
              "stringId": "1857629722438868992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the…?!",
              "maleVariant": "",
              "stringId": "1857631771960352768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I warned you.",
              "maleVariant": "",
              "stringId": "2004115740769206272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We will settle this quickly, whore.",
              "maleVariant": "We will settle this quickly, toyboy.",
              "stringId": "2230175985079525376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You leave me no choice!",
              "maleVariant": "",
              "stringId": "2230178248040751112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should have quit and left the city!",
              "maleVariant": "",
              "stringId": "2230182266821517312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh! Still you struggle?! Let us try another way!",
              "maleVariant": "",
              "stringId": "2230183836330074120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You... will not touch her… I... will not let you!",
              "maleVariant": "",
              "stringId": "2230199314201026568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And Takemura – where is he?! Fearful to face his apprentice?!",
              "maleVariant": "",
              "stringId": "2230201191688937480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Jack in]",
              "maleVariant": "",
              "stringId": "2231360085887631364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beware – he will activate camouflage! Look for breaks in the light!",
              "maleVariant": "",
              "stringId": "2252032229872345088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His mask disturbs targeting! You must try another way!",
              "maleVariant": "",
              "stringId": "2252051393278222344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He is finished for today, it's enough! Do not kill him – this was his duty!",
              "maleVariant": "",
              "stringId": "2252053442866814984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Distance, watch the distance! Stay away from his blade, he will slice you like fish!",
              "maleVariant": "",
              "stringId": "2252057682972614664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda thinks he fights for the good cause. No! YOU fight for what is right and true! You must stand, on your feet!",
              "maleVariant": "",
              "stringId": "2252059389282590728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Listen to singing]",
              "maleVariant": "",
              "stringId": "2453324305031774208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get up]",
              "maleVariant": "",
              "stringId": "2453407681940045824"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}