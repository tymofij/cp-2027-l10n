  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"危険だと警告されたの。オダとも連絡が取れないし、何か変だわ\" t=\"I was alerted of a threat. Oda is not answering. Something isn't right.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669447913484337152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"私が参加したのは、あくまでもお父様を追悼するため\" t=\"I only agreed to this to honor the memory of our father.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669449579881316352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"操り人形になるためじゃないわ\" t=\"But now I've started to feel like your puppet.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1669450106887225344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"パレードを中止すれば、弱さを露呈したと思われる\" t=\"If we cancel the celebrations, it will be taken as a sign of weakness.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714211719007453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"弱さ？ 誰の？ あなたの？\" t=\"Weakness? Whose? Yours?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212267806965760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"だが、お前が言うなら中止してもいい。全部やめだ。お前の安全が第一だからな\" t=\"But I understand. One word from you and we can end this, it can all be over. Your safety is most important to me.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714212716832374784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お父様…\" t=\"For father.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714220354475409408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お許しを。どうか、話だけでもお聞きください\" t=\"Please forgive me, Hanako-sama. I only ask that you listen to me.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714224100794187776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様は…\" t=\"Your father...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225132743966720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"父のことを口にしないで！\" t=\"How dare you speak of him?!\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225355629281280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様、無礼を働きたくありません。しかし…\" t=\"Hanako-sama, please. Don't make me do this...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1714225488303505408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, run!",
              "maleVariant": "",
              "stringId": "1714227320375492608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit! Shit! Shit!",
              "maleVariant": "",
              "stringId": "1714228292917149696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"兄さん…\" t=\"Yorinobu?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724068500475875328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"やり終えましょう\" t=\"We must finish it.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724113942068727808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありがとう、ハナコ。せっかくの式典だ――やり終えなければ、悔いが残る\" t=\"Thank you, Hanako. It would have been a shame not to finish so heartwarming a ceremony.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724116336999534592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thsp…",
              "maleVariant": "",
              "stringId": "1724118030575603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコ様\" t=\"Hanako-sama.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724119896722116608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"あなたは…\" t=\"It's you...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724121657927143424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"これが、俺にできる最後のご奉仕なのです\" t=\"This is the last thing I can do for him.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724149966509576192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"サブロウ様の死について真実を明かすことが\" t=\"To reveal to you the truth about his death.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724150642950148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"この見せ物では、みんなに役割がある。ミチコは文句を言ってないぞ\" t=\"Everyone has their role to play in this spectacle. Michiko has no problem with that.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724223690596290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"なぜ止まったんだ？\" t=\"Why did you stop?\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724231251970101248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"俺たちだ\" t=\"Ours.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1724233151520051200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whatever he did, now we're really fucked!",
              "maleVariant": "",
              "stringId": "1771027611004047360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako's inside.",
              "maleVariant": "",
              "stringId": "1773724698127908864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tryna call someone, I think.",
              "maleVariant": "",
              "stringId": "1773725998731247616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try to listen.",
              "maleVariant": "",
              "stringId": "1773726175479218176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do not forget to disarm the alarms. I must be able to enter.",
              "maleVariant": "",
              "stringId": "1773726519227596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goro? All systems jammed. It's now or never.",
              "maleVariant": "",
              "stringId": "1773729105720655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is she doing?",
              "maleVariant": "",
              "stringId": "1780695188894519296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hurry, V!",
              "maleVariant": "",
              "stringId": "1790741196538200068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, it will be too late soon!",
              "maleVariant": "",
              "stringId": "1790741196554977284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have no right to keep us here!",
              "maleVariant": "",
              "stringId": "1795474982084558848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On the ground! Everyone!",
              "maleVariant": "",
              "stringId": "1795475678641012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go fuck yourself! This is a violation of our civil rights!",
              "maleVariant": "",
              "stringId": "1795476053259468800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Halt!",
              "maleVariant": "",
              "stringId": "1795479668128759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Does anyone else have anything to say about civil rights?! You can join him!",
              "maleVariant": "",
              "stringId": "1795483864295030784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I almost got trampled... Broken ribs, I think…",
              "maleVariant": "",
              "stringId": "1801295829407907840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We need to get out of here!",
              "maleVariant": "",
              "stringId": "1801296487729086464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I look OK to you? We're about to go live.",
              "maleVariant": "",
              "stringId": "1801297323049893888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Three, two, one...",
              "maleVariant": "",
              "stringId": "1801298631119093760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is Gillean Jordan of N54 News coming to you live from Japantown, where a commemorative parade in honor of Saburo Arasaka descended into chaos today.",
              "maleVariant": "",
              "stringId": "1801299000486281216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eyewitnesses we've spoken to described a shootout on one of the celebratory floats. The float in question is said to have been transporting Hanako Arasaka.",
              "maleVariant": "",
              "stringId": "1801299245702070272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Unfortunately, I have no further details to report at this time.",
              "maleVariant": "",
              "stringId": "1801300495923429376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are still awaiting an official statement from the Arasaka Corporation regarding these events.",
              "maleVariant": "",
              "stringId": "1801300622893400064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey! Stay out of the shot!",
              "maleVariant": "",
              "stringId": "1801303594255794176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, dammit... Let's do it again!",
              "maleVariant": "",
              "stringId": "1801304953780715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you! People're working here!",
              "maleVariant": "",
              "stringId": "1801305575091355648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As the situation continues to be brought under control, we urge everyone to avoid Kanzaki Street.",
              "maleVariant": "",
              "stringId": "1863315822830493696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Knew we couldn't trust him! What happened to keepin' him on his leash?!",
              "maleVariant": "",
              "stringId": "1906936140640829440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'd best delta the fuck outta here.",
              "maleVariant": "",
              "stringId": "1906937283303456768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He shot her?!",
              "maleVariant": "",
              "stringId": "2007084788928552960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Deactivate  security measures]",
              "maleVariant": "",
              "stringId": "2034753975066525696"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}