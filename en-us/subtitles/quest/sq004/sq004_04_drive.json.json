  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh yeah... We even have a radio in here.",
              "maleVariant": "",
              "stringId": "1375164841633607680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, out.",
              "maleVariant": "",
              "stringId": "1376237517701738496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, let's go over the plan again.",
              "maleVariant": "",
              "stringId": "1376253101084602368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Holy... Do you see that?!",
              "maleVariant": "",
              "stringId": "1376328672795783168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good luck.",
              "maleVariant": "",
              "stringId": "1700875168543301632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We need to hide the car. Park beside that outcrop.",
              "maleVariant": "",
              "stringId": "1764683611163996160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, we don't have time! A storm is rolling in.",
              "maleVariant": "",
              "stringId": "1764770380677001216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know the perfect spot to roost. I'll have my eyes on you throughout.",
              "maleVariant": "",
              "stringId": "1776252189428297728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Ready as I'll ever be.",
              "maleVariant": "",
              "stringId": "1776255147251478532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right here, V.",
              "maleVariant": "",
              "stringId": "1776260259839619072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You might as well have ridden with me!",
              "maleVariant": "",
              "stringId": "1783732414881869824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have you lost your damned mind?!",
              "maleVariant": "",
              "stringId": "1822633049249165312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll have you covered. You find Saul and get him out of there.",
              "maleVariant": "",
              "stringId": "1822836259452760064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll meet up after.",
              "maleVariant": "",
              "stringId": "1822836525354856448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll find a place to roost and keep you in sight.",
              "maleVariant": "",
              "stringId": "1847678184782229504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Ready as I'll ever be. You?",
              "maleVariant": "",
              "stringId": "1847679575848316928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is one hell of a storm!",
              "maleVariant": "",
              "stringId": "1886342008745566208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hijacking a Petrochem tanker, opening the valves, cranking a few figure eights to set the desert ablaze and give the kids a fun show…",
              "maleVariant": "",
              "stringId": "1886495717622358016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's the Saul I remember.",
              "maleVariant": "",
              "stringId": "1886496439394967552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moving civilians across the border during a bombardment…",
              "maleVariant": "",
              "stringId": "1886497324510552064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All had our idols…",
              "maleVariant": "",
              "stringId": "1886498802029633536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If we're to do this, it's now or never.",
              "maleVariant": "",
              "stringId": "1886515716231602176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lookin'... bad.",
              "maleVariant": "",
              "stringId": "1886519217166819328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, it's bad in more than its look. We have to move quickly.",
              "maleVariant": "",
              "stringId": "1886519378613968896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I still can't believe he let himself get captured…",
              "maleVariant": "",
              "stringId": "1893697359283298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyone else... but Saul?",
              "maleVariant": "",
              "stringId": "1893697941083594752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Said yourself he's not at the top of his form.",
              "maleVariant": "",
              "stringId": "1893700340762353664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, but…",
              "maleVariant": "",
              "stringId": "1893700992943071232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take it for a spin if you like.",
              "maleVariant": "",
              "stringId": "1893707732317184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You just watch yourself down there.",
              "maleVariant": "",
              "stringId": "1893714389029953536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But if things get noisy, I'll be your long-distance insurance policy.",
              "maleVariant": "",
              "stringId": "1893755141609037824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll be linked at all times. So as soon as you find Saul, I'll fire up the engine and we'll speed out of there.",
              "maleVariant": "",
              "stringId": "1893756179799941120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You slip into the Wraiths' camp, soft as a fox…",
              "maleVariant": "",
              "stringId": "1893758231032700928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you ready?",
              "maleVariant": "",
              "stringId": "1895149296348372992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Storm's headed straight this way.",
              "maleVariant": "",
              "stringId": "1895152071132127232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Decent storm's rumbling in, looks like.",
              "maleVariant": "",
              "stringId": "1895152305593720832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Decent\"? That's a roof-ripper from hell! We have to get this done before she hits us.",
              "maleVariant": "",
              "stringId": "1895152816325730304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hop out and let's get to work.",
              "maleVariant": "",
              "stringId": "1895166198789513216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll cover you from that perch. You find Saul and get him out of there.",
              "maleVariant": "",
              "stringId": "1895166775120437248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dammit, too late to abort now.",
              "maleVariant": "",
              "stringId": "1895170199299891200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll meet up after.",
              "maleVariant": "",
              "stringId": "1895171037623820288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. It's just...",
              "maleVariant": "",
              "stringId": "1895181027650641920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang on. Let's sum things up.",
              "maleVariant": "",
              "stringId": "1957602384160468992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's sum things up, just to be sure.",
              "maleVariant": "",
              "stringId": "2277749476720746496"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}