        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear about Santiago? Used to be a nomad, long time ago...",
              "maleVariant": "",
              "stringId": "1944719384973692928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You hear about Santiago?",
              "maleVariant": "",
              "stringId": "1944721995693051908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ever heard of our lord and savior Jesus Christ?",
              "maleVariant": "",
              "stringId": "1944742553386242048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Friend o' mine did a job with him once.",
              "maleVariant": "",
              "stringId": "1944746130473242624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why's folks suddenly recollecting Santiago?",
              "maleVariant": "",
              "stringId": "1944746289621913600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's one old friend you got.",
              "maleVariant": "",
              "stringId": "1944752261555834880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's a legend, all right. Wouldn't say he was my role model, but well... he was the man for his time and place.",
              "maleVariant": "",
              "stringId": "1944752909257035776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds like he's not so legendary for you.",
              "maleVariant": "",
              "stringId": "1944776474148753412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You don't think he was all he was cracked up to be?",
              "maleVariant": "",
              "stringId": "1944787194756222976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want to talk about something else.",
              "maleVariant": "",
              "stringId": "1944794347302834176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Something else I want to talk about.",
              "maleVariant": "",
              "stringId": "1944794446053527552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They say he was a big city nomad. Best o' both worlds.",
              "maleVariant": "",
              "stringId": "1944819946264219648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cept there ain't no such thing. You're either loyal to your family, or you ain't.",
              "maleVariant": "",
              "stringId": "1944820228792537088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doesn't loyalty to your friends count for something?",
              "maleVariant": "",
              "stringId": "1944821342262808576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe it does. Then you end up like Santiago.",
              "maleVariant": "",
              "stringId": "1944821777279242240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He snuffed it 'cause some lunatic wanted to blow Arasaka to smithereens.",
              "maleVariant": "",
              "stringId": "1944826694379261952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What a friend.",
              "maleVariant": "",
              "stringId": "1944827662206521344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, take care now.",
              "maleVariant": "",
              "stringId": "2434857868263043072"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}