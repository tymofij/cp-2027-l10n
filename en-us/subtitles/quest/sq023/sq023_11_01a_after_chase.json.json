      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stop the damn car!",
              "maleVariant": "",
              "stringId": "1559111705497067520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No! Stop! Stop the killing!",
              "maleVariant": "",
              "stringId": "1560251716485406720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not their fault! Shoot the girl and our deal is dead!",
              "maleVariant": "It's not their fault! Shoot the guy and our deal is dead!",
              "stringId": "1560264606554677248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come with us. You'll find out all you need to know on the way.",
              "maleVariant": "",
              "stringId": "1560285676439494656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Noted. But you don't have a vote here.",
              "maleVariant": "",
              "stringId": "1560286823581638656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hah! That's it. Got no fuckin' idea what this is about, but if you don't go with them I'm never talking to you again.",
              "maleVariant": "",
              "stringId": "1560288335946997760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Finally! Somethin' that isn't a complete snorefest. Could binge-watch this all day if I had some popcorn…",
              "maleVariant": "",
              "stringId": "1563169581303750656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, this one time Rachel is right – we can't stay here.",
              "maleVariant": "",
              "stringId": "1563172895642771456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They're here to kill you!",
              "maleVariant": "",
              "stringId": "1755039294645886976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We take her with us.",
              "maleVariant": "We take him with us.",
              "stringId": "1755041991331700736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, no thanks. Just be on my way, now.",
              "maleVariant": "No thanks. Just be on my way, now.",
              "stringId": "1755045418463944704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, wastin' time here!",
              "maleVariant": "",
              "stringId": "1755045722852974592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please? I need you. Really.",
              "maleVariant": "",
              "stringId": "1755045913458925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Answer's no.",
              "maleVariant": "",
              "stringId": "1755048913460027392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why do you need me? Why so badly?",
              "maleVariant": "",
              "stringId": "1755049082003939328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now can we go?",
              "maleVariant": "",
              "stringId": "1755050212234649600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want me to come... to come with you?",
              "maleVariant": "You wa-- you want me to come with you?",
              "stringId": "1755052034408079360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, I need you.",
              "maleVariant": "",
              "stringId": "1755052977908375552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody and I mean nobody, will harm a hair on your head. I'm just uh, heh... borrowing you.",
              "maleVariant": "",
              "stringId": "1755053834771128320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the hell is this?",
              "maleVariant": "",
              "stringId": "1755055933751844864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V.",
              "maleVariant": "",
              "stringId": "1755087536876183552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Josh Stephenson. But you probably knew that already…",
              "maleVariant": "",
              "stringId": "1755087879114612736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your... associate – what was his name?",
              "maleVariant": "",
              "stringId": "1755089781483134976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's OK. You don't need to tell me. My name is Joshua Stephenson.",
              "maleVariant": "",
              "stringId": "1755096294062841856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaaand your... associate – what was his name?",
              "maleVariant": "",
              "stringId": "1755097256101961728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bill Jablonsky. Ring a bell?",
              "maleVariant": "",
              "stringId": "1755101055252193280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sadly, yes.",
              "maleVariant": "",
              "stringId": "1755101818967842816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Teach us to number our days, that we may gain a heart of wisdom!\"",
              "maleVariant": "",
              "stringId": "1755101954242535424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Already ID'd him. Bill Jablonsky.",
              "maleVariant": "",
              "stringId": "1755102971864571904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait a sec, you're V? Eh, I heard o' you!",
              "maleVariant": "",
              "stringId": "1755105826189107200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well I want to give her a job.",
              "maleVariant": "Well I want to give him a job.",
              "stringId": "1755106202200072192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Agh! Christ on a stick…",
              "maleVariant": "",
              "stringId": "1755106404382302208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta object, strongly.",
              "maleVariant": "",
              "stringId": "1772116061515124736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck! Brake, I said! Brake!",
              "maleVariant": "",
              "stringId": "1785261803226238976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stay close, I'm going to blow the motherfucker to bits myself!",
              "maleVariant": "",
              "stringId": "1785275500145381376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is it, let's go!",
              "maleVariant": "",
              "stringId": "1790938824107331584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First, gonna tell me what the hell's going on.",
              "maleVariant": "First you're gonna tell me what the hell is going on.",
              "stringId": "1801135282559356928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck are you doing? Stop!",
              "maleVariant": "",
              "stringId": "1802552528787705856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPD! Stay where you are! And get that goddamn semi outta here!",
              "maleVariant": "",
              "stringId": "1802555133047836672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right, let's just all shoot holes in each other.",
              "maleVariant": "",
              "stringId": "1802612284567031808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For crying out loud, fuck! We can't leave her here! Media vultures'll be here any minute!",
              "maleVariant": "For crying out loud, fuck! We can't leave him here! Media vultures'll be here any minute!",
              "stringId": "1802613003755950080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gregor? Busted an attempted hit on my guy…",
              "maleVariant": "",
              "stringId": "1802618336310169600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vasquez! Shoot!",
              "maleVariant": "",
              "stringId": "1804412503094484992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What?!",
              "maleVariant": "",
              "stringId": "1804425734043230208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're outta your fuckin' mind!",
              "maleVariant": "",
              "stringId": "1804426452644945920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, another word from you and I'm callin' the studio. Don't make me ruin your career.",
              "maleVariant": "",
              "stringId": "1804427281019985920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hhmm... What's your name?",
              "maleVariant": "",
              "stringId": "1804441986451136512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We can't take her, out of the question. She's way too big a threat.",
              "maleVariant": "We can't take him, out of the question. He's way too big a threat.",
              "stringId": "1804474842179198976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Screw this, I'm out.",
              "maleVariant": "",
              "stringId": "1804477268030414848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aahh... such a shame.",
              "maleVariant": "",
              "stringId": "1804477986900566016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's OK – go. Nobody's going to stop you.",
              "maleVariant": "",
              "stringId": "1804478108736708608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna tell me what it is you want from me?",
              "maleVariant": "",
              "stringId": "1804479647425519616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't say the same about you.",
              "maleVariant": "",
              "stringId": "1804480122556276736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. I'll go with you.",
              "maleVariant": "",
              "stringId": "1804487988772102144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not goin' anywhere.",
              "maleVariant": "",
              "stringId": "1804489174870941696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, move it. We're running late.",
              "maleVariant": "",
              "stringId": "1804491309335470080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah. One cazh.",
              "maleVariant": "",
              "stringId": "1804505357384966144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Loo-- I can explain, just... don't go! I... I need you.",
              "maleVariant": "",
              "stringId": "1812994947364184064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're insane if you think I'm getting in that car.",
              "maleVariant": "",
              "stringId": "1827265802063601664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Haha, I just, ah... I, um... I really need you, please…",
              "maleVariant": "",
              "stringId": "1827266797254500352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, ditch the bitch. We gotta go.",
              "maleVariant": "Ugh, ditch the dick. We gotta go.",
              "stringId": "1827266955933409280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. She's coming with us. She has to.",
              "maleVariant": "No. He's coming with us. He has to.",
              "stringId": "1827267238830825472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aughh... Such a shame...",
              "maleVariant": "",
              "stringId": "1827277033335971840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lieutenant Vasquez, NCPD! Stop right there!",
              "maleVariant": "",
              "stringId": "1902535604594249728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're protecting a murderer!",
              "maleVariant": "",
              "stringId": "1902545920149393408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait!",
              "maleVariant": "",
              "stringId": "1902566478379454468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drop it, or I shoot!",
              "maleVariant": "",
              "stringId": "1938886351573192704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want me to come... with you?",
              "maleVariant": "",
              "stringId": "1959291298781028352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not going anywhere.",
              "maleVariant": "",
              "stringId": "1959291298781028368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the hell you want from me?",
              "maleVariant": "",
              "stringId": "1959291298864914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Screw this, I'm out.",
              "maleVariant": "",
              "stringId": "1959291298864914448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why me?",
              "maleVariant": "",
              "stringId": "1959291299083018240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not about to go anywhere with you.",
              "maleVariant": "",
              "stringId": "1959291299083018256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got no idea what's going on here.",
              "maleVariant": "",
              "stringId": "1959291299200458752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're insane.",
              "maleVariant": "",
              "stringId": "1959291299200458768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No thanks, I'm outta here.",
              "maleVariant": "",
              "stringId": "1959291299200458784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V.",
              "maleVariant": "",
              "stringId": "1959291299284344832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's this about, tell me.",
              "maleVariant": "",
              "stringId": "1959291299284344864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bill Jablonsky.",
              "maleVariant": "",
              "stringId": "1959291299385008128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in] So be it, I'll go.",
              "maleVariant": "",
              "stringId": "1959291300928512000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPD! Stop right there!",
              "maleVariant": "",
              "stringId": "1960500864785014784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You fucking idiot!",
              "maleVariant": "",
              "stringId": "1996611625131782144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out]",
              "maleVariant": "",
              "stringId": "2086613906903871492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bill Jablonsky. No priors, but run a deep sweep just to be sure.",
              "maleVariant": "",
              "stringId": "2218695280691605504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This… this… what a fuckin' mess…",
              "maleVariant": "",
              "stringId": "2218695432105979904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Told you drop it, dickhead…",
              "maleVariant": "",
              "stringId": "2218716369232687104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shoulda listened, shoulda fuckin' listened…",
              "maleVariant": "",
              "stringId": "2218716736485945344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're clear to go. Boys from the precinct'll mop up here.",
              "maleVariant": "",
              "stringId": "2218727630821486592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm gonna do it... I'm gonna fucking kill him!",
              "maleVariant": "",
              "stringId": "2218728539861381120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Olga? We've got a situation...",
              "maleVariant": "",
              "stringId": "2218738687191379968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somebody just tried to drop our star.",
              "maleVariant": "",
              "stringId": "2218740669838880768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That psycho's out hiring professionals! It was only matter of time.",
              "maleVariant": "",
              "stringId": "2218741137453445120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Cause he's a fucking nutcase, that's why!",
              "maleVariant": "",
              "stringId": "2218741241606402048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What a cunt!",
              "maleVariant": "",
              "stringId": "2218741738211995648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, I owe you. No patrol car and stay off the frequencies, too. Can't have this comin' back.",
              "maleVariant": "",
              "stringId": "2219983097342291968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Out. Go. Now.",
              "maleVariant": "",
              "stringId": "2221547689587601408"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}