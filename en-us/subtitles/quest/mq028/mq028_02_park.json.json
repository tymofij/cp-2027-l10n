  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'd I tell ya? That's her! Blue Moon!",
              "maleVariant": "",
              "stringId": "1816784476100009984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, hey, sorry! Could I get a picture with you? I'm a huge fan...",
              "maleVariant": "",
              "stringId": "1816794588449513472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What're you doing here?",
              "maleVariant": "",
              "stringId": "1816830239278538756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What're you doing here?",
              "maleVariant": "",
              "stringId": "1816830513586020352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, yeah... Perfect...",
              "maleVariant": "",
              "stringId": "1816832978662367232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Her again? For fuck's sakes...",
              "maleVariant": "",
              "stringId": "1816833074393161728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bullshit. She's way too fat.",
              "maleVariant": "",
              "stringId": "1816902114381832192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"kuzu-yarou\" b=\"Speak for yourself, \" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1816902349296410624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh-hahahaha! In yo FACE!",
              "maleVariant": "",
              "stringId": "1816902534147776512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know!",
              "maleVariant": "",
              "stringId": "1816903874429231104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey! \"Pillow Fight's\" an amazing song!",
              "maleVariant": "",
              "stringId": "1816904872908464128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure. What's your name?",
              "maleVariant": "",
              "stringId": "1816909236477796352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Green Cloud!",
              "maleVariant": "",
              "stringId": "1816909491223044096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"San... Ni... Ichi... Chi-zu!\" b=\"Haha! Awesome! \" a=\" There we go!\"/>",
              "maleVariant": "",
              "stringId": "1816910463932473344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Admiring the view until you showed up.",
              "maleVariant": "",
              "stringId": "1816939166997585920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So you think I'm a kid, huh?! I gave you everything! Everything!!",
              "maleVariant": "",
              "stringId": "1816989011502342144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I don't even know who you are! V! V, help!",
              "maleVariant": "",
              "stringId": "1816989724164923392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, isn't... Fuck, it is her!",
              "maleVariant": "",
              "stringId": "1817192367567052800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So fuckin' bored. Gimme a hit.",
              "maleVariant": "",
              "stringId": "1817193248572219392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Gimme,\" he says. Gimme the eddies, first.",
              "maleVariant": "",
              "stringId": "1817193477296005120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A drink, hehe... that's what I need... just a liiiitle sip...",
              "maleVariant": "",
              "stringId": "1817239793904558080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... my lips! They're burnin'! Hot, hot, hot...!",
              "maleVariant": "",
              "stringId": "1817240458617856000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mh, fuck! She's incredible!",
              "maleVariant": "",
              "stringId": "1817264994004410368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "United in life... united in death!",
              "maleVariant": "",
              "stringId": "1821393765695397888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit! Bitch point-blank shot her, yo!",
              "maleVariant": "",
              "stringId": "1822683487969386496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck's goin' on over here?!",
              "maleVariant": "",
              "stringId": "1822684390667493376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're here... awesome. So listen up, in a second I'm gonna get up and walk a little, OK?",
              "maleVariant": "",
              "stringId": "1864881884746653696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fweet, fweet... What's a girl like you doing around here?",
              "maleVariant": "",
              "stringId": "1864947485808697344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lookin' to get lucky, eh?",
              "maleVariant": "",
              "stringId": "1864949292245098496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. You're lucky if I don't rip your eyes out. Any takers? No? Then shut your yaps.",
              "maleVariant": "",
              "stringId": "1864949556251369472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bleh. Crusty, old geezers.",
              "maleVariant": "",
              "stringId": "1864951654074458112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Change the fuckin' track, will ya?",
              "maleVariant": "",
              "stringId": "1865016212549967872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Watch out for those gangoons, Blue Moon...",
              "maleVariant": "",
              "stringId": "1865064865486512128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Careful. Those guys look like trouble.",
              "maleVariant": "",
              "stringId": "1865065471966093312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, just your neighborhood beefnecks. Hate mail isn't their style.",
              "maleVariant": "",
              "stringId": "1865065878813581312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Blue Moon!",
              "maleVariant": "",
              "stringId": "1866125689474949120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you, thank you so much!",
              "maleVariant": "",
              "stringId": "1866133680009945088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Put the camera away. Now.",
              "maleVariant": "",
              "stringId": "1867495788739993600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's with the pictures?",
              "maleVariant": "",
              "stringId": "1867496011742748676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll rephrase the question, and you rephrase the fuckin' answer. What're the pictures for? Hm?",
              "maleVariant": "I'll rephrase the question, and you rephrase the fuckin' answer. What're the pictures for?",
              "stringId": "1867498766460952576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uhhh, I don't know? For, like, eddies? To sell? That girl there's Blue Moon...? From Us Cracks...?",
              "maleVariant": "",
              "stringId": "1867499997254295552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Put the camera away.",
              "maleVariant": "",
              "stringId": "1867503052636987396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Noticed anything out of the ordinary?",
              "maleVariant": "",
              "stringId": "1867512219691032576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, right! Blow me.",
              "maleVariant": "",
              "stringId": "1867514536943329280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I tried being polite.",
              "maleVariant": "",
              "stringId": "1867518002377957376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm... Take pictures of her often?",
              "maleVariant": "Huh... Take pictures of her often?",
              "stringId": "1867522197269159936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah. Why? That against the law?",
              "maleVariant": "",
              "stringId": "1867528116757504000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyone hang around her too much? Try to think.",
              "maleVariant": "",
              "stringId": "1867531733170298880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And why would I spill that?",
              "maleVariant": "",
              "stringId": "1867531929899933696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Draw weapon]",
              "maleVariant": "",
              "stringId": "1867540376255111168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why are you following Blue Moon?",
              "maleVariant": "",
              "stringId": "1867573180678447104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice nickname.",
              "maleVariant": "",
              "stringId": "1867573390712414208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I heard you're following Blue Moon?",
              "maleVariant": "I heard you're following Blue Moon.",
              "stringId": "1867774440597790720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excuse me? This is the first I've seen her in person!",
              "maleVariant": "",
              "stringId": "1867781750481133568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice nickname. Green Cloud.",
              "maleVariant": "",
              "stringId": "1867783058097348608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What are you doing here?",
              "maleVariant": "",
              "stringId": "1867783857296809984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That so? I just spoke with a still-taker who begs to differ. Has the shots to prove it, too.",
              "maleVariant": "",
              "stringId": "1867784509443973120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Um... thanks.",
              "maleVariant": "",
              "stringId": "1867786327959969792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"GC\" for short. That's how you sign your letters, right?",
              "maleVariant": "",
              "stringId": "1867786535125032960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck...!",
              "maleVariant": "",
              "stringId": "1867786739924508672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What're you up to?",
              "maleVariant": "",
              "stringId": "1867787343015092224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just taking a stroll... That not allowed?",
              "maleVariant": "",
              "stringId": "1867787562729512960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, sweet kid.",
              "maleVariant": "",
              "stringId": "1867843632789364736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Draw weapon]",
              "maleVariant": "",
              "stringId": "1871943042253246464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hilarious.",
              "maleVariant": "",
              "stringId": "1871943294582575104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Because if some psycho decides to off Blue Moon, you won't have anyone to snap stills of.",
              "maleVariant": "",
              "stringId": "1871958054959108096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Girl in green, over there. See her in every single frame. Funny, ain't it?",
              "maleVariant": "",
              "stringId": "1871958536549093376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Hilarious.",
              "maleVariant": "",
              "stringId": "1871959597691228160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not so close, V. Keep your distance.",
              "maleVariant": "",
              "stringId": "1896814549259165696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're getting too close, V!",
              "maleVariant": "",
              "stringId": "1896823816473190400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Argh, whatever, this is pointless. I'm going home.",
              "maleVariant": "",
              "stringId": "1896826019858522112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just keep an eye on Blue Moon, hm?",
              "maleVariant": "",
              "stringId": "1915386260258725888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I killed your stalker.",
              "maleVariant": "",
              "stringId": "1915463148159279104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your stalker's dead. Problem solved.",
              "maleVariant": "",
              "stringId": "1915464299529281536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck?! We had a deal, V! No killing!",
              "maleVariant": "",
              "stringId": "1915464624302628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better him than you though, right?",
              "maleVariant": "",
              "stringId": "1915465368422490112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is a waste of time. Let's just call it a day, all right?",
              "maleVariant": "",
              "stringId": "1915498437791621120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're not taking this seriously, V. I'm going home.",
              "maleVariant": "",
              "stringId": "1915541770421313536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. You can go.",
              "maleVariant": "",
              "stringId": "1922679128618868736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When I call you, you'll start following me... But don't get too close, OK? We can't spook our guy.",
              "maleVariant": "",
              "stringId": "1924010139200049152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got it.",
              "maleVariant": "",
              "stringId": "1924011067735068672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Phew... OK, here goes. Red Menace is keeping an extra lookout from a vantage point high up. Wish me luck!",
              "maleVariant": "",
              "stringId": "1924011425022660608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Argh! OK, I'll handle this – make sure nobody spills to the media vultures. Bye, V. Thanks for your help, I guess...",
              "maleVariant": "",
              "stringId": "1924043929016389632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need a new faceplate, like, ASAP!",
              "maleVariant": "",
              "stringId": "1957638791374217216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why were you in a gun store?",
              "maleVariant": "",
              "stringId": "1957746802470068224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What were you looking for in an iron depot?",
              "maleVariant": "",
              "stringId": "1957747721190416384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What? What are you talking about? I wasn't in any store!",
              "maleVariant": "",
              "stringId": "1957749148428181504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're a shitty liar.",
              "maleVariant": "",
              "stringId": "1957749658472325120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dammit... no explosive rounds...",
              "maleVariant": "",
              "stringId": "1957752263520985088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll just have to go with regular.",
              "maleVariant": "",
              "stringId": "1957755496993824768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This alley's perfect for an ambush... Keep an eye out!",
              "maleVariant": "",
              "stringId": "1958827136381001728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Phew... Another false alarm.",
              "maleVariant": "",
              "stringId": "1959041641912377344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, we've been everywhere... Maybe he isn't here.",
              "maleVariant": "",
              "stringId": "1959043155687022592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's just forget it, V. I'll tell Red Menace to pick us up.",
              "maleVariant": "",
              "stringId": "1963248519896944640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well... yeah, but...",
              "maleVariant": "",
              "stringId": "1963252548995698688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Found our stalker. Stay where you are.",
              "maleVariant": "Found your stalker. Stay where you are.",
              "stringId": "1967544093053722624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, OK... Be careful. And remember, no killing! Please!",
              "maleVariant": "",
              "stringId": "1967545886370340864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let me know when the coast is clear.",
              "maleVariant": "",
              "stringId": "1967588025485664256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK... One last shot. If he doesn't show, we're calling it quits.",
              "maleVariant": "",
              "stringId": "1974823601985200128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Say nothing]",
              "maleVariant": "",
              "stringId": "1975313822573043712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep your eyes peeled for anyone lurking around, watching me, spying on me from above... whatever. Anything creepy like that.",
              "maleVariant": "",
              "stringId": "1979120590027804672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One less suspect to worry about...",
              "maleVariant": "",
              "stringId": "2015453437229178880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure thing.",
              "maleVariant": "",
              "stringId": "2070921218581766148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm-hm-hmmm... Just look natural, calm... Everything's OK...",
              "maleVariant": "",
              "stringId": "2231705026017607680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ooh, still haven't done a spread for them. What issue is this...?",
              "maleVariant": "",
              "stringId": "2231705233753096192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, what what was that? Another weirdo like him and I'll hurl.",
              "maleVariant": "",
              "stringId": "2231705351747256320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, you there? Can't see you. Should be following me.",
              "maleVariant": "",
              "stringId": "2233147213842464768"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}