  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enny for your thoughts?",
              "maleVariant": "",
              "stringId": "1202569308387950592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have none. I mean, I don't even know.",
              "maleVariant": "",
              "stringId": "1202570965339357184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Really pleasant here.",
              "maleVariant": "",
              "stringId": "1212354433107640320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Be right there with you tomorrow. Whatever happens, we'll be together, we'll manage.",
              "maleVariant": "",
              "stringId": "1327214699397267456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's a nice spot up there. Quiet.",
              "maleVariant": "",
              "stringId": "1752168369827368960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An enny for your thoughts?",
              "maleVariant": "",
              "stringId": "1793772490433810432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got something on your mind, I can feel it .",
              "maleVariant": "",
              "stringId": "1793772636932460544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Killer view.",
              "maleVariant": "",
              "stringId": "1793772664312877056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somethin's gnawin' at you, I can tell. Whatever it is, you can tell me.",
              "maleVariant": "",
              "stringId": "1793773794174488576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What a view, huh?",
              "maleVariant": "",
              "stringId": "1793773842811637764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Screw views.",
              "maleVariant": "",
              "stringId": "1793775166802415616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothing's gnawing at me. Stop, this is irksome.",
              "maleVariant": "",
              "stringId": "1793775441831317504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm so tense I barely caught a wink last night. It's likely to be the same tonight.",
              "maleVariant": "",
              "stringId": "1793776939013296128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feel responsible for 'em. Who wouldn't?",
              "maleVariant": "",
              "stringId": "1793777411828797456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You feel responsible for them. Who in their right mind wouldn't?",
              "maleVariant": "",
              "stringId": "1793778606836338692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mere.",
              "maleVariant": "",
              "stringId": "1793778635877699588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm just looking at this with a cool head.",
              "maleVariant": "",
              "stringId": "1793782969214488576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul has made all the wrong decisions to be a little more like me.",
              "maleVariant": "",
              "stringId": "1793783636209487872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But only I know what I'm good at and at what I stink.",
              "maleVariant": "",
              "stringId": "1793783797505642496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not a miracle worker, V! I'm scared to the bone something will go wrong. That everyone will--",
              "maleVariant": "",
              "stringId": "1793783910684741632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody's gonna die tomorrow.",
              "maleVariant": "",
              "stringId": "1793784278223212544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry for dragging you into this.",
              "maleVariant": "",
              "stringId": "1793784278223212560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Be right there beside you.",
              "maleVariant": "",
              "stringId": "1793784609808109568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry to've dragged you into this. Sorry.",
              "maleVariant": "",
              "stringId": "1793786505851621376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody's dyin' tomorrow, Panam.",
              "maleVariant": "",
              "stringId": "1793786550764228612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You don't know that.",
              "maleVariant": "",
              "stringId": "1793789450789900288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, stop.",
              "maleVariant": "",
              "stringId": "1793790931479875584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Accept me into your family and how do I repay you? By taking you on an excursion through hell.",
              "maleVariant": "",
              "stringId": "1793791318698020864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1793792414636101632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's eatin' ya?",
              "maleVariant": "",
              "stringId": "1817170311849496576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, let's see.",
              "maleVariant": "",
              "stringId": "1817173075543511040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tomorrow I'll lead my whole clan to near-certain death.",
              "maleVariant": "",
              "stringId": "1817173256301236224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As if that's not enough, Saul is suddenly concurring with everything I say. And I don't even know if that's good or bad â€“ fuck.",
              "maleVariant": "",
              "stringId": "1817174133145653248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not the responsibility I'm worried about.",
              "maleVariant": "",
              "stringId": "1817174754590511104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice of you to be a good listener, but some input would be nice, too, from you.",
              "maleVariant": "",
              "stringId": "1817184032894275584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't know what I would do without you.",
              "maleVariant": "",
              "stringId": "1817194727429951488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'd do what you needed to.",
              "maleVariant": "",
              "stringId": "1817196976432533504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What do you know.",
              "maleVariant": "",
              "stringId": "1817197125951082496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I amâ€¦ all the fuckin' time.",
              "maleVariant": "",
              "stringId": "1817199008220499968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Very, all the time.",
              "maleVariant": "",
              "stringId": "1817199095814344708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, I'm not.",
              "maleVariant": "",
              "stringId": "1817199409531506688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try my damndest not to think about it.",
              "maleVariant": "",
              "stringId": "1817199466842476544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Hug Panam] I'm afraid for you.",
              "maleVariant": "",
              "stringId": "1817199515697729536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, I'm not.",
              "maleVariant": "",
              "stringId": "1817199815187812356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try my damndest not to think about it.",
              "maleVariant": "I try my damndest not to think about it.",
              "stringId": "1817199867281068036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not as much as I'm afraid for you.",
              "maleVariant": "",
              "stringId": "1817199922192896004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're great at hiding it.",
              "maleVariant": "",
              "stringId": "1817204489253081088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I envy you.",
              "maleVariant": "",
              "stringId": "1817204932859449344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So do I. But the harder I try, the worse it gets.",
              "maleVariant": "",
              "stringId": "1817205128850886656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People are doubting us... I can feel it.",
              "maleVariant": "",
              "stringId": "1837410053190897664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But they still believe if it all goes to shit, Panam will be there for them. Because who else, right?",
              "maleVariant": "",
              "stringId": "1837417985727057920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] So this is where you go.",
              "maleVariant": "",
              "stringId": "1886648873538678784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Darn, so nice here.",
              "maleVariant": "",
              "stringId": "1886649512146628608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So this is where you escape when you need to think.",
              "maleVariant": "So this is where you escape to when you need to think.",
              "stringId": "1886649579523928064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I never escape.",
              "maleVariant": "",
              "stringId": "1886651455216676864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm.",
              "maleVariant": "",
              "stringId": "1886652509513707520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is, uh, somethin' the matter?",
              "maleVariant": "",
              "stringId": "1886655372142964736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam.",
              "maleVariant": "",
              "stringId": "1886657562660171776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have you...",
              "maleVariant": "",
              "stringId": "1886657790981304320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fam, they're tougher than you think.",
              "maleVariant": "",
              "stringId": "1886684933949878272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bullets don't distinguish between tough and weak.",
              "maleVariant": "",
              "stringId": "1886685402369748992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It isn't fair of me, Panam. I wasn't fair from the start.",
              "maleVariant": "Isn't fair of me, Panam. Wasn't fair from the start.",
              "stringId": "1886687770540896256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know what's not fair? That you want me to massage your bruised conscience.",
              "maleVariant": "",
              "stringId": "1886688033993519104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, I won't do it. We're all in this together.",
              "maleVariant": "",
              "stringId": "1886691127762481152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahhhh, you're right.",
              "maleVariant": "Aagh, you're right.",
              "stringId": "1886691580361437184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah?",
              "maleVariant": "",
              "stringId": "1886694434803412992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you not afraid to die?",
              "maleVariant": "",
              "stringId": "1886694492651253760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry.",
              "maleVariant": "",
              "stringId": "1886695516480860160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I guess.",
              "maleVariant": "",
              "stringId": "1886696946335870976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wouldn't you rather be alone, maybe?",
              "maleVariant": "",
              "stringId": "1886700608735014912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, please.",
              "maleVariant": "",
              "stringId": "1886701044825190400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just need to get away from everything, but for a minute.",
              "maleVariant": "",
              "stringId": "1886701375269236736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To gather my thoughts before tomorrow.",
              "maleVariant": "",
              "stringId": "1886703427072421888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try not to bother you, then.",
              "maleVariant": "",
              "stringId": "1886703709214863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Exactly. Who if not you?",
              "maleVariant": "",
              "stringId": "1935969796615696384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Exactly. Who if not you?",
              "maleVariant": "",
              "stringId": "1935970658025074688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's what I mean! It shouldn't be me, it should be...",
              "maleVariant": "",
              "stringId": "1935971665480118272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul?",
              "maleVariant": "",
              "stringId": "1935974444307181568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If only he hadn't gone and made all the wrong decisions to be a little more like me.",
              "maleVariant": "",
              "stringId": "1935974510677848064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm scared to the bone something will go wrong. That everyone willâ€¦",
              "maleVariant": "",
              "stringId": "1935975156214788096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm thinking about the first night I spent up here.",
              "maleVariant": "",
              "stringId": "1935977991044198400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd been on the road, was on my way back and wanted to clear my head before hitting camp.",
              "maleVariant": "",
              "stringId": "1935979839591411712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was completely dark, empty. Not a star in the sky. All I heard was the wind and a few engines in the distance.",
              "maleVariant": "",
              "stringId": "1935981141805363200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An enny for your thoughts?",
              "maleVariant": "",
              "stringId": "1935983639949602820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time to head back?",
              "maleVariant": "",
              "stringId": "1935983759051059200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Hug] I could sit here with you forever.",
              "maleVariant": "",
              "stringId": "1935983884980842496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, before another likely shouting match with Saul.",
              "maleVariant": "Before another likely shouting match with Saul.",
              "stringId": "1935986629196509184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I sat here waiting for the world to fall away. For my mind's RPM to drop to zero.",
              "maleVariant": "",
              "stringId": "1935992855288475648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Time to head back, maybe?",
              "maleVariant": "",
              "stringId": "1935993313054814208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Let's sit here a while longer.",
              "maleVariant": "",
              "stringId": "1935993674536710144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is doing me good, more than I thought.",
              "maleVariant": "",
              "stringId": "1935994097876201472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I could sit here with you all night.",
              "maleVariant": "",
              "stringId": "1935994550693261312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I think I'd like that.",
              "maleVariant": "",
              "stringId": "1935996392781246464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have no idea how much I needed that.",
              "maleVariant": "",
              "stringId": "1935997050112569344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did it?",
              "maleVariant": "",
              "stringId": "1935999232811593728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. I realized that night it wouldn't happen again. That my mind would always be on the road, even while I was sitting, freezing on a cliff.",
              "maleVariant": "",
              "stringId": "1935999398973140992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We headin' back?",
              "maleVariant": "",
              "stringId": "1989571848086945792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could stay here with you forever.",
              "maleVariant": "",
              "stringId": "1989571848103723016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's on your mind?",
              "maleVariant": "",
              "stringId": "1989571848103723032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enny for your thoughts?",
              "maleVariant": "",
              "stringId": "2013987358175260672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Enny for your thoughts?",
              "maleVariant": "",
              "stringId": "2013988180695052288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will you say something?",
              "maleVariant": "",
              "stringId": "2243448137760657408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, nothing?",
              "maleVariant": "",
              "stringId": "2243448492414226436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't just stand there. Have a seat.",
              "maleVariant": "",
              "stringId": "2243452240796602368"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}