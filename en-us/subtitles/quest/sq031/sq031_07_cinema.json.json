    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But Silver Pixel's still hangin' in there.",
              "maleVariant": "",
              "stringId": "1321835944315936768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, V. Tell me what's going on. For real.",
              "maleVariant": "",
              "stringId": "1321836921689432064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds like a preem place for a date.",
              "maleVariant": "",
              "stringId": "1570532363560771584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin'?",
              "maleVariant": "Meaning?",
              "stringId": "1570536269095329792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck. I don't believe it.",
              "maleVariant": "",
              "stringId": "1570763549688160256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It really has been years.",
              "maleVariant": "",
              "stringId": "1570768282993778688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Managed to shut the whole thing down.",
              "maleVariant": "",
              "stringId": "1570768405903663104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Entrance is over here.",
              "maleVariant": "",
              "stringId": "1570928690224390144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Bushido X – Fade to Black.\" Must be a classic by now.",
              "maleVariant": "",
              "stringId": "1570967026733375488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Some crew takin' you along as bait?",
              "maleVariant": "",
              "stringId": "1760902348966420480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That a compliment?",
              "maleVariant": "",
              "stringId": "1760903041127243776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You look... different.",
              "maleVariant": "",
              "stringId": "1760903459920109568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh, try again.",
              "maleVariant": "",
              "stringId": "1760904214458621952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Rogue. Ready?",
              "maleVariant": "",
              "stringId": "1760905606564896768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Again – you.",
              "maleVariant": "",
              "stringId": "1760907709404372992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We goin'?",
              "maleVariant": "",
              "stringId": "1760908046978736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where's Johnny? Couldn't make it after all?",
              "maleVariant": "",
              "stringId": "1760909665694867456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He'll join us later.",
              "maleVariant": "",
              "stringId": "1760910851189727236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm just the driver.",
              "maleVariant": "",
              "stringId": "1760911134741454848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm just your driver.",
              "maleVariant": "",
              "stringId": "1760911822171103232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He'll join us later.",
              "maleVariant": "",
              "stringId": "1760913731502170112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drive-ins went out of style a century ago.",
              "maleVariant": "",
              "stringId": "1760951635209314304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought so, too.",
              "maleVariant": "",
              "stringId": "1760952662998343680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't stop us, won't stop us. We asked you out on a date.",
              "maleVariant": "",
              "stringId": "1760966723462979584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rogue?",
              "maleVariant": "",
              "stringId": "1761109194507509760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just taking it all in.",
              "maleVariant": "",
              "stringId": "1761109194507509772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, a selfless Johnny Silverhand.",
              "maleVariant": "",
              "stringId": "1761111209451151360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Silver Pixel Cloud your favorite place?",
              "maleVariant": "",
              "stringId": "1761147999335702528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here we are.",
              "maleVariant": "",
              "stringId": "1761151765820694528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Looks like I found the projector.",
              "maleVariant": "",
              "stringId": "1761253806861672448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, V.",
              "maleVariant": "",
              "stringId": "1779967069262667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't mention it.",
              "maleVariant": "",
              "stringId": "1779967893040750592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Once, probably. Haven't been there in ages.",
              "maleVariant": "",
              "stringId": "1780003495685611520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. Do your thing, I'll watch.",
              "maleVariant": "",
              "stringId": "1781967527372087296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take pills] You kids have fun now.",
              "maleVariant": "",
              "stringId": "1782086298149543940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have fun, you crazy kids.",
              "maleVariant": "",
              "stringId": "1782091963983159296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Exactly where I was supposed to take her. Fifty odd years ago.",
              "maleVariant": "",
              "stringId": "1822873091378057216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just, this time don't go roamin' around town. And no tattoos. And don't clean out my cred.",
              "maleVariant": "",
              "stringId": "1822891917561892864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe you'll find some charm third time around.",
              "maleVariant": "",
              "stringId": "1822904972383424512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You look amazing.",
              "maleVariant": "",
              "stringId": "1822918847761813504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You look... wow.",
              "maleVariant": "",
              "stringId": "1822920251243040768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That Johnny's \"wow\" or yours? When's he gonna show?",
              "maleVariant": "",
              "stringId": "1822921934366904320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See you keep him on a short leash.",
              "maleVariant": "",
              "stringId": "1822925555527651328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, wise beyond your years.",
              "maleVariant": "",
              "stringId": "1822926432422400000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So it wasn't his idea.",
              "maleVariant": "",
              "stringId": "1823030719949377536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Which is why I suggested Johnny take me there. A million years ago.",
              "maleVariant": "",
              "stringId": "1823034147853373440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course not.",
              "maleVariant": "",
              "stringId": "1823035520934285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His idea of a romantic night out was blowing up Arasaka HQ.",
              "maleVariant": "",
              "stringId": "1823044875658268672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny doesn't do a thing without an ulterior motive.",
              "maleVariant": "",
              "stringId": "1824439212782718976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So what's in play this time?",
              "maleVariant": "",
              "stringId": "1824444991325671424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny's changed.",
              "maleVariant": "",
              "stringId": "1824447356091043840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He really does wanna spend some time with you.",
              "maleVariant": "He really does wanna spend some time with ya.",
              "stringId": "1824447754969354240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No hooks, no lines, no sinkers.",
              "maleVariant": "",
              "stringId": "1824448077511331840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right...",
              "maleVariant": "",
              "stringId": "1824448721739649024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Apparently, you're a positive influence on him.",
              "maleVariant": "",
              "stringId": "1824454395139788800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, try to get inside.",
              "maleVariant": "",
              "stringId": "1824462193894428672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, gotta be the pass, this.",
              "maleVariant": "",
              "stringId": "1881339081475899392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guess we really are going to the movies.",
              "maleVariant": "",
              "stringId": "1881339798450225152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wham bam sha-la-la.",
              "maleVariant": "",
              "stringId": "1895759908900323328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, let's go.",
              "maleVariant": "",
              "stringId": "1895773731279486976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well done.",
              "maleVariant": "",
              "stringId": "1895808615289077760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Closed? Really has been a while, I guess.",
              "maleVariant": "",
              "stringId": "1913960967086821376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Technically, it's open – been here before.",
              "maleVariant": "",
              "stringId": "1913965546880913408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know a way in. Practically have the place all to ourselves.",
              "maleVariant": "I know a way in. Practically have the place all to ourselves.",
              "stringId": "1913985264807333888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK...",
              "maleVariant": "",
              "stringId": "1913988120708927488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You ready?",
              "maleVariant": "",
              "stringId": "1922721542259339268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Use] See in a sec.",
              "maleVariant": "",
              "stringId": "1922734488901382148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You wouldn't happen to know how to get it up an' running?",
              "maleVariant": "",
              "stringId": "1934328665788403712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try and get the projector started – I'll get us some seats.",
              "maleVariant": "",
              "stringId": "1953427813204639744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure thing.",
              "maleVariant": "",
              "stringId": "1953430858034462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right, let's go check the projector.",
              "maleVariant": "",
              "stringId": "1953435073326759936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Bout to see.",
              "maleVariant": "",
              "stringId": "1953442911105204224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look at that – last played \"Bushido X\"! Perfect.",
              "maleVariant": "",
              "stringId": "1953443484701442048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like a spurt of runny shit.",
              "maleVariant": "",
              "stringId": "1953444269606715392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, V. Run along. I'll take it from here.",
              "maleVariant": "",
              "stringId": "1953445991049740292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, we're at a drive-in. Movie's the last thing we care about.",
              "maleVariant": "",
              "stringId": "1954552420875653120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just want some background noise – a little blood, tits and gore, that sorta thing.",
              "maleVariant": "",
              "stringId": "1954552785780101120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Bushido\" will give you that.",
              "maleVariant": "",
              "stringId": "1954553932788027392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go.",
              "maleVariant": "",
              "stringId": "1962231657350971396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This Johnny's Porsche? Ahaha!",
              "maleVariant": "",
              "stringId": "1962275180083298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd you get it?",
              "maleVariant": "",
              "stringId": "1962276016477847552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's what was in the container, the one Grayson was talking about.",
              "maleVariant": "",
              "stringId": "1962276317897310208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, OK. Hehe, got a lotta memories in this car.",
              "maleVariant": "",
              "stringId": "1962276661528248320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't stop us now.",
              "maleVariant": "",
              "stringId": "2074766443780390916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Wait until evening]",
              "maleVariant": "",
              "stringId": "2511473844437118980"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}