      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How did Weyland help you, exactly?",
              "maleVariant": "How'd Weyland help you, exactly?",
              "stringId": "1566659289744629760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He made sure we'll find Grayson at home when we stop by.",
              "maleVariant": "",
              "stringId": "1566663534933925888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So we know where to look for him.",
              "maleVariant": "",
              "stringId": "1566663860730683392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure you're up for this?",
              "maleVariant": "Ya sure you're up for this?",
              "stringId": "1566668552177147904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good luck!",
              "maleVariant": "",
              "stringId": "1728878062231838720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Eey, V.",
              "maleVariant": "",
              "stringId": "1728905734521577472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland? Of THE Weylands?",
              "maleVariant": "",
              "stringId": "1728907600416432132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna join us?",
              "maleVariant": "",
              "stringId": "1728910347064795136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland? Of THE Weylands?",
              "maleVariant": "",
              "stringId": "1728911906339250176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, girl. I take it ya heard of old Andrew Boa Boa?",
              "maleVariant": "Yeah, man. I take it ya heard of old Andrew Boa Boa?",
              "stringId": "1728912362494976000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like everybody in this town.",
              "maleVariant": "",
              "stringId": "1728913090273828864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna be joinin' us?",
              "maleVariant": "Ya gonna be joinin' us?",
              "stringId": "1728913679925862400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah, I already did me part.",
              "maleVariant": "",
              "stringId": "1728913679925862408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't tell me you don't wanna see Smasher dead.",
              "maleVariant": "",
              "stringId": "1728913679925862416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look who we have here.",
              "maleVariant": "",
              "stringId": "1750353986764627968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, Crispin Weyland. He's helping us.",
              "maleVariant": "",
              "stringId": "1750367610015014912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I hear you're hunting Smasher.",
              "maleVariant": "",
              "stringId": "1750370234978230272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ready? We'll talk on the way.",
              "maleVariant": "",
              "stringId": "1750370823405527040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's roll.",
              "maleVariant": "",
              "stringId": "1750372432676085760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got something to tend to first.",
              "maleVariant": "",
              "stringId": "1750372601219997696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got something I gotta do first.",
              "maleVariant": "I got somethin' I gotta do first.",
              "stringId": "1750373616476446720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go.",
              "maleVariant": "",
              "stringId": "1750374823966236672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jump in.",
              "maleVariant": "",
              "stringId": "1750443672895971328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Comin' back later?",
              "maleVariant": "",
              "stringId": "1750589189592408064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We goin'?",
              "maleVariant": "",
              "stringId": "1778080573580599296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotcha a little somethin'.",
              "maleVariant": "",
              "stringId": "1778102505277448192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try it on.",
              "maleVariant": "",
              "stringId": "1778120545666818052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Finally lookin' half decent.",
              "maleVariant": "",
              "stringId": "1778139018790920192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks.",
              "maleVariant": "",
              "stringId": "1796725428324732932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks.",
              "maleVariant": "",
              "stringId": "1796725576601767936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This Johnny's?",
              "maleVariant": "",
              "stringId": "1796726193785212928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This Johnny's jacket?",
              "maleVariant": "Is this Johnny's jacket?",
              "stringId": "1796735138021052416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "With me wearin' it, sure you won't confuse us?",
              "maleVariant": "With me wearin' it, you sure you won't confuse us?",
              "stringId": "1796735572802605056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A replica. Made-to-order.",
              "maleVariant": "",
              "stringId": "1796736105311440896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Quite sure, thanks.",
              "maleVariant": "",
              "stringId": "1796740295907676160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Real jacket musta disintegrated into dust years ago.",
              "maleVariant": "",
              "stringId": "1796742607371833344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had something for her, too.",
              "maleVariant": "",
              "stringId": "1796774531981185024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ya know... it seems you're starting to show your age.",
              "maleVariant": "",
              "stringId": "1796774993958604800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just like me grandma â€“ that woman never threw a ting away.",
              "maleVariant": "",
              "stringId": "1796782328554786816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't push it, Crispin.",
              "maleVariant": "",
              "stringId": "1796782844504510464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never know when it might come in handy.",
              "maleVariant": "",
              "stringId": "1796787953904979968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah â€“ minor details that happened to slip your mind earlier?",
              "maleVariant": "",
              "stringId": "1796788447255793664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. 'Zackly.",
              "maleVariant": "",
              "stringId": "1796788739867217920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Make it quick.",
              "maleVariant": "",
              "stringId": "1796798327509291008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is nice. But me don't share me father's aspirations.",
              "maleVariant": "",
              "stringId": "1796826583730032640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So me expect to live longer than he did.",
              "maleVariant": "",
              "stringId": "1796828006303731712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Me sure would. But not so close.",
              "maleVariant": "",
              "stringId": "1796832611213316096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De bastard has a bad habit of comin' back from de dead.",
              "maleVariant": "",
              "stringId": "1796832937077182464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. True bastards somehow always find a way.",
              "maleVariant": "",
              "stringId": "1796833389256708096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe, dunno. Don't wait up.",
              "maleVariant": "",
              "stringId": "1796837560726917120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What â€“ you getting cold feet?",
              "maleVariant": "",
              "stringId": "1796853820382580736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Justâ€¦ don't wanna see Johnny haze you.",
              "maleVariant": "No. Justâ€¦ I don't wanna see Johnny haze you.",
              "stringId": "1796854137975279616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Adam Smasher's his obsession, not yours.",
              "maleVariant": "",
              "stringId": "1796854637131010048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, think so?",
              "maleVariant": "",
              "stringId": "1796855263122493440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, the fuck was that s'posed to be?",
              "maleVariant": "",
              "stringId": "1796855510754201600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just tryin' to play fair.",
              "maleVariant": "",
              "stringId": "1796855812358213632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Here's as far as we go.",
              "maleVariant": "",
              "stringId": "1889577025730920452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too many rats around. Better wait till a little darkness creeps in.",
              "maleVariant": "",
              "stringId": "1889577649910464512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe Smasher or Grayson'll show.",
              "maleVariant": "",
              "stringId": "1889587242183049220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whadda we do till then?",
              "maleVariant": "",
              "stringId": "1889588005579931648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And in the meantime, we do what?",
              "maleVariant": "",
              "stringId": "1889588182780887040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait and watch.",
              "maleVariant": "",
              "stringId": "1889590394387054592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe Smasher or Grayson'll rear a head.",
              "maleVariant": "",
              "stringId": "1889591150318075904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Least it's dark. Should be easier.",
              "maleVariant": "",
              "stringId": "1889596895122046976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure you're OK with this?",
              "maleVariant": "",
              "stringId": "1893837423736803332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weird you didn't run into Smasher sooner.",
              "maleVariant": "",
              "stringId": "1893882399761911808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Little weird you didn't run into Smasher before now.",
              "maleVariant": "It's a little weird you didn't run into Smasher before now.",
              "stringId": "1893896093828927488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Came up in conversation. Time to time.",
              "maleVariant": "",
              "stringId": "1893898741357498368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But, uh, never tried to track him down?",
              "maleVariant": "Yeah, but, y-you never tried to track him down?",
              "stringId": "1893899547871825920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, V. Fact that Johnny's back changes things, OK?",
              "maleVariant": "",
              "stringId": "1893981305510318080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, sure. Believe me, I'd know.",
              "maleVariant": "OK, sure. Believe me, I know.",
              "stringId": "1893982412538138624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll talk later.",
              "maleVariant": "",
              "stringId": "1893990995946393600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe. Don't hold your breath, though. Wouldn't be caught dead here in daylight.",
              "maleVariant": "",
              "stringId": "1894008378786336768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Good as it's gonna get.",
              "maleVariant": "",
              "stringId": "1894009558862155776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been here before.",
              "maleVariant": "",
              "stringId": "1911478520118132736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Grayson's been sitting on the docks since his last Maelstrom op.",
              "maleVariant": "",
              "stringId": "1912908436864782360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He works for them? Not for Smasher?",
              "maleVariant": "So he works for them, not for Smasher.",
              "stringId": "1912908436881559552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smasher always had a few irons in the fire.",
              "maleVariant": "",
              "stringId": "1912908436881559564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Assuming they're in it together â€“ master and apprentice.",
              "maleVariant": "",
              "stringId": "1912908436881559576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'd Johnny say about me?",
              "maleVariant": "",
              "stringId": "1912908437066108932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know you two talk all the time.",
              "maleVariant": "",
              "stringId": "1912908437082886144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny tends to talk about himself, mostly.",
              "maleVariant": "",
              "stringId": "1912908437116440576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Said you're the best.",
              "maleVariant": "",
              "stringId": "1912908437116440592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Any idea what Grayson's intentions are?",
              "maleVariant": "",
              "stringId": "1912908437116440608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny talks about himself, mostly.",
              "maleVariant": "Oh, Johnny talks about himself, mostly.",
              "stringId": "1912908437149995012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, right.",
              "maleVariant": "",
              "stringId": "1912908437166772232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Said you're the best. Always wereâ€¦",
              "maleVariant": "",
              "stringId": "1912908437200326660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, little too open-handed, but all right.",
              "maleVariant": "",
              "stringId": "1912908437217103872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The best. Too bad that comes at a price.",
              "maleVariant": "",
              "stringId": "1912908437217103884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take jacket]",
              "maleVariant": "",
              "stringId": "2230518559926906880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Any idea what Grayson's got planned? Weyland tell you anything?",
              "maleVariant": "",
              "stringId": "2238819533832404992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She a shy one? Don't look it.",
              "maleVariant": "He a shy one? Don't look it.",
              "stringId": "2250411038145593344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You ghostin' already, sistah?",
              "maleVariant": "You ghostin' already, bredda?",
              "stringId": "2250413745535270912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in]",
              "maleVariant": "",
              "stringId": "2511788698825912324"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}