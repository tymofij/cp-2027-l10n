  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't believe they bothered to truck it all the way out here.",
              "maleVariant": "",
              "stringId": "1759527621505015808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, better than the junkyard where I landed.",
              "maleVariant": "It's better than the junkyard where I landed.",
              "stringId": "1759530032390955008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Cast a cold eye, on life, on death,\"",
              "maleVariant": "",
              "stringId": "1760466663507714048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Horseman, pass by!\"",
              "maleVariant": "",
              "stringId": "1760469278975025152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go, NC beckons.",
              "maleVariant": "",
              "stringId": "1760500499612393472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'd you expect? Headstone, flag and flowers?",
              "maleVariant": "",
              "stringId": "1898260234274361344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah, I... I dunno. A marker?",
              "maleVariant": "",
              "stringId": "1898261060820684800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Something, anything.",
              "maleVariant": "",
              "stringId": "1898263257696456704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Inscribe Johnny's initials] Let's do something about that.",
              "maleVariant": "",
              "stringId": "1898263364533768196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really need a symbol?",
              "maleVariant": "",
              "stringId": "1898265297285828608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better now?",
              "maleVariant": "",
              "stringId": "1898268332619747328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really need symbolic, empty gestures?",
              "maleVariant": "",
              "stringId": "1898269423323336704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So that's how it is.",
              "maleVariant": "",
              "stringId": "1898269945816174592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] D'ya expect a headstone?",
              "maleVariant": "",
              "stringId": "1898270619706609668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothing here at all.",
              "maleVariant": "",
              "stringId": "1898271676268564480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guess I do. Thought I'd feel like I'd closed a chapter here.",
              "maleVariant": "",
              "stringId": "1898280375154397184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like I'd said goodbye to the old Silverhand, hello to the new.",
              "maleVariant": "",
              "stringId": "1898281763737456640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A bit. But let's say it was my real grave – what would you write?",
              "maleVariant": "",
              "stringId": "1898282555085180928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Here lies Johnny Silverhand--\"",
              "maleVariant": "",
              "stringId": "1898283620622946304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Relentless Rockerboy.\"",
              "maleVariant": "",
              "stringId": "1898284271880916992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Night City Legend.\"",
              "maleVariant": "",
              "stringId": "1898286555125796864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Terrorist and Raging Asshole.\"",
              "maleVariant": "",
              "stringId": "1898287619187167232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"The Guy who Saved My Life.\"",
              "maleVariant": "",
              "stringId": "1898287996724858880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And how do you feel?",
              "maleVariant": "",
              "stringId": "1898292126721789952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like I never even was. Or like I was still inside Mikoshi.",
              "maleVariant": "",
              "stringId": "1898292542914187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Relentless Rockerboy Who Never Surrendered.\" Not even in death.",
              "maleVariant": "",
              "stringId": "1898300040954007552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Night City Legend.\"",
              "maleVariant": "",
              "stringId": "1898300519473762304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Terrorist and Raging Asshole.\"",
              "maleVariant": "",
              "stringId": "1898300940799987712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"The Guy who Saved My Life.\"",
              "maleVariant": "",
              "stringId": "1898301193565523968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks. But what's that do for me now?",
              "maleVariant": "",
              "stringId": "1898301952382226432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds good. But what does it really mean?",
              "maleVariant": "",
              "stringId": "1898303755060531200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Harsh.",
              "maleVariant": "",
              "stringId": "1898305214007234560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But if I saw a headstone like that, I'd wanna meet the guy.",
              "maleVariant": "",
              "stringId": "1898305707811033088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V... You don't know how much I want that to be true.",
              "maleVariant": "",
              "stringId": "1898306947294973952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's gotten into you?",
              "maleVariant": "",
              "stringId": "1898308846073171968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What'd I achieve fighting Arasaka?",
              "maleVariant": "",
              "stringId": "1898308994769637376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't win this war alone.",
              "maleVariant": "",
              "stringId": "1898310204071366660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So many dead – and those suits're still on top.",
              "maleVariant": "",
              "stringId": "1898313659406110720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What did I achieve, shouting \"Chippin' In\" to a bunch of angry, pimply kids?",
              "maleVariant": "",
              "stringId": "1898314517829144576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gave 'em hope.",
              "maleVariant": "",
              "stringId": "1898318641601728516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now you're just whinin'.",
              "maleVariant": "",
              "stringId": "1898319079789056000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just feeling sorry for yourself.",
              "maleVariant": "",
              "stringId": "1898319646741516288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's gotten into you all of a sudden?",
              "maleVariant": "",
              "stringId": "1898320291204714496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's gotten into you?",
              "maleVariant": "",
              "stringId": "1898320603831357440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, I realize I fucked up a lotta things.",
              "maleVariant": "",
              "stringId": "1898321549529468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That wasn't a compliment.",
              "maleVariant": "",
              "stringId": "1898323561436434436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did you just come here to feel sorry for yourself?",
              "maleVariant": "",
              "stringId": "1898324016820408320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't win this war goin' solo.",
              "maleVariant": "",
              "stringId": "1898328542038216704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Might be a good thing we're in this together.",
              "maleVariant": "",
              "stringId": "1898329427220910080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's just it. Don't wanna drag you down into this swamp with me.",
              "maleVariant": "",
              "stringId": "1898331214782943232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just feeling sorry for yourself.",
              "maleVariant": "",
              "stringId": "1898332559057375232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ya gave those kids hope. That the suits couldn't buy everything.",
              "maleVariant": "",
              "stringId": "1898336405183811584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I remember what I thought on stage. That even if they all died for their ideals, it'd be worth it.",
              "maleVariant": "",
              "stringId": "1898337288269352960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Just finally thinking clearly.",
              "maleVariant": "",
              "stringId": "1898341994412658688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where's Johnny I'm-the-fuckin'-greatest Silverhand? Don't even recognize you.",
              "maleVariant": "Where's Johnny I'm-the-fucking-greatest Silverhand? Don't even recognize you.",
              "stringId": "1898346962246979584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe I'm done pretending.",
              "maleVariant": "",
              "stringId": "1898347473599746048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But when I look at you...",
              "maleVariant": "",
              "stringId": "1898348702094610432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A hard road, but looks like we made it.",
              "maleVariant": "",
              "stringId": "1898364526532513796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah, fucked that up too.",
              "maleVariant": "",
              "stringId": "1898366872775839744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny, no need to bare your soul to me.",
              "maleVariant": "",
              "stringId": "1898367531214458880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been a long, bumpy road, but we made it.",
              "maleVariant": "It's been a long, bumpy road, but we made it.",
              "stringId": "1898369052052312064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, Johnny. You fucked that up, too.",
              "maleVariant": "",
              "stringId": "1898373196813193216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You used me, lied to me... Can't trust you at all.",
              "maleVariant": "You used me, lied to me... I can't trust you at all.",
              "stringId": "1898373894057512960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahem, is it too late to ask for a second chance?",
              "maleVariant": "",
              "stringId": "1898375509485309952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No need to confess all your wrongs to me, Johnny.",
              "maleVariant": "",
              "stringId": "1898375845298065408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. But maybe I want to.",
              "maleVariant": "",
              "stringId": "1898376421612212224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too late. This was your second chance.",
              "maleVariant": "",
              "stringId": "1898377951392321540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What do you want from me?",
              "maleVariant": "",
              "stringId": "1898378367081402368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too late. This was your second chance.",
              "maleVariant": "",
              "stringId": "1898381341396586496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You died, came back and still had no idea what to do with it all.",
              "maleVariant": "",
              "stringId": "1898381789885124608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think you're right.",
              "maleVariant": "",
              "stringId": "1898382875589103616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What d'you want from me?",
              "maleVariant": "",
              "stringId": "1898386989295357952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Most people I thought were my friends, they couldn't even stand to be in the same room with me.",
              "maleVariant": "",
              "stringId": "1898389573859037184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're fuckin' closest to me by a long shot. There twenty-four seven.",
              "maleVariant": "",
              "stringId": "1898392341663469568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And yet... you don't seem to hate my living guts.",
              "maleVariant": "",
              "stringId": "1898393022818439168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "At least, so it seemed… till now.",
              "maleVariant": "",
              "stringId": "1898398247579045888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You were a real dick in the beginning.",
              "maleVariant": "",
              "stringId": "1898398402600521732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. But as second chances go, this is your last.",
              "maleVariant": "",
              "stringId": "1898402574339166208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's not hate… Just had enough of you.",
              "maleVariant": "",
              "stringId": "1898404193206292480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. But as second chances go, this is your last. I refuse to be that naive.",
              "maleVariant": "",
              "stringId": "1898405185863831552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll try damned hard.",
              "maleVariant": "",
              "stringId": "1898409465312202752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't hate you… just had enough. Look at you – still trying to manipulate me.",
              "maleVariant": "I don't hate you, I just... had enough. Look at you – still trying to manipulate me.",
              "stringId": "1898415130189516800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit. I really fucked up.",
              "maleVariant": "",
              "stringId": "1898416097446350848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can we go now?",
              "maleVariant": "",
              "stringId": "1898416994356318208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, won't keep you. Wanna go see Hanako, set things up?",
              "maleVariant": "",
              "stringId": "1898417000261898240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah. Eventually.",
              "maleVariant": "",
              "stringId": "1898417006050037760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh. Johnny Silverhand. Terrorist and Raging Asshole.",
              "maleVariant": "",
              "stringId": "1898431241433927680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V. First among suckers.",
              "maleVariant": "",
              "stringId": "1898432346800803840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shame we didn't talk like this earlier.",
              "maleVariant": "",
              "stringId": "1898436243057561600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You were a real dickwipe at first.",
              "maleVariant": "",
              "stringId": "1898436984006529024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hah. Johnny Silverhand. Night City Legend.",
              "maleVariant": "",
              "stringId": "1898438395993804800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny Silverhand. Relentless Rockerboy Who Never Gives Up.",
              "maleVariant": "",
              "stringId": "1898439740805107712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too bad we didn't talk like this earlier. Woulda made things easier.",
              "maleVariant": "It's too bad we didn't talk like this earlier. Woulda made things easier.",
              "stringId": "1898441894261444608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never found the right moment.",
              "maleVariant": "",
              "stringId": "1898442783403560960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't mean that as a compliment.",
              "maleVariant": "Didn't mean it as a compliment.",
              "stringId": "1898444007100141568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know.",
              "maleVariant": "",
              "stringId": "1898444518033477632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remember wakin' up at Viktor's, not knowin' about me? \"I'm seeing things, I'm scared.\"",
              "maleVariant": "",
              "stringId": "1898447232066932736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still feel like you can't breathe?",
              "maleVariant": "",
              "stringId": "1898448071129059332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remember you whinin' for smokes in the middle of the night, complainin' about not bein' able to kill me.",
              "maleVariant": "Remember you whinin' for smokes in the middle of the night? Complainin' about not bein' able to kill me.",
              "stringId": "1898470680524771328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then moanin' at Misty's 'bout how you didn't wanna die?",
              "maleVariant": "",
              "stringId": "1898471180066377728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sorta deserved it.",
              "maleVariant": "",
              "stringId": "1898474707224383488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Never thought we'd make it this far.",
              "maleVariant": "",
              "stringId": "1898477952374165504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When you said you let down your friends...",
              "maleVariant": "",
              "stringId": "1898478867873288192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When you said you let your friends down – did ya mean Rogue?",
              "maleVariant": "",
              "stringId": "1898485285410066432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still feel like you can't breathe in here?",
              "maleVariant": "",
              "stringId": "1898485659474874368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah. Not sayin' it's great, but it's different.",
              "maleVariant": "",
              "stringId": "1898486863709884416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sometimes when I wake up, feels like I'm back for a while.",
              "maleVariant": "",
              "stringId": "1898488332471259136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, like I got this body to myself. Like I'm free.",
              "maleVariant": "",
              "stringId": "1898488839394840576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seconds later, feels like I'm missing something – something really important.",
              "maleVariant": "",
              "stringId": "1898489196682432512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What d'you mean – back?",
              "maleVariant": "",
              "stringId": "1898489311287595008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then I realize you're there, always were, and this stupid wave of relief washes over me.",
              "maleVariant": "",
              "stringId": "1898490715507019776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When you said you let your friends down...",
              "maleVariant": "",
              "stringId": "1898491283935875076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have the same dreams...",
              "maleVariant": "",
              "stringId": "1898491863857123328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have similar dreams sometimes. That you never died, that I'm you.",
              "maleVariant": "I have similar dreams sometimes. That you never died, that I'm you.",
              "stringId": "1898492190492741632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not like I think, \"What would Johnny do?\" It's more I do something and I feel... fuckin' great.",
              "maleVariant": "",
              "stringId": "1898497054241214464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like I've finally started to fit into this world.",
              "maleVariant": "",
              "stringId": "1898498758386929664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, prolly not a great sign. We better do something. Fast.",
              "maleVariant": "",
              "stringId": "1898499279940243456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rogue, Alt, Kerry, Santiago...",
              "maleVariant": "",
              "stringId": "1898501047738716160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not all's lost yet, least with Rogue.",
              "maleVariant": "",
              "stringId": "1898502255312392192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't pretend nothing's changed over fifty years, can't just insert myself into her life.",
              "maleVariant": "",
              "stringId": "1898503292228227072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, already have.",
              "maleVariant": "",
              "stringId": "1898517033741209604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smasher biz really got to her.",
              "maleVariant": "",
              "stringId": "1898519874576863232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smasher biz really got to her. Can't leave it like this, I don't think.",
              "maleVariant": "Smasher biz really got to her. Ya can't leave it like this, I don't think.",
              "stringId": "1898521710977372160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, already have.",
              "maleVariant": "C'mon, ya already have.",
              "stringId": "1898522041136205824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "True, that.",
              "maleVariant": "",
              "stringId": "1898523347980668928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Might be right. She was acting weird.",
              "maleVariant": "",
              "stringId": "1898524363220340736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y'know, did promise her I'd take her to the movies. Long, long ago.",
              "maleVariant": "",
              "stringId": "1898525471657447424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You oughta talk to her.",
              "maleVariant": "",
              "stringId": "1898525965243142144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good idea.",
              "maleVariant": "",
              "stringId": "1898527651303018496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Call her for me, ask if she's free some night?",
              "maleVariant": "",
              "stringId": "1898528074390851584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thing is, you'd have to surrender control – again.",
              "maleVariant": "",
              "stringId": "1898529252503736320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I'll call Rogue.",
              "maleVariant": "",
              "stringId": "1898530024289226756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Forgot about that…",
              "maleVariant": "",
              "stringId": "1898530734535888896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, I'll call her.",
              "maleVariant": "",
              "stringId": "1898531220538281984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Forgot about that.",
              "maleVariant": "",
              "stringId": "1898532885240762368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just an idea. Won't press it.",
              "maleVariant": "",
              "stringId": "1898533919438688256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1898534681862492160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's delta. Nothin' to see here after all.",
              "maleVariant": "",
              "stringId": "1898535567548502016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1898536066721009664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Worth comin' out all the same?",
              "maleVariant": "",
              "stringId": "1898536670734340096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Absolutely. Thanks, V.",
              "maleVariant": "",
              "stringId": "1898538534716592128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of all the heads I coulda popped up in, hella glad it was yours.",
              "maleVariant": "",
              "stringId": "1898546176654594048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Funny. Place is familiar. Made a pit stop here once.",
              "maleVariant": "Funny. Place is familiar. I made a pit stop here once.",
              "stringId": "1902800024470573060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why here?",
              "maleVariant": "",
              "stringId": "1902800026584502272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to be a nomad fridge set up here for people on the road. Take a beer, leave a beer from your region in return.",
              "maleVariant": "There used to be a nomad fridge set up here for people on the road. Ya take a beer, leave a beer from your region in return.",
              "stringId": "1902800028849426432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm.",
              "maleVariant": "",
              "stringId": "1902800031114350592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look. Still here.",
              "maleVariant": "",
              "stringId": "1902800035560312836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A fridge.",
              "maleVariant": "",
              "stringId": "1902801855720476672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll figure something out.",
              "maleVariant": "",
              "stringId": "1902809088260853764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Either let down or used every last person who gave me their trust. Blind, selfish bastard that I was.",
              "maleVariant": "",
              "stringId": "1902816086457077760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But I've managed one thing for now. Not to fuck this up, what we have.",
              "maleVariant": "",
              "stringId": "1902816090248728576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know you're not fond of me, yeah.",
              "maleVariant": "",
              "stringId": "1902818835521359872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To put it mildly.",
              "maleVariant": "",
              "stringId": "1902837180752744452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ooh, another serious talk.",
              "maleVariant": "",
              "stringId": "1902837783658778624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An' that's mildly put.",
              "maleVariant": "",
              "stringId": "1902838919576965120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But, ooh, another big talk. Not a fan of those, either.",
              "maleVariant": "",
              "stringId": "1902839731577442304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can tell you're tryna be disarmingly honest. Keep goin', tell me what this is really about.",
              "maleVariant": "",
              "stringId": "1902841048890888192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want you to give me a second chance.",
              "maleVariant": "",
              "stringId": "1902841052095336448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let down my friends, showed you the worst of me.",
              "maleVariant": "",
              "stringId": "1902842799710818304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I noticed. Been watchin' you for some time now.",
              "maleVariant": "",
              "stringId": "1902845774613204992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Just one last request.",
              "maleVariant": "",
              "stringId": "1905315289959780352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Request? Think this is a good time?",
              "maleVariant": "",
              "stringId": "1905316683559227396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm listening.",
              "maleVariant": "",
              "stringId": "1905325723123208192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Request? Seriously? This seem like a good time to you?",
              "maleVariant": "Request? Seriously? Does this seem like a good time to you?",
              "stringId": "1905326211038203904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Just go to North Oak – whenever you think it's a \"good time.\"",
              "maleVariant": "",
              "stringId": "1905329310897295360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm listening.",
              "maleVariant": "I'm listenin'.",
              "stringId": "1905341523787796480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take me to North Oak.",
              "maleVariant": "",
              "stringId": "1905351081918746624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What for?",
              "maleVariant": "",
              "stringId": "1905354467846148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kerry. Tell you more once you get me there.",
              "maleVariant": "",
              "stringId": "1908068490328952832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1908068969989558272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been here.",
              "maleVariant": "",
              "stringId": "1911628114735022080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2008457079267520516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2008512730668724224"
            }
          ]
        }
      }
