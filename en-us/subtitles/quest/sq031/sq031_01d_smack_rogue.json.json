      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... Khe khe...",
              "maleVariant": "",
              "stringId": "1788805295008079872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, fuck.",
              "maleVariant": "",
              "stringId": "1788810308879081472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, feelin' better?",
              "maleVariant": "",
              "stringId": "1788815630494556164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The hell is this place?",
              "maleVariant": "",
              "stringId": "1788815743120007172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh. No.",
              "maleVariant": "Hm. No.",
              "stringId": "1788825514556481536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Place where you can puke all over the carpet, no problem.",
              "maleVariant": "",
              "stringId": "1788826667067334656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beggar, not chooser.",
              "maleVariant": "",
              "stringId": "1788826963285860352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure. First time you walked up, sensed there was something familiar about you.",
              "maleVariant": "",
              "stringId": "1795455182183927808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Blah, blah, Johnny told you.",
              "maleVariant": "",
              "stringId": "1795455763229249536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did we...? You know…",
              "maleVariant": "",
              "stringId": "1795457417378861056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You don't seem surprised.",
              "maleVariant": "",
              "stringId": "1795458088803045376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feel like shit.",
              "maleVariant": "",
              "stringId": "1795458484879560704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't seem surprised.",
              "maleVariant": "You don't seem surprised.",
              "stringId": "1795459506528129024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, feel like shit.",
              "maleVariant": "",
              "stringId": "1795461387472146432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was surprised, but not anymore. You prolly don't remember.",
              "maleVariant": "",
              "stringId": "1795464400458813440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No surprise there. Don't remember a thing, huh?",
              "maleVariant": "",
              "stringId": "1795465457977069568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's just say I see some huge fuckin' black holes.",
              "maleVariant": "",
              "stringId": "1795466334385278976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got flashes here an' there. Not everything, thank God.",
              "maleVariant": "I got flashes here an' there. Not everything, thank God.",
              "stringId": "1795467281576562688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, did we...? You know...",
              "maleVariant": "Did we, uh...? You know...",
              "stringId": "1795467840459182080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. Gonk was too drunk.",
              "maleVariant": "",
              "stringId": "1795470306458275840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He tell you about Smasher?",
              "maleVariant": "",
              "stringId": "1795486087946506244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feels weird, this.",
              "maleVariant": "",
              "stringId": "1795493846855258112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smasher – he tell you about 'im?",
              "maleVariant": "",
              "stringId": "1795495847857033216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feels weird, this… you knowin' about Johnny.",
              "maleVariant": "",
              "stringId": "1795496077067358208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. Didn't know he'd come back to NC.",
              "maleVariant": "",
              "stringId": "1795505963092217856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny found some way to get at him.",
              "maleVariant": "",
              "stringId": "1795506592841797632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait... Grayson?",
              "maleVariant": "",
              "stringId": "1795507013916364800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jeremiah Grayson. Works for Smasher. Johnny got a tip off some stripper, don't ask me how.",
              "maleVariant": "",
              "stringId": "1795507311040860160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need some help?",
              "maleVariant": "",
              "stringId": "1795509543467552772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know what Smasher's been doing all this time?",
              "maleVariant": "",
              "stringId": "1795510389609664512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know what Smasher's been up to all this time?",
              "maleVariant": "",
              "stringId": "1795514649160257536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can I help at all?",
              "maleVariant": "",
              "stringId": "1795514958917996544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In your state? Heh, you're useless. Almost.",
              "maleVariant": "",
              "stringId": "1795515424385077248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Holobuzz you when I learn something.",
              "maleVariant": "",
              "stringId": "1795515622322671616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1795515949411274752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Unbelievable that bastard's somewhere in your head.",
              "maleVariant": "",
              "stringId": "1795519915259924480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Face is changing, y'know?",
              "maleVariant": "",
              "stringId": "1795521939246485504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still look like V, but that mean smirk – I'd know it anywhere.",
              "maleVariant": "",
              "stringId": "1795522367937908736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How he talks, moves... how he smokes.",
              "maleVariant": "",
              "stringId": "1795523748484681728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don't smoke.",
              "maleVariant": "",
              "stringId": "1795524227390312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. Could never confuse you two.",
              "maleVariant": "",
              "stringId": "1795528585943257088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So sleep soundly.",
              "maleVariant": "",
              "stringId": "1795529035035774976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Ebunike.\" Just that. Still, lemme see what I can do.",
              "maleVariant": "",
              "stringId": "1795530615701180416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're gonna get that son of a bitch.",
              "maleVariant": "",
              "stringId": "1795531101602910208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Disappeared after the 'Saka Tower assault in 2023. All thought he was gone for good.",
              "maleVariant": "",
              "stringId": "1795536996260974592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then suddenly reappeared. Did a few mondo jobs for Arasaka, then went right back to ground.",
              "maleVariant": "",
              "stringId": "1795537538785808384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody's seen him in years.",
              "maleVariant": "",
              "stringId": "1795537945012539392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Cept Grayson, apparently.",
              "maleVariant": "",
              "stringId": "1795539829597212672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, never again.",
              "maleVariant": "",
              "stringId": "1796459783775391748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck. Never again.",
              "maleVariant": "",
              "stringId": "1796460117222559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You tricked me. Were just supposed to talk to Rogue.",
              "maleVariant": "",
              "stringId": "1796461589658144768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You oughta be the one puking here.",
              "maleVariant": "",
              "stringId": "1796462272591499264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You oughta be the one pukin' right now.",
              "maleVariant": "",
              "stringId": "1796467281647439872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You tricked me. Just supposed to talk to Rogue.",
              "maleVariant": "",
              "stringId": "1796467601823830016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had no idea the pills would lay you out like this.",
              "maleVariant": "",
              "stringId": "1796484651803807744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, well, life ain't fair.",
              "maleVariant": "",
              "stringId": "1796487731849007104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure picked the right moment to bolt outta my body.",
              "maleVariant": "Yeah, sure picked the right moment to bolt outta my body.",
              "stringId": "1796488273383985152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe wouldn't be bad if you hadn't chased 'em down with fuck knows what.",
              "maleVariant": "",
              "stringId": "1796501798068228096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did talk to 'er.",
              "maleVariant": "",
              "stringId": "1796515255073181696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And the stripper? Empathy brawl? Tattoo?",
              "maleVariant": "",
              "stringId": "1796534891764322304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All required, man. Had to give Rogue something real, didn't I?",
              "maleVariant": "",
              "stringId": "1796536229881516032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oughta be happy I left at all.",
              "maleVariant": "",
              "stringId": "1796551220676333568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Actually, for a moment there I thought...",
              "maleVariant": "",
              "stringId": "1796557204740182020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am happy. Positively glowing.",
              "maleVariant": "",
              "stringId": "1796558047979835392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Ebunike.\" That's real?",
              "maleVariant": "",
              "stringId": "1796564645989126148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coulda done it myself.",
              "maleVariant": "",
              "stringId": "1796565479061786624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For a moment there, did seem like you were thinkin' about stayin'.",
              "maleVariant": "For a moment there, you did seem like you were thinkin' about stayin'.",
              "stringId": "1796581167855898624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am. Look at me, spewin' joy and rainbows.",
              "maleVariant": "Oh, I am. Look at me, spewin' joy and rainbows.",
              "stringId": "1796581885618753536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Like a fuckin' ray of sunshine.",
              "maleVariant": "",
              "stringId": "1796582412658216960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well? We movin'?",
              "maleVariant": "",
              "stringId": "1796582808432742400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I mean, such a good time.",
              "maleVariant": "I mean, you had such a good time.",
              "stringId": "1796594351409008640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't trust me after all, huh?",
              "maleVariant": "",
              "stringId": "1796597968039907328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"Ebunike\"? A lead on Grayson's supposed to be a lead on Smasher?",
              "maleVariant": "",
              "stringId": "1796599575867625472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lead to lead – that's real?",
              "maleVariant": "",
              "stringId": "1796600713882963968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Real enough for Rogue.",
              "maleVariant": "",
              "stringId": "1796602896296775680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Quit mumble-whinin' and let's delta.",
              "maleVariant": "",
              "stringId": "1796603428637839360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Coulda done all that myself.",
              "maleVariant": "",
              "stringId": "1796605238245134336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know you thought about it. Saw it.",
              "maleVariant": "",
              "stringId": "1796606738161799168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I did exactly what we agreed. Let's get outta here.",
              "maleVariant": "",
              "stringId": "1796607241344061440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh yeah? Woulda talked to Rogue by yourself, too?",
              "maleVariant": "",
              "stringId": "1796608567029338112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or woulda needed me to whisper in your ear?",
              "maleVariant": "",
              "stringId": "1796609163341926400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck. I think I'm gonna blow.",
              "maleVariant": "Ah, fuck. Think I'm gonna blow.",
              "stringId": "1796609553999400960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's get outta here. Get you some air.",
              "maleVariant": "",
              "stringId": "1796611110606278656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He called me early morning. 'Course, I thought it was you.",
              "maleVariant": "",
              "stringId": "1892529470903701504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I thought, \"Cute kid. Too bad she's gone completely whacked.\"",
              "maleVariant": "And I thought, \"Cute kid. Too bad he's gone completely whacked.\"",
              "stringId": "1892529471558012928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Came to see for yourself, huh? Morbid curiosity?",
              "maleVariant": "",
              "stringId": "1892529472178769920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. And I found Johnny Silverhand.",
              "maleVariant": "",
              "stringId": "1892529472833081344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1914074064732573696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm... Ugh.",
              "maleVariant": "",
              "stringId": "1914074513304997888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spit blood first thing every morning?",
              "maleVariant": "",
              "stringId": "1914075541932888064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Talk about something else, please?",
              "maleVariant": "",
              "stringId": "1914075834242322432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1963314385100992516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2128480590018375684"
            }
          ]
        }
      }
    },
