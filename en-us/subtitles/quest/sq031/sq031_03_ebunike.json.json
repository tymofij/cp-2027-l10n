        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maelstrom's got the docks covered, and you'll cover me.",
              "maleVariant": "",
              "stringId": "1750509373849559048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon. Let's go.",
              "maleVariant": "",
              "stringId": "1750509373849559056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weyland saw Grayson somewhere around the docks. He was with somebody.",
              "maleVariant": "",
              "stringId": "1751957451383201792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Smasher.",
              "maleVariant": "",
              "stringId": "1751958386327117824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So I hope. Gotta get to the Maelstrom dataterm first.",
              "maleVariant": "",
              "stringId": "1751973563986567168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So what's our plan?",
              "maleVariant": "",
              "stringId": "1752060664715370496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ready?",
              "maleVariant": "",
              "stringId": "1752952913720467456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There she is. The \"Ebunike.\"",
              "maleVariant": "",
              "stringId": "1753741636431208448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not lookin' so hot.",
              "maleVariant": "",
              "stringId": "1753742316982198272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kinda hard to believe he's got his hidey-hole here.",
              "maleVariant": "",
              "stringId": "1753745583606816768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did ya expect Smasher to put up a sign or something?",
              "maleVariant": "",
              "stringId": "1753745583606816776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta search the deck.",
              "maleVariant": "",
              "stringId": "1753761192021286912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1753761565549223936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That one's yours.",
              "maleVariant": "",
              "stringId": "1758078778762551296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Watch it.",
              "maleVariant": "",
              "stringId": "1758079373548412928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, look alive.",
              "maleVariant": "",
              "stringId": "1758079394788368384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's finish this.",
              "maleVariant": "",
              "stringId": "1758079415910883328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fucker's gotta be around here somewhere.",
              "maleVariant": "",
              "stringId": "1779365959497900032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We're goin' to Maelstrom's main storehouse.",
              "maleVariant": "",
              "stringId": "1779371727420874752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dataterm's there. Gotta check, see if they have anything on Smasher.",
              "maleVariant": "",
              "stringId": "1779372922478747648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was she always this curt with you, too?",
              "maleVariant": "",
              "stringId": "1779388340320821252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Main stash house. Dataterm's there.",
              "maleVariant": "",
              "stringId": "1790608732934856704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't know you worked for Maelstrom.",
              "maleVariant": "",
              "stringId": "1790619154236792832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know where their dataterm is â€“ how, exactly?",
              "maleVariant": "You know where their dataterm is? How, exactly?",
              "stringId": "1790619330145902596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. We can start.",
              "maleVariant": "",
              "stringId": "1790619351721402372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Work for anybody. Long as it's on my terms.",
              "maleVariant": "",
              "stringId": "1790621711050956800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See I missed a lot these last fifty years.",
              "maleVariant": "",
              "stringId": "1790621919373647872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Set up the buy myself.",
              "maleVariant": "",
              "stringId": "1790622720385048576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme lead. I know the place.",
              "maleVariant": "",
              "stringId": "1790628138519846912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She's tense.",
              "maleVariant": "",
              "stringId": "1790629297389264896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come on.",
              "maleVariant": "",
              "stringId": "1790692867535757312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See 'em all?",
              "maleVariant": "",
              "stringId": "1790730040242823168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like they're hidin' out on the ship.",
              "maleVariant": "",
              "stringId": "1790787977690701828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Let's see what we turn up.",
              "maleVariant": "",
              "stringId": "1790789883163009024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. We got it.",
              "maleVariant": "",
              "stringId": "1791157074513321984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Scan it, see if there's anything we can use.",
              "maleVariant": "",
              "stringId": "1791157840460341248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nah, knows exactly where to find 'im.",
              "maleVariant": "",
              "stringId": "1791176008641765376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can you open it?",
              "maleVariant": "",
              "stringId": "1791421008340279296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, execution, looks like to me.",
              "maleVariant": "",
              "stringId": "1792091878675144704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shot point-blank. Hollow-point ammo.",
              "maleVariant": "",
              "stringId": "1792092740956938240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's got a signature style, Smasher.",
              "maleVariant": "",
              "stringId": "1792099513382166528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Recordings, seem like to me.",
              "maleVariant": "",
              "stringId": "1792141059758448644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think you can play 'em back?",
              "maleVariant": "",
              "stringId": "1792141059775225860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, can give it a shot.",
              "maleVariant": "Yeah, can give it a shot.",
              "stringId": "1792141059775225868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There a fuckin' point to this?",
              "maleVariant": "",
              "stringId": "1792141060026884100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea who's backing you, but if anything happens to Malloryâ€¦",
              "maleVariant": "",
              "stringId": "1792266351822987264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fail to back out of the Night Corp deal by then, the girl dies.",
              "maleVariant": "",
              "stringId": "1792270501449261056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, don't you understand? You can't blackmail the entire Militech board by kidnapping one--",
              "maleVariant": "",
              "stringId": "1792270850650234880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Twenty-four hours.",
              "maleVariant": "",
              "stringId": "1792271201864474624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's corpo life for you.",
              "maleVariant": "",
              "stringId": "1792272104310923264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How much's left?",
              "maleVariant": "",
              "stringId": "1792275223900020740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An hour. It's not going to work. Like I said.",
              "maleVariant": "",
              "stringId": "1792275223900020748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So why all this? The girl...",
              "maleVariant": "",
              "stringId": "1792275223900020756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She dies in an hour. As ordered.",
              "maleVariant": "",
              "stringId": "1792275223900020764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why'd he even record that?",
              "maleVariant": "",
              "stringId": "1792275223950352388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Body we found â€“ could be this Mallory.",
              "maleVariant": "",
              "stringId": "1792279675616845824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We got no idea when that happened.",
              "maleVariant": "We got no idea when it happened.",
              "stringId": "1792280478624735232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now I know who jumped Kang Tao last month.",
              "maleVariant": "",
              "stringId": "1792501006958551044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thoroughly pro work.",
              "maleVariant": "",
              "stringId": "1792501006975328260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure it was this exact merch?",
              "maleVariant": "Ya sure it was this exact merch?",
              "stringId": "1792501006975328268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yep, real fuckin' preem.",
              "maleVariant": "",
              "stringId": "1792501007143100420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Even shittier on the inside.",
              "maleVariant": "",
              "stringId": "1796915671132868608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's go.",
              "maleVariant": "",
              "stringId": "1796915842361135104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't wander off.",
              "maleVariant": "",
              "stringId": "1796916600724852736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cover her, just in case.",
              "maleVariant": "",
              "stringId": "1796922193829236736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta get to the dataterm. Quietly.",
              "maleVariant": "",
              "stringId": "1796924355405746176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Might be our lucky day â€“ drop Grayson and Smasher in one go.",
              "maleVariant": "",
              "stringId": "1796925320699645952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ATHENA D-III. External database.",
              "maleVariant": "",
              "stringId": "1796941254759878656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll watch the door.",
              "maleVariant": "",
              "stringId": "1812872038536515584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She's stallin'. Told ya â€“ Rogue's still scared of the asshole.",
              "maleVariant": "",
              "stringId": "1814213857690062848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just find the motherfucker and do it.",
              "maleVariant": "",
              "stringId": "1814318375987646464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have twenty-four hours.",
              "maleVariant": "",
              "stringId": "1816931139113521152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know. I'm blackmailing you.",
              "maleVariant": "",
              "stringId": "1816945094452121600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Upstairs â€“ summits, handshakes, back-slapping, dick-waving.",
              "maleVariant": "",
              "stringId": "1817010448704540672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Downstairs â€“ Smasher's holdin' a gun to somebody's head.",
              "maleVariant": "",
              "stringId": "1817010751751393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Grayson might've. To have dirt on Smasher.",
              "maleVariant": "",
              "stringId": "1817012601808556032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think you're givin' Grayson too much credit.",
              "maleVariant": "",
              "stringId": "1817013630990094336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "True. Another day in the life for Smasher.",
              "maleVariant": "",
              "stringId": "1817014774541914112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "679275-PN1H. Sent three crews out myself to find it for the Chinese.",
              "maleVariant": "",
              "stringId": "1817037794107002880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whole city was after it. Gotta admit the bastard's good.",
              "maleVariant": "",
              "stringId": "1817038599581143040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now we find and kill the bad guy, OK?",
              "maleVariant": "",
              "stringId": "1817042427185532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For a while.",
              "maleVariant": "",
              "stringId": "1817046671401750528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tri-Phet.",
              "maleVariant": "",
              "stringId": "1869862073709510656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, even got a label! \"Supplements.\"",
              "maleVariant": "",
              "stringId": "1869862334108680196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just sittin' here before they cut it, repack it, unleash it on the street.",
              "maleVariant": "",
              "stringId": "1894108944321499136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why're you even doin' this?",
              "maleVariant": "",
              "stringId": "1897947614879174656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm settling a score.",
              "maleVariant": "",
              "stringId": "1897953148457996288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Avengin' Johnny?",
              "maleVariant": "",
              "stringId": "1897954773683687424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That too.",
              "maleVariant": "",
              "stringId": "1897959263115694080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rogue's been terrified of him since the Tower.",
              "maleVariant": "",
              "stringId": "1897959622701764608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "High time she faced her fear.",
              "maleVariant": "",
              "stringId": "1897970598608130048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D'ya see that?",
              "maleVariant": "",
              "stringId": "1898025977564123136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got an idea.",
              "maleVariant": "",
              "stringId": "1898026287539965952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Over there, to the right.",
              "maleVariant": "",
              "stringId": "1898035855301038080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somethin's burnin'.",
              "maleVariant": "",
              "stringId": "1898042369843679232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drop him â€“ quietly.",
              "maleVariant": "",
              "stringId": "1898042622592438272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shhh, tryna wake 'em up?",
              "maleVariant": "",
              "stringId": "1898042885474635776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whatever Rogue can do...",
              "maleVariant": "",
              "stringId": "1898044829769420800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Always loved watchin' her in action.",
              "maleVariant": "",
              "stringId": "1898046673736454144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait a sec, then follow me.",
              "maleVariant": "",
              "stringId": "1898050120464486400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Watch it, now.",
              "maleVariant": "",
              "stringId": "1898055222264877056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Won't touch that.",
              "maleVariant": "",
              "stringId": "1908683275202482176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like they tossed a bunch of stuff outta here.",
              "maleVariant": "",
              "stringId": "1908691953418784768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Musta upped the huscle after my last visit.",
              "maleVariant": "",
              "stringId": "1911495907236265984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We'll manage.",
              "maleVariant": "",
              "stringId": "1911497156987863040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's the plan?",
              "maleVariant": "",
              "stringId": "1916908206649241600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Had no idea you work with Maelstrom.",
              "maleVariant": "",
              "stringId": "1916908206649241616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know where their dataterm is â€“ how?",
              "maleVariant": "",
              "stringId": "1916908206649241636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. We can start.",
              "maleVariant": "",
              "stringId": "1916908206649241668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Guns a-blazing, then.",
              "maleVariant": "",
              "stringId": "1960994083227713536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ooph. Execution, probably.",
              "maleVariant": "",
              "stringId": "1961039143893876740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Interesting...",
              "maleVariant": "",
              "stringId": "1961045412214095872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, well, well...",
              "maleVariant": "",
              "stringId": "1961046526925881344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Should be some info on Smasher there.",
              "maleVariant": "",
              "stringId": "1979442188788715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out]",
              "maleVariant": "",
              "stringId": "2121470272725577732"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}