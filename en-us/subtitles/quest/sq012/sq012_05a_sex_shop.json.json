    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He's your CI. Doesn't know me.",
              "maleVariant": "He's your CI. He doesn't know me.",
              "stringId": "1699481196188782592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK.",
              "maleVariant": "",
              "stringId": "1699481573927800836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, uh... last we met, it didn't go so well. For him.",
              "maleVariant": "",
              "stringId": "1699557581964038144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People he ratted out got wind of it.",
              "maleVariant": "",
              "stringId": "1699562282973515776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I think you got a better chance of gettin' him to talk without me. At least, voluntarily.",
              "maleVariant": "",
              "stringId": "1699562983975931904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You lookin' for, uh, general inspiration, or somethin' specific?",
              "maleVariant": "",
              "stringId": "1699654078806216704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Igor says hi.",
              "maleVariant": "",
              "stringId": "1699654281206550528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Lean down] Igor sent me.",
              "maleVariant": "",
              "stringId": "1699657270403125252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, no! Said I was done, I was out!",
              "maleVariant": "",
              "stringId": "1699658060911988736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Leave me the fuck alone – all of you!",
              "maleVariant": "",
              "stringId": "1699658535824642048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuuuuuck…",
              "maleVariant": "",
              "stringId": "1699667659912237056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heeey, Neil.",
              "maleVariant": "",
              "stringId": "1699669092199944192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Piss off.",
              "maleVariant": "",
              "stringId": "1699669259787554816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Relax. Just got one question to ask you.",
              "maleVariant": "",
              "stringId": "1699669447222611968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fingers? I'll break limbs that'll make you forget all about those fingers.",
              "maleVariant": "",
              "stringId": "1699687461405970432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Start with your shins. Any idea how much that hurts?",
              "maleVariant": "",
              "stringId": "1699687908367781888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll make sure nothing happens to you.",
              "maleVariant": "",
              "stringId": "1699691151571406852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasn't here then. Now I am. I'll make sure nothing happens to you.",
              "maleVariant": "Wasn't here then. Now I am. I'll make sure nothin' happens to ya.",
              "stringId": "1751451388271513600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nobody'll find out you helped us.",
              "maleVariant": "",
              "stringId": "1751452517294264324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We can. And we will.",
              "maleVariant": "",
              "stringId": "1751453082468339712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We can, actually. And we will.",
              "maleVariant": "",
              "stringId": "1751453971979554816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just tell us where to find Red Queen's Race.",
              "maleVariant": "",
              "stringId": "1751454738731880448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, well. That's what I call a breakthrough.",
              "maleVariant": "",
              "stringId": "1811424540472496128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Comin' with?",
              "maleVariant": "",
              "stringId": "1811427883081125888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure.",
              "maleVariant": "",
              "stringId": "1811428710533419012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's meet there.",
              "maleVariant": "",
              "stringId": "1811429181872525312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seems promising. But I'd still hit that market.",
              "maleVariant": "",
              "stringId": "1811429911916302336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. Let's check it out.",
              "maleVariant": "",
              "stringId": "1811430647110684672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's just go to the club.",
              "maleVariant": "",
              "stringId": "1811431078654234624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure.",
              "maleVariant": "",
              "stringId": "1811431432552828928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's meet there.",
              "maleVariant": "",
              "stringId": "1811431432552828944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. Let's go see this market of yours.",
              "maleVariant": "",
              "stringId": "1811433038954483712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit, shit, shit!",
              "maleVariant": "",
              "stringId": "1865035772114620416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My guy works here.",
              "maleVariant": "",
              "stringId": "1935615461327331328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You go talk to him. I'll wait outside.",
              "maleVariant": "",
              "stringId": "1935617538766102528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He doesn't know me.",
              "maleVariant": "",
              "stringId": "1935618742497796096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell him Igor says hi. He'll know what it's about.",
              "maleVariant": "",
              "stringId": "1935627487571304448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. Wait here, then.",
              "maleVariant": "",
              "stringId": "1935628198136733696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mind tellin' me a bit more about him?",
              "maleVariant": "",
              "stringId": "1935646441731743744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is this guy?",
              "maleVariant": "",
              "stringId": "1935647013633482752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The proud proprietor. He's got all sorts of clients and he's nosy as hell. So he knows all about everyone.",
              "maleVariant": "",
              "stringId": "1935655137396350976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plus, he's a coward. Perfect combo.",
              "maleVariant": "",
              "stringId": "1935658274819629056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Cause I can help with both.",
              "maleVariant": "",
              "stringId": "1935714629387509760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Red Queen's Race – where is it?",
              "maleVariant": "",
              "stringId": "1935720251097047040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lookin' for a club. Red Queen's Race?",
              "maleVariant": "",
              "stringId": "1935722063657132032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I was you, I'd try somewhere a little more... welcoming. Lizzie's over in Kabuki is a good start. Or Clouds in Japantown.",
              "maleVariant": "",
              "stringId": "1935738410973749248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want an address.",
              "maleVariant": "",
              "stringId": "1935746925226319872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, club's invitation-only, right? You don't know the address, means you're not welcome.",
              "maleVariant": "",
              "stringId": "1935750141871611904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goddammit!",
              "maleVariant": "",
              "stringId": "1935752185990832128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme alone, got it? Can't make me do a damn thing.",
              "maleVariant": "",
              "stringId": "1935788344599076864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll break more than your fingers.",
              "maleVariant": "",
              "stringId": "1935814504255021056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Red Queen's Race. Give us the address, we're gone.",
              "maleVariant": "",
              "stringId": "1935858848265625600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heard that already last time. Then those motherfuckers showed, broke all my fingers.",
              "maleVariant": "",
              "stringId": "1935859794517385216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I dunno!",
              "maleVariant": "",
              "stringId": "1935870494019117056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Hit him] So remember.",
              "maleVariant": "",
              "stringId": "1935880828297633796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're lying.",
              "maleVariant": "",
              "stringId": "1935895652293369856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyone asks, we'll say we got the address off a body we fished out of the bay.",
              "maleVariant": "",
              "stringId": "1935911745753931776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's outside city limits, near the river. Bonita Street area.",
              "maleVariant": "",
              "stringId": "1935912163036848128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And this'll truly be the last time Detective Ward'll visit you.",
              "maleVariant": "And this'll truly be the last time Detective Ward will visit you.",
              "stringId": "1935926665295912964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Isn't that right, Detective?",
              "maleVariant": "",
              "stringId": "1935926670916280320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right – an' I swear it on the spirits of my ancestors. All we want is the coords for Red Queen's Race.",
              "maleVariant": "",
              "stringId": "1935926675999776768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two of us, one of you. Better talk.",
              "maleVariant": "",
              "stringId": "1935928729816227840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So fuckin' remember.",
              "maleVariant": "",
              "stringId": "1935928795046043648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're lyin'. When I asked inside, you knew exactly what I was talking about.",
              "maleVariant": "",
              "stringId": "1935929217311793152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right! Jeeeesus. Chill, OK?",
              "maleVariant": "",
              "stringId": "1935982680527822848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And make it quick.",
              "maleVariant": "",
              "stringId": "1935990284935192576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, OK, I'll tell you. But come here again and I'll fuckin' lodge an official complaint.",
              "maleVariant": "",
              "stringId": "1935992409819930624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This's some goddamn harassment right here.",
              "maleVariant": "",
              "stringId": "1935993061379891200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ughhh... Fine.",
              "maleVariant": "",
              "stringId": "1935996064853876736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gimme the address, we'll leave you alone.",
              "maleVariant": "",
              "stringId": "1936066879419015168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rest assured. Nobody'll find out you talked to us.",
              "maleVariant": "",
              "stringId": "1936085227804516352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right... but this is the last time, for real.",
              "maleVariant": "",
              "stringId": "1936097413348155392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You work retail, right? So you should be able to count to two.",
              "maleVariant": "",
              "stringId": "1936100735522136064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two of us, one of you.",
              "maleVariant": "",
              "stringId": "1936107656123736064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell us what we wanna know, you won't need to learn what it feels like not to have the upper hand. Least not the hard way.",
              "maleVariant": "",
              "stringId": "1936109373087244288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not telling you another fucking thing.",
              "maleVariant": "",
              "stringId": "1936138975260139520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Made some deliveries – goods, sometimes peeps. But got no idea why you wanna go pokin' around there.",
              "maleVariant": "",
              "stringId": "1936139019702984704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kiss my ass, both of you.",
              "maleVariant": "",
              "stringId": "1936139734395609088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You been there?",
              "maleVariant": "",
              "stringId": "1936143931618844672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Club's empty. They shut it all down.",
              "maleVariant": "",
              "stringId": "1936145467623301120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut it down? Before or after Mayor Rhyne died?",
              "maleVariant": "",
              "stringId": "1936146125575380992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uhhh... Soon after, I guess.",
              "maleVariant": "",
              "stringId": "1936147356200951808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, Neil.",
              "maleVariant": "",
              "stringId": "1936147557812756480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure. Thanks.",
              "maleVariant": "",
              "stringId": "1936154796829138944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Better meet there.",
              "maleVariant": "",
              "stringId": "1936154819998474240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fine with me.",
              "maleVariant": "",
              "stringId": "1936166521217441792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Market?",
              "maleVariant": "",
              "stringId": "1936167755752103936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Horváth's employer, one I mentioned before. I just wouldn't let that lead drop.",
              "maleVariant": "",
              "stringId": "1936167926879707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm headed straight for Bonita Street.",
              "maleVariant": "",
              "stringId": "1936170195243196416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, let's head to the club. Seems a stronger lead to me.",
              "maleVariant": "",
              "stringId": "1936170943809019904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We gonna take my car?",
              "maleVariant": "",
              "stringId": "1936174159196020736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right... Guess I'll go with your gut.",
              "maleVariant": "",
              "stringId": "1936183050197753856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, fine. Chill.",
              "maleVariant": "",
              "stringId": "1937205067260846080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin' to see here, sir.",
              "maleVariant": "",
              "stringId": "1957914902705758208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C'mon, ghost, I said!",
              "maleVariant": "",
              "stringId": "1957915616693739520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit and wait]",
              "maleVariant": "",
              "stringId": "2120262746814898176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, sure.",
              "maleVariant": "",
              "stringId": "2506862608513720320"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}