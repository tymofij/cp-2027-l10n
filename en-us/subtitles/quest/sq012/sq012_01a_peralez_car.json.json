    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you V?",
              "maleVariant": "",
              "stringId": "1692517901624004608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please, step in the vehicle.",
              "maleVariant": "",
              "stringId": "1692563268675686400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, V. I'm Jefferson Peralez.",
              "maleVariant": "",
              "stringId": "1692563738639060992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You've already spoken with Elizabeth, my wife.",
              "maleVariant": "",
              "stringId": "1692564911735558144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard] All right. Lemme see the scroll.",
              "maleVariant": "",
              "stringId": "1692576723600265220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right. See what I can do.",
              "maleVariant": "",
              "stringId": "1692588839669452800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excellent. We'd be happy with any find that would clarify things, alleviate lingering doubts.",
              "maleVariant": "",
              "stringId": "1692596960932515840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The braindance – it has to be back where it belongs today. Or a lot of people will be in a lot of trouble.",
              "maleVariant": "",
              "stringId": "1692618948061286400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in] Thanks.",
              "maleVariant": "",
              "stringId": "1895403992388427780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK…",
              "maleVariant": "",
              "stringId": "1895405202512240640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice to meet you, Mr. Peralez.",
              "maleVariant": "",
              "stringId": "1895430810952753152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, as you're probably aware, Mayor Lucius Rhyne recently passed.",
              "maleVariant": "",
              "stringId": "1895430840296103936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, right again, Liz.",
              "maleVariant": "",
              "stringId": "1895437736436523008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You see, my wife is a great judge of character. Said this would work.",
              "maleVariant": "",
              "stringId": "1895438108639059968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now I see why.",
              "maleVariant": "",
              "stringId": "1895439288094121984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's this job?",
              "maleVariant": "",
              "stringId": "1895445033468518404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice to meet you.",
              "maleVariant": "",
              "stringId": "1895451939306168320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Straight to the matter at hand, huh? Hmhm…",
              "maleVariant": "",
              "stringId": "1895456546866552832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could engage in some small talk first.",
              "maleVariant": "",
              "stringId": "1898073547883634692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So – whaddaya got for me?",
              "maleVariant": "",
              "stringId": "1898073604221526016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My time' valuable, so is yours.",
              "maleVariant": "",
              "stringId": "1898092288285564928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I mean, guess we could chat about the weather or debate where they serve the best ramen…",
              "maleVariant": "I mean, I guess we could chat up the weather or debate where they serve the best ramen.",
              "stringId": "1898097348176470016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My time's pretty valuable. Pretty sure yours is too.",
              "maleVariant": "",
              "stringId": "1898097373342294016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All the same to me, but…",
              "maleVariant": "",
              "stringId": "1898100265231015936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hahahahaha... Fair enough. To business it is.",
              "maleVariant": "",
              "stringId": "1898100707042222080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mrs. Peralez.",
              "maleVariant": "",
              "stringId": "1898107700003954688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Elizabeth?",
              "maleVariant": "",
              "stringId": "1898113524667473920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "True.",
              "maleVariant": "",
              "stringId": "1898124911363973120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Far as I know, case is closed already.",
              "maleVariant": "",
              "stringId": "1898125643521679360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Case is closed.",
              "maleVariant": "",
              "stringId": "1898125681253638148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His death was no secret. The media missed something?",
              "maleVariant": "",
              "stringId": "1898167619713458176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There was an attempt on his life days before his passing.",
              "maleVariant": "",
              "stringId": "1898168678120906752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Far as I remember, someone tried to zero Rhyne a few days before his death.",
              "maleVariant": "",
              "stringId": "1898176246557925376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was all over the news.",
              "maleVariant": "",
              "stringId": "1898176268049539072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is. Of course the police wrote it off as natural causes. Rhyne passed at home due to a \"cardioimplant malfunction.\"",
              "maleVariant": "",
              "stringId": "1898177668426981376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The NCPD sees no link. They're saying it was a random cyberpsycho attack.",
              "maleVariant": "",
              "stringId": "1898293739632943104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pf, of course it was. Media feasted for weeks. Pieces like that are their lifeblood.",
              "maleVariant": "",
              "stringId": "1898328188441550848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And claimin' the mayor died of natural causes…",
              "maleVariant": "",
              "stringId": "1898330350521376768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What makes you think the cops got it wrong? Got any evidence?",
              "maleVariant": "",
              "stringId": "1898331076521844736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Causes unrelated to the attack. It's in the official reports. Rhyne died at home due to a \"cardioimplant malfunction.\"",
              "maleVariant": "",
              "stringId": "1898343528772886528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We want to know how, why, whatever else is pertinent.",
              "maleVariant": "",
              "stringId": "1898354419434217472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right. Think they missed somethin'?",
              "maleVariant": "",
              "stringId": "1898368044714647552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have reason to believe they did, yes.",
              "maleVariant": "",
              "stringId": "1898368395073249280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But we're left wondering about the cyberpsycho attack at City Hall a few days earlier.",
              "maleVariant": "",
              "stringId": "1898371169018142720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What makes you think the cops got it wrong?",
              "maleVariant": "",
              "stringId": "1898372570049896452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The police say it was completely unrelated.",
              "maleVariant": "",
              "stringId": "1898374735669714944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This got anything to do with the upcoming election?",
              "maleVariant": "",
              "stringId": "1898411669653774336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Peralez? The one running for mayor?",
              "maleVariant": "",
              "stringId": "1924389261430972416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Peralez? The one running for mayor of Night City?",
              "maleVariant": "Peralez? Heh... The one running for mayor of Night City?",
              "stringId": "1924390084974174208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The one.",
              "maleVariant": "",
              "stringId": "1924394652655116288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So the one who was DA until recently but just vaulted into a seat on the city council.",
              "maleVariant": "",
              "stringId": "1924401832968019968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I gather you now understand why discretion is of the utmost importance.",
              "maleVariant": "",
              "stringId": "1924401942288359424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These suspicions of yours… wouldn't have anything to do with the upcoming election, would they?",
              "maleVariant": "",
              "stringId": "1924436682752090112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "'Course they would. If Lucius Rhyne was murdered, we want to know, need to know.",
              "maleVariant": "",
              "stringId": "1924453433560752128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shortly before Rhyne passed, he made cuts in the NCPD budget. Perhaps got on someone's bad side that way.",
              "maleVariant": "",
              "stringId": "1924454857711509504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In any case, we have the cyberpsycho attack on a BD.",
              "maleVariant": "",
              "stringId": "1924463438435409920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our chief of security scanned it but didn't find anything out of the ordinary, so to speak... We need a braindance editor, a real one. It's raw footage.",
              "maleVariant": "",
              "stringId": "1924464487044964352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you agree to help, you can see it right now.",
              "maleVariant": "",
              "stringId": "1924465207374733312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd you get it?",
              "maleVariant": "",
              "stringId": "1924492786148270080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where'd you get the scroll?",
              "maleVariant": "",
              "stringId": "1924494741398577152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rhyne was making a public appearance. BD scrolling's standard procedure for events like that.",
              "maleVariant": "",
              "stringId": "1924506122088833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Following the attack, the NCPD impounded the footage as evidence.",
              "maleVariant": "",
              "stringId": "1924508021890441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We had to pull many strings to extract it.",
              "maleVariant": "",
              "stringId": "1924508423469883392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme look at it now, then.",
              "maleVariant": "",
              "stringId": "1947712436352409600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Carter, let's go.",
              "maleVariant": "",
              "stringId": "1957819339482095616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit and wait]",
              "maleVariant": "",
              "stringId": "2065155347913728004"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}