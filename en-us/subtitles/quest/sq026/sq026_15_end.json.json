  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She was right.",
              "maleVariant": "",
              "stringId": "1754881289358393348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, V!",
              "maleVariant": "",
              "stringId": "1754881759019778048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You just stood there and watched her play us?",
              "maleVariant": "",
              "stringId": "1754888968457928704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Decided to give her a chance.",
              "maleVariant": "",
              "stringId": "1754889761382711296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Change should come gradually.",
              "maleVariant": "",
              "stringId": "1754889816428756992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She was right, Judy. Granted, didn't tell us what she was plannin', but she prolly knew you wouldn't agree to it.",
              "maleVariant": "",
              "stringId": "1754890155697618944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Decided to give her a chance. Figured you had a reason to take this to her.",
              "maleVariant": "",
              "stringId": "1754892121282691072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Changes this big need to come gradually. Revolution always sounds great in theory.",
              "maleVariant": "",
              "stringId": "1754892204229246976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fine... Augh, I'm sorry. Got carried away, I... Just, this ain't how I imagined things panning out.",
              "maleVariant": "",
              "stringId": "1754892626880872448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's OK, I get it.",
              "maleVariant": "",
              "stringId": "1754894151862697988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Evelyn also wanted everything in one go.",
              "maleVariant": "",
              "stringId": "1754894895663792128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your frustration, I get it.",
              "maleVariant": "",
              "stringId": "1754895331804299264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't take this personally, but I gotta cool down.",
              "maleVariant": "",
              "stringId": "1754896129057603584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Evelyn wanted everything in one go too, and... heh, immediately.",
              "maleVariant": "Evelyn wanted everything in one go too, and uh, heh... immediately.",
              "stringId": "1754896322784116736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's true.",
              "maleVariant": "",
              "stringId": "1754896322800893952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna go.",
              "maleVariant": "",
              "stringId": "1754897013082669056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for the help, V. Thank you, again.",
              "maleVariant": "",
              "stringId": "1754897753259884544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm sorry.",
              "maleVariant": "",
              "stringId": "1754901624552144896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasn't my fault.",
              "maleVariant": "",
              "stringId": "1754901624568922112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sorry, Judy. I-I didn't plan for it to happen.",
              "maleVariant": "I'm sorry, Judy, I... I didn't plan for it to happen.",
              "stringId": "1754901954341879808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know.",
              "maleVariant": "",
              "stringId": "1754902467758243840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wouldn't've happened if she hadn't come at me. Wasn't my fault.",
              "maleVariant": "",
              "stringId": "1754902813318561792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, sorry, but... I don't buy that. Could've found another way.",
              "maleVariant": "",
              "stringId": "1754902813318561800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You... Y-you killed her?!",
              "maleVariant": "",
              "stringId": "1775165119327617024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck... Should've seen that comin'.",
              "maleVariant": "",
              "stringId": "1775165237657321476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Clouds is free, that's what's important.",
              "maleVariant": "",
              "stringId": "1775167152491315200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's a difference between being a gonk and having faith in people.",
              "maleVariant": "",
              "stringId": "1775167152491315216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who cares that she was schemin'? We did it, that's what matters. Clouds belongs to the dolls now.",
              "maleVariant": "Who cares that she was schemin'? Heh, we did it, that's what matters. Clouds belongs to the dolls now.",
              "stringId": "1775167152558424068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're right. That was the plan all along.",
              "maleVariant": "",
              "stringId": "1775167152575201280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't be dramatic, Judy. There's a difference between bein' a gonk and believin' people wanna do good.",
              "maleVariant": "Ah, don't be dramatic, Judy. There's a difference between bein' a gonk and believin' people wanna do good.",
              "stringId": "1775167152608755716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know you do. Don't need to believe that. I just know it.",
              "maleVariant": "",
              "stringId": "1775167152608755724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Least you had the decency not to take her eddies.",
              "maleVariant": "",
              "stringId": "1775174024422211584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As if that wasn't enough, you took her eddies, too.",
              "maleVariant": "",
              "stringId": "1775174225866244100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bullshit! You're just spewing words now, V.",
              "maleVariant": "",
              "stringId": "1775176021649113092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You took her eddies, so the sich is clear, cut and dried for me.",
              "maleVariant": "",
              "stringId": "1775210963204554752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you.",
              "maleVariant": "",
              "stringId": "1775231205888737280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thank you.",
              "maleVariant": "",
              "stringId": "1775231376731127812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Didn't want a bloodbath.",
              "maleVariant": "",
              "stringId": "1782345598342512640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I hadn't settled it quietly, Hiromi wouldn't've been the last casualty.",
              "maleVariant": "",
              "stringId": "1782345995912200196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She would've been worse than Hiromi.",
              "maleVariant": "",
              "stringId": "1782349004838780928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Given what I saw, I knew she'd be a bigger jackbooted tyrant than Hiromi.",
              "maleVariant": "",
              "stringId": "1782349433580535808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And that was reason enough to take her life?",
              "maleVariant": "",
              "stringId": "1782350665967714304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O-oh my fucking god... You killed her.",
              "maleVariant": "",
              "stringId": "1892205587805724672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe you're right. I'm sorry.",
              "maleVariant": "",
              "stringId": "1892275977655640068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She should've been aware of the consequences.",
              "maleVariant": "",
              "stringId": "1892277587983163392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe you're right. I-I let her get to me. Sorry things ended this way.",
              "maleVariant": "Yeah, maybe you're right. I let her get to me. I'm sorry things ended this way.",
              "stringId": "1892282898525675520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So am I.",
              "maleVariant": "",
              "stringId": "1892291141373222912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maiko knew what she was starting, what she was doing. She knew it could all come crashing down.",
              "maleVariant": "",
              "stringId": "1892293021595820032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought she was untouchable all her life…",
              "maleVariant": "",
              "stringId": "1892296319258288128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well... not my fault she was wrong.",
              "maleVariant": "Well... it's not my fault she was wrong.",
              "stringId": "1892296690605187072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Probably had this planned from the start – same sec we walked into her office.",
              "maleVariant": "",
              "stringId": "1892332694913376256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jesus, am I a gonk.",
              "maleVariant": "",
              "stringId": "1892333729262297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you made it happen.",
              "maleVariant": "",
              "stringId": "1892344652135882752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well fuck!",
              "maleVariant": "",
              "stringId": "1892422475668217856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Club as big as this, operating in such a niche, needs a strong hand at the helm.",
              "maleVariant": "A club as big as this, operating in such a niche, needs a strong hand at the helm.",
              "stringId": "1892422658254659584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Seeing as you trusted her, why should I have doubted her?",
              "maleVariant": "Seein' as you trusted her, why should I have doubted her?",
              "stringId": "1892438285358948352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't pin this on me.",
              "maleVariant": "",
              "stringId": "1892438861136224256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm not. She knows the harsh realities at Clouds better than I do... Better than you do, even.",
              "maleVariant": "I'm not. She knows the harsh realities at Clouds better'n I do... Better'n you, even.",
              "stringId": "1892439077830746112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But Night City's about wheelin' and dealin'.",
              "maleVariant": "",
              "stringId": "1892445515466629120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's where you and I differ. For you, it's about profit. For me, it's about the people.",
              "maleVariant": "",
              "stringId": "1892445781955928064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're a merc. She's a soulless, conniving bitch, and I'm a sorry-ass gonk.",
              "maleVariant": "",
              "stringId": "1892456948099145728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First casualties of revolution – revolutionaries. Bring anarchy to Clouds and it'd go to shit. Everybody'd wind up in the street.",
              "maleVariant": "First casualties of revolution – revolutionaries. Bring anarchy to Clouds and it'd go to shit. Everybody'd wind up on the street.",
              "stringId": "1892461376864739328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How many lives would you be willin' to sacrifice for your pipe dream?",
              "maleVariant": "",
              "stringId": "1892464001190420480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But... guess I should get used to it... Heh, no matter what you do, life's always gonna throw you a curveball.",
              "maleVariant": "",
              "stringId": "1892499926679248896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And a strong gang to back it up.",
              "maleVariant": "",
              "stringId": "1892514551982415872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'd always tell her that.",
              "maleVariant": "",
              "stringId": "1892536555586412544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just, gimme a sec. Gotta gather my wits.",
              "maleVariant": "",
              "stringId": "1904483304446058496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I, uh... I gotta process all this. In peace. Alone.",
              "maleVariant": "",
              "stringId": "1910142416004907008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Be seein' ya, V.",
              "maleVariant": "",
              "stringId": "1911209990960513024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See you later, V.",
              "maleVariant": "",
              "stringId": "1912548097131610112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Step away] See ya, Judy.",
              "maleVariant": "",
              "stringId": "1914240191504121860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, then... see ya, Judy.",
              "maleVariant": "",
              "stringId": "1914240515069509632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Step away] No problem. See ya.",
              "maleVariant": "",
              "stringId": "1914271987885211648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't mention it. See ya around, Judy.",
              "maleVariant": "",
              "stringId": "1914271988975730692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Step away] See ya.",
              "maleVariant": "",
              "stringId": "1914272426374529024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, Judy.",
              "maleVariant": "",
              "stringId": "1914272427397939204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Step away] Take care, Judy.",
              "maleVariant": "",
              "stringId": "1914272550123274240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See you around, Judy.",
              "maleVariant": "",
              "stringId": "1914272551163461636"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}