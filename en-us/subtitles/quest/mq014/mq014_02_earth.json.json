        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is a meditation of the Earth element.",
              "maleVariant": "",
              "stringId": "1815424808219807744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spirit of nature, Mother Earth, I send you greetings. We are of one blood – I am you, you are me.",
              "maleVariant": "",
              "stringId": "1816921764108668928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cleanse my body and soul from pain, sickness, impurity and the negative energy from negative people, worlds and dimensions.",
              "maleVariant": "",
              "stringId": "1822658939899056128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give me the energy of life – give me health, youth, wisdom, knowledge, strength, joy, happiness and inner peace.",
              "maleVariant": "",
              "stringId": "1822659844442656768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We sense joy, tranquility, lightness, purity – our heart and soul.",
              "maleVariant": "",
              "stringId": "1822667226233147392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Warmth and joy surrounds us. Our legs absorb the earth's energy, filling every cell of our bodies. Our souls and spirits are cleansed.",
              "maleVariant": "",
              "stringId": "1822667455175036928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We thank you, Mother Earth, with our mind, body and soul. There is no life without you, or the other elements.",
              "maleVariant": "",
              "stringId": "1822684701398110208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We walk the earth, through meadows, nature in full bloom.",
              "maleVariant": "",
              "stringId": "1918338054580310016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O, Mother Earth, I and all other creatures from you created.",
              "maleVariant": "",
              "stringId": "1925539654566645760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We remove our shoes, feel our bare feet touch the soil. We absorb the earth's energy.",
              "maleVariant": "",
              "stringId": "1925541063785680896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We walk the earth – a butterfly rests on blooming flower, bees are gathering nectar.",
              "maleVariant": "",
              "stringId": "1925541516384636928"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
