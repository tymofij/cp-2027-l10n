        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is a meditation of the Fire element.",
              "maleVariant": "",
              "stringId": "1815550918190403588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are in the forest. We see rays of sunlight pierce the canopy, dancing and weaving into the tongues of flame below.",
              "maleVariant": "",
              "stringId": "1816922686184460288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am you, and you are me. There is no life without you. From you I was created, and to you I will return.",
              "maleVariant": "",
              "stringId": "1822765850526978048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spirit of the Sun, Primordial Fire, burn all impurity from my body and soul, and the negative energy of negative people, worlds and dimensions.",
              "maleVariant": "",
              "stringId": "1822767135779164160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cleanse every cell of my body. Give me the energy of life, health, youth, wisdom, knowledge, strength, joy and happiness.",
              "maleVariant": "",
              "stringId": "1822770179350695936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Through our eyes, the energy of fire, of the sun, touches our heads and flows down through every organ, until it arrives at our toes.",
              "maleVariant": "",
              "stringId": "1822774555922370560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And from within us, light emerges.",
              "maleVariant": "",
              "stringId": "1822775532222124032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We thank you, Spirit of the Sun, element of Fire, with our mind, body and soul.",
              "maleVariant": "",
              "stringId": "1822781310043656192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spirit of Fire, Spirit of the Sun â€“ I send you greetings. You give beauty to all other things.",
              "maleVariant": "",
              "stringId": "1925616428986109952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give me lasting inner peace, so that no one may shake me from my sacred equilibrium.",
              "maleVariant": "",
              "stringId": "1925617208791109632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Through half-closed eyes, we absorb the sun's divine energy.",
              "maleVariant": "",
              "stringId": "1925617534923411456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Each and every cell within our body, including the subtle and etheric, is replenished with the sun's energy.",
              "maleVariant": "",
              "stringId": "1925619216168226816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are the sun.",
              "maleVariant": "",
              "stringId": "1925620860083736576"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
