        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is a meditation of the Air element.",
              "maleVariant": "",
              "stringId": "1815551910009081860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are in the mountains, heading toward the peak. We squeeze between two boulders.",
              "maleVariant": "",
              "stringId": "1816923004649574400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spirit of the Air, Primordial Wind – I send you greetings. We are of one blood, you and I.",
              "maleVariant": "",
              "stringId": "1822800929655590912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "O! Great Spirit of the Air, please – cleanse my body and soul, relinquish pain, sickness, the negative energy from negative people, worlds, and dimensions.",
              "maleVariant": "",
              "stringId": "1822803656137740288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give me the energy of life. Give me health, wisdom, knowledge, strength, joy, happiness, and lasting inner peace.",
              "maleVariant": "",
              "stringId": "1822805065189003264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am an eagle. The wind cushions my wings, caresses my feathers, enters every fiber of my body, fills the spaces of every atom and molecule.",
              "maleVariant": "",
              "stringId": "1822808334732857344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Circling upwards, we rise up high.",
              "maleVariant": "",
              "stringId": "1822809097089552384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We thank you, Spirit of the Air, with our mind, body and soul.",
              "maleVariant": "",
              "stringId": "1822819343270907904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On the first boulder sits an eagle. The spirit of the Air passes through every fibre of this eagle.",
              "maleVariant": "",
              "stringId": "1925622134648836096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are within me, and I am within you. Without you, life would not last even a few minutes, so vital are you to us.",
              "maleVariant": "",
              "stringId": "1925622914168623104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The wind cleanses the body, the soul, and the subtle body.",
              "maleVariant": "",
              "stringId": "1925623428809723904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We feel lightness, purity, joy. Our body is filled with happiness, peace and harmony.",
              "maleVariant": "",
              "stringId": "1925623593645871104"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}