  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Started to think you wouldn't show.",
              "maleVariant": "",
              "stringId": "1792432330482032640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK... So what's this about?",
              "maleVariant": "",
              "stringId": "1792531005510164484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck me. It's you.",
              "maleVariant": "",
              "stringId": "1792531476882825220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's me. Lizzy Wizzy.",
              "maleVariant": "",
              "stringId": "1792531866852433920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where do I come in?",
              "maleVariant": "",
              "stringId": "1792532344852094980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is he?",
              "maleVariant": "",
              "stringId": "1792532709353889792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What makes you think he's cheating?",
              "maleVariant": "",
              "stringId": "1792532872998854656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So what's my role here? What do you want?",
              "maleVariant": "",
              "stringId": "1792550454095159296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Proof. And then I'll end things. For good.",
              "maleVariant": "",
              "stringId": "1792554907976245248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can count on me.",
              "maleVariant": "",
              "stringId": "1792570683911217156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta think it over.",
              "maleVariant": "",
              "stringId": "1792581826901876740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I understand. It's good to think things through – not act first and regret it later.",
              "maleVariant": "",
              "stringId": "1792582527954624512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Perfect.",
              "maleVariant": "",
              "stringId": "1792588537251405824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] You can count on me.",
              "maleVariant": "",
              "stringId": "1792591127787773952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Need to think about it.",
              "maleVariant": "",
              "stringId": "1792591127787773968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remember, I want specifics. Details. Recordings or pics if you can.",
              "maleVariant": "",
              "stringId": "1803970679038451712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shouldn't have, to be honest.",
              "maleVariant": "",
              "stringId": "1945752777125056512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why not?",
              "maleVariant": "",
              "stringId": "1945753901483741184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Normally only work with fixers. Clients that avoid 'em tend to be bad news.",
              "maleVariant": "",
              "stringId": "1945754033268772864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just care about my privacy.",
              "maleVariant": "",
              "stringId": "1945754887849824256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You and everyone else.",
              "maleVariant": "",
              "stringId": "1945756282439127044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why?",
              "maleVariant": "",
              "stringId": "1945756490006843392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone wants their privacy.",
              "maleVariant": "",
              "stringId": "1945757412200075264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe. But not as much as me.",
              "maleVariant": "",
              "stringId": "1945757827889156096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. And why's that, if I might ask?",
              "maleVariant": "Huh. And why's that, if I might ask?",
              "stringId": "1945758169540382720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is why.",
              "maleVariant": "",
              "stringId": "1945758797780013056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sit, please. Let's talk.",
              "maleVariant": "",
              "stringId": "1945760336301051904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I think you can guess. I'm in trouble.",
              "maleVariant": "",
              "stringId": "1945762786714112000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. And what kind?",
              "maleVariant": "Mhm. And what kind?",
              "stringId": "1945763534004867072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hah. Gooood question.",
              "maleVariant": "",
              "stringId": "1945763845071228928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I fell in love.",
              "maleVariant": "",
              "stringId": "1945764910088900608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm so sorry.",
              "maleVariant": "",
              "stringId": "1945768400655798272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm sorry.",
              "maleVariant": "",
              "stringId": "1945768620236001284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's not a problem.",
              "maleVariant": "",
              "stringId": "1945768840269189120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not the worst thing in the world.",
              "maleVariant": "It's not the worst thing in the world.",
              "stringId": "1945769062382751744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. And why's that?",
              "maleVariant": "",
              "stringId": "1945770376240091136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sooner or later, love'll only let you down.",
              "maleVariant": "",
              "stringId": "1945770936079011840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is to me.",
              "maleVariant": "",
              "stringId": "1945774347776217088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have something to say. And that – it's a distraction. I need to focus on my art.",
              "maleVariant": "",
              "stringId": "1945774460351336448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So – we're talkin' a man? Woman?",
              "maleVariant": "So – we talkin' a man? A woman?",
              "stringId": "1945774857551925248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guess it'll be sooner.",
              "maleVariant": "",
              "stringId": "1945776029490794496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh. Anyway, a man. His name's Liam, Liam Northom. Been together a few months, but... I think he's cheating on me.",
              "maleVariant": "",
              "stringId": "1945776580991438848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You confront him?",
              "maleVariant": "",
              "stringId": "1945779616879005696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Liam Northom.... Nope, don't know him. Tell me about him.",
              "maleVariant": "Liam Northom.... Nah, don't know him. Tell me about him.",
              "stringId": "1945780041543897088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Forty-ish. Sensitive. Guy who never wanted for anything. You know the type.",
              "maleVariant": "",
              "stringId": "1945781606505828352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I heard they exist. How'd you meet?",
              "maleVariant": "Heard they exist. How'd you meet?",
              "stringId": "1945782725294477312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hehhh... He's my manager.",
              "maleVariant": "",
              "stringId": "1945786056729481216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know, I know how it sounds. I never meant to. He's not even my type. But...",
              "maleVariant": "",
              "stringId": "1945786467284733952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But?",
              "maleVariant": "",
              "stringId": "1945789721930420224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your body can be chrome... but the heart never changes. It wants what it wants.",
              "maleVariant": "",
              "stringId": "1945789868529733632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK – why the suspicion?",
              "maleVariant": "",
              "stringId": "1945792317936160768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... It's so clichéd, it's embarrassing.",
              "maleVariant": "",
              "stringId": "1945792787345887232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whispered phone calls in the bathroom. Strange receipts. Secret meetings in the middle of the night.",
              "maleVariant": "",
              "stringId": "1945793051503153152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Any specific event come to mind?",
              "maleVariant": "",
              "stringId": "1945796130055180288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hah. My mind. That's... that's a touchy subject.",
              "maleVariant": "",
              "stringId": "1945797027401355264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why?",
              "maleVariant": "",
              "stringId": "1945797593414291456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't know each other well enough for that, V.",
              "maleVariant": "",
              "stringId": "1945797729561399296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You talk to him at all? Tell him to his face?",
              "maleVariant": "",
              "stringId": "1945800422103240704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hah! You're adorable.",
              "maleVariant": "",
              "stringId": "1945806120400769024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Imagine this – we're never alone, not really. Always surrounded by assistants, PR reps, makeup artists...",
              "maleVariant": "",
              "stringId": "1945806690289242112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I stop smiling at him for one millisecond, it'll be all over every screamsheet in this town.",
              "maleVariant": "",
              "stringId": "1945807364347449344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Understood.",
              "maleVariant": "",
              "stringId": "1945807904976457728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No – you don't understand. Not until you've lived it. Millions of eyes, trained on you, constantly. It can be beautiful... and terrifying.",
              "maleVariant": "",
              "stringId": "1945808367104872448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Liam goes to Riot all the time. Think he meets someone there. I wanna know who.",
              "maleVariant": "",
              "stringId": "1945811742361964544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll find out for me?",
              "maleVariant": "",
              "stringId": "1945812165785341952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's talk cred first.",
              "maleVariant": "",
              "stringId": "1945817915605700608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not so fast. The scratch?",
              "maleVariant": "",
              "stringId": "1945818408889405440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll get it.",
              "maleVariant": "",
              "stringId": "1945823364694462464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure hope so. Question is, how much?",
              "maleVariant": "",
              "stringId": "1945823933089763328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "God, how mundane. No idea. Scads 'n' scads of scratch. You happy?",
              "maleVariant": "",
              "stringId": "1945825037533892608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. Y'know, for us mere mortals, the mundane matters. Sometimes means the difference between life and death.",
              "maleVariant": "Mhm. Y'know, for us mere mortals, the mundane matters. Sometimes means the difference between life and death.",
              "stringId": "1945825602741522432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't lecture me. You want the job or not?",
              "maleVariant": "",
              "stringId": "1945826667088105472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, keep it in mind.",
              "maleVariant": "",
              "stringId": "1945829471550754816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good. Now go.",
              "maleVariant": "",
              "stringId": "1945829651771609088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh shit, these hands... Sometimes seems like I just brush something, and sparks fly…",
              "maleVariant": "",
              "stringId": "1946062477636980736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been a rough year. Creative block. Can't even say the last time I made anything worth listening to, and to make things worse...",
              "maleVariant": "",
              "stringId": "1963309036221026304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "1973597722804920336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Sit.",
              "maleVariant": "",
              "stringId": "2483716571303682048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Holy shit... you're…",
              "maleVariant": "",
              "stringId": "3508410862281932828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yup. Lizzy Wizzy. None other.",
              "maleVariant": "",
              "stringId": "3508410862298710024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Grab a seat, please. Let's talk.",
              "maleVariant": "",
              "stringId": "3508410862298710048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saw you at the Black Sapphire – hella gig you put on…",
              "maleVariant": "",
              "stringId": "3508416135746797568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, I don't generally brag about visiting that place, so... our little secret, OK?",
              "maleVariant": "",
              "stringId": "3514538978358464512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyway… enough of that.",
              "maleVariant": "",
              "stringId": "3514540029266497536"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}