  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is not all of them.",
              "maleVariant": "",
              "stringId": "1730128440841826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I believe... Yes, that is the last of Yorinobu's people.",
              "maleVariant": "",
              "stringId": "1730239100456103936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I believe a war has just begun.",
              "maleVariant": "",
              "stringId": "1743214428115791872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見事だ\" t=\"Excellent.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1781020138217160704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I await you.",
              "maleVariant": "",
              "stringId": "1795388015658274816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's just weird, that's all. You call the biggest board meeting in a decade without invitin' your own sister?",
              "maleVariant": "",
              "stringId": "1813996648192294912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I repeat. Internal security has not been compromised. Over.",
              "maleVariant": "",
              "stringId": "1813999114141057024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All systems are go for me. Standing by. Over.",
              "maleVariant": "",
              "stringId": "1814000516229775360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, sir. Everything is under control. Over.",
              "maleVariant": "",
              "stringId": "1814001760612663300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mh-hm-hm! This view's to die for! And the air, man, the air...!",
              "maleVariant": "",
              "stringId": "1814004720918872064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Glad they called us here instead of the tower...",
              "maleVariant": "",
              "stringId": "1814005033042198528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hellman! We are done! We need the AV, now!",
              "maleVariant": "",
              "stringId": "1822853255962640384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These people... they simply left me no choice.",
              "maleVariant": "",
              "stringId": "1835821347927625728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep it together.",
              "maleVariant": "",
              "stringId": "1835869897550073860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did what we had to do.",
              "maleVariant": "",
              "stringId": "1835870221518114816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have a job to do.",
              "maleVariant": "",
              "stringId": "1835870770200186880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did what we had to.",
              "maleVariant": "",
              "stringId": "1835870844321927168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is why I have remained outside, uninvolved.",
              "maleVariant": "",
              "stringId": "1837312048028848128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So as not to have to make decisions such as these.",
              "maleVariant": "",
              "stringId": "1837312558039437312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to keep it together, for all our sakes!",
              "maleVariant": "",
              "stringId": "1837321457111011328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need do nothing.",
              "maleVariant": "",
              "stringId": "1837322820746350592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is just that…",
              "maleVariant": "",
              "stringId": "1837328094026665984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, it's not like she has any official position…",
              "maleVariant": "",
              "stringId": "1837358695802867712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Psh. Since when do you have to be official to pull the strings?",
              "maleVariant": "",
              "stringId": "1837359202307018752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Those gold fingers of hers? They're a symbol – wraps peole around them. Just like she did her dad.",
              "maleVariant": "",
              "stringId": "1837360025581785088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There's your answer, then. Our new Emperor Arasaka isn't about to get tangled up in any gold fingers.",
              "maleVariant": "",
              "stringId": "1837360181526007808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You're right, though, it is a little weird...",
              "maleVariant": "",
              "stringId": "1837360676537765888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Goddamn Smasher... Can you believe it? 'Saka 'lites like us steppin' aside for some punk-ass ex-merc? So what if he's biopodded?",
              "maleVariant": "",
              "stringId": "1844219229852536832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe it's just temporary.",
              "maleVariant": "",
              "stringId": "1844219539358617600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu must've had a good reason to drag him out of his cave. Looks like he's not gonna crawl back in there anytime soon.",
              "maleVariant": "",
              "stringId": "1845817765694148608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will go left. The right side is yours.",
              "maleVariant": "",
              "stringId": "1873562604761268224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There is a netrunner in the building. I will eliminate him.",
              "maleVariant": "",
              "stringId": "1873563575105110016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is clear inside. I will monitor the situation from here.",
              "maleVariant": "",
              "stringId": "1873565188888739840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will reach you shortly, V.",
              "maleVariant": "",
              "stringId": "1873566118833045504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They know you are here.",
              "maleVariant": "",
              "stringId": "1908332456134590464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You must neutralize them. All of them.",
              "maleVariant": "",
              "stringId": "1908333041239998464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am in the bedroom in the central part of the building.",
              "maleVariant": "",
              "stringId": "1908340320051937280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anders, you get the AV.",
              "maleVariant": "",
              "stringId": "1908341071721545728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But first, we must... neutralize the four guards. V? Is your task clear?",
              "maleVariant": "",
              "stringId": "1908341643891720192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But first, we must... neutralize the four guards. Takemura, V? Is your task clear?",
              "maleVariant": "",
              "stringId": "1908342161905041408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We will take the left side, through the garden.",
              "maleVariant": "",
              "stringId": "1908342834923061248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will obstruct their communications so that no support arrives.",
              "maleVariant": "",
              "stringId": "1908345087952175104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Careful, now. There are five more guards outside. And a dweller in the garage on the left.",
              "maleVariant": "",
              "stringId": "1908346169327632384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One guard in the courtyard, another near the pool. Number three – on the terrace, the last inside.",
              "maleVariant": "",
              "stringId": "1908347682095644672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm nearing the residence.",
              "maleVariant": "",
              "stringId": "1908352095912738816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What have you done?! Now you will have to face them all!",
              "maleVariant": "",
              "stringId": "1908354188501336064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Patrol, two men.",
              "maleVariant": "",
              "stringId": "1908359178498359296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You take one, I will handle the other myself.",
              "maleVariant": "",
              "stringId": "1908359976791851008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Neutralized. Down to the last.",
              "maleVariant": "",
              "stringId": "1908362355952074752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good. I am disabling lockdown. I await you.",
              "maleVariant": "",
              "stringId": "1908362894936915968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good. Land by the pool.",
              "maleVariant": "",
              "stringId": "1908363579833204736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please.",
              "maleVariant": "",
              "stringId": "1915253453191462912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in]",
              "maleVariant": "",
              "stringId": "1915351422116904964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "War's… a bit much, innit? Though a loaded word for any Arasaka.",
              "maleVariant": "War's a bit much, isn't it? Though a loaded word for any Arasaka.",
              "stringId": "1937339808506462208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will not need this anymore…",
              "maleVariant": "",
              "stringId": "1963563808545665024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He did not have time to call for backup... Good.",
              "maleVariant": "",
              "stringId": "1963569362592690176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That gives us the upper hand, but we must hurry.",
              "maleVariant": "",
              "stringId": "1963643195563536384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. We \"neutralize.\"",
              "maleVariant": "Uh-huh. We  \"neutralize.\"",
              "stringId": "2000940801754734592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Roger that. Over and out.",
              "maleVariant": "",
              "stringId": "2007179881702301696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anders, you get the AV.",
              "maleVariant": "",
              "stringId": "2007195287800324096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No blood was spilled – this is good.",
              "maleVariant": "",
              "stringId": "2007211512710373376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just counting bodies. You, me and Hellman makes three. Actually, two and a half.",
              "maleVariant": "",
              "stringId": "2008132269175021568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just counting bodies. You, me, Goro and Hellman makes four. Actually, three and a half.",
              "maleVariant": "",
              "stringId": "2008132363530084352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is that what you believe?",
              "maleVariant": "",
              "stringId": "2008203766220529664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There is Oda, too. We will meet him there.",
              "maleVariant": "",
              "stringId": "2008208250132832256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oda would be here, if you had not…",
              "maleVariant": "",
              "stringId": "2008210348778004488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He was provided with excellent care.",
              "maleVariant": "",
              "stringId": "2008212456919080960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was him or me, Hanako.",
              "maleVariant": "",
              "stringId": "2008213793475993600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pretty quick to get back on his feet.",
              "maleVariant": "",
              "stringId": "2008214134691012608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My brother is not all of Arasaka. Not yet, at least.",
              "maleVariant": "",
              "stringId": "2008218423517847552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He knows this well. It is the reason he surrounds himself with criminals like Adam Smasher.",
              "maleVariant": "",
              "stringId": "2008218793925222400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That supposed to make me feel better?",
              "maleVariant": "",
              "stringId": "2008224008619499520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My father was \"on his own.\" And he built an empire.",
              "maleVariant": "",
              "stringId": "2008224535642185728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Reality is we're on our own.",
              "maleVariant": "",
              "stringId": "2008226207642755072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyway, against all Arasaka and its armies? That's not much.",
              "maleVariant": "",
              "stringId": "2008228149890719744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For now. Things could still get ugly.",
              "maleVariant": "",
              "stringId": "2008349264663355392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rather not get your hands dirty?",
              "maleVariant": "",
              "stringId": "2008349264680132620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yet now…",
              "maleVariant": "",
              "stringId": "2008364373116788736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For now. Could get a lot uglier down the line.",
              "maleVariant": "",
              "stringId": "2008426324547149840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You ready for that?",
              "maleVariant": "",
              "stringId": "2008426324547149856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Always.",
              "maleVariant": "",
              "stringId": "2008426324563927040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Rather not dirty your hands, huh?",
              "maleVariant": "",
              "stringId": "2008428072011636740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chances are you'll have to soon enough.",
              "maleVariant": "",
              "stringId": "2008428072011636756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am well aware.",
              "maleVariant": "",
              "stringId": "2008428072011636772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"Kuso.\" b=\"\" a=\" We must beware of the mech.\"/>",
              "maleVariant": "",
              "stringId": "2011315843168542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out] \"Neutralize.\" Got it.",
              "maleVariant": "",
              "stringId": "2133278409131900932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are waiting.",
              "maleVariant": "",
              "stringId": "2257350875024826368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can we not simply depart… and leave her?",
              "maleVariant": "Can we not simply depart… and leave him?",
              "stringId": "2257353090875629576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V! Get in!",
              "maleVariant": "",
              "stringId": "2257354536752881672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V! Arasaka-sama –  run and get her! We will be there soon!",
              "maleVariant": "",
              "stringId": "2257398606321786880"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}