  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Every loyal soldier must stand ready.",
              "maleVariant": "",
              "stringId": "1753439384113266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We shall await your signal.",
              "maleVariant": "",
              "stringId": "1753439567807004672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was in counterintel a while. Never heard of this place, though.",
              "maleVariant": "Was in counterintel a while. Never heard of this place, though.",
              "stringId": "1821289547171762176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course you did not hear of it.",
              "maleVariant": "",
              "stringId": "1821321276662091776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is one of Arasaka's most closely guarded secrets.",
              "maleVariant": "",
              "stringId": "1821323272496484352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Father's office. A faithful replica of the original in Tokyo.",
              "maleVariant": "",
              "stringId": "1821324270522732544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He ordered one made in every Arasaka branch on the planet.",
              "maleVariant": "",
              "stringId": "1821324549947265024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Every last detail, every item... arranged as if he could navigate it blind.",
              "maleVariant": "",
              "stringId": "1821327312986968064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is the way he is. Meticulous. Fastidious.",
              "maleVariant": "",
              "stringId": "1821329848645062656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where are we actually goin'?",
              "maleVariant": "",
              "stringId": "1821433581064294400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This way.",
              "maleVariant": "",
              "stringId": "1821449271318024192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was.",
              "maleVariant": "",
              "stringId": "1822868108110336000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I beg your pardon?",
              "maleVariant": "",
              "stringId": "1822876060477165568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meant to say \"was.\"",
              "maleVariant": "",
              "stringId": "1822876116865388544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Takemura. Gather your force.",
              "maleVariant": "",
              "stringId": "1822885497711497216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Odds of that happening are...?",
              "maleVariant": "",
              "stringId": "1841690843489898496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, you must come with me.",
              "maleVariant": "",
              "stringId": "1842859818361950208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Symbols are more resilient than you think.",
              "maleVariant": "",
              "stringId": "1842861158895063040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Higher than you may think.",
              "maleVariant": "",
              "stringId": "1842997280702861312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The question is if the faction leaders remain loyal to Arasaka values.",
              "maleVariant": "",
              "stringId": "1844343836416806912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And if they will find the courage to admit their mistake.",
              "maleVariant": "",
              "stringId": "1844346672991047680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My testimony's worth shit.",
              "maleVariant": "",
              "stringId": "1844347581762506756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Chances of that happening are...?",
              "maleVariant": "",
              "stringId": "1844347722036809728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honestly? Wishful thinkin' at best.",
              "maleVariant": "",
              "stringId": "1844350650868961280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't have a choice. Corp's future is on the line.",
              "maleVariant": "",
              "stringId": "1844350926099189760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They've got no choice. We're talking about the future of Arasaka here.",
              "maleVariant": "",
              "stringId": "1844351183243579392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A monolith whose first crack appeared with Yorinobu. Goro's words, not mine.",
              "maleVariant": "",
              "stringId": "1844373897060683776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A crack… indeed.",
              "maleVariant": "",
              "stringId": "1844376056338714624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yet no one did anything to seal it.",
              "maleVariant": "",
              "stringId": "1844376513484296192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can't blame yourself.",
              "maleVariant": "",
              "stringId": "1844378741246922752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do not, but they could blame me.",
              "maleVariant": "",
              "stringId": "1844379212602806272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My testimony's worth shit here. And not as if you've got much leverage, either.",
              "maleVariant": "",
              "stringId": "1844386383554023424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We shall soon know.",
              "maleVariant": "",
              "stringId": "1844387415688351744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh shit…!",
              "maleVariant": "Ah, shit…!",
              "stringId": "1844402972579991556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Slowly…",
              "maleVariant": "",
              "stringId": "1844402978619789316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh. Thanks.",
              "maleVariant": "Ugh... thanks.",
              "stringId": "1844402981555802112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can do it. We are almost there.",
              "maleVariant": "",
              "stringId": "1844402984424706048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will she manage?",
              "maleVariant": "Will he manage?",
              "stringId": "1844402990481281028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She should.",
              "maleVariant": "He should.",
              "stringId": "1844402993903833088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh… I'll be fine.",
              "maleVariant": "",
              "stringId": "1844402996923731968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hang on, is this…?",
              "maleVariant": "",
              "stringId": "1863349118742372352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1895369521335713792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come, come.",
              "maleVariant": "",
              "stringId": "1913787885628243972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can do this.",
              "maleVariant": "",
              "stringId": "1913787886030897152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have very little time for this.",
              "maleVariant": "",
              "stringId": "1913787886098006020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get out]",
              "maleVariant": "",
              "stringId": "1917011090325192708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh fuck…",
              "maleVariant": "",
              "stringId": "1954422090967891972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Probably won't get another chance.",
              "maleVariant": "",
              "stringId": "2008472534385500160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To do what?",
              "maleVariant": "",
              "stringId": "2008472613892726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To fill me in on your plan.",
              "maleVariant": "",
              "stringId": "2008472678736666624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What if I have none?",
              "maleVariant": "",
              "stringId": "2008472798274330624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't play dumb with me, Hanako.",
              "maleVariant": "",
              "stringId": "2008472950863110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm... You and Takemura had a plan the day of the parade.",
              "maleVariant": "",
              "stringId": "2008473060435107840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Moments later you had nothing, your plan was in tatters.",
              "maleVariant": "",
              "stringId": "2008473505316544512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And yet, we are here now. Do you know why?",
              "maleVariant": "",
              "stringId": "2008474449538260992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Because sometimes you have to look the truth in the eye.",
              "maleVariant": "",
              "stringId": "2008482695556702208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Back then, that is what I did. Thanks to you.",
              "maleVariant": "",
              "stringId": "2008483086230953984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have a... a very honest look.",
              "maleVariant": "",
              "stringId": "2008484156399230976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's how we're swinging the Arasaka board? You want me to stare at them?",
              "maleVariant": "",
              "stringId": "2008484371785129984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Weapon at the ready.",
              "maleVariant": "",
              "stringId": "2008488133924823040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If my charms don't work, we go for more tried-and-tested methods. I see what you're getting at.",
              "maleVariant": "",
              "stringId": "2008488681113722880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You have yet to see. As do they all.",
              "maleVariant": "",
              "stringId": "2008503592434868224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come.",
              "maleVariant": "",
              "stringId": "2013916409912696832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Grab hand]",
              "maleVariant": "",
              "stringId": "2025833805022052352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2042827347669893120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? This gathering is not one to which we wish to be late.",
              "maleVariant": "",
              "stringId": "2239262414837489664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You expect a red carpet? Get out.",
              "maleVariant": "",
              "stringId": "2239262732799287300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have started something, we must finish it. Come, V.",
              "maleVariant": "",
              "stringId": "2239263043983089664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are running out of time.",
              "maleVariant": "",
              "stringId": "2239272875649769480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Such a big girl, but she's afraid to board the elevator.",
              "maleVariant": "Such a big boy, but he's afraid to board the elevator.",
              "stringId": "2239273052565512200"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}