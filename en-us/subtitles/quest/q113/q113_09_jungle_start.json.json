  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's an island, just one.",
              "maleVariant": "",
              "stringId": "1730332124934664192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And do not utter a word until I ask you to.",
              "maleVariant": "",
              "stringId": "1730334429100732416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No matter what happens... you must trust me.",
              "maleVariant": "",
              "stringId": "1730334717266194432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "With twelve thousand inhabitants.",
              "maleVariant": "",
              "stringId": "1730335383405555712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We could... relocate them. Archipelago, y'know, so there're other islands around.",
              "maleVariant": "",
              "stringId": "1730335640147292160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It sounds very costly.",
              "maleVariant": "",
              "stringId": "1730336438491115520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am late, apologies.",
              "maleVariant": "",
              "stringId": "1730342228258025472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep your weapon at hand.",
              "maleVariant": "",
              "stringId": "1730343109951057920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My brother – where is he?",
              "maleVariant": "",
              "stringId": "1730343430211334144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then begin without him we shall.",
              "maleVariant": "",
              "stringId": "1730343735070126080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I wanna know what's goin' on.",
              "maleVariant": "",
              "stringId": "1730345230624714752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu murdered my father and exploited his death to justify his warmongering.",
              "maleVariant": "",
              "stringId": "1730347484425580544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have an eyewitness.",
              "maleVariant": "",
              "stringId": "1730347754924634112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's true. I was there.",
              "maleVariant": "",
              "stringId": "1730348236564951040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lapped up Yorinobu's lies like dogs, you did.",
              "maleVariant": "",
              "stringId": "1730348664350404608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You didn't even order a proper inquiry.",
              "maleVariant": "",
              "stringId": "1730348783435083776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's true – all of it. Saw it with my own eyes.",
              "maleVariant": "",
              "stringId": "1730349503261536256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can confirm this.",
              "maleVariant": "",
              "stringId": "1730349944670089216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Says a wanted man!",
              "maleVariant": "",
              "stringId": "1730350205086035968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If our testimony is not enough, perhaps you will listen to my father.",
              "maleVariant": "",
              "stringId": "1730350797221097472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh?",
              "maleVariant": "",
              "stringId": "1730352110474457088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見下げ果てた奴らめ\" t=\"You are all despicable.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1730354735387340800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It won't be much longer, V.",
              "maleVariant": "",
              "stringId": "1761818864418570240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, but we better hurry.",
              "maleVariant": "",
              "stringId": "1761819092118945796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do I have any fuckin' choice?!",
              "maleVariant": "",
              "stringId": "1761819306749870080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's getting worse.",
              "maleVariant": "",
              "stringId": "1761819467307827200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, growin' weaker by the minute. We gotta hurry.",
              "maleVariant": "Growin' weaker by the minute. We gotta hurry.",
              "stringId": "1761821759696289792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't have a fuckin' choice, now, do I?",
              "maleVariant": "",
              "stringId": "1761822181324505088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ahem, on a downward spiral, but... I gotta try.",
              "maleVariant": "On a downward spiral… but I gotta try.",
              "stringId": "1761822710830219264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dire moments are upon us.",
              "maleVariant": "",
              "stringId": "1761823166230970368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako-sama.",
              "maleVariant": "",
              "stringId": "1762185025043841024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a good memory for faces, Oda.",
              "maleVariant": "",
              "stringId": "1762186170172719104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, unacceptable.",
              "maleVariant": "",
              "stringId": "1763182300645834752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A serious accusation, admittedly… but nothing more.",
              "maleVariant": "",
              "stringId": "1763195064835653632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They're here!",
              "maleVariant": "",
              "stringId": "1763400557260718080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spare no one.",
              "maleVariant": "",
              "stringId": "1763607659929419776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What... What are you doing? You cannot…",
              "maleVariant": "",
              "stringId": "1811268650187898880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, this is your seat.",
              "maleVariant": "",
              "stringId": "1811269326745911296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Move your ass.",
              "maleVariant": "",
              "stringId": "1811423298689449988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "May I...?",
              "maleVariant": "",
              "stringId": "1811425393811406848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What would her father say to this...?",
              "maleVariant": "",
              "stringId": "1811426227622264832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you deaf?!",
              "maleVariant": "",
              "stringId": "1811427691048484864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hear that? Plant your ass somewhere else.",
              "maleVariant": "",
              "stringId": "1811428200488648704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "May I...? Everyone's waiting.",
              "maleVariant": "",
              "stringId": "1811428300732514308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is outrageous…",
              "maleVariant": "",
              "stringId": "1811430041838436352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Thank you.",
              "maleVariant": "",
              "stringId": "1811434432335200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will not let you treat me this way…",
              "maleVariant": "",
              "stringId": "1812536846438121472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Protect the VIPs!",
              "maleVariant": "",
              "stringId": "1815589611446599680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He... told us to start without him...",
              "maleVariant": "",
              "stringId": "1816945237225504768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako? What're you doing here?",
              "maleVariant": "",
              "stringId": "1816945798020726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Begin what?",
              "maleVariant": "",
              "stringId": "1816947693426704384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako. I believe you owe us an explanation.",
              "maleVariant": "",
              "stringId": "1816959946733518848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give up your seat. Now.",
              "maleVariant": "",
              "stringId": "1816975677135126528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And why you dragged these… people in here.",
              "maleVariant": "",
              "stringId": "1816977436226224128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Much obliged.",
              "maleVariant": "",
              "stringId": "1825551335476510720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You in on this?",
              "maleVariant": "",
              "stringId": "1837123952670806016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In on what?",
              "maleVariant": "",
              "stringId": "1837124158661464064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo's comeback.",
              "maleVariant": "",
              "stringId": "1837124208305246208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just spoke to his construct.",
              "maleVariant": "",
              "stringId": "1837127671642722304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No... He would have told me.",
              "maleVariant": "",
              "stringId": "1837130183661719552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feel that, Goro?",
              "maleVariant": "",
              "stringId": "1837130817957924864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City just gave you another kick in the nuts – running start this time.",
              "maleVariant": "",
              "stringId": "1837131082903719936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What?!",
              "maleVariant": "",
              "stringId": "1837131762313859072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He digitized his engram, made copies. In case he died.",
              "maleVariant": "",
              "stringId": "1837132297792262144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? So you'd take your job as his bodyguard lightly?",
              "maleVariant": "",
              "stringId": "1837136196263612416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ありえん…\" t=\"It cannot be...\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1837140337132179456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then what? Need me to \"neutralize\" them, too?",
              "maleVariant": "",
              "stringId": "1837156410191646720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fascinating.",
              "maleVariant": "",
              "stringId": "1837177179026100224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Careful what you say.",
              "maleVariant": "",
              "stringId": "1837179497805123584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, indeed – words and nothing else. Is this all you have?",
              "maleVariant": "",
              "stringId": "1837192778481537024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You lapped up Yorinobu's lies eagerly like the dogs you are.",
              "maleVariant": "",
              "stringId": "1837194905530535936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka is murdered, and you don't order an inquiry into the cause of his death.",
              "maleVariant": "",
              "stringId": "1837195050166915072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will you stay with us?",
              "maleVariant": "",
              "stringId": "1837301592165847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please, calm…",
              "maleVariant": "",
              "stringId": "1837304529940254720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll be calm when all this is over!",
              "maleVariant": "",
              "stringId": "1837304977908699136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Indeed we do.",
              "maleVariant": "",
              "stringId": "1837306949600677888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course, Hanako-sama.",
              "maleVariant": "",
              "stringId": "1841477113854431232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Forget relocation. Twelve thousand? I doubt anyone will notice.",
              "maleVariant": "",
              "stringId": "1841525616517451776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A-Arasaka-sama?",
              "maleVariant": "",
              "stringId": "1843127674853003264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"いずれにせよ、仮にも役員である者たちが、なんと情けない\" t=\"Whatever the case may be, you do not merit your positions.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843132983399026688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This...? Is this to convince us?",
              "maleVariant": "",
              "stringId": "1843133783890333696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"バヘット。お前がミリテクとの全面戦争を憂慮していた時、私はこう言ったはずだ\" t=\"Barreto, I told you this once before, when you feared all-out war with Militech.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843134920429277184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A magic trick!",
              "maleVariant": "",
              "stringId": "1843136731294863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"お前には3つのものが欠けている。自信、知性、そして根性だ\" t=\"You lack three things – confidence, intelligence and a dick.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843144370431070208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"見たところ、あれから何も変わっていないようだな\" t=\"It is clear nothing has changed in that regard.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843151640200089600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's... it's him…",
              "maleVariant": "",
              "stringId": "1843153757384085504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama... I...",
              "maleVariant": "",
              "stringId": "1843154157889785856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"赤子のようにまんまと騙されたか、日和見主義に走ったか…\" t=\"Either you let yourself be fooled like children or you were guided by opportunism.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843157165960728576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama... what is your command?",
              "maleVariant": "",
              "stringId": "1843188214396751872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコの指示に従え\" t=\"To follow Hanako's orders.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1843190186021621760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Objections?",
              "maleVariant": "",
              "stringId": "1843212599610445824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You.",
              "maleVariant": "",
              "stringId": "1845868980276695040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V is with us.",
              "maleVariant": "",
              "stringId": "1845869530049286144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To me or to my money?",
              "maleVariant": "",
              "stringId": "1845871355410386944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This little stage show is starting to bore me. Will someone call security?",
              "maleVariant": "",
              "stringId": "1845880640576032768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Great idea. They can go straight to Yorinobu and ask him what happened at Konpeki Plaza.",
              "maleVariant": "",
              "stringId": "1845882236408041472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just make sure they're armed. Your boss starts strangling people when he gets mad.",
              "maleVariant": "",
              "stringId": "1845885426243342336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What did you see?",
              "maleVariant": "",
              "stringId": "1845889650041688064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I saw Yorinobu strangle Saburo Arasaka.",
              "maleVariant": "",
              "stringId": "1845889901028839424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "More interesting with each minute.",
              "maleVariant": "",
              "stringId": "1845890535610257408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Convenient to be so docile?",
              "maleVariant": "",
              "stringId": "1845893248402198528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Once again – who is this?",
              "maleVariant": "",
              "stringId": "1845894798096863232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A serious accusation, admittedly… but nothing more.",
              "maleVariant": "",
              "stringId": "1845998954392457216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"周囲で怪しい動きがあれば直ちに報告しろ\" t=\"Report any suspicious activity on the premises.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1863401380156088320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is just these?",
              "maleVariant": "",
              "stringId": "1863407712498163712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The rest are in position, ready to do as instructed.",
              "maleVariant": "",
              "stringId": "1863407943470096384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These are the most effective by much. And loyal.",
              "maleVariant": "",
              "stringId": "1863409061923201024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I would say it's twenty percent to eighty, Hanako-sama.",
              "maleVariant": "",
              "stringId": "1863410370260836352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Very well. It will have to do.",
              "maleVariant": "",
              "stringId": "1863411138456002560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now come.",
              "maleVariant": "",
              "stringId": "1863412863774564352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"ハナコの発する言葉は、すべて私の言葉同然だ\" t=\"You will treat every word she speaks as if it came from my lips.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1865012892738068480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is it you?! Is this your doing, Hanako?",
              "maleVariant": "",
              "stringId": "1865019346614296576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To lock us in here so we devour each other like rats in a cage?",
              "maleVariant": "",
              "stringId": "1865022177148739584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have you brought more of this filth from the street?!",
              "maleVariant": "",
              "stringId": "1865022845267812352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hear that?",
              "maleVariant": "",
              "stringId": "1865023541572608000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I asked you a question!",
              "maleVariant": "",
              "stringId": "1865026292784373760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the hell's happening?",
              "maleVariant": "",
              "stringId": "1865029326943887360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Clearly, someone objects.",
              "maleVariant": "",
              "stringId": "1865029933725458432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who activated the lockdown?!",
              "maleVariant": "",
              "stringId": "1865030265780117504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aaaarrrghh!",
              "maleVariant": "",
              "stringId": "1865038608200441856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The removal of Yorinobu from his position as CEO of Arasaka.",
              "maleVariant": "",
              "stringId": "1866466116045185024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did I hear you correctly?",
              "maleVariant": "",
              "stringId": "1866473600847781888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Used to work a few floors down – Abernathy's department.",
              "maleVariant": "",
              "stringId": "1872075979106361344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I know \"shrugging your shoulders and finding a scapegoat\" just isn't correct procedure when the CEO dies.",
              "maleVariant": "",
              "stringId": "1872076665529376768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm an eyewitness to what happened. You should ask me questions, then proceed with apprehending Yorinobu.",
              "maleVariant": "",
              "stringId": "1872078813432455168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "These your people?",
              "maleVariant": "",
              "stringId": "1905582051571023872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In a manner of speaking.",
              "maleVariant": "",
              "stringId": "1905582686051778560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V? Your place.",
              "maleVariant": "",
              "stringId": "1914104974943535108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will you sit?",
              "maleVariant": "",
              "stringId": "1914104975060975620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whether she sits or stands, this meeting is a farce.",
              "maleVariant": "Whether he sits or stands, this meeting is a farce.",
              "stringId": "1914104975178416128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Absurd!",
              "maleVariant": "",
              "stringId": "2000974106692755456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you knew of this. All of you.",
              "maleVariant": "",
              "stringId": "2007155863423098880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meaning?",
              "maleVariant": "",
              "stringId": "2025682974039416832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To what extent they have faith in me, or my money – I do not know.",
              "maleVariant": "",
              "stringId": "2025683231888449536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guess it'd be all the same to them, wouldn't it?",
              "maleVariant": "",
              "stringId": "2025683680225992704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Anyway, motivations aside, what's important's that they shoot in the right direction.",
              "maleVariant": "",
              "stringId": "2025720647395385344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are never one to mince words.",
              "maleVariant": "",
              "stringId": "2025739414993182720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's why you brought me here.",
              "maleVariant": "",
              "stringId": "2025740170622214144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As a last resort – and only then.",
              "maleVariant": "",
              "stringId": "2025748002562187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You mean when the board rejects every word out of our mouths?",
              "maleVariant": "",
              "stringId": "2025753643615854592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"If\" – not \"when.\"",
              "maleVariant": "",
              "stringId": "2025770605951655936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If we cannot convince them, we could lose control of the situation.",
              "maleVariant": "",
              "stringId": "2025797265853849600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You got a good memory.",
              "maleVariant": "",
              "stringId": "2085340488626421764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is she?! And... a-am I seeing right, she's armed?!",
              "maleVariant": "Who is he?! And... am I seeing right, he's armed?!",
              "stringId": "2257441266453553152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What this, now?!",
              "maleVariant": "",
              "stringId": "2257444837886631944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Security? We've had an intrusion.",
              "maleVariant": "",
              "stringId": "2257445247066152968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is this… person?!",
              "maleVariant": "",
              "stringId": "2257445434551541768"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}