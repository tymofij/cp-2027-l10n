  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, nice to see you.",
              "maleVariant": "",
              "stringId": "1697943261782384640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Sure.",
              "maleVariant": "",
              "stringId": "1697944056284225540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wouldn't mind one.",
              "maleVariant": "",
              "stringId": "1697945210154033152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm good, thanks.",
              "maleVariant": "",
              "stringId": "1697945291003437056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] No thanks.",
              "maleVariant": "",
              "stringId": "1697945770798260224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna take part in the ceremony.",
              "maleVariant": "",
              "stringId": "1697948702516092932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna take part in the ceremony, but... wanna do it the right way – in line with tradition.",
              "maleVariant": "",
              "stringId": "1697949664689430528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could help you with that.",
              "maleVariant": "",
              "stringId": "1697949734381985792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Oye\" b=\"\" a=\". Sit, sit. You want a beer?\"/>",
              "maleVariant": "",
              "stringId": "1702514181381464064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Drink]",
              "maleVariant": "",
              "stringId": "1702514922464649216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme help you.",
              "maleVariant": "",
              "stringId": "1722525784814297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Salud.\" t=\"Cheers.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1825605937628237824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"No hay bronca.\" t=\"No problem.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1825607631439187968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been better.",
              "maleVariant": "",
              "stringId": "1864865408376164356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanging in there.",
              "maleVariant": "",
              "stringId": "1864865530866618368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Been better, to tell you the truth.",
              "maleVariant": "",
              "stringId": "1864866024754302976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hangin' in there, somehow?",
              "maleVariant": "Hangin' in there, somehow.",
              "stringId": "1864866333522186240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is happening?",
              "maleVariant": "",
              "stringId": "1864871197220327424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That night, when... when Jackie fell... Let's just say things haven't exactly been easy since.",
              "maleVariant": "That night, when Jackie fell... Let's just say things haven't exactly been easy since.",
              "stringId": "1864871267181318144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Yo sé cómo te sientes. Todos lo sabemos.\" t=\"I know how you feel. We all do.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864871351604269056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somehow.",
              "maleVariant": "",
              "stringId": "1864871887401439232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Nuestra casa es tu casa.\" b=\"You could have come over for dinner. \" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864871950198558720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm. But OK – we put it behind us, hm?",
              "maleVariant": "",
              "stringId": "1864872077336301568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"mi niña.\" b=\"But your heart – it's in the right place, \" a=\"\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"mi niño.\" b=\"Mh, but your heart is in the right place, \" a=\"\"/>",
              "stringId": "1864881922038878208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fff, don't worry, it's symbolic. Look for something in his garage. His past is all there.",
              "maleVariant": "",
              "stringId": "1864881973729480704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His garage?",
              "maleVariant": "",
              "stringId": "1864883745504153604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Jackie had a garage?",
              "maleVariant": "",
              "stringId": "1864885510282727424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, don't mention it, V. You have respect. I admire that.",
              "maleVariant": "",
              "stringId": "1864886540772888576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He used it for his motorcycle, mostly. But not so long ago he moved in. You'll find something there, I'm sure.",
              "maleVariant": "",
              "stringId": "1864886962216554496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't mind me digging through his stuff?",
              "maleVariant": "You won't mind me digging through his stuff?",
              "stringId": "1864887180974673920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"No, mija.\" b=\"\" a=\". He's gone. And funerals are for the living, not the dead. <Rich color=\\\"MainColors.ActiveBlue\\\">Toma</>, take the key.\"/>",
              "maleVariant": "<mothertongue l=\"mex\" m=\"No, mijo.\" b=\"\" a=\" He's gone. And funerals are for the living, not the dead. <Rich color=\\\"MainColors.ActiveBlue\\\">Toma</>, take the key.\"/>",
              "stringId": "1864887304723419136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ahí te veo.\" t=\"See you.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1864889318509432832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A lot of time has passed. How you been?",
              "maleVariant": "",
              "stringId": "1896490727361007616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am in the middle of preparing an ofrenda. There is still so much to take care of...",
              "maleVariant": "",
              "stringId": "1924407058265731072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Gracias, mi cielo\" b=\"\" a=\", but I will manage.\"/>",
              "maleVariant": "",
              "stringId": "1926981639971205120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It's enough you come to say goodbye.",
              "maleVariant": "",
              "stringId": "1926985406405865472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But somethin' I-I'd lay by his casket, that'd do justice to his memory – I... I don't got anything.",
              "maleVariant": "But something I'd lay by his casket... That'd do justice to his memory, I – I don't got anything...",
              "stringId": "1927026960986206208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm...?",
              "maleVariant": "",
              "stringId": "1928580049748815872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, Mama Welles.",
              "maleVariant": "",
              "stringId": "1928581776711544832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What's an ofrenda?",
              "maleVariant": "",
              "stringId": "1934506005374103552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An ofrenda? Wh-what's that, exactly?",
              "maleVariant": "",
              "stringId": "1934506268591845376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, friends and family… we gather to celebrate the departed, our memories of them. And we bring things they loved in life and might need in the after.",
              "maleVariant": "",
              "stringId": "1934507718629847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure some beer'd be at the top of Jackie's list.",
              "maleVariant": "",
              "stringId": "1934508314640445440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heheheh. And there will be beer.",
              "maleVariant": "",
              "stringId": "1934526562161324032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The garage is on Republic.",
              "maleVariant": "",
              "stringId": "1934545599184433152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take key and stand] Thanks.",
              "maleVariant": "",
              "stringId": "1943264135333453824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The pool table, we got to get rid of it.",
              "maleVariant": "",
              "stringId": "1976395134477742080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really…? It's pretty massive.",
              "maleVariant": "",
              "stringId": "1976445675203407872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So...?",
              "maleVariant": "",
              "stringId": "1976446243716149248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe we scooch it, put it up against the wall?",
              "maleVariant": "",
              "stringId": "1976446577716965376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, it's me.",
              "maleVariant": "",
              "stringId": "1983628376209567744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"sí\" b=\"It's Montserrat Welles. The Coyote, \" a=\".\"/>",
              "maleVariant": "",
              "stringId": "1983631029392060420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Vamos, responde, carajo.\" t=\"C'mooon, answer.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1983631138360078340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is me. I'm having an ofrenda for Jackie.",
              "maleVariant": "",
              "stringId": "1983631263484555268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I call you many times. You don't answer, you don't call back…",
              "maleVariant": "",
              "stringId": "1988173848277590016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am having an ofrenda. I must send my son off.",
              "maleVariant": "",
              "stringId": "1998391940310036480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, consider yourself invited.",
              "maleVariant": "",
              "stringId": "1998392346553544704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. See you then.",
              "maleVariant": "",
              "stringId": "1998392598832541696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I need a special delivery.",
              "maleVariant": "",
              "stringId": "1998395163884318720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beer – an even dozen cases. Sure, yes, ten and two of the ginger stuff.",
              "maleVariant": "",
              "stringId": "1998395575093882880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"mex\" m=\"Sí\" b=\"\" a=\", lemons and limes to match.\"/>",
              "maleVariant": "",
              "stringId": "1998396004741607424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tequila… ayy, fuck it, a dozen cases too…",
              "maleVariant": "",
              "stringId": "1998396724970713088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm...",
              "maleVariant": "",
              "stringId": "1998405380252561408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh... You can't?",
              "maleVariant": "",
              "stringId": "1998408648806559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I see.",
              "maleVariant": "",
              "stringId": "1998408909239283712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Pfff. Bai.\" t=\"Pff. Bye.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1998409164420739072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hrghh... One heavy... ugh... bastard.",
              "maleVariant": "",
              "stringId": "2238924446696128512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hrgh... argh, yeah... C'mon, chingado…",
              "maleVariant": "",
              "stringId": "2238983336569782272"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}