      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You there! Miss! Did Dakota send you?",
              "maleVariant": "You there! Sir! Did Dakota send you?",
              "stringId": "1795243207413358592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That's right. You got the goods?",
              "maleVariant": "",
              "stringId": "1795243702727106560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hands in the air! Right fucking now!",
              "maleVariant": "",
              "stringId": "1795308339233447936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right here. Dynalar SuperKidney, especially for sickle cell anemics Just like we agreed.",
              "maleVariant": "",
              "stringId": "1795311382200999936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They never make it easy, do they?",
              "maleVariant": "",
              "stringId": "1795313040326811648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I won't if you can give me an explanation. And it better be fucking good.",
              "maleVariant": "",
              "stringId": "1795334517411438596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don't shoot! Don't shoot!",
              "maleVariant": "",
              "stringId": "1795338754933547008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The phone call with Dakota... it was tapped... the police â€“ they made me do it!",
              "maleVariant": "",
              "stringId": "1795339108026834944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Won't let you get away with this.",
              "maleVariant": "",
              "stringId": "1795342351347900420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'll spare you this time.",
              "maleVariant": "",
              "stringId": "1795342827166523392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did anyone tamper with the implant?",
              "maleVariant": "",
              "stringId": "1795343016413519872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Unlike the badges, I don't kill people without a good reason.",
              "maleVariant": "",
              "stringId": "1795343840007053312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, God... Thank you... Thank you...",
              "maleVariant": "",
              "stringId": "1795346978151751680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Decisions, decisions... We've all gotta make 'em.",
              "maleVariant": "",
              "stringId": "1795347310206410756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, no, no, please, no!",
              "maleVariant": "",
              "stringId": "1795347310223187968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait! Wait!",
              "maleVariant": "",
              "stringId": "1795347752437047296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No one's touched this implant apart from you, right?",
              "maleVariant": "",
              "stringId": "1795348236560392196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think carefully now. I don't want to have to jack into you and find out for myself.",
              "maleVariant": "",
              "stringId": "1795348236560392204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You'll pay for this, motherfucker.",
              "maleVariant": "",
              "stringId": "1795350143962079236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out of my sight.",
              "maleVariant": "",
              "stringId": "1795350633705791488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get out of my sight.",
              "maleVariant": "",
              "stringId": "1795351658290372608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, Mother of God, thank you, thank you...",
              "maleVariant": "",
              "stringId": "1795351658290372616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This better be worth it...",
              "maleVariant": "",
              "stringId": "1795352815180079104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\"First, do no harm.\" Ever hear that phrase?",
              "maleVariant": "\"First, do no harm.\" You ever hear that phrase?",
              "stringId": "1851635880394907652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, it wasn't my idea. I didn't want to...",
              "maleVariant": "",
              "stringId": "1851635880394907660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eddies'll get to you--",
              "maleVariant": "Eddies'll get to you in--",
              "stringId": "1853225931964207104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, yes, later, I know. I have to go now. I'm operating.",
              "maleVariant": "",
              "stringId": "1853226259338022912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice try. Badges wouldn't go to all that trouble over a kidney implant.",
              "maleVariant": "Hah! Nice try. Badges wouldn't go to all that trouble over a kidney implant.",
              "stringId": "1853232444342587392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait! Please! It's the truth! They said they're rounding up every last Raffen Shiv member in Night City!",
              "maleVariant": "",
              "stringId": "1853234480173862912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But Dakota isn't... Never mind. You won't get it.",
              "maleVariant": "But Dakota isn't-- Ah, fuck it, never mind. You won't get it.",
              "stringId": "1853234902137622528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Please, let me go! I... I didn't want to get mixed up in this! I had no choice!",
              "maleVariant": "",
              "stringId": "1853235471908655104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How do you take your lead? In your head, or in your leg?",
              "maleVariant": "Now, how do you take your lead? In your head, or in your leg?",
              "stringId": "1853238847601954816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let's find out, shall we?",
              "maleVariant": "",
              "stringId": "1853239934899769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should be thankin' me, not God. Isn't that what docs always say?",
              "maleVariant": "",
              "stringId": "1853248889772363776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The SuperKidney... the police put a virus onto it... Use the firmware from this shard if you want the patient to live.",
              "maleVariant": "",
              "stringId": "1853249532272627712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard without a word]",
              "maleVariant": "",
              "stringId": "1853251674941214724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard] Thanks for your honesty.",
              "maleVariant": "",
              "stringId": "1853252066051674112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your honesty's much appreciated.",
              "maleVariant": "",
              "stringId": "1853252931990900736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It... it's the least I owed you.",
              "maleVariant": "",
              "stringId": "1853253525266812928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, well, uh... I can't say for certain...",
              "maleVariant": "",
              "stringId": "1853255418944737280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The police, they... they put a virus on the SuperKidney. The original firmware is on this shard... You'll need to swap them out.",
              "maleVariant": "",
              "stringId": "1853256554963587072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lame excuse.",
              "maleVariant": "",
              "stringId": "1853258551854944256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Out of my sight!",
              "maleVariant": "",
              "stringId": "1853265443532623872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard]",
              "maleVariant": "",
              "stringId": "2111164456553570308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure did.",
              "maleVariant": "",
              "stringId": "2128584841188237316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why shouldn't I? Talk. Now.",
              "maleVariant": "",
              "stringId": "2167619436340469764"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}