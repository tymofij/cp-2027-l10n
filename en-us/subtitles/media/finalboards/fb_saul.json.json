        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, V. Saul speaking, from the Aldecaldos.",
              "maleVariant": "",
              "stringId": "1790695726440685572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, V. Um, Saul speaking.",
              "maleVariant": "",
              "stringId": "1790695726491017220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, V. Um, Saul speaking.",
              "maleVariant": "",
              "stringId": "1790695726524571652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Um, I heard some big noise comin' out of Night City with, uh, you involved.",
              "maleVariant": "",
              "stringId": "1790933088948506624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No idea what that was all about... But um, if you're ever in trouble and need a hand or a place to disappear – we're here.",
              "maleVariant": "",
              "stringId": "1790933316816654336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You saved an Aldecaldo's life, and we will never forget that. It's a debt we're happy to owe.",
              "maleVariant": "",
              "stringId": "1790933526582185984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stay in touch, V.",
              "maleVariant": "",
              "stringId": "1790933720023486464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heard about something big happening in Night City – with a certain V in the lead role.",
              "maleVariant": "",
              "stringId": "1790934765562810368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Um, don't know what all that was about, or what you've got goin' on... but if you ever need a hand, our camp's your second home.",
              "maleVariant": "",
              "stringId": "1790934957074731008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Head up, eyes forward, V.",
              "maleVariant": "",
              "stringId": "1790935335031853056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I will never forget what you did for us, and for me. It's a debt I'm happy to owe you.",
              "maleVariant": "",
              "stringId": "1790935825044000768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heard about somethin' big happening in Night City – with a certain V in the lead role.",
              "maleVariant": "",
              "stringId": "1790937466375819264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, don't know what you plan to do now... but just remember – we always take care of our own.",
              "maleVariant": "",
              "stringId": "1790937702867456000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And, um, you're one of us now, V. You're an Aldecaldo. Remember that.",
              "maleVariant": "",
              "stringId": "1790938071160901632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take care of yourself, V. And see you around.",
              "maleVariant": "",
              "stringId": "1790938509381783552"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}