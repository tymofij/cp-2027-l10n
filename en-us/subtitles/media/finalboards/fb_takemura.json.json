        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"もしもし、Vか\" t=\"Good evening, V.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1790682030662393860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before samurai committed seppuku, they wrote jisei. A death poem.",
              "maleVariant": "",
              "stringId": "1790682030712725508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am calling to say goodbye.",
              "maleVariant": "",
              "stringId": "1790922098093641728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka-sama... has a new task for me. A task that would require me to keep to the shadows.",
              "maleVariant": "",
              "stringId": "1790922338477592576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For both of us, I hope our paths never cross.",
              "maleVariant": "",
              "stringId": "1790922521399578624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And I hope you will shape your own destiny.",
              "maleVariant": "",
              "stringId": "1790922941131968512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"jpn\" o=\"達者でな\" t=\"Goodbye.\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1790923083771858944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It contained their final thoughts. About life. About death.",
              "maleVariant": "",
              "stringId": "1790924310018576384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have read many of them of late. Some are truly works of beauty.",
              "maleVariant": "",
              "stringId": "1790924510388867072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yet, I am not a samurai, so I bid you farewell with simple words.",
              "maleVariant": "",
              "stringId": "1790924744598802432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<mothertongue l=\"jpn\" m=\"kuso-ama\" b=\"Rot in hell, \" a=\".\"/>",
              "maleVariant": "<mothertongue l=\"jpn\" m=\"kuso-yarou\" b=\"Rot in hell, \" a=\".\"/>",
              "stringId": "1790925161076412416"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
