        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City. Our home. Our pride.",
              "maleVariant": "",
              "stringId": "1792227944698912768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From the moment I became your mayor, unemployment has dropped three percent and the murder rate by five.",
              "maleVariant": "",
              "stringId": "1800892884618821632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But some cynics out there don't see Night City's beautiful potential as I do...",
              "maleVariant": "",
              "stringId": "1800893453181894656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do I love this city? Do I love its people? I have never loved anything--\"",
              "maleVariant": "",
              "stringId": "1800894075583053824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You placed your trust in me before. Let's work together again.",
              "maleVariant": "",
              "stringId": "1800894500482826240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I'm Weldon Holt, and I approve this message.",
              "maleVariant": "",
              "stringId": "1800894993867194368"
            }
          ]
        }
      }
    },
