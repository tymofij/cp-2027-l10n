          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, it's time for your local news. A raid on an organic meat trafficking ring, and a malaria outbreak in the Glen.",
              "maleVariant": "",
              "stringId": "1744334830032183296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Most of the district was cut off from the world for several hours as residents could not connect to the Net, local subnets or comm systems.",
              "maleVariant": "",
              "stringId": "1744341886478901248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The NCPD has urged calm, claiming the outage was a result of standard maintenance work.",
              "maleVariant": "",
              "stringId": "1744350285975314432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We continue our look at local news after the break.",
              "maleVariant": "",
              "stringId": "1744350608886390784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But first we turn to a mysterious connectivity failure in Pacifica.",
              "maleVariant": "",
              "stringId": "1744352399770312704"
            }
          ]
        }
      }
    },
