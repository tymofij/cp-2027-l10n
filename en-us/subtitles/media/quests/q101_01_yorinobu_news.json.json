          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...is ultimately to defend the well-being of my family and my employees.",
              "maleVariant": "",
              "stringId": "1792139386969563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is why I have decided to double the amount of our active Night City security forces.",
