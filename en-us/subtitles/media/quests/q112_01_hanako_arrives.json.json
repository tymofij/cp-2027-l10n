            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Flags on all city buildings were lowered to half-mast and all major public events have been postponed until further notice.",
              "maleVariant": "",
              "stringId": "1743077386448326656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Daughter and heiress, Hanako Arasaka, recently arrived in the city in the wake of the tragedy.",
              "maleVariant": "",
              "stringId": "1743077839198277632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A wake will be held at Arasaka Headquarters tomorrow at noon, after which the late CEO's remains will be transported to their final resting place back in his native Japan.",
