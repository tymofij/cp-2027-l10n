        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, hello. I'm Arif Iqbal, and you're watching WNS News.",
              "maleVariant": "",
              "stringId": "1672049718354419712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fortunately, Arasaka's security forces – under the personal direction of Yorinobu Arasaka – quickly tracked down the abductors and secured Hanako-sama.",
              "maleVariant": "",
              "stringId": "1680710522355634176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To those who raise a hand against the Arasaka family, I promise you...",
              "maleVariant": "",
              "stringId": "1680711038976446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "New details have emerged on the attempted abduction of Hanako Arasaka at the Arasaka-sponsored dashi parade.",
              "maleVariant": "",
              "stringId": "1680711838058467328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Early accounts first reported that the assailants struck as the parade was in full swing. They overwhelmed then seized control of the Arasaka heiress's parade platform.",
              "maleVariant": "",
              "stringId": "1680712023195045888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu-sama has since issued a public statement condemning the perpetrators' deplorable actions and promising swift retribution.",
              "maleVariant": "",
              "stringId": "1680712560284061696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We will rip that hand off and pour salt in the bloody stump.",
              "maleVariant": "",
              "stringId": "1680713657933418496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, looks like the gloves're comin' off.",
              "maleVariant": "",
              "stringId": "1680713922577223680"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}