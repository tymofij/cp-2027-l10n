        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good evening, Night City. We begin with, uh… uh…",
              "maleVariant": "",
              "stringId": "1672054587723259904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panic has saturated the market... This latest slump has led to losses in excess of 500 billion eurodollars.",
              "maleVariant": "",
              "stringId": "1680732233364983808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Following another attack on Arasaka's Night City HQ, the, uh, Japanese goliath has plummeted by 17 percent. And we should expect that slide to continue, say analysts.",
              "maleVariant": "",
              "stringId": "1680734240188452864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "According to a statement from the NCPD, the attack was carried out by a group of nomads who pierced the tower via a maglev railway tunnel.",
              "maleVariant": "",
              "stringId": "1680734635057008640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In light of these events, I have only one recommendation, my friends. Sell.",
              "maleVariant": "",
              "stringId": "1680735393403949056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We begin tonight with... with a look at the stock market. As always.",
              "maleVariant": "",
              "stringId": "1741544287841357824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Arasaka CEO, Yorinobu Arasaka, has since evacuated his Night City office for Tokyo.",
              "maleVariant": "",
              "stringId": "1741554562191659008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sell now, while you still can.",
              "maleVariant": "",
              "stringId": "1741555189407879168"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}