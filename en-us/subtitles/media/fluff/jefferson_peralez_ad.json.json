        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Last year, the city council of Night City absolved thirty-two corporations from their income tax obligations.",
              "maleVariant": "",
              "stringId": "1793610736917716992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Among them were Kang Tao, Arasaka, Moore Technologies and Dynalar.",
              "maleVariant": "",
              "stringId": "1803789058476589056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Put together, the gross income of these companies runs in the tens of trillions of eurodollars.",
              "maleVariant": "",
              "stringId": "1803789243176960000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The catastrophic \"City of Opportunity\" bill passed ninety-nine to one against.",
              "maleVariant": "",
              "stringId": "1803789581271416832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was that one voice of dissent.",
              "maleVariant": "",
              "stringId": "1803789797361958912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do I need to show you what'll happen to this bill if you, the people of Night City, choose me as your mayor?",
              "maleVariant": "",
              "stringId": "1803790006741614592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My name is Jefferson Peralez, and I approve this message.",
              "maleVariant": "",
              "stringId": "1803790278784172032"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}